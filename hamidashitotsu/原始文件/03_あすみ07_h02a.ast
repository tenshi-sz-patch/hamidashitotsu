astver = 2.0
ast = {block_00029={{"text"},linkback="block_00028",line=139,text={ja={{"「不过，亚澄不戴假发和彩色隐形眼镜，直接穿上服装的话，我可能会更加兴奋。不是稻草冠那种兴奋，而是火焰那种兴奋。」",{"rt2"},name={"智宏"}}}},linknext="block_00030"},block_00006={{"bg",ay=720,ax=1280,time=500,y=110,by=-360,path=":ev/ev_asu_h34/",bx=-640,file="zev_asu_h34_09",x=-550,set="ev_asu_h34"},{"text"},linkback="block_00005",line=44,text={ja={{"「嗯呐，呼啊，啊，前辈……好厉害呢，在我的脑海中一片空白……！」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10823"}}},linknext="block_00007"},block_00026={{"text"},linkback="block_00025",line=130,text={ja={{"「如果中途掉下来了，被前辈笑话的话，就会变得不只是色情的事情了，所以我很担心。」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10854"}}},linknext="block_00027"},block_00007={{"text"},linkback="block_00006",line=47,text={ja={{"「啊啊，嗯，每次前辈进来的时候，我就……！啊，前辈……啊，啊啊……！」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10824"}}},linknext="block_00008"},block_00018={{"text"},linkback="block_00017",line=94,text={ja={{"「因为自己都听不懂在说什么……无法进行实况报道，真对不起」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10832"}}},linknext="block_00019"},block_00009={{"text"},linkback="block_00008",line=53,text={ja={{"「是、是的……！今天，没问题的……啊，前辈……啊、啊啊……！」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10825"}}},linknext="block_00010"},block_00000={{"savetitle",text="ナイスパ！"},{"bg",time=500,file="ev_asu_h34_08",set="ev_asu_h34",path=":ev/ev_asu_h34/"},{"se",loop=1,file="seエロ_ピストン：中",id=7},{"lvo",loop=1,file="fem_asu_70237",ch="asu"},{"bgm",file="bgm20"},{"text"},line=18,text={ja={{"果然还是在这里里面……！",{"rt2"}}}},linknext="block_00001"},block_00004={{"text"},linkback="block_00003",line=37,text={ja={{"「我也……要出来了，一起来吧……！」",{"rt2"},name={"智宏"}}}},linknext="block_00005"},block_00030={{"text"},linkback="block_00029",line=142,text={ja={{"「会燃起火吗？」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10855"}}},linknext="block_00031"},block_00023={{"text"},linkback="block_00022",line=107,text={ja={{"要注意不要请求太多次，免得被认为是对平时的我没兴趣。也要注意不要让别人以为是因为诗姬的外表才选择了亚澄……",{"rt2"}}}},linknext="block_00024"},block_00012={{"text"},linkback="block_00011",line=62,text={ja={{"「要出来了……亚澄，要出来了！」",{"rt2"},name={"智宏"}}}},linknext="block_00013"},block_00020={{"text"},linkback="block_00019",line=100,text={ja={{"「非常兴奋，而且感觉很舒服……最重要的是能和穿着这身打扮的亚澄在一起，真开心」",{"rt2"},name={"智宏"}}}},linknext="block_00021"},block_00033={{"text"},linkback="block_00032",line=152,text={ja={{"「啊，刚才的话让我燃起来了。害羞的亚澄真可爱。可以再来一次吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00034"},block_00016={{"text"},linkback="block_00015",line=85,text={ja={{"「啊啊，在前辈面前，就连敬语也……都快忘记了……好害怕」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10831"}}},linknext="block_00017"},block_00002={{"bg",time=400,set="ev_asu_h34",sync=0,path=":ev/ev_asu_h34/",face="1",file="ev_asu_h34_09"},{"se",time=500,id=7,stop=1},{"lvo",loop=1,file="fem_asu_70238",ch="asu"},{"se",loop=1,file="seエロ_ピストン：強",id=6},{"text"},linkback="block_00001",line=31,text={ja={{"「啊，啊啊，前辈，好厉害……啊，啊啊，前辈，好厉害……！我的里面已经一团糟了……！」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10821"}}},linknext="block_00003"},block_00008={{"text"},linkback="block_00007",line=50,text={ja={{"「亚澄……要、要出来了……！」",{"rt2"},name={"智宏"}}}},linknext="block_00009"},block_00017={{"bg",time=400,set="ev_asu_h34",sync=0,path=":ev/ev_asu_h34/",face="1",file="ev_asu_h34_11"},{"text"},linkback="block_00016",line=91,text={ja={{"听到那样的话，身体前倾，力量仿佛一下子被抽走了。不过亚澄很轻，扶住她的手臂并不费力。希望肩膀不会疼。",{"rt2"}}}},linknext="block_00018"},label={top={block="block_00000",label=1}},block_00036={{"scene",file="scene11"},{"bgm",time=3000,stop=1},{"se",id=-1,time=3000,stop=1},{"vostop",time=3000,stop=1},{"ntclear"},{"cgdel",id=-1},{"bg",time=3000,file="white",path=":bg/"},{"cacheclear"},{"ex",time=0,func="wait"},{"excall",file="03_あすみ07_h02c"},{"text"},linkback="block_00035",line=163},block_00025={{"bg",sync=0,ay=720,y=110,bx=-640,time=400,set="ev_asu_h34",by=-360,path=":ev/ev_asu_h34/",face="1",file="zev_asu_h34_12",x=-550,ax=1280},{"text"},linkback="block_00024",line=127,text={ja={{"「说起来……因为是第一次，中途会不会掉下来我还挺担心的，但最后还是坚持住了……」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10853"}}},linknext="block_00026"},block_00028={{"text"},linkback="block_00027",line=136,text={ja={{"那确实有可能发生。与其说是感到沮丧，不如说连这样的失败也让人感到可爱。",{"rt2"}}}},linknext="block_00029"},block_00035={{"text"},linkback="block_00034",line=157,text={ja={{"当然了。这次要正面抱住你，好好享受穿着志希酱衣服的亚澄的原汁原味。",{"rt2"}}}},linknext="block_00036"},block_00019={{"text"},linkback="block_00018",line=97,text={ja={{"「诶？啊啊，没关系……而且，你为了我努力了呢。谢谢你」",{"rt2"},name={"智宏"}}}},linknext="block_00020"},block_00034={{"text"},linkback="block_00033",line=155,text={ja={{"「那、衣服就这样吗？」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10857"}}},linknext="block_00035"},block_00032={{"text"},linkback="block_00031",line=149,text={ja={{"「说起来，被你这么说我也感到害羞呢……肩膀和背部都是……」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10856"}}},linknext="block_00033"},block_00031={{"text"},linkback="block_00030",line=146,text={ja={{"「对。总之，看到露出肩膀的亚澄，我会感受到一种性感。」",{"rt2"},name={"智宏"}}}},linknext="block_00032"},block_00001={{"text"},linkback="block_00000",line=20,text={ja={{"虽然电子存在可以外出，但中出只能在现在。我紧紧握住亚澄的手臂，加快了腰部的动作。",{"rt2"}}}},linknext="block_00002"},block_00027={{"text"},linkback="block_00026",line=133,text={ja={{"「哈哈哈。」",{"rt2"},name={"智宏"}}}},linknext="block_00028"},block_00005={{"text"},linkback="block_00004",line=39,text={ja={{"听到还有一点点的声音，亚澄使劲儿地用膝盖支撑着。实际上，站立也已经接近极限了吧。",{"rt2"}}}},linknext="block_00006"},block_00015={{"text"},linkback="block_00014",line=82,text={ja={{"「呀、啊啊啊啊啊――啊啊、呜呜、啊、这种感觉……啊啊、即使现在，还是不习惯……」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10830"}}},linknext="block_00016"},block_00003={{"text"},linkback="block_00002",line=34,text={ja={{"「啊啊，啊，如果这样动的话……！啊，哈啊，我……对不起，啊，有什么东西上来了……！」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10822"}}},linknext="block_00004"},block_00022={{"text"},linkback="block_00021",line=105,text={ja={{"那个时候该怎么请求呢？是不是说「请穿上诗姬的样子」就可以了呢？",{"rt2"}}}},linknext="block_00023"},block_00021={{"text"},linkback="block_00020",line=103,text={ja={{"「好的……如果能让前辈开心的话，下次有需要再准备吧」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10833"}}},linknext="block_00022"},block_00013={{"bg",time=500,file="ev_asu_h34_09",set="ev_asu_h34",path=":ev/ev_asu_h34/"},{"text"},linkback="block_00012",line=67,text={ja={{"「现在、现在非常地……深入到最里面了……啊、啊啊啊啊啊――！」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10828"}}},linknext="block_00014"},block_00014={{"se",time=500,id=6,stop=1},{"lvo",stop=1,ch="asu"},{"bg",time=100,sync=1,path=":bg/",id=9,file="white",lv=9},{"bg",time=300,sync=1,path=":bg/",id=9,file="white",alpha=0,lv=9},{"bg",time=500,sync=1,path=":bg/",id=9,file="white",alpha=255,lv=9},{"cgdel",id=9,lv=9},{"bg",time=1000,set="ev_asu_h34",sync=0,vague=100,file="ev_asu_h34_10",path=":ev/ev_asu_h34/"},{"se",time=0,id=9,file="seエロ_射精中出し5",pan=0,vol=100},{"ex",time=100,func="wait"},{"text"},linkback="block_00013",line=79,text={ja={{"「呼唔、啊、啊啊啊啊啊――！前辈，啊、啊啊啊啊啊――！」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10829"}}},linknext="block_00015"},block_00011={{"text"},linkback="block_00010",line=59,text={ja={{"「啊，前辈……！好厉害呀……！啊，前辈……啊、啊啊啊啊……！」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10827"}}},linknext="block_00012"},block_00010={{"text"},linkback="block_00009",line=56,text={ja={{"「啊，啊啊……深入里面……每次前辈插进来的时候……啊，好像，好痒痒的……！」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10826"}}},linknext="block_00011"},block_00024={{"savetitle",text="ピロートークしてもろて"},{"bg",ay=720,ax=1280,time=500,y=110,by=-360,path=":ev/ev_asu_h34/",bx=-640,file="zev_asu_h34_11",x=-550,set="ev_asu_h34"},{"text"},linkback="block_00023",line=122,text={ja={{"「啊，顺便说一下……假发一直都没有脱落呢，真厉害。」",{"rt2"},name={"智宏"}}}},linknext="block_00025"}}