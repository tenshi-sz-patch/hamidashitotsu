astver = 2.0
ast = {block_00282={{"fg",mode=1,ch="あすみ",id=20,face="a0008h",lv=2.3999999999999999,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0010",head="asu_z2a",file="asu_z2a0210",hoho="h"},{"text"},line=1486,linkback="block_00281",text={vo={{"vo",file="fem_asu_10549",ch="asu"}},ja={{"「压住我……只、只要是学长的话」",{"rt2"},name={"あすみ"}}}},linknext="block_00283"},block_00022={{"fg",mode=3,ch="妃愛",id=13,face="b0028",lv=2.2000000000000002,size="no",path=":fg/hiy/no/",x=0,head="hiy_nob",ex05="hiy_nob0000",file="hiy_nob1000"},{"fgact",ch="妃愛",act="おじぎ",lv=2.2000000000000002,id=13},{"text"},linkback="block_00021",delay={vl01={{"fg",mode=3,ch="妃愛",id=13,face="b0002",lv=2.2000000000000002,size="no",path=":fg/hiy/no/",x=0,head="hiy_nob",ex05="hiy_nob0000",file="hiy_nob1000"}}},line=134,linknext="block_00023",text={vo={{"vo",file="fem_hiy_10152",ch="hiy"}},ja={{"「我看到的应该有负两亿种吧。可能达到两兆？可能已经超过兆了」",{"rt2"},name={"妃愛"}}}}},block_00041={{"fg",mode=1,ch="妃愛",face="b0002",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1b",ex05="hiy_z1b0000",file="hiy_z1b1000"},{"text"},linkback="block_00040",delay={vl01={{"fg",mode=1,ch="妃愛",face="b0039",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1b",ex05="hiy_z1b0000",file="hiy_z1b1000"}},vl02={{"fg",mode=1,ch="妃愛",face="b0010",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1b",ex05="hiy_z1b0000",file="hiy_z1b1000"}}},line=229,linknext="block_00042",text={vo={{"vo",file="fem_hiy_10161",ch="hiy"}},ja={{"「毕业典礼她可能会爬着也要来，但是之后的庆祝活动我想她可能无法参加。如果是在我们或者诗樱前辈的房间还好……里姐，你已经预定了店面了吗？」",{"rt2"},name={"妃愛"}}}}},block_00014={{"bgm",stop=1},{"bg",file="white",path=":bg/"},{"text"},line=85,linkback="block_00013",text={ja={{"那是上周……还是二月的、节假日前。",{"rt2"}}}},linknext="block_00015"},block_00087={{"fg",mode=3,ch="あすみ",id=10,face="b0042",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=280,head="asu_z1b",ex05="asu_z1b0001",file="asu_z1b1101"},{"fgact",ch="あすみ",act="うなづく",lv=2.2999999999999998,id=10},{"text"},line=496,linkback="block_00086",text={vo={{"vo",file="fem_asu_10462",ch="asu"}},ja={{"「是，长官！」",{"rt2"},name={"あすみ"}}}},linknext="block_00088"},block_00240={{"text"},line=1308,linkback="block_00239",text={ja={{"她穿着短裙坐着，双膝抱在胸前，露出了一副无比羞涩的样子，我不禁倒吸了一口冷气。从臀部到内裤全都暴露在我的眼前。",{"rt2"}}}},linknext="block_00241"},block_00201={{"fg",mode=1,ch="あすみ",face="a0022",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0010",file="asu_noa0210"},{"text"},linkback="block_00200",delay={vl01={{"fg",mode=1,ch="あすみ",face="a0031",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0010",file="asu_noa0210"}}},line=1137,linknext="block_00202",text={vo={{"vo",file="fem_asu_10514",ch="asu"}},ja={{"「听到你的声音就感到非常安心……而且，像这样把腿放在你的膝盖上也是。一直保持接触，我就完全不会感到悲伤……」",{"rt2"},name={"あすみ"}}}}},block_00103={{"se",time=500,id=1,stop=1},{"fg",mode=1,ch="妃愛",face="b0045",id=20,size="no",path=":fg/hiy/no/",lv=2.2000000000000002,head="hiy_nob",ex05="hiy_nob0000",file="hiy_nob0700"},{"text"},line=610,linkback="block_00102",text={vo={{"vo",file="fem_hiy_10177",ch="hiy"}},ja={{"「为什么呀呀呀？不用那么麻烦，我可以让你用我们家的洗衣机啊啊啊？难道你不想和哥哥的衣服一起洗吗！？」",{"rt2"},name={"妃愛"}}}},linknext="block_00104"},block_00184={{"fg",mode=3,ch="あすみ",face="b0002",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0200"},{"text"},line=1071,linkback="block_00183",text={vo={{"vo",file="fem_asu_10509",ch="asu"}},ja={{"「前辈，对不起……呜呜」",{"rt2"},name={"あすみ"}}}},linknext="block_00185"},block_00003={{"fg",mode=3,ch="あすみ",id=13,face="a0014",lv=2.2000000000000002,size="no",path=":fg/asu/no/",x=0,head="asu_noa",ex05="asu_noa0010",file="asu_noa0210"},{"text"},line=35,linkback="block_00002",text={vo={{"vo",file="fem_asu_10448",ch="asu"}},ja={{"「前辈还有和同学们的交往，却因为我而忙得不可开交」",{"rt2"},name={"あすみ"}}}},linknext="block_00004"},block_00048={{"fg",mode=1,ch="妃愛",face="a0055",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"},{"text"},linkback="block_00047",delay={vl01={{"fg",mode=1,ch="妃愛",face="a0019",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"}}},line=259,linknext="block_00049",text={vo={{"vo",file="fem_hiy_10164",ch="hiy"}},ja={{"「当然可以。不过我说的不仅是晚餐，而是让她在我们家住到毕业典礼那天，怎么样？」",{"rt2"},name={"妃愛"}}}}},block_00200={{"fg",mode=1,ch="あすみ",face="b0007",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0200"},{"text"},line=1131,linkback="block_00199",text={vo={{"vo",file="fem_asu_10513",ch="asu"}},ja={{"「那个，我已经没事了。听着你现在的话，还有你让我把腿放在你的膝盖上，我已经恢复精神了」",{"rt2"},name={"あすみ"}}}},linknext="block_00201"},block_00211={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg007a",path=":bg/",sync=0,ax=1280,ay=720},{"fg",mode=3,sync=0,ch="あすみ",id=13,lv=2.2999999999999998,resize=1,face="b0002h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0000",head="asu_z1b",file="asu_z1b0200",hoho="h"},{"extrans"},{"bgm",file="bgm18"},{"text"},line=1185,linkback="block_00210",text={ja={{"「那个，虽然亚澄的行李在我家，但我们为什么会来到亚澄的房间呢……」",{"rt2"},name={"智宏"}}}},linknext="block_00212"},block_00135={{"text"},line=817,linkback="block_00134",text={ja={{"看着亚澄害羞却又开心地微笑的脸庞，我突然有了自信，觉得此刻自己可以用恋爱论彻底击败斯坦达尔。爱人的笑容是最强的回复。",{"rt2"}}}},linknext="block_00136"},block_00239={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="b0023h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2b0000",head="asu_z2b",file="asu_z2b0200",hoho="h"},{"text"},line=1306,linkback="block_00238",text={vo={{"vo",file="fem_asu_10532",ch="asu"}},ja={{"「如果先辈喜欢这样……这样可以吗」",{"rt2"},name={"あすみ"}}}},linknext="block_00240"},block_00216={{"text"},line=1205,linkback="block_00215",text={ja={{"「所以我们才会在亚澄的房间里」",{"rt2"},name={"智宏"}}}},linknext="block_00217"},block_00132={{"fg",mode=3,ch="あすみ",id=13,face="a0009",lv=2.2000000000000002,size="no",path=":fg/asu/no/",x=0,head="asu_noa",ex05="asu_noa0010",file="asu_noa0710"},{"text"},linkback="block_00131",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="a0029",lv=2.2000000000000002,size="no",path=":fg/asu/no/",x=0,head="asu_noa",ex05="asu_noa0010",file="asu_noa0710"},{"fgact",ch="あすみ",act="おじぎ",lv=2.2000000000000002,id=13}},vl02={{"fg",mode=3,ch="あすみ",id=13,face="a0038",lv=2.2000000000000002,size="no",path=":fg/asu/no/",x=0,head="asu_noa",ex05="asu_noa0010",file="asu_noa0710"}}},line=800,linknext="block_00133",text={vo={{"vo",file="fem_asu_10482",ch="asu"}},ja={{"「明天就是毕业典礼了，真对不起！本来想和小狗聊一聊的……那个」",{"rt2"},name={"あすみ"}}}}},block_00033={{"fg",mode=1,ch="資正",face="a0003",size="no",path=":fg/suk/no/",id=20,head="suk_noa",lv=2.2000000000000002,file="suk_noa0000"},{"text"},line=185,linkback="block_00032",text={vo={{"vo",file="etc_suk_10005",ch="suk"}},ja={{"「咬」",{"rt2"},name={"資正"}}}},linknext="block_00034"},block_00202={{"fg",mode=1,ch="あすみ",face="a0019",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0010",file="asu_noa0210"},{"text"},linkback="block_00201",delay={vl01={{"fg",mode=1,ch="あすみ",face="a0002",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0000",file="asu_noa0200"}}},line=1143,linknext="block_00203",text={vo={{"vo",file="fem_asu_10515",ch="asu"}},ja={{"「我想我也能吃饭了。因为没吃午饭，所以现在开始有点饿了。只是那个，如果要去吃饭的话，我希望我们能手牵手」",{"rt2"},name={"あすみ"}}}}},block_00091={{"bg",time=500,file="bg002a",path=":bg/"},{"text"},line=531,linkback="block_00090",text={ja={{"就这么开始了，被萌得快发狂的我，飞快地跑向了资正。果然，资正以全力的冲撞向我猛攻过来。",{"rt2"}}}},linknext="block_00092"},block_00142={{"text"},line=862,linkback="block_00141",text={ja={{"「我没有注意到你被无法承受的孤独困扰……现在看到你看起来非常元气，真是太好了」",{"rt2"},name={"智宏"}}}},linknext="block_00143"},block_00110={{"fg",mode=1,ch="あすみ",face="b0046",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0700"},{"text"},line=650,linkback="block_00109",text={vo={{"vo",file="fem_asu_10472",ch="asu"}},ja={{"「别把我的内衣乱挥啊ー！」",{"rt2"},name={"あすみ"}}}},linknext="block_00111"},block_00198={{"bg",speed=150000,y2=100,x2=640,ay=720,by=-360,bx=-640,file="zbg007a",path=":bg/",ax=1280,y=100,x=-640},{"fg",mode=1,ch="あすみ",resize=1,face="b0012",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0010",file="asu_nob0210"},{"text"},line=1123,linkback="block_00197",text={vo={{"vo",file="fem_asu_10512",ch="asu"}},ja={{"「哇，为了我，让前辈错过一生中仅有的一次纪念日的庆祝活动……绝对不行……！」",{"rt2"},name={"あすみ"}}}},linknext="block_00199"},block_00178={{"text"},line=1041,linkback="block_00177",text={ja={{"在场的所有人都说「把她带到一个可以安心休息的地方去，你陪着她」，所以我们现在一起回到了亚澄的房间。行李全都交给了妃爱。",{"rt2"}}}},linknext="block_00179"},block_00024={{"text"},line=139,linkback="block_00023",text={ja={{"她是在我面前尽量表现得坚强吗？现在想想，她对我露出的笑容确实显得有些脆弱。她太可爱了，我都没注意到她其实有些失落。",{"rt2"}}}},linknext="block_00025"},block_00210={{"text"},line=1173,linkback="block_00209",text={ja={{"亚澄似乎有什么想说的，犹豫不决。她的脸颊比刚才还要红。这是，也就是说。",{"rt2"}}}},linknext="block_00211"},block_00114={{"text"},line=678,linkback="block_00113",text={ja={{"「哇！？」",{"rt2"},name={"智宏"}}}},linknext="block_00115"},block_00163={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg001c",path=":bg/",sync=0,ax=1280,ay=720},{"fg",mode=3,ch="あすみ",sync=0,resize=1,face="b0004",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0700"},{"extrans"},{"text"},line=969,linkback="block_00162",text={vo={{"vo",file="fem_asu_10502",ch="asu"}},ja={{"「但这一周……和前辈、还有日和一起度过的时间，在与学园无关的地方也能愉快地玩耍……」",{"rt2"},name={"あすみ"}}}},linknext="block_00164"},block_00053={{"fg",mode=3,ch="妃愛",id=13,face="b0013",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1b",ex05="hiy_z1b0000",file="hiy_z1b1000"},{"text"},linkback="block_00052",delay={vl01={{"fg",mode=3,ch="妃愛",id=13,face="b0054",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1b",ex05="hiy_z1b0000",file="hiy_z1b1000"}}},line=290,linknext="block_00054",text={vo={{"vo",file="fem_hiy_10167",ch="hiy"}},ja={{"「如果是在我们家，就可以说我和朋友在开睡衣派对，这样就不会成为邻居们茶余饭后的话题了。」",{"rt2"},name={"妃愛"}}}}},block_00134={{"fg",mode=3,ch="あすみ",id=13,face="a0054",lv=2.2000000000000002,size="no",path=":fg/asu/no/",x=0,head="asu_noa",ex05="asu_noa0000",file="asu_noa0700"},{"fgact",ch="あすみ",act="うなづく",lv=2.2000000000000002,id=13},{"text"},linkback="block_00133",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,lv=2.2000000000000002,face="a0013h",x=0,size="no",path=":fg/asu/no/",ex05="asu_noa0000",head="asu_noa",file="asu_noa0700",hoho="h"}}},line=810,linknext="block_00135",text={vo={{"vo",file="fem_asu_10483",ch="asu"}},ja={{"「哈、哈！谢谢……你。我也很开心……」",{"rt2"},name={"あすみ"}}}}},block_00192={{"fg",mode=3,ch="あすみ",face="b0012",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1b",ex05="asu_z1b0010",file="asu_z1b0210"},{"text"},line=1101,linkback="block_00191",text={ja={{"「所以，如果亚澄说和我相遇使她的每一天都闪耀起来，我希望她知道我也同样感激她。」",{"rt2"},name={"智宏"}}}},linknext="block_00193"},block_00229={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="a0008h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0000",head="asu_z2a",file="asu_z2a0200",hoho="h"},{"fgact",ch="あすみ",act="びっくり",lv=2.3999999999999999,id=13},{"text"},line=1268,linkback="block_00228",text={vo={{"vo",file="fem_asu_10527",ch="asu"}},ja={{"「呸，不是这样的，学长！」",{"rt2"},name={"あすみ"}}}},linknext="block_00230"},block_00025={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg002c",path=":bg/",sync=0,ax=1280,ay=720},{"fg",mode=3,ch="妃愛",sync=0,id=13,resize=1,face="a0038",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"},{"extrans",time=500},{"text"},line=149,linkback="block_00024",text={vo={{"vo",file="fem_hiy_10153",ch="hiy"}},ja={{"「放学后，我去邀请她一起去哥哥的教室，看到她在窗户边发呆。」",{"rt2"},name={"妃愛"}}}},linknext="block_00026"},block_00100={{"se",file="seドア(室内開き)",id=1},{"fg",mode=-1,mx=-40,id=13,ch="あすみ",lv=2.2999999999999998},{"text"},line=585,linkback="block_00099",text={ja={{"你不介意吗！？这种惊愕的反应，如果是在交往前，她应该会有的。亚澄现在显然很混乱，她抓起换洗衣物，像是逃跑一样向更衣室走去。",{"rt2"}}}},linknext="block_00101"},block_00092={{"cgdel",id=-1},{"bg",wait=500,file="black",path=":bg/",time=1000,vague=40,rule="rule_002"},{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg002c",path=":bg/",sync=0,ax=1280,ay=720},{"fg",mode=3,ch="あすみ",sync=0,id=13,lv=2.2999999999999998,face="b0002h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0000",head="asu_z1b",file="asu_z1b0700",hoho="h"},{"fg",mode=3,ch="妃愛",sync=0,id=4,resize=1,face="a0026",lv=2.2000000000000002,size="no",path=":fg/hiy/no/",x=520,head="hiy_noa",ex05="hiy_noa0000",file="hiy_noa0700"},{"extrans",vague=40,rule="rule_002r"},{"text"},line=549,linkback="block_00091",text={vo={{"vo",file="fem_asu_10465",ch="asu"}},ja={{"「那个，学长……我先洗澡了……！」",{"rt2"},name={"あすみ"}}}},linknext="block_00093"},block_00113={{"bgm",time=2000,stop=1},{"se",time=2000,id=-1,stop=1},{"vostop",time=2000,stop=1},{"ntclear"},{"cgdel",id=-1},{"bg",time=2000,file="black",path=":bg/"},{"cacheclear"},{"ex",func="wait",time=1000},{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg001a",zoom=120,sync=0,path=":bg/",ax=1280,ay=720},{"fg",mode=3,sync=0,ch="あすみ",id=13,lv=2.3999999999999999,resize=1,face="a0004h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0000",head="asu_z2a",file="asu_z2a0200",hoho="h"},{"extrans",rule="008b"},{"bgm",file="bgm33"},{"text"},line=675,linkback="block_00112",text={vo={{"vo",file="fem_asu_10473",ch="asu"}},ja={{"「前、前辈，早上好……今天早上是我来叫您起床的」",{"rt2"},name={"あすみ"}}}},linknext="block_00114"},block_00249={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="a0003h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1a0010",head="asu_z1a",file="asu_z1a0210",hoho="h"},{"text"},linkback="block_00248",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="a0020h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1a0010",head="asu_z1a",file="asu_z1a0210",hoho="h"}}},line=1350,linknext="block_00250",text={vo={{"vo",file="fem_asu_10536",ch="asu"}},ja={{"「上次是我在上面，为了支撑自己的身体，手都被占用了……所以与其说是用胸部「夹住」，不如说变成了「摩擦」。」",{"rt2"},name={"あすみ"}}}}},block_00156={{"text"},line=931,linkback="block_00155",text={ja={{"这个孩子直接表达出的感谢已经被提及过很多次。甚至被称为是命运。所以我也试图这样去想。",{"rt2"}}}},linknext="block_00157"},block_00064={{"fg",mode=-1,my=40,ch="妃愛",time=500,id=13,lv=2.3999999999999999},{"text"},line=343,linkback="block_00063",text={ja={{"他似乎真的没有考虑过要说这个。在回答了妃爱的请求后，他在沙发上疲惫地躺了下来。",{"rt2"}}}},linknext="block_00065"},block_00267={{"text"},line=1418,linkback="block_00266",text={ja={{"即使与那三人相比，亚澄也并不特别小……不过，这可能是我太偏心了。嗯，就算稍微小一点也没关系。",{"rt2"}}}},linknext="block_00268"},block_00001={{"fg",mode=3,ch="あすみ",id=13,face="a0028",lv=2.2000000000000002,size="no",path=":fg/asu/no/",x=0,head="asu_noa",ex05="asu_noa0000",file="asu_noa0200"},{"fgact",ch="あすみ",act="おじぎ",lv=2.2000000000000002,id=13},{"text"},line=28,linkback="block_00000",text={vo={{"vo",file="fem_asu_10447",ch="asu"}},ja={{"「嗯、嗯……对不起，给前辈添麻烦了」",{"rt2"},name={"あすみ"}}}},linknext="block_00002"},block_00128={{"text"},line=763,linkback="block_00127",text={ja={{"不过亚澄，果然还是没有好好睡觉啊。是啊，说起来她说过因为寂寞而哭泣……",{"rt2"}}}},linknext="block_00129"},block_00051={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg002c",path=":bg/",sync=0,ax=1280,ay=720},{"fg",mode=3,ch="妃愛",sync=0,id=13,face="a0023",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"},{"extrans"},{"text"},linkback="block_00050",delay={vl01={{"fg",mode=3,ch="妃愛",id=13,face="a0054",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"}},vl02={{"fg",mode=3,ch="妃愛",id=13,face="a0052",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1a",ex05="hiy_z1a0010",file="hiy_z1a1010"}}},line=278,linknext="block_00052",text={vo={{"vo",file="fem_hiy_10165",ch="hiy"}},ja={{"「锦现在最怕的就是独自一人，对吧。那么在一个不需要回去的地方，和哥哥一起度过一天就好。睡觉的时候可以在我的房间。」",{"rt2"},name={"妃愛"}}}}},block_00256={{"fg",mode=3,ch="あすみ",id=13,face="a0004",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0200"},{"text"},line=1377,linkback="block_00255",text={vo={{"vo",file="fem_asu_10539",ch="asu"}},ja={{"「里姐老师在双手都被占用的时候，想要搬运东西，就会先夹在胸口」",{"rt2"},name={"あすみ"}}}},linknext="block_00257"},block_00032={{"text"},line=180,linkback="block_00031",text={ja={{"「因为我，我很喜欢亚澄啊。呵呵」",{"rt2"},name={"智宏"}}}},linknext="block_00033"},block_00186={{"text"},line=1077,linkback="block_00185",text={ja={{"「能与这么可爱的恋人一起度过，被你如此舍不得分开，这难道不是最好的毕业礼物吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00187"},block_00148={{"text"},line=893,linkback="block_00147",text={ja={{"「嗯。现在作为会长和恋人，我会认真听你说话」",{"rt2"},name={"智宏"}}}},linknext="block_00149"},block_00063={{"fg",mode=3,ch="妃愛",id=13,face="b0045",lv=2.3999999999999999,size="z2",path=":fg/hiy/z2/",x=0,head="hiy_z2b",ex05="hiy_z2b0010",file="hiy_z2b1010"},{"fgact",ch="妃愛",act="quake",lv=2.3999999999999999,id=13},{"text"},line=339,linkback="block_00062",text={vo={{"vo",file="fem_hiy_10172",ch="hiy"}},ja={{"「呃哈！」",{"rt2"},name={"妃愛"}}}},linknext="block_00064"},block_00018={{"fg",mode=3,ch="妃愛",id=13,face="a0009",lv=2.2000000000000002,size="no",path=":fg/hiy/no/",x=0,head="hiy_noa",ex05="hiy_noa0010",file="hiy_noa1010"},{"fgact",ch="妃愛",act="いいえ",lv=2.2000000000000002,id=13},{"text"},line=112,linkback="block_00017",text={vo={{"vo",file="fem_hiy_10150",ch="hiy"}},ja={{"「我看她每天都在变得越来越消沉啊」",{"rt2"},name={"妃愛"}}}},linknext="block_00019"},block_00279={{"bg",time=500,file="bg007a",path=":bg/"},{"text"},line=1476,linkback="block_00278",text={ja={{"他双膝着地，稍微抬起臀部，形成了一个三角形的空隙。亚澄钻进那个空隙，仰面躺下。",{"rt2"}}}},linknext="block_00280"},block_00138={{"fg",mode=3,ch="あすみ",id=13,face="a0022",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0700"},{"text"},linkback="block_00137",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="a0054",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0700"}}},line=845,linknext="block_00139",text={vo={{"vo",file="fem_asu_10487",ch="asu"}},ja={{"「美味的饭菜，舒适的浴室，还有深深的睡眠……结果我整整一周都在你们家打扰了」",{"rt2"},name={"あすみ"}}}}},block_00057={{"text"},line=305,linkback="block_00056",text={ja={{"「我去和亚澄说说看……」",{"rt2"},name={"智宏"}}}},linknext="block_00058"},block_00061={{"fg",mode=3,ch="妃愛",id=13,face="a0048",lv=2.3999999999999999,size="z2",path=":fg/hiy/z2/",x=0,head="hiy_z2a",ex05="hiy_z2a0000",file="hiy_z2a1000"},{"fgact",ch="妃愛",act="いいえ",lv=2.3999999999999999,id=13},{"text"},linkback="block_00060",delay={vl01={{"fg",mode=3,ch="妃愛",id=13,face="a0022",lv=2.3999999999999999,size="z2",path=":fg/hiy/z2/",x=0,head="hiy_z2a",ex05="hiy_z2a0000",file="hiy_z2a1000"}},vl02={{"fg",mode=3,ch="妃愛",id=13,face="a0041",lv=2.3999999999999999,size="z2",path=":fg/hiy/z2/",x=0,head="hiy_z2a",ex05="hiy_z2a0010",file="hiy_z2a1010"}}},line=330,linknext="block_00062",text={vo={{"vo",file="fem_hiy_10171",ch="hiy"}},ja={{"「哼～你这个哥哥，总是这么温柔地对我说话。包括这种关心在内，我觉得自己被你和锦救赎了。说出来吧，喜欢」",{"rt2"},name={"妃愛"}}}}},block_00190={{"fg",mode=3,ch="あすみ",face="b0002",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1b",ex05="asu_z1b0010",file="asu_z1b0210"},{"text"},line=1093,linkback="block_00189",text={ja={{"「被强迫参加了学生会，在邀请亚澄和常磐加入之前我都只是每天闲混过日子打手游。而现在却可以和恋人单独在一间屋子里。」",{"rt2"},name={"智宏"}}}},linknext="block_00191"},block_00140={{"text"},line=856,linkback="block_00139",text={ja={{"「听到你这么说，我很放心。妃爱也一定和我有同样的感觉。我们两个人都很担心你，亚澄」",{"rt2"},name={"智宏"}}}},linknext="block_00141"},block_00006={{"fg",mode=3,ch="あすみ",id=13,face="b0008",lv=2.2000000000000002,size="no",path=":fg/asu/no/",x=0,head="asu_nob",ex05="asu_nob0010",file="asu_nob0210"},{"text"},line=46,linkback="block_00005",text={vo={{"vo",file="fem_asu_10449",ch="asu"}},ja={{"「啊，和我一样……」",{"rt2"},name={"あすみ"}}}},linknext="block_00007"},block_00160={{"text"},line=951,linkback="block_00159",text={ja={{"「偶尔穿着制服上学怎么样？反正我在大学也不可能交到朋友」",{"rt2"},name={"智宏"}}}},linknext="block_00161"},block_00105={{"fg",mode=1,ch="妃愛",face="b0031",id=20,size="no",path=":fg/hiy/no/",lv=2.2000000000000002,head="hiy_nob",ex05="hiy_nob0000",file="hiy_nob0700"},{"text"},linkback="block_00104",delay={vl01={{"fg",mode=1,ch="妃愛",face="b0019",id=20,size="no",path=":fg/hiy/no/",lv=2.2000000000000002,head="hiy_nob",ex05="hiy_nob0000",file="hiy_nob0700"}},vl02={{"fg",mode=1,ch="妃愛",face="a0041",id=20,size="no",path=":fg/hiy/no/",lv=2.2000000000000002,head="hiy_noa",ex05="hiy_noa0010",file="hiy_noa0710"}}},line=623,linknext="block_00106",text={vo={{"vo",file="fem_hiy_10178",ch="hiy"}},ja={{"「因为直接烘干会损伤衣物，我们家基本上都是室内晾干的……我们有专门的室内晾干洗涤剂和衣物烘干除湿器，所以你不用担心衣物会有湿气味哦！」",{"rt2"},name={"妃愛"}}}}},block_00242={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="b0028h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2b0000",head="asu_z2b",file="asu_z2b0200",hoho="h"},{"text"},line=1316,linkback="block_00241",text={vo={{"vo",file="fem_asu_10533",ch="asu"}},ja={{"「是、是的。我自己也觉得很害羞。如果对方不是学长的话，我是做不出来的。」",{"rt2"},name={"あすみ"}}}},linknext="block_00243"},block_00052={{"fg",mode=3,ch="妃愛",id=13,face="a0038",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"},{"text"},linkback="block_00051",delay={vl01={{"fg",mode=3,ch="妃愛",id=13,face="a0019",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"}}},line=284,linknext="block_00053",text={vo={{"vo",file="fem_hiy_10166",ch="hiy"}},ja={{"「你去锦的家住也行，但要考虑到邻居的眼光啊。平时去玩可能没问题，但一个人住的女孩家里过夜……」",{"rt2"},name={"妃愛"}}}}},block_00281={{"text"},line=1481,linkback="block_00280",text={ja={{"「看着亚澄从上面向下看，感觉有点奇妙……如果我现在把双手放下去，就像是压住了亚澄一样」",{"rt2"},name={"智宏"}}}},linknext="block_00282"},block_00288={{"bgm",stop=1},{"cgdel",id=-1},{"bg",time=1000,file="white",path=":bg/"},{"excall",file="03_あすみ14_h04"},{"text"},line=1507,linkback="block_00287"},block_00102={{"fg",mode=1,ch="あすみ",resize=1,face="a0034",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0000",file="asu_noa0700"},{"text"},line=603,linkback="block_00101",text={vo={{"vo",file="fem_asu_10468",ch="asu"}},ja={{"「嗯、嗯……我要先回家，自己洗……」",{"rt2"},name={"あすみ"}}}},linknext="block_00103"},block_00005={{"text"},line=41,linkback="block_00004",text={ja={{"「我和同学的关系，可能，和你在班上，以及你和你的同学的关系差不多」",{"rt2"},name={"智宏"}}}},linknext="block_00006"},block_00133={{"text"},line=803,linkback="block_00132",text={ja={{"「没事。在重要的日子前一天晚上，能和喜欢的人聊天我很开心」",{"rt2"},name={"智宏"}}}},linknext="block_00134"},block_00081={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="a0029h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0001",head="asu_z2a",file="asu_z2a1101",hoho="h"},{"text"},linkback="block_00080",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="a0014h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0001",head="asu_z2a",file="asu_z2a1101",hoho="h"},{"fgact",ch="あすみ",act="いいえ",lv=2.3999999999999999,id=13}},vl02={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="a0046h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0001",head="asu_z2a",file="asu_z2a1101",hoho="h"}}},line=442,linknext="block_00082",text={vo={{"vo",file="fem_asu_10460",ch="asu"}},ja={{"「如果我睡过头，刚醒的样子会被学长看到……呜呜呜我没那个勇气，对不起呜呜呜……」",{"rt2"},name={"あすみ"}}}}},block_00167={{"text"},line=989,linkback="block_00166",text={ja={{"「这样的话，我很欢迎呢」",{"rt2"},name={"智宏"}}}},linknext="block_00168"},block_00176={{"text"},line=1037,linkback="block_00175",text={ja={{"仪式结束后，妃爱似乎一直在我身边，给我发来了「总算没事」的消息。在最后的HR期间，我心神不宁。",{"rt2"}}}},linknext="block_00177"},block_00012={{"text"},line=77,linkback="block_00011",text={ja={{"这比我预想的要沮丧得多……正因为是能创作出超乎常人、细腻而可爱的乐曲的亚澄，我应该早点注意到她心灵的过度脆弱。",{"rt2"}}}},linknext="block_00013"},block_00099={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="a0046h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1a0000",head="asu_z1a",file="asu_z1a0700",hoho="h"},{"fgact",ch="あすみ",act="いいえ",lv=2.2999999999999998,id=13},{"text"},linkback="block_00098",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="a0012h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1a0000",head="asu_z1a",file="asu_z1a0700",hoho="h"},{"fgact",ch="あすみ",act="びっくり",lv=2.2999999999999998,id=13}}},line=578,linknext="block_00100",text={vo={{"vo",file="fem_asu_10467",ch="asu"}},ja={{"「不行啊啊啊啊啊！啊，不是……我不是不愿意！」",{"rt2"},name={"あすみ"}}}}},block_00046={{"fg",mode=1,ch="妃愛",face="a0002",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"},{"text"},linkback="block_00045",delay={vl01={{"fg",mode=1,ch="妃愛",face="a0022",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"}}},line=250,linknext="block_00047",text={vo={{"vo",file="fem_hiy_10163",ch="hiy"}},ja={{"「嗯，那样也好，但是为什么不反过来带她到我们家来？」",{"rt2"},name={"妃愛"}}}}},block_00112={{"text"},line=656,linkback="block_00111",text={ja={{"话说回来，妃爱在别人面前也叫我哥哥……那两个人真的关系很好啊。",{"rt2"}}}},linknext="block_00113"},block_00245={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="b0007h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0000",head="asu_z1b",file="asu_z1b0200",hoho="h"},{"text"},line=1333,linkback="block_00244",text={vo={{"vo",file="fem_asu_10534",ch="asu"}},ja={{"「是、是的。之前试图用胸部来让你高兴却失败了，所以这次我想让你开心。」",{"rt2"},name={"あすみ"}}}},linknext="block_00246"},block_00222={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="b0002h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0000",head="asu_z1b",file="asu_z1b0200",hoho="h"},{"text"},line=1233,linkback="block_00221",text={vo={{"vo",file="fem_asu_10524",ch="asu"}},ja={{"「所以，我想做的事情也……比平时还要多」",{"rt2"},name={"あすみ"}}}},linknext="block_00223"},block_00084={{"fg",mode=3,zoom=120,ch="あすみ",id=13,lv=2.3999999999999999,face="a0013h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0001",head="asu_z2a",file="asu_z2a1101",hoho="h"},{"text"},line=460,linkback="block_00083",text={ja={{"我告诉她我多么喜欢她，无法自拔地喜欢她，最后她点点头答应留下来过夜。",{"rt2"}}}},linknext="block_00085"},block_00226={{"text"},line=1257,linkback="block_00225",text={ja={{"啊，这种感觉，现在就觉得不妙了。",{"rt2"}}}},linknext="block_00227"},block_00162={{"text"},line=959,linkback="block_00161",text={ja={{"虽然是为了鼓励亚澄，但我感觉自己说多了。她竟然希望我玩角色扮演游戏",{"rt2"}}}},linknext="block_00163"},block_00017={{"text"},line=106,linkback="block_00016",text={ja={{"基本上还是像以前那样叫她，但最近的妃爱时不时地模仿我，直呼亚澄的名字。真希望她能停止这种恋人般的亲昵行为。",{"rt2"}}}},linknext="block_00018"},block_00117={{"text"},line=689,linkback="block_00116",text={ja={{"「没事没事。谢谢你叫我起床。今天也是一觉醒来就感觉很棒了」",{"rt2"},name={"智宏"}}}},linknext="block_00118"},block_00261={{"text"},line=1396,linkback="block_00260",text={ja={{"「嗯。啊，不，我是想问亚澄有没有这种愿望。你之前说过那时候害怕男性」",{"rt2"},name={"智宏"}}}},linknext="block_00262"},block_00074={{"text"},line=394,linkback="block_00073",text={ja={{"亚澄一边低下头，一边斜着眼睛，支支吾吾地说出了她的回答。看到她并没有因为犹豫而拒绝，真是太好了。或者说……",{"rt2"}}}},linknext="block_00075"},block_00104={{"fg",mode=1,ch="あすみ",face="a0012",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0000",file="asu_noa0700"},{"text"},linkback="block_00103",delay={vl01={{"fg",mode=1,ch="あすみ",face="a0007",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0000",file="asu_noa0700"}}},line=616,linknext="block_00105",text={vo={{"vo",file="fem_asu_10469",ch="asu"}},ja={{"「我、我并不是不想和先辈一起……！要、要是有烘干机的话我想借用一下！」",{"rt2"},name={"あすみ"}}}}},block_00154={{"fg",mode=1,ch="あすみ",face="b0020",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0700"},{"text"},linkback="block_00153",delay={vl01={{"fg",mode=1,ch="あすみ",face="b0028",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0700"}}},line=923,linknext="block_00155",text={vo={{"vo",file="fem_asu_10496",ch="asu"}},ja={{"「作为雪景式的每一天都在顺利进行，而作为锦亚澄的生活却越来越隐退到阴影中，现实生活中看不到未来，也没有和人的联系……」",{"rt2"},name={"あすみ"}}}}},block_00223={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="b0004h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0000",head="asu_z1b",file="asu_z1b0200",hoho="h"},{"text"},line=1238,linkback="block_00222",text={vo={{"vo",file="fem_asu_10525",ch="asu"}},ja={{"「这周我都很有精神，所以我已经为毕业典礼和前辈……做好了准备。你可以放心……」",{"rt2"},name={"あすみ"}}}},linknext="block_00224"},block_00098={{"fg",mode=3,ch="妃愛",id=4,face="a0045",lv=2.2000000000000002,size="no",path=":fg/hiy/no/",x=520,head="hiy_noa",ex05="hiy_noa0000",file="hiy_noa0700"},{"text"},linkback="block_00097",delay={vl01={{"fg",mode=3,ch="妃愛",id=4,face="a0043",lv=2.2000000000000002,size="no",path=":fg/hiy/no/",x=520,head="hiy_noa",ex05="hiy_noa0000",file="hiy_noa0700"}}},line=570,linknext="block_00099",text={vo={{"vo",file="fem_hiy_10175",ch="hiy"}},ja={{"「啊，你怎么说得好像是我的错。明明锦也很高兴。气死我了，明天就让哥哥和锦两个人泡吧」",{"rt2"},name={"妃愛"}}}}},block_00126={{"fg",mode=3,ch="あすみ",id=9,lv=2.2999999999999998,face="b0008h",x=-280,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0010",head="asu_z1b",file="asu_z1b0210",hoho="h"},{"fgact",ch="あすみ",act="びっくり",lv=2.2999999999999998,id=9},{"text"},linkback="block_00125",delay={vl03={{"fg",mode=3,ch="あすみ",id=9,face="b0047",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=-280,head="asu_z1b",ex05="asu_z1b0010",file="asu_z1b0210"}},vl02={{"fg",mode=3,ch="あすみ",id=9,lv=2.2999999999999998,face="b0029h",x=-280,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0010",head="asu_z1b",file="asu_z1b0210",hoho="h"}},vl01={{"fg",mode=3,ch="あすみ",id=9,lv=2.2999999999999998,face="b0014h",x=-280,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0010",head="asu_z1b",file="asu_z1b0210",hoho="h"}},vl04={{"fg",mode=3,ch="あすみ",id=9,face="a0038",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=-280,head="asu_z1a",ex05="asu_z1a0010",file="asu_z1a0210"},{"fgact",ch="あすみ",act="おじぎ",lv=2.2999999999999998,id=9}}},line=751,linknext="block_00127",text={vo={{"vo",file="fem_asu_10478",ch="asu"}},ja={{"「对、对对对，是这样的吧，说出那么厚颜无耻的话真是对不起对不起非常抱歉。难道说，我刚醒来头脑还没清醒吗……」",{"rt2"},name={"あすみ"}}}}},block_00028={{"text"},line=160,linkback="block_00027",text={ja={{"「那是一个还带着稚气的少女注视着她所爱之人的眼神……太美了，我都担心她会被雕刻成雕塑。」",{"rt2"},name={"智宏"}}}},linknext="block_00029"},block_00124={{"fg",mode=3,ch="あすみ",id=9,face="b0046",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=-280,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0200"},{"fgact",ch="あすみ",act="いいえ",lv=2.2999999999999998,id=9},{"text"},linkback="block_00123",delay={vl01={{"fg",mode=3,ch="あすみ",id=9,face="b0012",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=-280,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0200"}}},line=737,linknext="block_00125",text={vo={{"vo",file="fem_asu_10477",ch="asu"}},ja={{"「呜哇哇哇！？不行不行不行……啊，但是，如果妃爱也在的话……」",{"rt2"},name={"あすみ"}}}}},block_00275={{"text"},line=1454,linkback="block_00274",text={ja={{"「我怎么可能踩在这小小的、纤细的身体上呢。如果我坐上去，她似乎就会断掉。绝对不能将这可爱的天使当作垫子。」",{"rt2"},name={"智宏"}}}},linknext="block_00276"},block_00000={{"savetitle",text="強く儚い者たち"},{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,file="bg007a",path=":bg/"},{"fg",mode=3,ch="あすみ",sync=0,id=13,face="a0002",lv=2.2000000000000002,size="no",path=":fg/asu/no/",x=0,head="asu_noa",ex05="asu_noa0000",file="asu_noa0200"},{"extrans"},{"bgm",file="bgm06"},{"text"},line=22,linknext="block_00001",text={ja={{"「啊——到了到了。亚澄，累不累？先坐下休息一下吧」",{"rt2"},name={"智宏"}}}}},block_00090={{"fg",mode=-1,my=40,ch="妃愛",id=9,lv=2.2999999999999998},{"ex",func="wait",time=10},{"se",id=1,file="seジッパー2",vol=30},{"ex",func="wait",time=400},{"fg",mode=3,ch="あすみ",id=10,face="b0008",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=280,head="asu_z1b",ex05="asu_z1b0001",file="asu_z1b1101"},{"fgact",ch="あすみ",act="びっくり",lv=2.2999999999999998,id=10},{"text"},linkback="block_00089",delay={vl01={{"fg",mode=3,ch="あすみ",id=10,lv=2.2999999999999998,face="b0012h",x=280,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0001",head="asu_z1b",file="asu_z1b1101",hoho="h"}},vl02={{"fg",mode=3,ch="あすみ",id=10,lv=2.2999999999999998,face="b0046h",x=280,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0001",head="asu_z1b",file="asu_z1b1101",hoho="h"},{"fgact",ch="あすみ",act="いいえ",lv=2.2999999999999998,id=10}}},line=525,linknext="block_00091",text={vo={{"vo",file="fem_asu_10464",ch="asu"}},ja={{"「啊，我的行李别在这里打开！里面有各种东西……呀，被学长看到了……啊啊，内裤都露出来了！」",{"rt2"},name={"あすみ"}}}}},block_00153={{"fg",mode=1,ch="あすみ",face="b0003",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0010",file="asu_nob0710"},{"text"},linkback="block_00152",delay={vl01={{"fg",mode=1,ch="あすみ",face="b0023",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0700"}}},line=917,linknext="block_00154",text={vo={{"vo",file="fem_asu_10495",ch="asu"}},ja={{"「那个，我之前也说过……自从遇到了学长后，我的生活就像漫长的夜晚迎来了黎明一样开始闪耀……」",{"rt2"},name={"あすみ"}}}}},block_00182={{"text"},line=1064,linkback="block_00181",text={ja={{"看来一周时间里，我的智宏力量如妃爱所说的还不够充足。我倒是注入了很多爱情……哎呀，这种说法太庸俗了，还是别说了。物理上也没有注入什么。",{"rt2"}}}},linknext="block_00183"},block_00143={{"fg",mode=3,ch="あすみ",id=13,face="b0009",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0010",file="asu_z1b0710"},{"fgact",ch="あすみ",act="びっくり",lv=2.2999999999999998,id=13},{"text"},linkback="block_00142",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="b0007",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0700"}}},line=869,linknext="block_00144",text={vo={{"vo",file="fem_asu_10489",ch="asu"}},ja={{"「对、对不起！那个，以后我会没问题的！即使以后我一个人……即使前辈们和朋友出去玩或者大学社团去旅行……」",{"rt2"},name={"あすみ"}}}}},block_00054={{"fg",mode=3,ch="妃愛",id=13,face="b0019",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1b",ex05="hiy_z1b0000",file="hiy_z1b1000"},{"text"},line=295,linkback="block_00053",text={vo={{"vo",file="fem_hiy_10168",ch="hiy"}},ja={{"「在毕业前增加和哥哥共度的时间，充实一下她的「奥尼尔基」吧。」",{"rt2"},name={"妃愛"}}}},linknext="block_00055"},block_00002={{"text"},line=30,linkback="block_00001",text={ja={{"她显得筋疲力尽。虽然我预料到了这种情况。",{"rt2"}}}},linknext="block_00003"},block_00169={{"text"},line=993,linkback="block_00168",text={ja={{"嗯，明天是毕业典礼的日子……仅仅一天是不够的，看来至少要待到下周，让她待在我这里会更好。",{"rt2"}}}},linknext="block_00170"},block_00136={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg001c",path=":bg/",sync=0,ax=1280,ay=720},{"fg",mode=3,ch="あすみ",sync=0,id=13,resize=1,face="b0052",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0700"},{"extrans",time=500},{"text"},linkback="block_00135",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="b0019",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0700"}}},line=831,linknext="block_00137",text={vo={{"vo",file="fem_asu_10485",ch="asu"}},ja={{"「那个，这次真的非常感谢你们。一开始，我还觉得在前辈家里过夜这件事有些不合礼节……」",{"rt2"},name={"あすみ"}}}}},block_00233={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="a0002h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0000",head="asu_z2a",file="asu_z2a0200",hoho="h"},{"fgact",ch="あすみ",act="いいえ",lv=2.3999999999999999,id=13},{"text"},line=1284,linkback="block_00232",text={vo={{"vo",file="fem_asu_10529",ch="asu"}},ja={{"「啊，不，他们三位都说过，实际上并没有进行过那种行为的经验」",{"rt2"},name={"あすみ"}}}},linknext="block_00234"},block_00067={{"text"},line=368,linkback="block_00066",text={ja={{"他暗示强调了「这是妃爱的提议，这次并没有任何邪念」的部分。如果亚澄愿意的话那就另说，但他不能向一个虚弱的恋人提出这样的要求。",{"rt2"}}}},linknext="block_00068"},block_00175={{"text"},line=1035,linkback="block_00174",text={ja={{"里姐立刻跑过来问她要不要去保健室，但即使如此还想待到最后的亚澄，在我离场时看上去已经虚弱到极点了。",{"rt2"}}}},linknext="block_00176"},block_00043={{"fg",mode=1,ch="妃愛",face="b0046",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1b",ex05="hiy_z1b0000",file="hiy_z1b1000"},{"text"},linkback="block_00042",delay={vl01={{"fg",mode=1,ch="妃愛",face="a0007",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"}}},line=238,linknext="block_00044",text={vo={{"vo",file="fem_hiy_10162",ch="hiy"}},ja={{"「不行不行！只是进店吸一口烟就会昏过去！得想个办法，嗯……对！必须带她去高原之类的地方！像山小屋那样的地方！」",{"rt2"},name={"妃愛"}}}}},block_00217={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="a0013h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1a0000",head="asu_z1a",file="asu_z1a0200",hoho="h"},{"fgact",ch="あすみ",act="うなづく",lv=2.2999999999999998,id=13},{"text"},line=1211,linkback="block_00216",text={vo={{"vo",file="fem_asu_10521",ch="asu"}},ja={{"「嗯……」",{"rt2"},name={"あすみ"}}}},linknext="block_00218"},block_00155={{"fg",mode=1,ch="あすみ",face="b0023",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0700"},{"text"},linkback="block_00154",delay={vl01={{"fg",mode=1,ch="あすみ",face="b0054",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0700"}}},line=929,linknext="block_00156",text={vo={{"vo",file="fem_asu_10497",ch="asu"}},ja={{"「在那个时候遇到了学长，他珍视着锦亚澄，给了我再次穿上校服的机会，让我遇见了朋友们，改变了我的每一天。」",{"rt2"},name={"あすみ"}}}}},block_00123={{"fg",mode=3,ch="妃愛",id=10,face="a0023",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=280,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a0200"},{"text"},line=730,linkback="block_00122",text={vo={{"vo",file="fem_hiy_10182",ch="hiy"}},ja={{"「今晚我们三个人一起睡吧？」",{"rt2"},name={"妃愛"}}}},linknext="block_00124"},block_00079={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="b0028h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2b0001",head="asu_z2b",file="asu_z2b1101",hoho="h"},{"text"},line=427,linkback="block_00078",text={vo={{"vo",file="fem_asu_10458",ch="asu"}},ja={{"「在前辈的家里借用浴室会让我感到害羞……」",{"rt2"},name={"あすみ"}}}},linknext="block_00080"},block_00183={{"text"},line=1066,linkback="block_00182",text={ja={{"把亚澄抱起来放在膝盖上后，她似乎稍微有些精神了，并把头倚在我的肩上。当杯子靠近嘴唇时，她只喝了一小口红茶。",{"rt2"}}}},linknext="block_00184"},block_00035={{"bg",speed=150000,y2=100,x2=640,ay=720,by=-360,bx=-640,file="zbg002c",path=":bg/",ax=1280,y=100,x=-640},{"text"},line=196,linkback="block_00034",text={ja={{"「……但是话说回来，虽然我很担心亚澄会感到沮丧，但她应该已经做好从一年前开始的心理准备了吧，难道伴随离别的悲伤不是必然的吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00036"},block_00246={{"text"},line=1336,linkback="block_00245",text={ja={{"「亚澄的胸……那个？并没有失败啊？感觉很舒服啊。」",{"rt2"},name={"智宏"}}}},linknext="block_00247"},block_00204={{"text"},line=1151,linkback="block_00203",text={ja={{"「哈哈」",{"rt2"},name={"智宏"}}}},linknext="block_00205"},block_00203={{"fg",mode=1,ch="あすみ",face="a0019",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0000",file="asu_noa0200"},{"text"},line=1148,linkback="block_00202",text={vo={{"vo",file="fem_asu_10516",ch="asu"}},ja={{"「到庆功会的时候，我可能会有些任性。希望能一直待在你身边……」",{"rt2"},name={"あすみ"}}}},linknext="block_00204"},block_00127={{"text"},line=761,linkback="block_00126",text={ja={{"她害羞地低下头的模样无疑是艺术的题材。然而如果停止时间，这份可爱就无法表达出来了……！",{"rt2"}}}},linknext="block_00128"},block_00218={{"text"},line=1213,linkback="block_00217",text={ja={{"解释到这里，亚澄完全放松下来，把自己的身体交给了我。",{"rt2"}}}},linknext="block_00219"},block_00214={{"text"},line=1197,linkback="block_00213",text={ja={{"「确实，嗯。我不想让妃爱在自己家里感到尴尬或者有所顾忌……」",{"rt2"},name={"智宏"}}}},linknext="block_00215"},block_00232={{"text"},line=1278,linkback="block_00231",text={ja={{"「嗯……那三个人，果然都有丰富的实战经验吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00233"},block_00089={{"fg",mode=3,ch="あすみ",id=10,face="b0007",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=280,head="asu_z1b",ex05="asu_z1b0011",file="asu_z1b1111"},{"text"},line=511,linkback="block_00088",text={vo={{"vo",file="fem_asu_10463",ch="asu"}},ja={{"「明白了，小妹妹！但我的生日比你晚哦！」",{"rt2"},name={"あすみ"}}}},linknext="block_00090"},block_00044={{"text"},line=241,linkback="block_00043",text={ja={{"「毕业典礼后的庆祝活动在高原的山小屋？啊——不过，这听起来挺有趣的？要不要让里姐开车过去……？不，不，店面已经预定好了。」",{"rt2"},name={"智宏"}}}},linknext="block_00045"},block_00065={{"bgm",stop=1},{"cgdel",id=-1},{"bg",wait=500,file="black",path=":bg/",time=1000,vague=40,rule="rule_002"},{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg007a",path=":bg/",sync=0,ax=1280,ay=720},{"fg",mode=3,ch="あすみ",sync=0,id=13,face="a0010",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0001",file="asu_z1a1101"},{"extrans",vague=40,rule="rule_002r"},{"bgm",file="bgm03"},{"text"},line=363,linkback="block_00064",text={vo={{"vo",file="fem_asu_10452",ch="asu"}},ja={{"「诶，你是说在前辈家里住一晚……吗？」",{"rt2"},name={"あすみ"}}}},linknext="block_00066"},block_00037={{"fg",mode=1,ch="妃愛",face="b0038",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1b",ex05="hiy_z1b0000",file="hiy_z1b1000"},{"text"},linkback="block_00036",delay={vl01={{"fg",mode=1,ch="妃愛",face="b0003",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1b",ex05="hiy_z1b0000",file="hiy_z1b1000"}}},line=207,linknext="block_00038",text={vo={{"vo",file="fem_hiy_10158",ch="hiy"}},ja={{"「嗯，这倒也是。强烈的感情会成为艺术的良好养分。即使是悲伤，也会对她的创作产生好的影响。」",{"rt2"},name={"妃愛"}}}}},block_00116={{"fg",mode=3,ch="あすみ",id=13,face="a0009",lv=2.3999999999999999,size="z2",path=":fg/asu/z2/",x=0,head="asu_z2a",ex05="asu_z2a0000",file="asu_z2a0200"},{"text"},linkback="block_00115",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="a0038",lv=2.3999999999999999,size="z2",path=":fg/asu/z2/",x=0,head="asu_z2a",ex05="asu_z2a0000",file="asu_z2a0200"}}},line=686,linknext="block_00117",text={vo={{"vo",file="fem_asu_10474",ch="asu"}},ja={{"「对、对不起，我随便看你睡觉的脸了。但是，那个……你说除非有人叫你才会起床」",{"rt2"},name={"あすみ"}}}}},block_00058={{"text"},line=307,linkback="block_00057",text={ja={{"虽然感觉有些大胆，但既然是妃爱的建议，我决定毫不怀疑地遵从。真的是，兄长和朋友都很照顾我……",{"rt2"}}}},linknext="block_00059"},block_00082={{"quake",mode="mw"},{"text"},line=446,linkback="block_00081",text={ja={{"哦哦哦哦太可爱了我受不了啊啊啊啊啊。",{"rt2"}}}},linknext="block_00083"},block_00039={{"text"},line=216,linkback="block_00038",text={ja={{"「那，那么严重？无法承受悲伤吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00040"},block_00137={{"fg",mode=3,ch="あすみ",id=13,face="b0032",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0700"},{"text"},linkback="block_00136",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="a0034",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0700"}},vl02={{"fg",mode=3,ch="あすみ",id=13,face="a0013",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0700"}}},line=838,linknext="block_00138",text={vo={{"vo",file="fem_asu_10486",ch="asu"}},ja={{"「但是这里实在是太舒适了……让我感到很安心。以前我总觉得每个夜晚过得太寂寞，但在你们家的时候，我反而觉得时间过得太快了」",{"rt2"},name={"あすみ"}}}}},block_00173={{"bg",vague=40,lv=5,file="cinema01",id=5,path=":sys/",time=400,alpha=192,rule="cinema"},{"text"},line=1031,linkback="block_00172",text={ja={{"但昨晚，尽管她满怀信心地说自己没问题，然而典礼一开始不到两分钟就开始哭泣，在我接过毕业证书时已经哭得稀里哗啦。",{"rt2"}}}},linknext="block_00174"},block_00251={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="a0028h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1a0010",head="asu_z1a",file="asu_z1a0210",hoho="h"},{"text"},line=1357,linkback="block_00250",text={vo={{"vo",file="fem_asu_10537",ch="asu"}},ja={{"「老师或者学姐们可能能实现「不用手就能夹住」，但我实在是太笨了。」",{"rt2"},name={"あすみ"}}}},linknext="block_00252"},block_00042={{"text"},line=232,linkback="block_00041",text={ja={{"「烧烤……」",{"rt2"},name={"智宏"}}}},linknext="block_00043"},block_00272={{"fg",mode=3,ch="あすみ",id=13,face="b0023",lv=2.3999999999999999,size="z2",path=":fg/asu/z2/",x=0,head="asu_z2b",ex05="asu_z2b0010",file="asu_z2b0210"},{"text"},line=1444,linkback="block_00271",text={vo={{"vo",file="fem_asu_10546",ch="asu"}},ja={{"「好的，我先躺在地板上，然后学长你就在我上面……」",{"rt2"},name={"あすみ"}}}},linknext="block_00273"},block_00020={{"fg",mode=3,ch="妃愛",id=13,face="b0003",lv=2.2000000000000002,size="no",path=":fg/hiy/no/",x=0,head="hiy_nob",ex05="hiy_nob0000",file="hiy_nob1000"},{"text"},line=122,linkback="block_00019",text={vo={{"vo",file="fem_hiy_10151",ch="hiy"}},ja={{"「顺便问一下，你所说的正常的亚澄是哪种类型？有四种吗？还是五种？」",{"rt2"},name={"妃愛"}}}},linknext="block_00021"},block_00066={{"text"},line=366,linkback="block_00065",text={ja={{"「嗯，嗯。就是在我家住一晚，或者说和妃爱一起举办一个睡衣派对之类的。」",{"rt2"},name={"智宏"}}}},linknext="block_00067"},block_00170={{"text"},line=995,linkback="block_00169",text={ja={{"其实我想亲吻她，甚至更进一步，但妃爱就在附近，而且我们现在作为学生会长和秘书的关系很好，所以我只是握住了她的手。",{"rt2"}}}},linknext="block_00171"},block_00038={{"fg",mode=1,ch="妃愛",face="a0012",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"},{"text"},linkback="block_00037",delay={vl01={{"fg",mode=1,ch="妃愛",face="a0028",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"}}},line=213,linknext="block_00039",text={vo={{"vo",file="fem_hiy_10159",ch="hiy"}},ja={{"「只是那个孩子，心情的变化会直接影响到身体。最糟糕的情况，她可能会在毕业典礼前就倒下。」",{"rt2"},name={"妃愛"}}}}},block_00013={{"text"},line=79,linkback="block_00012",text={ja={{"不，妃爱已经指出过这一点了。正因为如此，我这一周都在做充分的准备。只是，应该更早些做好准备才对。",{"rt2"}}}},linknext="block_00014"},block_00030={{"text"},line=169,linkback="block_00029",text={ja={{"「我也看到了。那个因悲伤而痛苦的可爱少女……这简直就是川端康成的菜……他就喜欢年纪小的女孩子。」",{"rt2"},name={"智宏"}}}},linknext="block_00031"},block_00071={{"text"},line=379,linkback="block_00070",text={ja={{"「不，虽然提议是有点大胆，但是最主要的还是和妃爱一起开睡衣派对啊！？」",{"rt2"},name={"智宏"}}}},linknext="block_00072"},block_00146={{"text"},line=884,linkback="block_00145",text={ja={{"「不用做出那么愧疚的表情。如果亚澄的寂寞是由于学生会引起的，那么作为会长，我会承担起最后的责任」",{"rt2"},name={"智宏"}}}},linknext="block_00147"},block_00107={{"fg",mode=1,ch="妃愛",face="a0042",id=20,size="no",path=":fg/hiy/no/",lv=2.2000000000000002,head="hiy_noa",ex05="hiy_noa0000",file="hiy_noa0700"},{"text"},linkback="block_00106",delay={vl01={{"fg",mode=1,ch="妃愛",face="a0049",id=20,size="no",path=":fg/hiy/no/",lv=2.2000000000000002,head="hiy_noa",ex05="hiy_noa0000",file="hiy_noa0700"}},vl02={{"fg",mode=1,ch="妃愛",face="a0036",id=20,size="no",path=":fg/hiy/no/",lv=2.2000000000000002,head="hiy_noa",ex05="hiy_noa0000",file="hiy_noa0700"}}},line=635,linknext="block_00108",text={vo={{"vo",file="fem_hiy_10179",ch="hiy"}},ja={{"「我们以后还要长久相处呢，你就做好心理准备吧！你知道我在青春期的时候有多少次尴尬吗……你也该尝尝这种感觉！」",{"rt2"},name={"妃愛"}}}}},block_00010={{"fg",mode=3,ch="あすみ",id=13,face="b0038",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0200"},{"text"},linkback="block_00009",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="b0028",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0200"}}},line=73,linknext="block_00011",text={vo={{"vo",file="fem_asu_10451",ch="asu"}},ja={{"「我想……让学长们，让学长们愉快地毕业……」",{"rt2"},name={"あすみ"}}}}},block_00270={{"fg",mode=3,ch="あすみ",id=13,face="a0022",lv=2.3999999999999999,size="z2",path=":fg/asu/z2/",x=0,head="asu_z2a",ex05="asu_z2a0010",file="asu_z2a0210"},{"text"},line=1436,linkback="block_00269",text={vo={{"vo",file="fem_asu_10545",ch="asu"}},ja={{"「今天可以让我用我的方法做吗」",{"rt2"},name={"あすみ"}}}},linknext="block_00271"},block_00199={{"text"},line=1126,linkback="block_00198",text={ja={{"「那我们至少换个地方吧……现在的亚澄吃烤肉可能有些……」",{"rt2"},name={"智宏"}}}},linknext="block_00200"},block_00209={{"text"},line=1171,linkback="block_00208",text={ja={{"嗯？",{"rt2"}}}},linknext="block_00210"},block_00145={{"fg",mode=3,ch="あすみ",id=13,face="a0012",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0700"},{"text"},linkback="block_00144",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="a0029",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0700"}}},line=881,linknext="block_00146",text={vo={{"vo",file="fem_asu_10491",ch="asu"}},ja={{"「所以那个，我无法想象没有前辈们的人生，但却能轻易地想象出没有前辈们的学园……这让我感到很寂寞……」",{"rt2"},name={"あすみ"}}}}},block_00080={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="b0009h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2b0001",head="asu_z2b",file="asu_z2b1101",hoho="h"},{"fgact",ch="あすみ",act="びっくり",lv=2.3999999999999999,id=13},{"text"},linkback="block_00079",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="b0038h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2b0001",head="asu_z2b",file="asu_z2b1101",hoho="h"}}},line=434,linknext="block_00081",text={vo={{"vo",file="fem_asu_10459",ch="asu"}},ja={{"「呜，不只是借浴室，还可能被人看到洗完澡的样子……啊，头发都贴在头皮上了」",{"rt2"},name={"あすみ"}}}}},block_00094={{"text"},line=554,linkback="block_00093",text={ja={{"虽然有妹妹在，我强忍住自己的冲动。但刚洗完澡的亚澄太可爱了，我差点做出三周半旋转。",{"rt2"}}}},linknext="block_00095"},block_00151={{"fg",mode=1,ch="あすみ",face="a0023",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0000",file="asu_noa0700"},{"text"},line=908,linkback="block_00150",text={vo={{"vo",file="fem_asu_10494",ch="asu"}},ja={{"「那、那么我就毫不客气地依赖您了。如果是作为秘书和会长的话，我觉得更容易提出我的问题」",{"rt2"},name={"あすみ"}}}},linknext="block_00152"},block_00029={{"fg",mode=3,ch="妃愛",id=13,face="a0007",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"},{"fgact",ch="妃愛",act="おじぎ",lv=2.2999999999999998,id=13},{"text"},line=166,linkback="block_00028",text={vo={{"vo",file="fem_hiy_10155",ch="hiy"}},ja={{"「在公交车站分别的时候，她在车里用手捂住脸，完全低下了头。」",{"rt2"},name={"妃愛"}}}},linknext="block_00030"},block_00185={{"text"},line=1074,linkback="block_00184",text={ja={{"「没事。在我刚入了大马鞍的时候，我以为我的毕业就会在毕业典礼结束后一个人回家，就这么毫无波澜地度过一天。现在能有恋人陪在身边，我很开心。」",{"rt2"},name={"智宏"}}}},linknext="block_00186"},block_00194={{"text"},line=1106,linkback="block_00193",text={ja={{"「所以，明天或者以后我都可以和常磐、诗樱前辈谈话，今天不去吃烤肉，可以陪在亚澄身边吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00195"},block_00237={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="a0010h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0010",head="asu_z2a",file="asu_z2a0210",hoho="h"},{"text"},line=1299,linkback="block_00236",text={vo={{"vo",file="fem_asu_10531",ch="asu"}},ja={{"「大、大腿吗」",{"rt2"},name={"あすみ"}}}},linknext="block_00238"},block_00086={{"fg",mode=3,ch="妃愛",id=9,face="b0041",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=-280,head="hiy_z1b",ex05="hiy_z1b0010",file="hiy_z1b1010"},{"fgact",ch="妃愛",act="ジャンプ",lv=2.2999999999999998,id=9},{"text"},line=488,linkback="block_00085",text={vo={{"vo",file="fem_hiy_10173",ch="hiy"}},ja={{"「那就开始削土豆吧！今晚吃咖喱！」",{"rt2"},name={"妃愛"}}}},linknext="block_00087"},block_00283={{"text"},line=1488,linkback="block_00282",text={ja={{"不知为何，我们又互相接吻了。还是没有进行任何浪漫的对话。",{"rt2"}}}},linknext="block_00284"},block_00068={{"text"},line=370,linkback="block_00067",text={ja={{"下周就是毕业典礼了，然后……他打算这样。要储备一些力气。",{"rt2"}}}},linknext="block_00069"},block_00285={{"fg",mode=1,ch="あすみ",id=20,face="a0007h",lv=2.3999999999999999,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0010",head="asu_z2a",file="asu_z2a0210",hoho="h"},{"text"},line=1496,linkback="block_00284",text={vo={{"vo",file="fem_asu_10550",ch="asu"}},ja={{"「啊！ 不……我来，让我来做。只是在那之前……唔哼」",{"rt2"},name={"あすみ"}}}},linknext="block_00286"},block_00236={{"text"},line=1294,linkback="block_00235",text={ja={{"「这个人与人之间差别挺大的吧。如果是我，我会更喜欢被大腿夹住」",{"rt2"},name={"智宏"}}}},linknext="block_00237"},block_00161={{"fg",mode=1,ch="あすみ",face="b0008",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0700"},{"text"},linkback="block_00160",delay={vl01={{"fg",mode=1,ch="あすみ",face="b0052",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0700"}}},line=957,linknext="block_00162",text={vo={{"vo",file="fem_asu_10501",ch="asu"}},ja={{"「哎，不，不！您没必要为了我那么费心！啊，不过，我还是希望能偶尔见到穿着制服的前辈……即使您毕业了也是」",{"rt2"},name={"あすみ"}}}}},block_00072={{"fg",mode=3,ch="あすみ",id=13,face="a0022",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0001",file="asu_z1a1101"},{"text"},linkback="block_00071",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="a0055",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0001",file="asu_z1a1101"}}},line=385,linknext="block_00073",text={vo={{"vo",file="fem_asu_10453",ch="asu"}},ja={{"「如果能增加和前辈一起度过的时间，我想去打扰一下。如果妃爱不介意我去的话……」",{"rt2"},name={"あすみ"}}}}},block_00011={{"text"},line=75,linkback="block_00010",text={ja={{"虽然她这么说，但从外表上看，显然是无法吃烤肉的。光是闻到肉的味道，就可能会昏倒。她现在就是这么虚弱。",{"rt2"}}}},linknext="block_00012"},block_00073={{"fg",mode=3,ch="あすみ",id=13,face="a0052",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0001",file="asu_z1a1101"},{"text"},linkback="block_00072",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="a0038",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0001",file="asu_z1a1101"}},vl02={{"fg",mode=3,ch="あすみ",id=13,face="a0020",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0011",file="asu_z1a1111"}}},line=392,linknext="block_00074",text={vo={{"vo",file="fem_asu_10454",ch="asu"}},ja={{"「上学也可以从前辈家里出发，如果不会给你带来困扰的话。直播也可以借用你的房间……如果实在不行，那段时间我就回到这里来。但是，那个……」",{"rt2"},name={"あすみ"}}}}},block_00078={{"text"},line=422,linkback="block_00077",text={ja={{"说到这里，亚澄好像鼓起了勇气和我对视。但是那一刹那她的脸就红得像要爆炸一样，立刻低下了头。",{"rt2"}}}},linknext="block_00079"},label={top={label=1,block="block_00000"},scene={label=1,block="block_00000"}},block_00273={{"text"},line=1447,linkback="block_00272",text={ja={{"「我怎么可能把亚澄当作垫子呢」",{"rt2"},name={"智宏"}}}},linknext="block_00274"},block_00120={{"fg",mode=3,ch="あすみ",id=13,face="b0046",lv=2.3999999999999999,size="z2",path=":fg/asu/z2/",x=0,head="asu_z2b",ex05="asu_z2b0000",file="asu_z2b0200"},{"fgact",ch="あすみ",act="いいえ",lv=2.3999999999999999,id=13},{"text"},linkback="block_00119",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="a0029h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0000",head="asu_z2a",file="asu_z2a0200",hoho="h"}},vl02={{"fgact",ch="あすみ",act="おじぎ",lv=2.3999999999999999,id=13}}},line=703,linknext="block_00121",text={vo={{"vo",file="fem_asu_10475",ch="asu"}},ja={{"「呜哇哇！？啊，不行不行，那太……太害羞了。我会好好早起的……」",{"rt2"},name={"あすみ"}}}}},block_00287={{"text"},line=1500,linkback="block_00286",text={ja={{"接着亚澄解开衬衫的扣子，露出了胸部。但是因为我的裤子在碍事，阻挡住了物体直接触及胸部的位置，所以她稍微向后退了一些。",{"rt2"}}}},linknext="block_00288"},block_00286={{"text"},line=1498,linkback="block_00285",text={ja={{"亚澄像芋虫一样移动，把手移到容易操作的位置。然后，她稍微熟练地解开了我的皮带，拉下了拉链和内裤。",{"rt2"}}}},linknext="block_00287"},block_00230={{"text"},line=1270,linkback="block_00229",text={ja={{"不，没有错。我说的就是最让我兴奋的时候，穿上诗樱的服装最让我兴奋。从今天的亚澄身上，我感受到了坚定的决心。",{"rt2"}}}},linknext="block_00231"},block_00206={{"text"},line=1156,linkback="block_00205",text={ja={{"「如果你恢复精神了就好。但是不要勉强自己。如果你在途中晕倒了，我会带你回来，哪怕要退出庆功会！」",{"rt2"},name={"智宏"}}}},linknext="block_00207"},block_00111={{"cgdel",time=400,lv=5,rule="cinema",id=5},{"text"},line=654,linkback="block_00110",text={ja={{"我从浴缸里欣赏着我女朋友和妹妹的欢快对话。这个生活，太幸福了……。",{"rt2"}}}},linknext="block_00112"},block_00026={{"text"},line=152,linkback="block_00025",text={ja={{"「沐浴在黄昏光芒下的脆弱美少女……这画面太配了，我都怕印象派的画家会把它作为题材。」",{"rt2"},name={"智宏"}}}},linknext="block_00027"},block_00280={{"text"},line=1478,linkback="block_00279",text={ja={{"虽然他并没有张开很大的腿，但她却能轻松地挤进去。虽然没有胸部，但这种纤细正是亚澄的武器。小巧可爱。所以就算没有胸部也没关系，别说出这种话。",{"rt2"}}}},linknext="block_00281"},block_00278={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="b0002h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2b0000",head="asu_z2b",file="asu_z2b0200",hoho="h"},{"text"},linkback="block_00277",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="a0008h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0010",head="asu_z2a",file="asu_z2a0210",hoho="h"}},vl02={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="a0004h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0010",head="asu_z2a",file="asu_z2a0210",hoho="h"}}},line=1470,linknext="block_00279",text={vo={{"vo",file="fem_asu_10548",ch="asu"}},ja={{"「但是如果不从下面开始的话，我就无法夹住……啊！ 那、那么，请正坐……好的，这个位置，稍微抬一下腰……我就在这里插入」",{"rt2"},name={"あすみ"}}}}},block_00085={{"cgdel",id=-1},{"bg",wait=500,file="black",path=":bg/",time=1000,vague=40,rule="rule_002"},{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg002a",path=":bg/",sync=0,ax=1280,ay=720},{"fg",mode=3,ch="妃愛",sync=0,id=9,resize=1,face="b0004",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=-280,head="hiy_z1b",ex05="hiy_z1b0000",file="hiy_z1b1000"},{"fg",mode=3,ch="あすみ",sync=0,id=10,resize=1,face="a0052",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=280,head="asu_z1a",ex05="asu_z1a0001",file="asu_z1a1101"},{"extrans",vague=40,rule="rule_002r"},{"text"},linkback="block_00084",delay={vl01={{"fg",mode=3,ch="あすみ",id=10,face="a0006",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=280,head="asu_z1a",ex05="asu_z1a0011",file="asu_z1a1111"}}},line=482,linknext="block_00086",text={vo={{"vo",file="fem_asu_10461",ch="asu"}},ja={{"「所以，从今天开始请多关照！我会尽可能帮忙做家务的，如果有做得不好的地方请多多指教！」",{"rt2"},name={"あすみ"}}}}},block_00031={{"fg",mode=3,ch="妃愛",id=13,face="b0042",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1b",ex05="hiy_z1b0000",file="hiy_z1b1000"},{"fgact",ch="妃愛",act="いいえ",lv=2.2999999999999998,id=13},{"text"},linkback="block_00030",delay={vl01={{"fg",mode=3,ch="妃愛",id=13,face="b0043",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1b",ex05="hiy_z1b0010",file="hiy_z1b1010"}}},line=177,linknext="block_00032",text={vo={{"vo",file="fem_hiy_10156",ch="hiy"}},ja={{"「哥哥，你对亚澄的喜欢有点过头了。在妹妹面前，你也该有点自制力吧！」",{"rt2"},name={"妃愛"}}}}},block_00277={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="b0011h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2b0000",head="asu_z2b",file="asu_z2b0200",hoho="h"},{"text"},line=1463,linkback="block_00276",text={ja={{"不知为何，我们充满情感地交换了一个吻。并没有进行任何浪漫的对话。",{"rt2"}}}},linknext="block_00278"},block_00276={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="b0019h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2b0000",head="asu_z2b",file="asu_z2b0200",hoho="h"},{"text"},line=1459,linkback="block_00275",text={vo={{"vo",file="fem_asu_10547",ch="asu"}},ja={{"「学长对我太好了……」",{"rt2"},name={"あすみ"}}}},linknext="block_00277"},block_00259={{"text"},line=1387,linkback="block_00258",text={ja={{"「身体方面不应该拿自己和别人比较。亚澄有亚澄的优点。顺便问一下，这个话题算是男朋友特权吗？诗樱的胸部」",{"rt2"},name={"智宏"}}}},linknext="block_00260"},block_00274={{"fg",mode=3,ch="あすみ",id=13,face="b0009",lv=2.3999999999999999,size="z2",path=":fg/asu/z2/",x=0,head="asu_z2b",ex05="asu_z2b0010",file="asu_z2b0210"},{"text"},line=1451,linkback="block_00273",text={ja={{"明明谈话已经快达成一致了，但我还是拒绝了亚澄的提议。因为这实在是太过分了。",{"rt2"}}}},linknext="block_00275"},block_00181={{"text"},line=1062,linkback="block_00180",text={ja={{"「没事没事。如果我是主角，那你就是女主角了，这样两个人独处其实正是我所期待的。来，上来吧」",{"rt2"},name={"智宏"}}}},linknext="block_00182"},block_00271={{"text"},line=1439,linkback="block_00270",text={ja={{"「明白了。今天就依赖亚澄吧。我需要做什么？」",{"rt2"},name={"智宏"}}}},linknext="block_00272"},block_00036={{"text"},line=199,linkback="block_00035",text={ja={{"「虽然我不希望亚澄受苦，但如果我毕业了她会感到难过，那也是一种幸福。果然还是这样。」",{"rt2"},name={"智宏"}}}},linknext="block_00037"},block_00121={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg001a",path=":bg/",sync=0,ax=1280,ay=720},{"fg",mode=3,ch="あすみ",sync=0,id=9,resize=1,face="a0014",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=-280,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0200"},{"fg",mode=3,ch="妃愛",sync=0,id=10,resize=1,face="a0031",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=280,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a0200"},{"fg",mode=3,ch="妃愛",sync=0,id=10,face="a0031",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=280,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a0200"},{"extrans",time=500},{"text"},linkback="block_00120",delay={vl03={{"fg",mode=3,ch="妃愛",id=10,face="b0048",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=280,head="hiy_z1b",ex05="hiy_z1b0000",file="hiy_z1b0200"}},vl02={{"fgact",ch="妃愛",act="うんうん",lv=2.2999999999999998,id=10}},vl01={{"fg",mode=3,ch="妃愛",id=10,face="a0048",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=280,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a0200"}}},line=719,linknext="block_00122",text={vo={{"vo",file="fem_hiy_10181",ch="hiy"}},ja={{"「就算我来叫你起床，也不会开门的啦！话说回来，昨晚我和锦一起睡觉了呐呵呵呵。真是舒服啊～」",{"rt2"},name={"妃愛"}}}}},block_00077={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="b0018h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2b0001",head="asu_z2b",file="asu_z2b1101",hoho="h"},{"text"},linkback="block_00076",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="b0007h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2b0001",head="asu_z2b",file="asu_z2b1101",hoho="h"},{"fgact",ch="あすみ",act="びっくり",lv=2.3999999999999999,id=13}}},line=420,linknext="block_00078",text={vo={{"vo",file="fem_asu_10457",ch="asu"}},ja={{"「但，但是，那个……」",{"rt2"},name={"あすみ"}}}}},block_00269={{"text"},line=1431,linkback="block_00268",text={ja={{"她自己承认自己小了……对啊，逃避并不能解决任何问题。能正视现实的我的恋人……真强！",{"rt2"}}}},linknext="block_00270"},block_00195={{"fg",mode=3,ch="あすみ",face="b0046",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1b",ex05="asu_z1b0010",file="asu_z1b0210"},{"fgact",ch="あすみ",act="いいえ",lv=2.2999999999999998,id=13},{"text"},line=1112,linkback="block_00194",text={vo={{"vo",file="fem_asu_10511",ch="asu"}},ja={{"「那是不行的呜呜呜呜呜」",{"rt2"},name={"あすみ"}}}},linknext="block_00196"},block_00109={{"fg",mode=1,ch="妃愛",face="b0041",id=20,size="no",path=":fg/hiy/no/",lv=2.2000000000000002,head="hiy_nob",ex05="hiy_nob0010",file="hiy_nob0710"},{"text"},line=645,linkback="block_00108",text={vo={{"vo",file="fem_hiy_10180",ch="hiy"}},ja={{"「你这是拿我出气，哈哈哈！」",{"rt2"},name={"妃愛"}}}},linknext="block_00110"},block_00141={{"text"},line=859,linkback="block_00140",text={ja={{"「因为我知道亚澄非常珍视和我一起度过的时间……正因为如此，我更加担心亚澄一个人在学校里度过的日子」",{"rt2"},name={"智宏"}}}},linknext="block_00142"},block_00034={{"fg",mode=3,ch="妃愛",id=13,face="b0046",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1b",ex05="hiy_z1b0010",file="hiy_z1b1010"},{"fgact",ch="妃愛",act="びっくり",lv=2.2999999999999998,id=13},{"text"},line=191,linkback="block_00033",text={vo={{"vo",file="fem_hiy_10157",ch="hiy"}},ja={{"「啊，不是的，资正！那个人不是可疑人物，是哥哥啊！要用气味来判断，而不是看脸！」",{"rt2"},name={"妃愛"}}}},linknext="block_00035"},block_00083={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg007a",zoom=140,sync=0,path=":bg/",ax=1280,ay=720},{"se",file="se抱きつく",id=1},{"fg",mode=3,zoom=120,ch="あすみ",sync=0,id=13,lv=2.3999999999999999,face="a0027h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0001",head="asu_z2a",file="asu_z2a1101",hoho="h"},{"extrans",time=500},{"text"},line=456,linkback="block_00082",text={ja={{"我无法承受这股可爱的暴力，忍不住紧紧抱住眼前的小天使。亚澄一开始没动，但后来她小手紧紧抓住我的胸口。",{"rt2"}}}},linknext="block_00084"},block_00266={{"text"},line=1416,linkback="block_00265",text={ja={{"但是比起这些美言，不知为何生活在被聚集到学生会的爆乳三国志包围中更糟糕。魏国的曹乳，蜀国的乳备，吴国的孙乳，它们都成为了标准。",{"rt2"}}}},linknext="block_00267"},block_00265={{"text"},line=1414,linkback="block_00264",text={ja={{"亚澄并不是那种会在意胸部大小的孩子。她知道了爱情，为了我，才有了这样的想法。",{"rt2"}}}},linknext="block_00266"},block_00264={{"bg",vague=40,lv=5,file="cinema01",id=5,path=":sys/",time=400,alpha=192,rule="cinema"},{"text"},line=1412,linkback="block_00263",text={ja={{"原来如此。能从亚澄的口中听到真好。原来还有这样的事情……虽然也不能算是什么大事。我对亚澄的理解又深了一层。",{"rt2"}}}},linknext="block_00265"},block_00263={{"fg",mode=3,ch="あすみ",id=13,face="a0038",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0010",file="asu_z1a0210"},{"text"},linkback="block_00262",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="a0002",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0010",file="asu_z1a0210"}}},line=1408,linknext="block_00264",text={vo={{"vo",file="fem_asu_10543",ch="asu"}},ja={{"「虽然并没有憧憬，但是从和学长开始交往……不，从喜欢学长开始，就觉得诗樱的身体有点令人羡慕……」",{"rt2"},name={"あすみ"}}}}},block_00262={{"fg",mode=3,ch="あすみ",id=13,face="b0018",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0010",file="asu_z1b0210"},{"text"},linkback="block_00261",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="b0007",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0010",file="asu_z1b0210"}}},line=1402,linknext="block_00263",text={vo={{"vo",file="fem_asu_10542",ch="asu"}},ja={{"「并不是想让大家用那种眼光看诗樱，但是当我在画中看到时，觉得稍微大一点会更可爱，所以自己做了判断……」",{"rt2"},name={"あすみ"}}}}},block_00260={{"fg",mode=3,ch="あすみ",id=13,face="b0029",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0200"},{"fgact",ch="あすみ",act="おじぎ",lv=2.2999999999999998,id=13},{"text"},line=1393,linkback="block_00259",text={vo={{"vo",file="fem_asu_10541",ch="asu"}},ja={{"「是的。我是特意要求稍微大一点的」",{"rt2"},name={"あすみ"}}}},linknext="block_00261"},block_00258={{"fg",mode=3,ch="あすみ",id=13,face="b0038",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0200"},{"text"},line=1384,linkback="block_00257",text={vo={{"vo",file="fem_asu_10540",ch="asu"}},ja={{"「从刚才学长的反应来看，果然还是很有兴趣的呢……」",{"rt2"},name={"あすみ"}}}},linknext="block_00259"},block_00257={{"text"},line=1379,linkback="block_00256",text={ja={{"好厉害啊……啊！又不小心想象了里姐老师的胸部！这可不行。",{"rt2"}}}},linknext="block_00258"},block_00215={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="a0003h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1a0000",head="asu_z1a",file="asu_z1a0200",hoho="h"},{"text"},line=1202,linkback="block_00214",text={vo={{"vo",file="fem_asu_10520",ch="asu"}},ja={{"「嗯，好的。我也这么认为。我不想给妃爱造成麻烦」",{"rt2"},name={"あすみ"}}}},linknext="block_00216"},block_00075={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg007a",zoom=120,sync=0,path=":bg/",ax=1280,ay=720},{"fg",mode=3,ch="あすみ",sync=0,id=13,resize=1,face="b0002",lv=2.3999999999999999,size="z2",path=":fg/asu/z2/",x=0,head="asu_z2b",ex05="asu_z2b0001",file="asu_z2b1101"},{"extrans",time=500},{"text"},line=404,linkback="block_00074",text={vo={{"vo",file="fem_asu_10455",ch="asu"}},ja={{"「我确实想要打扰前辈的家，在妃爱那里吃饭。而且，这些都是为了我……」",{"rt2"},name={"あすみ"}}}},linknext="block_00076"},block_00254={{"fg",mode=3,ch="あすみ",id=13,face="a0002",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0200"},{"text"},line=1369,linkback="block_00253",text={vo={{"vo",file="fem_asu_10538",ch="asu"}},ja={{"「常磐学姐说是为了画画……诗樱学姐想要挟住妃爱的脸。」",{"rt2"},name={"あすみ"}}}},linknext="block_00255"},block_00027={{"fg",mode=3,ch="妃愛",id=13,face="a0003",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"},{"text"},line=157,linkback="block_00026",text={vo={{"vo",file="fem_hiy_10154",ch="hiy"}},ja={{"「我们三个人一起喝茶时，看到哥哥去洗手间，她用那双闪亮的眼睛看着他的背影。」",{"rt2"},name={"妃愛"}}}},linknext="block_00028"},block_00253={{"fg",mode=3,ch="あすみ",id=13,face="a0009",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0200"},{"text"},line=1364,linkback="block_00252",text={ja={{"因为我轻率地产生了好奇心，亚澄露出了惊愕的表情。糟了，刚才的确实不只是好奇心，我脑海里浮现出了那三个人的胸部。",{"rt2"}}}},linknext="block_00254"},block_00252={{"text"},line=1360,linkback="block_00251",text={ja={{"「什么？那三个人，有想要用胸部挟住什么的打算吗？有这种愿望吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00253"},block_00250={{"text"},line=1352,linkback="block_00249",text={ja={{"嗯，可能确实有这方面的原因。虽然到现在为止我一直在帮助亚澄，但这次我直接点头同意了。",{"rt2"}}}},linknext="block_00251"},block_00224={{"text"},line=1240,linkback="block_00223",text={ja={{"虽然我担心亚澄的体力，但被她这么说，我怎么可能忍得住。在亲吻她的唇瓣时，我忍不住把她推倒了。",{"rt2"}}}},linknext="block_00225"},block_00015={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,file="bg002c",path=":bg/"},{"fg",mode=3,ch="妃愛",sync=0,id=13,face="a0007",lv=2.2000000000000002,size="no",path=":fg/hiy/no/",x=0,head="hiy_noa",ex05="hiy_noa0000",file="hiy_noa1000"},{"extrans"},{"bgm",file="bgm05"},{"text"},line=101,linkback="block_00014",text={vo={{"vo",file="fem_hiy_10149",ch="hiy"}},ja={{"「亚澄有点不妙啊」",{"rt2"},name={"妃愛"}}}},linknext="block_00016"},block_00247={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="b0012h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0010",head="asu_z1b",file="asu_z1b0210",hoho="h"},{"text"},line=1341,linkback="block_00246",text={vo={{"vo",file="fem_asu_10535",ch="asu"}},ja={{"「但是学长，那时候你是不是硬着头皮努力的呢……」",{"rt2"},name={"あすみ"}}}},linknext="block_00248"},block_00248={{"text"},line=1344,linkback="block_00247",text={ja={{"「我不太清楚强行做出来的意义，这种事情不是靠自己努力就能做到的，如果不舒服，我觉得就不应该勉强。」",{"rt2"},name={"智宏"}}}},linknext="block_00249"},block_00244={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg007a",path=":bg/",sync=0,ax=1280,ay=720},{"fg",mode=3,sync=0,ch="あすみ",id=13,lv=2.2999999999999998,resize=1,face="b0002h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0000",head="asu_z1b",file="asu_z1b0200",hoho="h"},{"extrans",time=500},{"text"},line=1328,linkback="block_00243",text={ja={{"「啊，那个……男性最喜欢的行为之类的……」",{"rt2"},name={"智宏"}}}},linknext="block_00245"},block_00243={{"text"},line=1318,linkback="block_00242",text={ja={{"被我责备后，亚澄显得有些沮丧。她本来是想让我高兴的，我做错了什么吗……。",{"rt2"}}}},linknext="block_00244"},block_00241={{"text"},line=1311,linkback="block_00240",text={ja={{"「喂！天使不能做这种事情，喂！虽然我们确实在交往，甚至发生过关系，但亚澄你是纯白的天使，这是不行的！」",{"rt2"},name={"智宏"}}}},linknext="block_00242"},block_00238={{"text"},line=1301,linkback="block_00237",text={ja={{"啊，我现在说了多余的话。我真是又一次认识到自己有多笨。",{"rt2"}}}},linknext="block_00239"},block_00095={{"text"},line=556,linkback="block_00094",text={ja={{"在酒店，在亚澄家，我已经看过无数次洗完澡的亚澄了。但她穿着睡衣，在我家里的这个事实，太危险了。",{"rt2"}}}},linknext="block_00096"},block_00219={{"text"},line=1216,linkback="block_00218",text={ja={{"「不过这也得看亚澄的体力和精神状态能否恢复。毕竟还有庆功会，如果参加不了就太过分了，所以今天就只亲亲好了」",{"rt2"},name={"智宏"}}}},linknext="block_00220"},block_00108={{"fg",mode=1,ch="あすみ",face="b0009",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0700"},{"text"},line=640,linkback="block_00107",text={vo={{"vo",file="fem_asu_10471",ch="asu"}},ja={{"「那些事跟我没关系。只需要让妃爱你自己感到尴尬就好了」",{"rt2"},name={"あすみ"}}}},linknext="block_00109"},block_00235={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="a0007h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0010",head="asu_z2a",file="asu_z2a0210",hoho="h"},{"text"},line=1291,linkback="block_00234",text={vo={{"vo",file="fem_asu_10530",ch="asu"}},ja={{"「让男性感到快乐的行为……果然还是用胸部夹住最好……」",{"rt2"},name={"あすみ"}}}},linknext="block_00236"},block_00234={{"text"},line=1286,linkback="block_00233",text={ja={{"是在小看我吗。那三个人就算了，但是信任他们话的亚澄也真是的。",{"rt2"}}}},linknext="block_00235"},block_00152={{"text"},line=910,linkback="block_00151",text={ja={{"如果还没有开始交往，我是不可能穿着睡衣听她倾诉的……啊，除非是在训练营的时候可能会有这种情况吧？",{"rt2"}}}},linknext="block_00153"},block_00076={{"fg",mode=3,ch="あすみ",id=13,face="b0029",lv=2.3999999999999999,size="z2",path=":fg/asu/z2/",x=0,head="asu_z2b",ex05="asu_z2b0001",file="asu_z2b1101"},{"fgact",ch="あすみ",act="おじぎ",lv=2.3999999999999999,id=13},{"text"},linkback="block_00075",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="b0019",lv=2.3999999999999999,size="z2",path=":fg/asu/z2/",x=0,head="asu_z2b",ex05="asu_z2b0001",file="asu_z2b1101"}}},line=411,linknext="block_00077",text={vo={{"vo",file="fem_asu_10456",ch="asu"}},ja={{"「说真心话，我很害怕毕业典礼那天的到来，在床上每天都在颤抖……情况严重的时候甚至会哭泣。所以你们的提议真的让我很开心」",{"rt2"},name={"あすみ"}}}}},block_00231={{"fg",mode=3,ch="あすみ",id=13,lv=2.3999999999999999,face="a0038h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2a0000",head="asu_z2a",file="asu_z2a0200",hoho="h"},{"text"},line=1275,linkback="block_00230",text={vo={{"vo",file="fem_asu_10528",ch="asu"}},ja={{"「前几天，大家在一起聊天时，里姐老师、诗樱学姐和常磐学姐都在谈论男性最喜欢的行为……」",{"rt2"},name={"あすみ"}}}},linknext="block_00232"},block_00284={{"text"},line=1491,linkback="block_00283",text={ja={{"「那个……我自己取出来会更好吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00285"},block_00008={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg007a",path=":bg/",sync=0,ax=1280,ay=720},{"fg",mode=3,ch="あすみ",sync=0,id=13,resize=1,face="b0002",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0010",file="asu_z1b0210"},{"extrans",time=500},{"text"},line=58,linkback="block_00007",text={ja={{"「这个……烤肉可能有点难啊。果然」",{"rt2"},name={"智宏"}}}},linknext="block_00009"},block_00016={{"text"},line=104,linkback="block_00015",text={ja={{"「怎么了」",{"rt2"},name={"智宏"}}}},linknext="block_00017"},block_00045={{"text"},line=244,linkback="block_00044",text={ja={{"「话说回来，那么……该怎么办？明天，我想我会在放学后去亚澄的家……可能会回来得很晚。」",{"rt2"},name={"智宏"}}}},linknext="block_00046"},block_00023={{"text"},line=137,linkback="block_00022",text={ja={{"「那么多！？」",{"rt2"},name={"智宏"}}}},linknext="block_00024"},block_00268={{"msgoff"},{"fg",sync=0,mode=-2},{"cgdel",lv=5,sync=0,id=5,time=400,rule="cinema"},{"bg",by=-360,bx=-640,file="zbg007a",zoom=120,sync=0,path=":bg/",ax=1280,ay=720},{"fg",mode=3,ch="あすみ",sync=0,id=13,resize=1,face="a0006",lv=2.3999999999999999,size="z2",path=":fg/asu/z2/",x=0,head="asu_z2a",ex05="asu_z2a0010",file="asu_z2a0210"},{"extrans",time=500},{"text"},line=1429,linkback="block_00267",text={vo={{"vo",file="fem_asu_10544",ch="asu"}},ja={{"「在调查过程中，我找到了即使小也能夹住的方法」",{"rt2"},name={"あすみ"}}}},linknext="block_00269"},block_00227={{"text"},line=1259,linkback="block_00226",text={ja={{"完全恢复元气的亚澄蹭着我的脸颊，我意识到今天的亲密行为并不只是为了让身体感到舒服，而是为了心灵的相连。",{"rt2"}}}},linknext="block_00228"},block_00225={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg007a",zoom=120,sync=0,path=":bg/",ax=1280,ay=720},{"fg",mode=3,sync=0,ch="あすみ",id=13,lv=2.3999999999999999,resize=1,face="b0019h",x=0,size="z2",path=":fg/asu/z2/",ex05="asu_z2b0000",head="asu_z2b",file="asu_z2b0200",hoho="h"},{"extrans",time=500},{"text"},line=1255,linkback="block_00224",text={vo={{"vo",file="fem_asu_10526",ch="asu"}},ja={{"「其实从以前开始，我就想如果能在学长的毕业典礼那天和你在一起，我就想尽我所能做到的一切。」",{"rt2"},name={"あすみ"}}}},linknext="block_00226"},block_00221={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="a0055h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1a0000",head="asu_z1a",file="asu_z1a0200",hoho="h"},{"fgact",ch="あすみ",act="おじぎ",lv=2.2999999999999998,id=13},{"text"},linkback="block_00220",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="a0022h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1a0010",head="asu_z1a",file="asu_z1a0210",hoho="h"}}},line=1228,linknext="block_00222",text={vo={{"vo",file="fem_asu_10523",ch="asu"}},ja={{"「对不起，给你添麻烦了……我已经在昨天充分恢复了精神，现在已经完全恢复了。比平时还要有精神」",{"rt2"},name={"あすみ"}}}}},block_00019={{"text"},line=115,linkback="block_00018",text={ja={{"「在我面前的亚澄……虽然也不能说完全正常，但总的来说还算标准。啊，真的那么严重吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00020"},block_00220={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="a0052h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1a0000",head="asu_z1a",file="asu_z1a0200",hoho="h"},{"text"},line=1221,linkback="block_00219",text={vo={{"vo",file="fem_asu_10522",ch="asu"}},ja={{"「啊，没问题的……听着前辈的话，我心跳加速，感觉力量正在一点点恢复」",{"rt2"},name={"あすみ"}}}},linknext="block_00221"},block_00130={{"fg",mode=3,ch="あすみ",id=13,face="b0019",lv=2.2000000000000002,size="no",path=":fg/asu/no/",x=0,head="asu_nob",ex05="asu_nob0010",file="asu_nob0710"},{"text"},line=787,linkback="block_00129",text={vo={{"vo",file="fem_asu_10481",ch="asu"}},ja={{"「因为我和电话那头的人关系很好，所以为了让她们能不顾忌我而畅所欲言……我就说我去找学长聊天，然后就出来了」",{"rt2"},name={"あすみ"}}}},linknext="block_00131"},block_00096={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="b0019h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0000",head="asu_z1b",file="asu_z1b0700",hoho="h"},{"text"},line=561,linkback="block_00095",text={vo={{"vo",file="fem_asu_10466",ch="asu"}},ja={{"「我、我平时只洗淋浴，已经很久没泡澡了，感觉真舒服」",{"rt2"},name={"あすみ"}}}},linknext="block_00097"},block_00055={{"text"},line=297,linkback="block_00054",text={ja={{"什么是奥尼尔基啊。我知道你的意思，但这个名字听起来像是你的能量，而不是亚澄的吧？",{"rt2"}}}},linknext="block_00056"},block_00212={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="b0008h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0000",head="asu_z1b",file="asu_z1b0200",hoho="h"},{"text"},linkback="block_00211",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,lv=2.2999999999999998,face="b0018h",x=0,size="z1",path=":fg/asu/z1/",ex05="asu_z1b0000",head="asu_z1b",file="asu_z1b0200",hoho="h"}}},line=1191,linknext="block_00213",text={vo={{"vo",file="fem_asu_10519",ch="asu"}},ja={{"「啊，对了……啊，根据昨天的谈话，我今天也可以留宿在前辈家里对吧……？」",{"rt2"},name={"あすみ"}}}}},block_00150={{"bg",speed=150000,y2=100,x2=640,ay=720,by=-360,bx=-640,file="zbg001c",path=":bg/",ax=1280,y=100,x=-640},{"fg",mode=1,ch="あすみ",resize=1,face="a0022",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0000",file="asu_noa0700"},{"text"},linkback="block_00149",delay={vl01={{"fg",mode=1,ch="あすみ",face="a0054",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0000",file="asu_noa0700"}}},line=903,linknext="block_00151",text={vo={{"vo",file="fem_asu_10493",ch="asu"}},ja={{"「像会长一样的前辈……好久不见了呢。又像在公园的长凳上聊天一样，让人怀念」",{"rt2"},name={"あすみ"}}}}},block_00193={{"text"},line=1103,linkback="block_00192",text={ja={{"亚澄又抬头看着我，眼中闪烁着泪光。看来她已经恢复到能够握住我的手的程度了，用力地紧紧抓住我的手。",{"rt2"}}}},linknext="block_00194"},block_00188={{"text"},line=1085,linkback="block_00187",text={ja={{"「当然，我也想和她们聊天，诗樱前辈也说等一下会过来吃烤肉。但是现在最想先做的就是和亚澄你共话巧语。」",{"rt2"},name={"智宏"}}}},linknext="block_00189"},block_00207={{"fg",mode=1,ch="あすみ",face="b0003",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0200"},{"text"},linkback="block_00206",delay={vl01={{"fg",mode=1,ch="あすみ",face="b0052",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0200"}}},line=1162,linknext="block_00208",text={vo={{"vo",file="fem_asu_10517",ch="asu"}},ja={{"「嗯。如果我觉得有些吃力，我会提前告诉你的。但是和你这样聊天，我现在感觉精神越来越好了」",{"rt2"},name={"あすみ"}}}}},block_00088={{"fg",mode=3,ch="妃愛",id=9,face="a0003",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=-280,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"},{"text"},line=503,linkback="block_00087",text={vo={{"vo",file="fem_hiy_10174",ch="hiy"}},ja={{"「不是队长，叫我小妹妹！」",{"rt2"},name={"妃愛"}}}},linknext="block_00089"},block_00187={{"fg",mode=3,ch="あすみ",face="b0038",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1b",ex05="asu_z1b0010",file="asu_z1b0210"},{"text"},line=1082,linkback="block_00186",text={vo={{"vo",file="fem_asu_10510",ch="asu"}},ja={{"「但是，你也许还有跟常磐前辈、艾米丽前辈聊天的事情，还有小阳……」",{"rt2"},name={"あすみ"}}}},linknext="block_00188"},block_00119={{"text"},line=694,linkback="block_00118",text={ja={{"「那么明天……是不是我该去叫亚澄起床了呢」",{"rt2"},name={"智宏"}}}},linknext="block_00120"},block_00050={{"text"},line=264,linkback="block_00049",text={ja={{"我正想说「毕竟平日还要正常上学」，但妃爱像是能看穿我想说的一切，摇着头表示否定。",{"rt2"}}}},linknext="block_00051"},block_00196={{"text"},line=1114,linkback="block_00195",text={ja={{"我试图趁混乱之中将亚澄可能会拒绝的提议强行通过，但如我所料，还是被她拒绝了。真是的……",{"rt2"}}}},linknext="block_00197"},block_00191={{"text"},line=1096,linkback="block_00190",text={ja={{"「手游就手游而已，虽然很好玩，但如果没有遇见亚澄的话我可能到现在还只会沉迷于游戏，也无法遇见能如此珍视的恋人。这些都是事实。」",{"rt2"},name={"智宏"}}}},linknext="block_00192"},block_00189={{"text"},line=1088,linkback="block_00188",text={ja={{"「其实昨天我想告诉你这个。亚澄你曾说和我一起每一天都过得很快乐，其实对我来说也是这样的。」",{"rt2"},name={"智宏"}}}},linknext="block_00190"},block_00208={{"fg",mode=1,ch="あすみ",id=20,face="b0038h",lv=2.2000000000000002,size="no",path=":fg/asu/no/",ex05="asu_nob0000",head="asu_nob",file="asu_nob0200",hoho="h"},{"bgm",stop=1},{"text"},line=1169,linkback="block_00207",text={vo={{"vo",file="fem_asu_10518",ch="asu"}},ja={{"「那个，我希望你能多给我一些关爱……」",{"rt2"},name={"あすみ"}}}},linknext="block_00209"},block_00180={{"fg",mode=3,ch="あすみ",face="a0012",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0200"},{"fgact",ch="あすみ",act="いいえ",lv=2.2999999999999998,id=13},{"text"},linkback="block_00179",delay={vl01={{"fg",mode=3,ch="あすみ",face="a0028",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0200"}}},line=1059,linknext="block_00181",text={vo={{"vo",file="fem_asu_10508",ch="asu"}},ja={{"「啊啊……让前辈为我准备茶……对不起。今天本应是前辈主角的日子……」",{"rt2"},name={"あすみ"}}}}},block_00179={{"msgoff"},{"fg",sync=0,mode=-2},{"cgdel",lv=5,sync=0,id=5,time=400,rule="cinema"},{"bg",by=-360,bx=-640,file="zbg007a",path=":bg/",sync=0,ax=1280,ay=720},{"fg",mode=3,ch="あすみ",sync=0,resize=1,face="a0030",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0200"},{"extrans",time=500},{"text"},line=1052,linkback="block_00178",text={ja={{"「亚澄，我泡了红茶。喝点让自己放松下吧」",{"rt2"},name={"智宏"}}}},linknext="block_00180"},block_00056={{"fg",mode=3,ch="妃愛",id=13,face="b0038",lv=2.2999999999999998,size="z1",path=":fg/hiy/z1/",x=0,head="hiy_z1b",ex05="hiy_z1b0000",file="hiy_z1b1000"},{"text"},line=302,linkback="block_00055",text={vo={{"vo",file="fem_hiy_10169",ch="hiy"}},ja={{"「如果因为没参加庆祝活动，锦一生都会后悔，那不如参加吧？」",{"rt2"},name={"妃愛"}}}},linknext="block_00057"},block_00129={{"cgdel",id=-1},{"bg",wait=500,file="black",path=":bg/",time=1000,vague=40,rule="rule_002"},{"bg",vague=40,path=":bg/",file="bg001c",rule="rule_002r"},{"se",file="seドア(室内開き)",id=1},{"ex",func="wait",time=500},{"fg",mode=3,mx=-40,ch="あすみ",id=13,resize=1,face="b0002",lv=2.2000000000000002,size="no",path=":fg/asu/no/",x=0,head="asu_nob",ex05="asu_nob0010",file="asu_nob0710"},{"text"},line=779,linkback="block_00128",text={vo={{"vo",file="fem_asu_10480",ch="asu"}},ja={{"「那、那个。虽然已经说了晚安，但是对不起。接到了日和的电话，好像要谈工作的事情……」",{"rt2"},name={"あすみ"}}}},linknext="block_00130"},block_00172={{"bgm",stop=1},{"cgdel",id=-1},{"bg",time=1000,file="white",path=":bg/"},{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,file="bg007a",path=":bg/"},{"fg",mode=3,ch="あすみ",sync=0,id=13,resize=1,face="a0030",lv=2.2000000000000002,size="no",path=":fg/asu/no/",x=0,head="asu_noa",ex05="asu_noa0000",file="asu_noa0200"},{"extrans"},{"bgm",file="bgm03"},{"fgact",ch="あすみ",act="おじぎ",lv=2.2000000000000002,id=13},{"text"},line=1027,linkback="block_00171",text={vo={{"vo",file="fem_asu_10507",ch="asu"}},ja={{"「呜呜……对不起学长，我太弱了……对于自己内心的软弱我无言以对……」",{"rt2"},name={"あすみ"}}}},linknext="block_00173"},block_00164={{"fg",mode=3,ch="あすみ",face="b0022",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0700"},{"text"},linkback="block_00163",delay={vl01={{"fg",mode=3,ch="あすみ",face="b0055",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0700"}}},line=975,linknext="block_00165",text={vo={{"vo",file="fem_asu_10503",ch="asu"}},ja={{"「原本孤独的夜晚现在我能安心度过。而且，还见到了作为会长的前辈」",{"rt2"},name={"あすみ"}}}}},block_00171={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg001c",zoom=120,sync=0,path=":bg/",ax=1280,ay=720},{"fg",mode=3,ch="あすみ",sync=0,id=13,resize=1,face="a0055",lv=2.3999999999999999,size="z2",path=":fg/asu/z2/",x=0,head="asu_z2a",ex05="asu_z2a0000",file="asu_z2a0700"},{"extrans",time=500},{"text"},line=1005,linkback="block_00170",text={vo={{"vo",file="fem_asu_10506",ch="asu"}},ja={{"「谢谢你，学长。明天我会带着笑容送你走的。」",{"rt2"},name={"あすみ"}}}},linknext="block_00172"},block_00168={{"text"},line=991,linkback="block_00167",text={ja={{"她握住我的手，露出了由衷的安慰的笑容。看来四月份开始，她还能像平常一样做自己。",{"rt2"}}}},linknext="block_00169"},block_00131={{"se",time=500,id=1,stop=1},{"text"},line=792,linkback="block_00130",text={ja={{"「啊，是优子吗……我觉得你做得很对，谢谢」",{"rt2"},name={"智宏"}}}},linknext="block_00132"},block_00166={{"fg",mode=3,ch="あすみ",face="a0019",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1a",ex05="asu_z1a0010",file="asu_z1a0710"},{"text"},line=986,linkback="block_00165",text={vo={{"vo",file="fem_asu_10505",ch="asu"}},ja={{"「可能会有时候觉得寂寞，见到前辈时会过于依赖您……」",{"rt2"},name={"あすみ"}}}},linknext="block_00167"},block_00165={{"fg",mode=3,ch="あすみ",face="b0053",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1b",ex05="asu_z1b0010",file="asu_z1b0710"},{"text"},linkback="block_00164",delay={vl01={{"fg",mode=3,ch="あすみ",face="a0054",id=13,size="z1",path=":fg/asu/z1/",lv=2.2999999999999998,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0700"}}},line=981,linknext="block_00166",text={vo={{"vo",file="fem_asu_10504",ch="asu"}},ja={{"「只要我能一直在前辈身边，我珍视的东西就不会消失……这样想着，我就觉得没问题了。所以已经没事了」",{"rt2"},name={"あすみ"}}}}},block_00093={{"text"},line=552,linkback="block_00092",text={ja={{"「啊，嗯，好的。泡得舒服吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00094"},block_00159={{"fg",mode=1,ch="あすみ",face="b0038",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0700"},{"text"},linkback="block_00158",delay={vl01={{"fg",mode=1,ch="あすみ",face="b0002",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0700"}}},line=948,linknext="block_00160",text={vo={{"vo",file="fem_asu_10500",ch="asu"}},ja={{"「对于我来说，学长的毕业不仅仅是寂寞，更像是过去快乐的日子全部消失……更深层次地面对恐惧。」",{"rt2"},name={"あすみ"}}}}},block_00060={{"text"},line=322,linkback="block_00059",text={ja={{"「我不会让妃爱你一个人的」",{"rt2"},name={"智宏"}}}},linknext="block_00061"},block_00021={{"text"},line=125,linkback="block_00020",text={ja={{"「大概是1.5种吧。有时候即使我和她说话，她也会发呆，偶尔看到她，她的脸上总是带着忧郁的神情」",{"rt2"},name={"智宏"}}}},linknext="block_00022"},block_00158={{"fg",mode=1,ch="あすみ",face="a0020",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0000",file="asu_noa0700"},{"text"},linkback="block_00157",delay={vl01={{"fg",mode=1,ch="あすみ",face="a0029",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0000",file="asu_noa0700"}}},line=942,linknext="block_00159",text={vo={{"vo",file="fem_asu_10499",ch="asu"}},ja={{"「如果没有了学长，我的每一天就像是带上了阴影，就像是无法看到未来的漫长夜晚又要开始一样……」",{"rt2"},name={"あすみ"}}}}},block_00007={{"text"},line=48,linkback="block_00006",text={ja={{"亚澄似乎没有力气去质疑我话的真实性，老实地点了点头。虽然她知道，但在教室里她应该是一个人。",{"rt2"}}}},linknext="block_00008"},block_00157={{"fg",mode=1,ch="あすみ",face="a0023",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_noa",ex05="asu_noa0000",file="asu_noa0700"},{"text"},line=936,linkback="block_00156",text={vo={{"vo",file="fem_asu_10498",ch="asu"}},ja={{"「对于我来说的学园生活就是有学生会才存在的……自从遇到学长后，我一直在学长身边存在着，就好像这是理所当然一样。」",{"rt2"},name={"あすみ"}}}},linknext="block_00158"},block_00149={{"text"},line=895,linkback="block_00148",text={ja={{"以前是诗樱前辈给了我压力，这次是妃爱帮我铺好了路。但现在比以前更加根据自己的意愿行动，也做得到。",{"rt2"}}}},linknext="block_00150"},block_00118={{"text"},line=691,linkback="block_00117",text={ja={{"说实话，被看到头发乱糟糟的傻乎乎的脸有点伤心，但话说回来，我的脸好像并没有什么好时候和坏时候，平时也是一副傻乎乎的表情。",{"rt2"}}}},linknext="block_00119"},block_00144={{"fg",mode=3,ch="あすみ",id=13,face="b0020",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0700"},{"text"},linkback="block_00143",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="b0002",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0700"}}},line=875,linknext="block_00145",text={vo={{"vo",file="fem_asu_10490",ch="asu"}},ja={{"「只是那个，我知道我说了很多次可能有些过分，但在我心中学生会的存在太过重要了……包括在其中的前辈们」",{"rt2"},name={"あすみ"}}}}},block_00147={{"fg",mode=3,ch="あすみ",id=13,face="a0010",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0700"},{"text"},linkback="block_00146",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="a0008",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0700"}}},line=890,linknext="block_00148",text={vo={{"vo",file="fem_asu_10492",ch="asu"}},ja={{"「作为会长……啊」",{"rt2"},name={"あすみ"}}}}},block_00115={{"text"},line=680,linkback="block_00114",text={ja={{"睁开眼睛，看到眼前有张美得像艺术品一样可爱的脸，我吓了一跳。啊，真吓人，我还以为自己醒来就在卢浮宫里了。",{"rt2"}}}},linknext="block_00116"},block_00139={{"fg",mode=3,ch="あすみ",id=13,face="a0019",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0010",file="asu_z1a0710"},{"text"},linkback="block_00138",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="a0052",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0010",file="asu_z1a0710"}},vl02={{"fg",mode=3,ch="あすみ",id=13,face="a0055",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1a",ex05="asu_z1a0010",file="asu_z1a0710"}}},line=853,linknext="block_00140",text={vo={{"vo",file="fem_asu_10488",ch="asu"}},ja={{"「虽然我感到有些羞愧，也一直担心自己是不是给你们带来了麻烦……但是能得到你们的照顾，我真的很高兴。谢谢你们」",{"rt2"},name={"あすみ"}}}}},block_00213={{"text"},line=1194,linkback="block_00212",text={ja={{"「嗯。妃爱也是这么打算的。只是因为在我家有妃爱在，所以我们就不做那些恋人间的事情了吧」",{"rt2"},name={"智宏"}}}},linknext="block_00214"},block_00174={{"text"},line=1033,linkback="block_00173",text={ja={{"最后一击是我的答辞。那时候已经在抽泣的亚澄听到我的声音后肯定无法忍住，完全崩溃了。这是我从里姐那里听说的。",{"rt2"}}}},linknext="block_00175"},block_00125={{"text"},line=740,linkback="block_00124",text={ja={{"「不好意思，这张床上三个人是不可能的。就算努力也只能两个人」",{"rt2"},name={"智宏"}}}},linknext="block_00126"},block_00122={{"fg",mode=3,ch="あすみ",id=9,face="a0022",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=-280,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0200"},{"text"},linkback="block_00121",delay={vl01={{"fg",mode=3,ch="あすみ",id=9,face="a0055",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=-280,head="asu_z1a",ex05="asu_z1a0000",file="asu_z1a0200"}}},line=725,linknext="block_00123",text={vo={{"vo",file="fem_asu_10476",ch="asu"}},ja={{"「昨晚多亏了妃爱，我好久没这么安稳地睡过觉了……而且还很早就……你一直在抚摸我的头，谢谢你，妃爱」",{"rt2"},name={"あすみ"}}}}},block_00197={{"text"},line=1116,linkback="block_00196",text={ja={{"但是她已经恢复到能够依偎在我胸口的程度了。这是个好趋势。",{"rt2"}}}},linknext="block_00198"},block_00106={{"fg",mode=1,ch="あすみ",face="b0046",id=20,size="no",path=":fg/asu/no/",lv=2.2000000000000002,head="asu_nob",ex05="asu_nob0000",file="asu_nob0700"},{"text"},line=628,linkback="block_00105",text={vo={{"vo",file="fem_asu_10470",ch="asu"}},ja={{"「我、我不想让先辈看到我的内裤啊啊啊……」",{"rt2"},name={"あすみ"}}}},linknext="block_00107"},block_00004={{"text"},line=38,linkback="block_00003",text={ja={{"「哎呀，我和同学关系好的，就常磐、亚美利和新川这几个。常磐和亚美利都说要和亚澄一起，新川说他有工作的会议」",{"rt2"},name={"智宏"}}}},linknext="block_00005"},block_00228={{"text"},line=1262,linkback="block_00227",text={ja={{"「如果要说过去最让人兴奋的一次亲密行为是什么时候，那应该是亚澄穿上诗樱的服装那次，那次真让人高兴。」",{"rt2"},name={"智宏"}}}},linknext="block_00229"},block_00062={{"text"},line=333,linkback="block_00061",text={ja={{"「喜欢」",{"rt2"},name={"智宏"}}}},linknext="block_00063"},block_00097={{"text"},line=564,linkback="block_00096",text={ja={{"「是、是吗。但如果是这样，我应该让你一个人泡的。因为妃爱说要一起泡……」",{"rt2"},name={"智宏"}}}},linknext="block_00098"},block_00205={{"text"},line=1153,linkback="block_00204",text={ja={{"虽然有些害羞，但这是为了恋人。我会和里姐和妃爱商量一下，看怎么安排最好。",{"rt2"}}}},linknext="block_00206"},block_00101={{"msgoff"},{"bg",sync=0,file="bg002c",path=":bg/"},{"bg",lv=5,rule="cinema",vague=40,alpha=192,file="cinema01",id=5,time=400,sync=0,path=":sys/"},{"extrans",time=500},{"fg",mode=1,ch="妃愛",face="a0010",id=20,size="no",path=":fg/hiy/no/",lv=2.2000000000000002,head="hiy_noa",ex05="hiy_noa0000",file="hiy_noa0700"},{"text"},line=598,linkback="block_00100",text={vo={{"vo",file="fem_hiy_10176",ch="hiy"}},ja={{"「那个，锦，你要带走洗衣服吗？需要我帮你洗吗？」",{"rt2"},name={"妃愛"}}}},linknext="block_00102"},block_00255={{"text"},line=1372,linkback="block_00254",text={ja={{"「妃爱似乎会被挤得无法呼吸。」",{"rt2"},name={"智宏"}}}},linknext="block_00256"},block_00070={{"text"},line=376,linkback="block_00069",text={ja={{"「只是，怎么说呢，如果亚澄因为我要毕业而感到恐惧的话……我就想增加一些我们一起度过的时间。」",{"rt2"},name={"智宏"}}}},linknext="block_00071"},block_00040={{"fg",mode=1,ch="妃愛",face="a0007",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"},{"text"},linkback="block_00039",delay={vl01={{"fg",mode=1,ch="妃愛",face="a0038",id=20,size="z1",path=":fg/hiy/z1/",lv=2.2999999999999998,head="hiy_z1a",ex05="hiy_z1a0000",file="hiy_z1a1000"}}},line=222,linknext="block_00041",text={vo={{"vo",file="fem_hiy_10160",ch="hiy"}},ja={{"「不，你在说什么呢，哥哥。那个孩子就是那样的。只因为和你一起吃午餐，所以才能吃下去，但我想现在她可能除了午餐以外几乎什么都没有吃。」",{"rt2"},name={"妃愛"}}}}},block_00069={{"text"},line=373,linkback="block_00068",text={ja={{"「嘛，嘛，也不一定非得住一晚！就在我家吃个晚饭，然后我送你回家也可以。第二天还要上学……」",{"rt2"},name={"智宏"}}}},linknext="block_00070"},block_00059={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,bx=-640,file="zbg002c",zoom=120,sync=0,path=":bg/",ax=1280,ay=720},{"fg",mode=3,ch="妃愛",sync=0,id=13,resize=1,face="a0028",lv=2.3999999999999999,size="z2",path=":fg/hiy/z2/",x=0,head="hiy_z2a",ex05="hiy_z2a0000",file="hiy_z2a1000"},{"extrans",time=500},{"text"},linkback="block_00058",delay={vl01={{"fg",mode=3,ch="妃愛",id=13,face="a0023",lv=2.3999999999999999,size="z2",path=":fg/hiy/z2/",x=0,head="hiy_z2a",ex05="hiy_z2a0000",file="hiy_z2a1000"}}},line=319,linknext="block_00060",text={vo={{"vo",file="fem_hiy_10170",ch="hiy"}},ja={{"「嗯，我也很清楚被单独留下的恐惧。你要好好鼓励她哦」",{"rt2"},name={"妃愛"}}}}},block_00049={{"text"},line=262,linkback="block_00048",text={ja={{"「啊？」",{"rt2"},name={"智宏"}}}},linknext="block_00050"},block_00177={{"text"},line=1039,linkback="block_00176",text={ja={{"听完班主任的讲话，在退室时向她表示了感谢，然后我、常磐和亚美丽立刻赶到了。在保健室的亚澄被妃爱枕在膝头，看起来非常疲惫。",{"rt2"}}}},linknext="block_00178"},block_00047={{"text"},line=253,linkback="block_00046",text={ja={{"「啊！那可能更好，我们三个人一起吃晚饭……可以吃吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00048"},block_00009={{"fg",mode=3,ch="あすみ",id=13,face="b0009",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0010",file="asu_z1b0210"},{"fgact",ch="あすみ",act="いいえ",lv=2.2999999999999998,id=13},{"text"},linkback="block_00008",delay={vl01={{"fg",mode=3,ch="あすみ",id=13,face="b0007",lv=2.2999999999999998,size="z1",path=":fg/asu/z1/",x=0,head="asu_z1b",ex05="asu_z1b0000",file="asu_z1b0200"}}},line=65,linknext="block_00010",text={vo={{"vo",file="fem_asu_10450",ch="asu"}},ja={{"「啊，不！那个……我会去的。我想去，我想见学生会的大家……」",{"rt2"},name={"あすみ"}}}}}}