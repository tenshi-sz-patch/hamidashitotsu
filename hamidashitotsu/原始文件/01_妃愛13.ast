astver = 2.0
ast = {block_00059={{"fg",ex05="hiy_z1b0010",ch="妃愛",file="hiy_z1b0510",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0002",lv=2.2999999999999998,id=13,mode=3},{"fgact",id=13,ch="妃愛",lv=2.2999999999999998,act="びっくり"},{"text"},line=313,linkback="block_00058",text={ja={{"「这么吃惊？」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00628",ch="hiy"}}},linknext="block_00060"},block_00005={{"fg",ex05="hiy_noa0010",ch="妃愛",file="hiy_noa0510",head="hiy_noa",path=":fg/hiy/no/",size="no",face="a0041",lv=2.2000000000000002,id=13,mode=3},{"fgact",id=13,ch="妃愛",lv=2.2000000000000002,act="いいえ"},{"text"},line=44,linkback="block_00004",text={ja={{"「我可不想成为让哥哥厌恶的妹妹，所以我就停下啦～」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00600",ch="hiy"}}},linknext="block_00006"},block_00011={{"se",file="se物を置く音01",id=1},{"text"},line=80,linkback="block_00010",text={ja={{"「看你笑得这么开心，来让这款蛋糕让你更开心吧。来，无论是心形的可爱蛋糕还是绿葡萄口味的蛋糕，你喜欢哪个就吃哪个」",{"rt2"},name={"智宏"}}}},linknext="block_00012"},block_00056={{"quake",mode="mw"},{"text"},line=303,linkback="block_00055",text={ja={{"「啊啊啊啊啊啊啊！」",{"rt2"},name={"智宏"}}}},linknext="block_00057"},block_00008={{"text"},line=64,linkback="block_00007",text={ja={{"「即使你不这么绕来绕去地请求，只要你想听，我随时都会说给你听的。我爱你」",{"rt2"},name={"智宏"}}}},linknext="block_00009"},block_00017={{"text"},line=109,linkback="block_00016",text={ja={{"「好的，啊～」",{"rt2"},name={"智宏"}}}},linknext="block_00018"},block_00088={{"text"},line=442,linkback="block_00087",text={ja={{"如果妃爱找到的朋友是我完全不认识的孩子，那我可能会感到更加寂寞。但是，如果是锦、神田和饭田的话，我也很喜欢他们。可以毫无保留地感到高兴。",{"rt2"}}}},linknext="block_00089"},block_00090={{"text"},line=446,linkback="block_00089",text={ja={{"感谢学生会。请在接下来的一年里，好好照顾我的亲爱的妹妹。",{"rt2"}}}},linknext="block_00091"},block_00074={{"fg",ex05="hiy_z1a0010",ch="妃愛",file="hiy_z1a0510",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0006",lv=2.2999999999999998,id=13,mode=3},{"text"},line=375,linkback="block_00073",text={ja={{"「所以，如果我要参选，里姐会在接受立候选申请的最后一刻为我保留申请。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00636",ch="hiy"}}},linknext="block_00075"},block_00072={{"text"},line=364,linkback="block_00071",text={ja={{"更何况，我最不愿意看到这种情况。光是想象就让我觉得窒息。我不想让别的男生接近我的妹妹。",{"rt2"}}}},linknext="block_00073"},block_00009={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0052",lv=2.2999999999999998,id=13,mode=3},{"fgact",id=13,ch="妃愛",lv=2.2999999999999998,act="おじぎ"},{"text"},text={ja={{"「哥哥真是太温柔了……唉，今天我肯定会一直笑个不停的。嘻嘻嘻嘻」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00602",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0048",lv=2.2999999999999998,id=13,mode=3}},vl02={{"fgact",id=13,ch="妃愛",lv=2.2999999999999998,act="いいえ"}}},line=73,linkback="block_00008",linknext="block_00010"},block_00094={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0054",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「只有热爱哥哥的女孩子，所以没问题的！如果被傻傻爱上会很困扰的，所以我会公开哥哥买了妹妹向的色情ASMR的事实哦」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00645",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0008",lv=2.2999999999999998,id=13,mode=3},{"fgact",id=13,ch="妃愛",lv=2.2999999999999998,act="びっくり"}},vl02={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0026",lv=2.2999999999999998,id=13,mode=3}}},line=473,linkback="block_00093",linknext="block_00095"},block_00010={{"text"},line=75,linkback="block_00009",text={ja={{"妃爱露出了幸福的笑容。我希望能让她一生都保持这样的表情……",{"rt2"}}}},linknext="block_00011"},block_00032={{"text"},line=177,linkback="block_00031",text={ja={{"「不，那完全没有……我想知道妃爱是否因此而高兴。只要不让我听到自己的声音就行」",{"rt2"},name={"智宏"}}}},linknext="block_00033"},block_00003={{"text"},line=36,linkback="block_00002",text={ja={{"「没错，你知道我会反对……能不能不要在我面前看那个？虽然送出去的礼物我是准备好的，但是……」",{"rt2"},name={"智宏"}}}},linknext="block_00004"},block_00045={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0034",lv=2.2999999999999998,id=13,mode=3},{"text"},line=245,linkback="block_00044",text={ja={{"「我很高兴哥哥能找到值得信赖的朋友，但与此同时，我也有些寂寞。如果是以前的我的话」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00619",ch="hiy"}}},linknext="block_00046"},label={top={block="block_00000",label=1}},block_00080={{"text"},line=402,linkback="block_00079",text={ja={{"「最重要的是，我很高兴妃爱你能按照自己的意愿去交朋友。」",{"rt2"},name={"智宏"}}}},linknext="block_00081"},block_00101={{"bgm",stop=1,time=3000},{"se",stop=1,time=3000,id=-1},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",time=3000,file="black",path=":bg/"},{"cacheclear"},{"ex",func="wait",time=1000},{"bg",path=":cg/",lv=9,time=500,file="catch01",id=9},{"bgm",file="bgm91",loop=0},{"ex",func="wait",time=6000},{"bgm",stop=1,time=3000},{"se",stop=1,time=3000,id=-1},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",time=3000,file="black",path=":bg/"},{"cacheclear"},{"ex",func="wait",time=1000},{"excall",file="01_妃愛14_ed"},{"text"},line=501,linkback="block_00100"},block_00058={{"text"},line=307,linkback="block_00057",text={ja={{"虽然我理解了妃爱的话的含义，但脑海中涌现出的疑问接连不断，让我感到头脑要炸了。我张开口，却发不出声音。",{"rt2"}}}},linknext="block_00059"},block_00100={{"text"},line=493,linkback="block_00099",text={ja={{"妃爱也在想着和我一样的事。虽然世界变得开放是好事，但偶尔也需要躲进封闭的世界里。毕竟我和妃爱，都是喜欢待在家的人。",{"rt2"}}}},linknext="block_00101"},block_00019={{"text"},line=121,linkback="block_00018",text={ja={{"已经吃完蛋糕的妃爱移动到我旁边，把头靠在我的肩上。我还在吃，这让我有点难以动弹叉子。",{"rt2"}}}},linknext="block_00020"},block_00065={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0019",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「事实上，神田和饭田都想再做一年的学生会工作。他们有时会提起这样的话题。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00631",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0034",lv=2.2999999999999998,id=13,mode=3}}},line=338,linkback="block_00064",linknext="block_00066"},block_00007={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0013",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「我会停止看视频，因为我想听到真实的你的声音。请说你爱我。你在视频里已经这么说过了」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00601",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0019",lv=2.2999999999999998,id=13,mode=3}},vl02={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0035",lv=2.2999999999999998,id=13,mode=3}}},line=61,linkback="block_00006",linknext="block_00008"},block_00060={{"text"},line=316,linkback="block_00059",text={ja={{"「啊……不是吧，工作……」",{"rt2"},name={"智宏"}}}},linknext="block_00061"},block_00098={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0032",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「但是呢，虽然我会和大家一起出去玩，但也需要和哥哥两人独处的时间。娱乐和舒缓都是必要的哦」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00646",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0010",ch="妃愛",file="hiy_z1b0510",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0006",lv=2.2999999999999998,id=13,mode=3}}},line=486,linkback="block_00097",linknext="block_00099"},block_00024={{"fg",ex05="hiy_z2a0000",ch="妃愛",file="hiy_z2a0500",head="hiy_z2a",path=":fg/hiy/z2/",size="z2",face="a0035",lv=2.3999999999999999,id=13,mode=3},{"text"},text={ja={{"「虽然会害羞，但更多的是因为高兴而害羞。感觉就像是自己的工作被赞美了一样」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00608",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z2a0010",ch="妃愛",file="hiy_z2a0510",head="hiy_z2a",path=":fg/hiy/z2/",size="z2",face="a0041",lv=2.3999999999999999,id=13,mode=3},{"fgact",id=13,ch="妃愛",lv=2.3999999999999999,act="うなづく"}}},line=147,linkback="block_00023",linknext="block_00025"},block_00041={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0028",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「我是为了哥哥的文化节，哥哥是为了我的生日，我觉得这里没有什么区别……」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00616",ch="hiy"}}},delay={vl01={{"fgact",id=13,ch="妃愛",lv=2.2999999999999998,act="おじぎ"}}},line=227,linkback="block_00040",linknext="block_00042"},block_00064={{"text"},line=332,linkback="block_00063",text={ja={{"嗯，确实，我作为现任学生会长在学生会里工作最少。除了一年中零星的几个忙碌时期，学生会长这个职务其实挺闲的。",{"rt2"}}}},linknext="block_00065"},block_00076={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0035",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「虽然这样有点小聪明，但里姐笑着说，这总比没有候选人要好。所以，我想我会接受她的好意。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00638",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0019",lv=2.2999999999999998,id=13,mode=3}}},line=388,linkback="block_00075",linknext="block_00077"},block_00020={{"fg",lv=2.2999999999999998,mx=40,ch="妃愛",id=13,mode=-1},{"bg",by=-360,path=":bg/",bx=-640,zoom=120,ay=720,time=500,file="zbg002c",ax=1280},{"fg",resize=1,ex05="hiy_z2a0000",ch="妃愛",file="hiy_z2a0500",head="hiy_z2a",path=":fg/hiy/z2/",size="z2",mx=40,face="a0025",lv=2.3999999999999999,id=13,mode=3},{"text"},line=129,linkback="block_00019",text={ja={{"「如果我在哥哥身边看刚才的视频，会不会觉得害羞呢」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00606",ch="hiy"}}},linknext="block_00021"},block_00095={{"text"},line=476,linkback="block_00094",text={ja={{"「别说了。那只是被封面画风吸引而已」",{"rt2"},name={"智宏"}}}},linknext="block_00096"},block_00015={{"text"},line=102,linkback="block_00014",text={ja={{"「我在网上看到它的评价是4.6，希望它能长久经营下去。说起来，这个心形蛋糕真好吃」",{"rt2"},name={"智宏"}}}},linknext="block_00016"},block_00044={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0027",lv=2.2999999999999998,id=13,mode=3},{"text"},line=240,linkback="block_00043",text={ja={{"「相比我只能无可奈何，哥哥却主动信任生徒会的大家，将今天的准备工作交给他们……」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00618",ch="hiy"}}},linknext="block_00045"},block_00063={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0053",lv=2.2999999999999998,id=13,mode=3},{"text"},line=330,linkback="block_00062",text={ja={{"「如果有锦和神田和饭田陪着我，我就可以放心地把工作交给他们了」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00630",ch="hiy"}}},linknext="block_00064"},block_00012={{"se",stop=1,time=500,id=1},{"fg",ex05="hiy_z1b0010",ch="妃愛",file="hiy_z1b0510",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0054",lv=2.2999999999999998,id=13,mode=3},{"fgact",id=13,ch="妃愛",lv=2.2999999999999998,act="ジャンプ"},{"text"},text={ja={{"「哇，那我就选闪亮的葡萄口味蛋糕啦！毕竟今天是我的生日！」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00603",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0010",ch="妃愛",file="hiy_z1b0510",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0041",lv=2.2999999999999998,id=13,mode=3}}},line=89,linkback="block_00011",linknext="block_00013"},block_00089={{"text"},line=444,linkback="block_00088",text={ja={{"诗樱前辈的电影化也对我的妹妹的人际关系有所帮助。即使我们毕业了，也能保持联系。",{"rt2"}}}},linknext="block_00090"},block_00087={{"text"},line=440,linkback="block_00086",text={ja={{"真是幸好我再做了一年学生会。我自己的朋友和伙伴在学生会的前半年就找到了。而接下来坚持做下去的一年，可能就是为了这个妹妹吧。",{"rt2"}}}},linknext="block_00088"},block_00062={{"text"},line=325,linkback="block_00061",text={ja={{"「这不是本末倒置了吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00063"},block_00085={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",path=":bg/",file="bg002c",sync=0},{"fg",resize=1,ex05="hiy_nob0000",ch="妃愛",file="hiy_nob0500",head="hiy_nob",path=":fg/hiy/no/",size="no",mode=3,face="b0023",lv=2.2000000000000002,id=13,sync=0},{"bg",alpha=192,file="cinema11",vague=40,path=":sys/",lv=1,rule="cinema",time=400,id=5,sync=0},{"extrans",time=500},{"text"},line=436,linkback="block_00084",text={ja={{"只对我亲近的妃爱交到了朋友。正如妃爱所说，这种情况下确实会同时感到喜悦和寂寞。",{"rt2"}}}},linknext="block_00086"},block_00043={{"text"},line=235,linkback="block_00042",text={ja={{"妃爱说话的内容正反复无常。我没能抓住谈话的主题，只能默默等待她继续解释。",{"rt2"}}}},linknext="block_00044"},block_00084={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0004",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「就像哥哥展示给我看他能做到的事情一样，现在轮到我了。我已经从家里迈出了一步，你要看好我接下来的表现哦。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00643",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0041",lv=2.2999999999999998,id=13,mode=3}}},line=426,linkback="block_00083",linknext="block_00085"},block_00028={{"text"},line=165,linkback="block_00027",text={ja={{"「哎，没必要道歉。只不过，确实我听到自己录音的声音总会感到羞耻。但我并不讨厌」",{"rt2"},name={"智宏"}}}},linknext="block_00029"},block_00022={{"fg",ex05="hiy_z2a0000",ch="妃愛",file="hiy_z2a0500",head="hiy_z2a",path=":fg/hiy/z2/",size="z2",face="a0013",lv=2.3999999999999999,id=13,mode=3},{"text"},line=137,linkback="block_00021",text={ja={{"「哥哥你也在我旁边玩那个我出演的社交游戏呢」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00607",ch="hiy"}}},linknext="block_00023"},block_00083={{"fg",ex05="hiy_z1b0010",ch="妃愛",file="hiy_z1b0510",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0015",lv=2.2999999999999998,id=13,mode=3},{"text"},line=420,linkback="block_00082",text={ja={{"「但是，我还是希望有一天能够说出「这是我自己的朋友」，所以我也会尽量表现出真实的自我，努力一试。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00642",ch="hiy"}}},linknext="block_00084"},block_00061={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0023",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「我打算明年为了备考而减少工作。在那个间隙，只要做今年同样的工作应该可以。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00629",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0055",lv=2.2999999999999998,id=13,mode=3}}},line=322,linkback="block_00060",linknext="block_00062"},block_00082={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0023",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「诗樱前辈、常磐和天梨前辈等人，在学生会以外的工作中也能和我保持良好关系，所以我觉得我们以后也能成为好朋友。这都是因为有哥哥牵线。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00641",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0053",lv=2.2999999999999998,id=13,mode=3}},vl02={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0055",lv=2.2999999999999998,id=13,mode=3}}},line=415,linkback="block_00081",linknext="block_00083"},block_00030={{"text"},line=169,linkback="block_00029",text={ja={{"是想和我逗逗乐，还是想看我害羞的样子，还是想试试看我为了她能忍受多大的羞耻感。这个妹妹有时候喜欢恶作剧。",{"rt2"}}}},linknext="block_00031"},block_00081={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0004",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「虽然说，除了悠羽之外，我能够表现真实自我、撒娇的对象，都是在哥哥在场的情况下才有的，所以现在来看，哥哥的存在是必不可少的。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00640",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0035",lv=2.2999999999999998,id=13,mode=3}}},line=408,linkback="block_00080",linknext="block_00082"},block_00079={{"text"},line=399,linkback="block_00078",text={ja={{"「哎呀，听到这个消息的时候我确实吓了一跳……但如果你已经考虑到如何参选的问题，那应该没问题吧。不行的话就算了，你们可以为了其他目的聚在一起。」",{"rt2"},name={"智宏"}}}},linknext="block_00080"},block_00039={{"text"},line=214,linkback="block_00038",text={ja={{"但看来我的担忧是多余的，妃爱看到我那满脸的不安，急忙摇了摇头。",{"rt2"}}}},linknext="block_00040"},block_00078={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0020",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「不过，我还是觉得如果没有现任学生会干部中的任何一人在场会有些不安，所以我想先和大家商量下。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00639",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0034",lv=2.2999999999999998,id=13,mode=3}}},line=396,linkback="block_00077",linknext="block_00079"},block_00070={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0035",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「虽然我自己说出来有点自大，但这真的是可能发生的情况，如果真的发生了，我相信我的经纪人会露出讨厌的表情。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00634",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0053",lv=2.2999999999999998,id=13,mode=3}}},line=360,linkback="block_00069",linknext="block_00071"},block_00033={{"fg",ex05="hiy_z2a0000",ch="妃愛",file="hiy_z2a0500",head="hiy_z2a",path=":fg/hiy/z2/",size="z2",face="a0015",lv=2.3999999999999999,id=13,mode=3},{"text"},text={ja={{"「当然高兴了，非常高兴。只是呢，我想说的并不仅仅是对哥哥的感谢」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00612",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z2a0000",ch="妃愛",file="hiy_z2a0500",head="hiy_z2a",path=":fg/hiy/z2/",size="z2",face="a0002",lv=2.3999999999999999,id=13,mode=3}},vl02={{"fg",ex05="hiy_z2a0000",ch="妃愛",file="hiy_z2a0500",head="hiy_z2a",path=":fg/hiy/z2/",size="z2",face="a0013",lv=2.3999999999999999,id=13,mode=3}}},line=184,linkback="block_00032",linknext="block_00034"},block_00031={{"fg",ex05="hiy_z2b0000",ch="妃愛",file="hiy_z2b0500",head="hiy_z2b",path=":fg/hiy/z2/",size="z2",face="b0010",lv=2.3999999999999999,id=13,mode=3},{"text"},line=174,linkback="block_00030",text={ja={{"「不要谈论视频本身的事情？」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00611",ch="hiy"}}},linknext="block_00032"},block_00021={{"text"},line=132,linkback="block_00020",text={ja={{"「会、会觉得害羞的。虽然今天是妃爱的生日，我想尽可能地对你好，但是让我在你旁边听自己的声音还是有点尴尬呢」",{"rt2"},name={"智宏"}}}},linknext="block_00022"},block_00004={{"text"},line=38,linkback="block_00003",text={ja={{"但是，用充满感情的声音真诚地对面说出来，简直就像受刑一样。实在是受不了了，我都快要哭出来了。",{"rt2"}}}},linknext="block_00005"},block_00053={{"text"},line=286,linkback="block_00052",text={ja={{"「等等，你是不是在暗示……」",{"rt2"},name={"智宏"}}}},linknext="block_00054"},block_00096={{"text"},line=478,linkback="block_00095",text={ja={{"我那个能够找到自己信任的朋友的妹妹，今后一定会更加闪耀，成为更受欢迎的人。希望她能被所有人永远爱着。",{"rt2"}}}},linknext="block_00097"},block_00091={{"text"},line=448,linkback="block_00090",text={ja={{"虽然说这都取决于候选人的结果。虽然这是我个人的想法，但还是希望男生们不要参选！",{"rt2"}}}},linknext="block_00092"},block_00066={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0031",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「虽然亚澄的主业似乎越来越忙，但如果我恳求她，我想她应该还愿意陪我再做一年吧。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00632",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0053",lv=2.2999999999999998,id=13,mode=3}}},line=344,linkback="block_00065",linknext="block_00067"},block_00035={{"msgoff"},{"bg",by=-360,path=":bg/",ay=720,sync=0,bx=-640,file="zbg002c",ax=1280},{"fg",resize=1,ex05="hiy_z1b0010",ch="妃愛",file="hiy_z1b0510",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",mode=3,face="b0046",lv=2.2999999999999998,id=13,sync=0},{"extrans",time=500},{"fgact",id=13,ch="妃愛",lv=2.2999999999999998,act="びっくり"},{"text"},text={ja={{"「我、我、我准备以后要多说很多次谢谢哥哥，因为我真的很高兴。但是，我想说的是另一件事」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00613",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0035",lv=2.2999999999999998,id=13,mode=3}},vl02={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0018",lv=2.2999999999999998,id=13,mode=3}}},line=201,linkback="block_00034",linknext="block_00036"},block_00071={{"text"},line=362,linkback="block_00070",text={ja={{"如果是为了保证妃爱的出勤天数，或许她会勉强接受。但考虑到现在妃爱优先上学，那个经纪人应该不会允许她和不认识的男生待在同一个空间里。",{"rt2"}}}},linknext="block_00072"},block_00069={{"text"},line=354,linkback="block_00068",text={ja={{"嗯，说实话，这就是我最担心的地方。这比任何事情都让我感到不安。",{"rt2"}}}},linknext="block_00070"},block_00018={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0055",lv=2.2999999999999998,id=13,mode=3},{"fgact",id=13,ch="妃愛",lv=2.2999999999999998,act="うなづく"},{"text"},text={ja={{"「啊姆。呜哇！ 哥哥真的喂我了！今天他对我特别温柔……而且好好吃……」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00605",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0010",ch="妃愛",file="hiy_z1b0510",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0051",lv=2.2999999999999998,id=13,mode=3},{"fgact",id=13,ch="妃愛",lv=2.2999999999999998,act="びっくり"}},vl02={{"fg",ex05="hiy_z1b0010",ch="妃愛",file="hiy_z1b0510",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0048",lv=2.2999999999999998,id=13,mode=3},{"fgact",id=13,ch="妃愛",lv=2.2999999999999998,act="いいえ"}}},line=119,linkback="block_00017",linknext="block_00019"},block_00047={{"fg",ex05="hiy_z1a0010",ch="妃愛",file="hiy_z1a0510",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0052",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「所以，我可以真心为哥哥有朋友而高兴，同时也对他感到钦佩。啊，并没有什么别的含义，就是字面上的意思」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00621",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0009",lv=2.2999999999999998,id=13,mode=3}}},line=258,linkback="block_00046",linknext="block_00048"},block_00067={{"text"},line=347,linkback="block_00066",text={ja={{"「你们四个人都能当选……真的可能吗？不过我们去年也通过了……话说回来」",{"rt2"},name={"智宏"}}}},linknext="block_00068"},block_00023={{"text"},line=140,linkback="block_00022",text={ja={{"「那个，那是专业和业余的区别吧。啊？ 真的会感到害羞吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00024"},block_00040={{"fg",ex05="hiy_z1a0010",ch="妃愛",file="hiy_z1a0510",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0045",lv=2.2999999999999998,id=13,mode=3},{"fgact",id=13,ch="妃愛",lv=2.2999999999999998,act="いいえ"},{"text"},text={ja={{"「啊，不！不是那样的！我去年也把我对哥哥的重要感情，比对哥哥说还早就告诉了诗樱前辈！」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00615",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0014",lv=2.2999999999999998,id=13,mode=3}}},line=221,linkback="block_00039",linknext="block_00041"},block_00014={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0055",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「这家蛋糕店，从它开业的时候我就一直想来。千玉的甜品店也越来越好了呢」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00604",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0052",lv=2.2999999999999998,id=13,mode=3}}},line=99,linkback="block_00013",linknext="block_00015"},block_00073={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0031",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「其实不久前，我和里姐开玩笑地谈过这个问题。我们讨论了明年的学生会该怎么办呢——」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00635",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0052",lv=2.2999999999999998,id=13,mode=3}}},line=370,linkback="block_00072",linknext="block_00074"},block_00092={{"msgoff"},{"cgdel",lv=1,rule="cinema",time=400,id=5,sync=0},{"fg",sync=0,mode=-2},{"bg",by=-360,path=":bg/",ay=720,sync=0,bx=-640,file="zbg002c",ax=1280},{"fg",resize=1,ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",mode=3,face="b0054",lv=2.2999999999999998,id=13,sync=0},{"extrans",time=500},{"text"},text={ja={{"「休息日可能会和朋友们出去玩。但偶尔也会邀请哥哥一起，那时我们一起去吧！」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00644",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0031",lv=2.2999999999999998,id=13,mode=3}},vl02={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0052",lv=2.2999999999999998,id=13,mode=3}}},line=462,linkback="block_00091",linknext="block_00093"},block_00086={{"text"},line=438,linkback="block_00085",text={ja={{"但是，作为哥哥的我，怎么可能不为妹妹的成长感到高兴呢。从小到大，比任何人都要被爱护的妃爱，现在找到了她自己应该去爱的朋友。",{"rt2"}}}},linknext="block_00087"},block_00099={{"fg",ex05="hiy_z1b0010",ch="妃愛",file="hiy_z1b0510",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0054",lv=2.2999999999999998,id=13,mode=3},{"text"},line=491,linkback="block_00098",text={ja={{"「能够两人独处的休息日，就在家里呆着吧」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00647",ch="hiy"}}},linknext="block_00100"},block_00057={{"text"},line=305,linkback="block_00056",text={ja={{"竞选那个！？等等，不只是那个……我原本以为她只是打算说「我要和大家一起出去玩，不做学生会的工作了！」这样的事情……",{"rt2"}}}},linknext="block_00058"},block_00055={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0019",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「虽然还要看其他人的意愿，但我打算在今年11月的选举中，竞选学生会长。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00627",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0052",lv=2.2999999999999998,id=13,mode=3}}},line=298,linkback="block_00054",linknext="block_00056"},block_00001={{"text"},line=25,linkback="block_00000",text={ja={{"吃完晚饭，期待已久的蛋糕时间到了。在烧水冲红茶的时候，妃爱毫无预警地开始播放视频。",{"rt2"}}}},linknext="block_00002"},block_00048={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0055",lv=2.2999999999999998,id=13,mode=3},{"text"},line=263,linkback="block_00047",text={ja={{"「去年11月份时主动提名自己，接手学生会的运营，在这个过程中还找到了值得信赖的朋友，这样的哥哥真是太棒了」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00622",ch="hiy"}}},linknext="block_00049"},block_00054={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0003",lv=2.2999999999999998,id=13,mode=3},{"fgact",id=13,ch="妃愛",lv=2.2999999999999998,act="うんうん"},{"text"},line=292,linkback="block_00053",text={ja={{"「嗯。我也想要按照自己的意愿去尝试一下。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00626",ch="hiy"}}},linknext="block_00055"},block_00025={{"fg",ex05="hiy_z2b0000",ch="妃愛",file="hiy_z2b0500",head="hiy_z2b",path=":fg/hiy/z2/",size="z2",face="b0053",lv=2.3999999999999999,id=13,mode=3},{"text"},line=152,linkback="block_00024",text={ja={{"「当听到哥哥转发了自己的画作时，常磐的反应，或者知道有人在视频下评论时，锦的反应，可能都和这种害羞相近吧」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00609",ch="hiy"}}},linknext="block_00026"},block_00052={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0053",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「看到哥哥交到朋友，我能真心地为他高兴，那么如果这个情况反过来，哥哥是否也能对我有同样的感觉呢？」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00625",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0055",lv=2.2999999999999998,id=13,mode=3}},vl02={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0038",lv=2.2999999999999998,id=13,mode=3}}},line=283,linkback="block_00051",linknext="block_00053"},block_00051={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0053",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「看着哥哥送我的视频，我心中涌现出幸福、对哥哥的感激之情，还有另一种感觉……」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00624",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0032",lv=2.2999999999999998,id=13,mode=3}}},line=276,linkback="block_00050",linknext="block_00052"},block_00002={{"fg",ex05="hiy_nob0010",ch="妃愛",file="hiy_nob0510",head="hiy_nob",path=":fg/hiy/no/",size="no",face="b0048",lv=2.2000000000000002,id=13,mode=3},{"fgact",id=13,ch="妃愛",lv=2.2000000000000002,act="おじぎ"},{"text"},text={ja={{"「嘿嘿～，随时都能听到哥哥的声音～。我之前就很想录下哥哥的声音了。但我知道哥哥肯定会反对，所以就放弃了～」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00599",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_nob0010",ch="妃愛",file="hiy_nob0510",head="hiy_nob",path=":fg/hiy/no/",size="no",face="b0041",lv=2.2000000000000002,id=13,mode=3}},vl02={{"fg",ex05="hiy_noa0010",ch="妃愛",file="hiy_noa0510",head="hiy_noa",path=":fg/hiy/no/",size="no",face="a0035",lv=2.2000000000000002,id=13,mode=3}}},line=33,linkback="block_00001",linknext="block_00003"},block_00050={{"text"},line=270,linkback="block_00049",text={ja={{"能信任别人并把计划交给他们，又能坦诚表达对妹妹的感情，这样的我很了不起。我明白了，如果去年的我听到这个话，可能也会有同样的感觉。",{"rt2"}}}},linknext="block_00051"},block_00036={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0053",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「哥哥把对我的重要感情寄托在那封信上，我感受得非常深刻。因此，我觉得哥哥能在事前告诉大家，说明他非常信任学生会的大家」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00614",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0034",lv=2.2999999999999998,id=13,mode=3}}},line=207,linkback="block_00035",linknext="block_00037"},block_00026={{"text"},line=155,linkback="block_00025",text={ja={{"「啊，那我明白了。因为我是在赞美别人，所以当我觉得对方因此而高兴时，我也会感到害羞」",{"rt2"},name={"智宏"}}}},linknext="block_00027"},block_00042={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0038",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「但是，我的朗诵剧是学生会的公务，而哥哥以「庆祝我的生日」为名的行动则是私人的，可能在这一点上有些区别吧」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00617",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0002",lv=2.2999999999999998,id=13,mode=3}}},line=233,linkback="block_00041",linknext="block_00043"},block_00068={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0054",lv=2.2999999999999998,id=13,mode=3},{"text"},line=352,linkback="block_00067",text={ja={{"「你担心喜欢我、想追我的男生会参选学生会吗？」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00633",ch="hiy"}}},linknext="block_00069"},block_00046={{"fg",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0500",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0053",lv=2.2999999999999998,id=13,mode=3},{"text"},text={ja={{"「但是现在的哥哥，我可以放心他不会离开我……这种自信，在过去的一年中我也有了。因为我得到了很多爱」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00620",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z1a0010",ch="妃愛",file="hiy_z1a0510",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0006",lv=2.2999999999999998,id=13,mode=3}},vl02={{"fg",ex05="hiy_z1a0010",ch="妃愛",file="hiy_z1a0510",head="hiy_z1a",path=":fg/hiy/z1/",size="z1",face="a0031",lv=2.2999999999999998,id=13,mode=3}}},line=252,linkback="block_00045",linknext="block_00047"},block_00093={{"text"},line=465,linkback="block_00092",text={ja={{"「偶尔吗？不过话说回来，我被四个女孩子围在学校外面，我真的没有信心能愉快地交谈……」",{"rt2"},name={"智宏"}}}},linknext="block_00094"},block_00013={{"text"},line=91,linkback="block_00012",text={ja={{"其实说实话，我也觉得比起心形的，这个更好吃。但是我想红色的心形蛋糕女孩子应该会很喜欢吧……没关系的。",{"rt2"}}}},linknext="block_00014"},block_00049={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0034",lv=2.2999999999999998,id=13,mode=3},{"text"},line=268,linkback="block_00048",text={ja={{"「我自知不太会信任别人，所以我真心觉得能向他人坦诚自己的重要感情的哥哥很了不起」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00623",ch="hiy"}}},linknext="block_00050"},block_00016={{"fg",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0500",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0021",lv=2.2999999999999998,id=13,mode=3},{"text"},line=106,linkback="block_00015",text={ja={{"看到妃爱羡慕的眼神，我切了一小块心形蛋糕给她。她高兴得不得了，还张大嘴巴说「喂我～」",{"rt2"}}}},linknext="block_00017"},block_00097={{"text"},line=480,linkback="block_00096",text={ja={{"虽然我这么说，但在家里我还是要独占她。无论是哥哥的位置还是恋人的位置，我都不会让给任何人。",{"rt2"}}}},linknext="block_00098"},block_00038={{"text"},line=212,linkback="block_00037",text={ja={{"想起在大家面前读信时，妃爱问「为什么要在这个时候」，我开始担心她是否有被背叛的感觉。",{"rt2"}}}},linknext="block_00039"},block_00037={{"text"},line=210,linkback="block_00036",text={ja={{"「你不喜欢吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00038"},block_00034={{"text"},line=187,linkback="block_00033",text={ja={{"「哦？」",{"rt2"},name={"智宏"}}}},linknext="block_00035"},block_00006={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,path=":bg/",ay=720,sync=0,bx=-640,file="zbg002c",ax=1280},{"fg",resize=1,ex05="hiy_z1b0010",ch="妃愛",file="hiy_z1b0510",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",mode=3,face="b0055",lv=2.2999999999999998,id=13,sync=0},{"extrans",time=500},{"text"},line=54,linkback="block_00005",text={ja={{"「妃爱是个明事理、非常疼爱哥哥的好孩子。好孩子、好孩子」",{"rt2"},name={"智宏"}}}},linknext="block_00007"},block_00000={{"savetitle",text="お兄ちゃんのきもち"},{"msgoff"},{"bg",path=":bg/",file="bg002c",sync=0},{"fg",ex05="hiy_noa0000",ch="妃愛",file="hiy_noa0500",head="hiy_noa",path=":fg/hiy/no/",size="no",mode=3,face="a0032",lv=2.2000000000000002,id=13,sync=0},{"extrans",vague=40,rule="rule_002r"},{"bgm",file="bgm01"},{"text"},line=23,text={ja={{"『感谢你的降生。从我们初次相遇开始，没有任何一天我不对有你这个家庭成员存在表示感激——』",{"rt2"},name={"智宏"}}}},linknext="block_00001"},block_00027={{"fg",ex05="hiy_z2b0000",ch="妃愛",file="hiy_z2b0500",head="hiy_z2b",path=":fg/hiy/z2/",size="z2",face="b0035",lv=2.3999999999999999,id=13,mode=3},{"text"},text={ja={{"「所以，我可能难以判断哥哥是只是感到尴尬还是真的不喜欢。对我来说这是一件开心的事情。对不起」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00610",ch="hiy"}}},delay={vl01={{"fg",ex05="hiy_z2b0000",ch="妃愛",file="hiy_z2b0500",head="hiy_z2b",path=":fg/hiy/z2/",size="z2",face="b0055",lv=2.3999999999999999,id=13,mode=3}},vl02={{"fgact",id=13,ch="妃愛",lv=2.3999999999999999,act="おじぎ"}}},line=162,linkback="block_00026",linknext="block_00028"},block_00077={{"text"},line=390,linkback="block_00076",text={ja={{"原来还有这样的内幕。虽然有点罪恶感，但既然成年人都说可以，那应该没问题吧。果然是里姐，很靠得住。",{"rt2"}}}},linknext="block_00078"},block_00075={{"fg",ex05="hiy_z1b0010",ch="妃愛",file="hiy_z1b0510",head="hiy_z1b",path=":fg/hiy/z1/",size="z1",face="b0041",lv=2.2999999999999998,id=13,mode=3},{"text"},line=382,linkback="block_00074",text={ja={{"「然后，在接受申请的最后一天，如果到下午15点59分还没有男生参选，她就会开始处理我的申请。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00637",ch="hiy"}}},linknext="block_00076"},block_00029={{"text"},line=167,linkback="block_00028",text={ja={{"但是，我认为像妃爱这样了解我的人，应该能看出我真的很害羞，所以她应该在一定程度上已经明白了，然后才来问我。",{"rt2"}}}},linknext="block_00030"}}