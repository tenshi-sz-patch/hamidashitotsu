astver = 2.0
ast = {block_00082={{"fg",ex05="ame_noa0000",x=-230,size="no",file="ame_noa0500",head="ame_noa",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="a0010"},{"text"},text={ja={{"「直接说『在学生会里，常磐的画技是最好的』就行了」",{"rt2"},name={"智宏"}}}},line=456,linknext="block_00083",linkback="block_00081"},block_00148={{"se",stop=1,id=1,time=500},{"bgm",stop=1},{"cgdel",id=-1},{"bg",rule="rule_002",vague=40,wait=500,path=":bg/",file="black",time=1000},{"excall",file="05_天梨07_2"},{"text"},line=864,linkback="block_00147"},block_00046={{"fg",ex05="ame_noa0000",x=-230,size="no",file="ame_noa0500",head="ame_noa",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="a0008"},{"text"},text={ja={{"「哎？你真的一天就完成了……哇，这是什么？这就是所谓的三视图吗？哎？好厉害」",{"rt2"},name={"智宏"}}}},line=253,linknext="block_00047",linkback="block_00045"},block_00020={{"fg",ex05="ame_z1b0000",lv=2.2999999999999998,size="z1",file="ame_z1b0500",head="ame_z1b",mode=3,path=":fg/ame/z1/",ch="天梨",id=13,face="b0034"},{"text"},text={ja={{"「学长为了文化祭把大家聚在一起，真的好酷！计划已经有实现的可能了，真的很厉害呢」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00491",ch="ame"}}},linknext="block_00021",delay={vl01={{"fg",ex05="ame_z1b0010",lv=2.2999999999999998,size="z1",file="ame_z1b0510",head="ame_z1b",mode=3,path=":fg/ame/z1/",ch="天梨",id=13,face="b0052"}}},line=112,linkback="block_00019"},block_00091={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0001"},{"text"},text={ja={{"「天梨的爸爸这么年轻就是管理职员，好厉害……而且从行业来看，他的工作很时尚，好酷。」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00305",ch="kan"}}},linknext="block_00092",delay={vl01={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0055"}}},line=510,linkback="block_00090"},block_00011={{"fg",ex05="ame_nob0010",lv=2.2000000000000002,size="no",file="ame_nob0510",head="ame_nob",mode=3,path=":fg/ame/no/",ch="天梨",id=13,face="b0043"},{"fgact",id=13,ch="天梨",act="おじぎ",lv=2.2000000000000002},{"text"},text={ja={{"「对啊！我以为他们九点半就开门了呢！既然伊贺田屋还没开，连乔拉奈也没开，真是太早了！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00488",ch="ame"}}},linknext="block_00012",delay={vl01={{"fg",ex05="ame_nob0010",lv=2.2000000000000002,size="no",file="ame_nob0510",head="ame_nob",mode=3,path=":fg/ame/no/",ch="天梨",id=13,face="b0047"}}},line=72,linkback="block_00010"},block_00061={{"fg",ex05="kan_noa0010",x=230,size="no",file="kan_noa0510",head="kan_noa",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="a0010"},{"text"},text={ja={{"「啊，有经验的人……那、那样的话，他的知识和技术肯定比我多。对不起，我怀疑你了。」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00292",ch="kan"}}},linknext="block_00062",delay={vl01={{"fg",ex05="kan_noa0010",x=230,size="no",file="kan_noa0510",head="kan_noa",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="a0020"}},vl02={{"fg",ex05="kan_noa0000",x=230,size="no",file="kan_noa0500",head="kan_noa",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="a0028"}}},line=335,linkback="block_00060"},block_00088={{"fg",ex05="kan_nob0010",x=230,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0008"},{"text"},text={ja={{"「家具的设计图……天梨的爸爸，是做什么的？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00304",ch="kan"}}},linknext="block_00089",delay={vl01={{"fg",ex05="kan_nob0010",x=230,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0010"}}},line=492,linkback="block_00087"},block_00086={{"fg",ex05="ame_nob0000",x=-230,size="no",file="ame_nob0500",head="ame_nob",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="b0052"},{"text"},text={ja={{"「啊！那我来给你一些建议怎么样？虽然不是衣服而是家具，但我看过我爸爸画的设计图！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00506",ch="ame"}}},linknext="block_00087",delay={vl01={{"fg",ex05="ame_nob0000",x=-230,size="no",file="ame_nob0500",head="ame_nob",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="b0054"}}},line=479,linkback="block_00085"},block_00064={{"fg",ex05="kan_noa0010",x=230,size="no",file="kan_noa0510",head="kan_noa",mode=3,path=":fg/kan/no/",id=10,hoho="h",ch="華乃",lv=2.2000000000000002,face="a0050h"},{"text"},text={ja={{"我出于好奇问了一句，但话还没说完，常磐的脸就已经红了，我立刻意识到这是不能问的事情。",{"rt2"}}}},line=344,linknext="block_00065",linkback="block_00063"},block_00070={{"fg",ex05="kan_z1a0010",x=230,size="z1",file="kan_z1a0510",head="kan_z1a",mode=3,path=":fg/kan/z1/",id=10,hoho="h",ch="華乃",lv=2.2999999999999998,face="a0042h"},{"fgact",id=10,ch="華乃",act="うんうん",lv=2.2999999999999998},{"text"},text={ja={{"「如果是为自己做的话，即使有点难也能做出来！但那更接近于改造而不是制作！所以为别人做就完全没办法！」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00296",ch="kan"}}},linknext="block_00071",delay={vl01={{"fg",ex05="kan_z1a0010",x=230,size="z1",file="kan_z1a0510",head="kan_z1a",mode=3,path=":fg/kan/z1/",id=10,hoho="h",ch="華乃",lv=2.2999999999999998,face="a0036h"}}},line=379,linkback="block_00069"},block_00141={{"fg",ex05="kan_nob0010",x=0,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=13,ch="華乃",lv=2.2000000000000002,face="b0051"},{"text"},text={ja={{"「我正打算吃午饭呢。诗樱前辈要和我们一起去买面包吗？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00315",ch="kan"}}},linknext="block_00142",delay={vl01={{"fg",ex05="kan_nob0010",x=0,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=13,ch="華乃",lv=2.2000000000000002,face="b0055"}}},line=820,linkback="block_00140"},block_00034={{"text"},text={ja={{"不过我觉得还算积极。没有自怜自艾。对于我来说，算是做得很好了。",{"rt2"}}}},line=170,linknext="block_00035",linkback="block_00033"},block_00138={{"se",stop=1,id=1,time=500},{"fg",ex05="shi_nob0010",x=-380,size="no",file="shi_nob0510",head="shi_nob",mode=3,path=":fg/shi/no/",id=7,ch="詩桜",lv=2.2000000000000002,face="b0052"},{"text"},text={ja={{"「嗨，你好。我想看看常磐在设计的工作场景，所以特地过来打扰了一下。」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_01723",ch="shi"}}},line=799,linknext="block_00139",linkback="block_00137"},block_00137={{"fg",ex05="ame_nob0000",x=380,size="no",file="ame_nob0500",head="ame_nob",mode=3,path=":fg/ame/no/",id=8,ch="天梨",lv=2.2000000000000002,face="b0010"},{"text"},text={ja={{"回来的常磐带回了一个出乎意料的人。常磐是事先知道她会来的吗？至少我是没听说过。",{"rt2"}}}},line=792,linknext="block_00138",linkback="block_00136"},block_00109={{"fg",ex05="ame_z1a0000",x=-230,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",id=9,ch="天梨",lv=2.2999999999999998,face="a0002"},{"text"},text={ja={{"「哎呀……是我没有读懂气氛吗」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00513",ch="ame"}}},line=616,linknext="block_00110",linkback="block_00108"},block_00056={{"text"},text={ja={{"「那么这个，今天内让学生会全体确认一下比较好吧？毕竟我们要尽可能节省画制版的时间……」",{"rt2"},name={"智宏"}}}},line=304,linknext="block_00057",linkback="block_00055"},block_00055={{"fg",ex05="kan_nob0010",x=230,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0053"},{"text"},text={ja={{"「听天梨说可爱，我就放心了。虽然我对吸引宅男有自信，但是阳光孩子们的感觉我就不太懂了，啊哈哈……」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00289",ch="kan"}}},linknext="block_00056",delay={vl01={{"fg",ex05="kan_nob0010",x=230,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0034"}},vl02={{"fgact",id=10,ch="華乃",act="うんうん",lv=2.2000000000000002}}},line=301,linkback="block_00054"},block_00115={{"fg",ex05="kan_z1b0010",x=230,size="z1",file="kan_z1b0510",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=10,ch="華乃",lv=2.2999999999999998,face="b0054"},{"fg",ex05="ame_z1a0000",x=-230,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",id=9,hoho="h",ch="天梨",lv=2.2999999999999998,face="a0009h"},{"text"},text={ja={{"「或者我们可以买些面包回来，把土豆沙拉夹在里面做成三明治也不错。我记得冰箱里应该还有生菜」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00311",ch="kan"}}},linknext="block_00116",delay={vl01={{"fg",ex05="kan_z1b0010",x=230,size="z1",file="kan_z1b0510",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=10,ch="華乃",lv=2.2999999999999998,face="b0053"}}},line=657,linkback="block_00114"},block_00008={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",file="bg018a",path=":bg/",sync=0},{"fg",ex05="ame_noa0000",lv=2.2000000000000002,size="no",file="ame_noa0500",head="ame_noa",mode=3,sync=0,path=":fg/ame/no/",id=13,ch="天梨",face="a0010",resize=1},{"extrans",time=500},{"text"},text={ja={{"「话说回来，我总是比你晚到。抱歉让你在这么热的天气里等我」",{"rt2"},name={"智宏"}}}},line=54,linknext="block_00009",linkback="block_00007"},block_00116={{"fg",ex05="ame_z1a0000",x=0,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",id=9,lv=2.2999999999999998,hoho="h",ch="天梨",x2=-230,face="a0047h"},{"fgact",id=9,ch="天梨",act="いいえ",lv=2.2999999999999998},{"fg",ex05="kan_z1b0010",x=230,size="z1",file="kan_z1b0510",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=10,ch="華乃",lv=2.2999999999999998,face="b0008"},{"fg",id=10,x=380,file="kan_z1b0510",head="kan_z1b",mode=3,path=":fg/kan/z1/",ex05="kan_z1b0010",lv=2.2999999999999998,ch="華乃",x2=230,face="b0008"},{"text"},text={ja={{"「呜呜呜，华乃真贴心！ 谢谢你，谢谢你！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00514",ch="ame"}}},linknext="block_00117",delay={vl01={{"fgact",id=9,ch="天梨",act="縦揺れ",lv=2.2999999999999998}}},line=669,linkback="block_00115"},block_00019={{"text"},text={ja={{"「啊，啊，嗯。为了学生会……也为了我自己」",{"rt2"},name={"智宏"}}}},line=105,linknext="block_00020",linkback="block_00018"},block_00067={{"fg",ex05="kan_z1b0000",x=230,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=10,hoho="h",ch="華乃",lv=2.2999999999999998,face="b0009h"},{"text"},text={ja={{"「但是你可以慢慢地修改，花时间去做，最终会得到一个适合自己身材的模型！这难道不对吗！？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00295",ch="kan"}}},linknext="block_00068",delay={vl01={{"fg",ex05="kan_z1b0000",x=230,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=10,hoho="h",ch="華乃",lv=2.2999999999999998,face="b0036h"},{"fgact",id=10,ch="華乃",act="びっくり",lv=2.2999999999999998}}},line=365,linkback="block_00066"},block_00053={{"fg",ex05="ame_noa0000",x=-230,size="no",file="ame_noa0500",head="ame_noa",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="a0053"},{"text"},text={ja={{"「我们的目标终究是「提案」，我觉得这样就够了。外观和印象很重要。」",{"rt2"},name={"智宏"}}}},line=285,linknext="block_00054",linkback="block_00052"},block_00072={{"fg",ex05="ame_noa0000",x=-230,size="no",file="ame_noa0500",head="ame_noa",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="a0029"},{"text"},text={ja={{"「我犹豫了好久，但是我还是投这个设计一票！华乃你画的真好啊！我有点嫉妒呢～」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00503",ch="ame"}}},linknext="block_00073",delay={vl01={{"fg",ex05="ame_noa0000",x=-230,size="no",file="ame_noa0500",head="ame_noa",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="a0052"},{"fgact",id=9,ch="天梨",act="うんうん",lv=2.2000000000000002}},vl02={{"fg",ex05="ame_noa0010",x=-230,size="no",file="ame_noa0510",head="ame_noa",ex03="a0097",path=":fg/ame/no/",id=9,lv=2.2000000000000002,ch="天梨",face="a0054",mode=3}}},line=392,linkback="block_00071"},block_00099={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",file="bg006a",path=":bg/",sync=0},{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,sync=0,path=":fg/kan/no/",id=10,lv=2.2000000000000002,ch="華乃",face="b0020",resize=1},{"fg",ex05="ame_noa0000",x=-230,size="no",file="ame_noa0500",head="ame_noa",mode=3,sync=0,path=":fg/ame/no/",id=9,lv=2.2000000000000002,ch="天梨",face="a0002",resize=1},{"extrans",time=500},{"text"},text={ja={{"「啊？话说回来，如果设计已经完成了，那在决定使用哪一个之前，我们就没有什么要做的了吧？」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00510",ch="ame"}}},line=555,linknext="block_00100",linkback="block_00098"},block_00076={{"fg",ex05="ame_noa0010",x=-230,size="no",file="ame_noa0510",head="ame_noa",ex03="a0097",path=":fg/ame/no/",id=9,lv=2.2000000000000002,ch="天梨",face="a0008",mode=3},{"fgact",id=9,ch="天梨",act="びっくり",lv=2.2000000000000002},{"text"},text={ja={{"「哎，不一样吗？但我又没有别的表达方式……」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00504",ch="ame"}}},linknext="block_00077",delay={vl01={{"fg",ex05="ame_noa0010",x=-230,size="no",file="ame_noa0510",head="ame_noa",ex03="a0097",path=":fg/ame/no/",id=9,lv=2.2000000000000002,ch="天梨",face="a0002",mode=3}}},line=419,linkback="block_00075"},block_00095={{"fg",ex05="kan_z1b0000",x=380,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=8,ch="華乃",lv=2.2999999999999998,face="b0029"},{"fg",ex05="ame_z1a0000",lv=1,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",id=9,x=0,ch="天梨",x2=0,face="a0055"},{"text"},text={ja={{"天梨抱着她的手臂，一边请求，常磐就答应了去龙闲家的邀请。这两个人，感觉真的很般配……。",{"rt2"}}}},line=538,linknext="block_00096",linkback="block_00094"},block_00132={{"fg",ex05="ame_z2a0000",lv=2.3999999999999999,size="z2",file="ame_z2a0500",head="ame_z2a",mode=3,path=":fg/ame/z2/",ch="天梨",id=9,face="a0054"},{"fgact",id=9,ch="天梨",act="うんうん",lv=2.3999999999999999},{"text"},text={ja={{"「好！ 好！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00519",ch="ame"}}},line=757,linknext="block_00133",linkback="block_00131"},block_00066={{"fg",ex05="kan_z1b0000",x=230,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=10,ch="華乃",lv=2.2999999999999998,face="b0038"},{"fg",ex05="ame_noa0000",x=-230,size="no",file="ame_noa0500",head="ame_noa",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="a0034"},{"text"},text={ja={{"「当然刚开始的时候，可能会做得不合身，或者太松，或者太紧，甚至有些成品质量很差根本不能看。」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00294",ch="kan"}}},linknext="block_00067",delay={vl01={{"fg",ex05="kan_z1b0000",x=230,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=10,ch="華乃",lv=2.2999999999999998,face="b0043"}}},line=358,linkback="block_00065"},block_00001={{"text"},text={ja={{"为了避免在等待另一方到来时无事可做，从而影响设计工作，我和天梨决定在常磐家分头行动，并在车站前集合。",{"rt2"}}}},line=21,linknext="block_00002",linkback="block_00000"},block_00016={{"text"},text={ja={{"没关系。无论我的长相如何，我一直注意着天梨说的那个让男生受欢迎的秘诀——「清洁感」。我每天都会洗脸洗头，整理好发型。",{"rt2"}}}},line=87,linknext="block_00017",linkback="block_00015"},block_00029={{"fg",ex05="ame_z1b0000",lv=2.2999999999999998,size="z1",file="ame_z1b0500",head="ame_z1b",mode=3,path=":fg/ame/z1/",hoho="h",ch="天梨",id=13,face="b0008h"},{"fgact",id=13,ch="天梨",act="ドッキリ",lv=2.2999999999999998},{"text"},text={ja={{"「啊……」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00494",ch="ame"}}},line=148,linknext="block_00030",linkback="block_00028"},block_00041={{"text"},text={ja={{"这是，她把额头靠在我肩胛骨之间……虽然周围的人已经少了，但这里毕竟还是道路啊。",{"rt2"}}}},line=210,linknext="block_00042",linkback="block_00040"},block_00047={{"fg",ex05="ame_noa0010",x=-230,size="no",file="ame_noa0510",head="ame_noa",ex03="a0097",path=":fg/ame/no/",id=9,lv=2.2000000000000002,ch="天梨",face="a0008",mode=3},{"text"},text={ja={{"「还给我们详细地解说了……好厉害！华乃你真厉害！啊——工作效率也太高了吧？啊——」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00501",ch="ame"}}},linknext="block_00048",delay={vl01={{"fg",ex05="ame_noa0010",x=-230,size="no",file="ame_noa0510",head="ame_noa",ex03="a0097",path=":fg/ame/no/",id=9,lv=2.2000000000000002,ch="天梨",face="a0051",mode=3},{"fgact",id=9,ch="天梨",act="ジャンプ",lv=2.2000000000000002}},vl02={{"fg",ex05="ame_noa0010",x=-230,size="no",file="ame_noa0510",head="ame_noa",ex03="a0097",path=":fg/ame/no/",id=9,lv=2.2000000000000002,ch="天梨",face="a0054",mode=3}}},line=262,linkback="block_00046"},block_00120={{"fg",ex05="ame_z1a0000",x=0,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",id=9,hoho="h",ch="天梨",lv=2.2999999999999998,face="a0002h"},{"text"},text={ja={{"「会长，谢谢你……」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00515",ch="ame"}}},line=683,linknext="block_00121",linkback="block_00119"},block_00027={{"text"},text={ja={{"「因为天梨看起来很开心。」",{"rt2"},name={"智宏"}}}},line=135,linknext="block_00028",linkback="block_00026"},block_00080={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0034"},{"text"},text={ja={{"常磐似乎并不认为自己比专业人士更优秀，她笑着说。但是即使从我这个角度来看，我不得不再次确认，常磐的画画技术确实很高超。",{"rt2"}}}},line=444,linknext="block_00081",linkback="block_00079"},block_00032={{"fg",ex05="ame_z1a0000",lv=2.2999999999999998,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",ch="天梨",id=13,face="a0034"},{"fgact",id=13,ch="天梨",act="うなづく",lv=2.2999999999999998},{"text"},text={ja={{"「啊，对，对对对！就是这样！这可不只是我的计划，大家都参与进来的计划啊！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00496",ch="ame"}}},linknext="block_00033",delay={vl01={{"fg",ex05="ame_z1a0010",lv=2.2999999999999998,size="z1",file="ame_z1a0510",head="ame_z1a",ex03="a0097",path=":fg/ame/z1/",id=13,ch="天梨",face="a0041",mode=3}}},line=166,linkback="block_00031"},block_00140={{"fg",ex05="shi_nob0000",x=-380,size="no",file="shi_nob0500",head="shi_nob",mode=3,path=":fg/shi/no/",id=7,ch="詩桜",lv=2.2000000000000002,face="b0031"},{"text"},text={ja={{"「和泉老师告诉我的。我来看看专业现场，当然也带了礼物。五包章鱼烧怎么样？」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_01724",ch="shi"}}},linknext="block_00141",delay={vl01={{"fg",ex05="shi_nob0000",x=-380,size="no",file="shi_nob0500",head="shi_nob",mode=3,path=":fg/shi/no/",id=7,ch="詩桜",lv=2.2000000000000002,face="b0032"}},vl02={{"fg",ex05="shi_nob0000",x=-380,size="no",file="shi_nob0500",head="shi_nob",mode=3,path=":fg/shi/no/",id=7,ch="詩桜",lv=2.2000000000000002,face="b0006"}}},line=813,linkback="block_00139"},block_00060={{"text"},text={ja={{"「我正打算解释这个。他以前去过裁缝课堂。」",{"rt2"},name={"智宏"}}}},line=326,linknext="block_00061",linkback="block_00059"},block_00014={{"fg",ex05="ame_noa0000",lv=2.2000000000000002,size="no",file="ame_noa0500",head="ame_noa",mode=3,path=":fg/ame/no/",ch="天梨",id=13,face="a0054"},{"text"},text={ja={{"「那么，我们去你家吧」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00489",ch="ame"}}},line=81,linknext="block_00015",linkback="block_00013"},block_00045={{"bgm",stop=1,time=1000},{"cgdel",id=-1},{"bg",rule="rule_001",vague=40,wait=250,path=":bg/",file="black",time=1000},{"msgoff"},{"bg",file="bg006a",path=":bg/",sync=0},{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,sync=0,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0001"},{"fg",ex05="ame_noa0000",x=-230,size="no",file="ame_noa0500",head="ame_noa",mode=3,sync=0,path=":fg/ame/no/",id=9,lv=2.2000000000000002,ch="天梨",face="a0001",resize=1},{"extrans",vague=40,rule="rule_001"},{"bgm",file="bgm07"},{"fgact",id=10,ch="華乃",act="うなづく",lv=2.2000000000000002},{"text"},text={ja={{"「好了，这就是设计稿。我先画了两种款式试试。」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00286",ch="kan"}}},linknext="block_00046",delay={vl01={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0027"}}},line=248,linkback="block_00044"},block_00079={{"fg",ex05="ame_noa0010",x=-230,size="no",file="ame_noa0510",head="ame_noa",ex03="a0097",path=":fg/ame/no/",id=9,lv=2.2000000000000002,ch="天梨",face="a0008",mode=3},{"text"},text={ja={{"「原来衣服设计是这么画出来的！哇～真的吗～虽然我能感觉到很时尚，但可能还是会觉得华乃你更厉害呢！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00505",ch="ame"}}},linknext="block_00080",delay={vl01={{"fg",ex05="ame_noa0010",x=-230,size="no",file="ame_noa0510",head="ame_noa",ex03="a0097",path=":fg/ame/no/",id=9,lv=2.2000000000000002,ch="天梨",face="a0054",mode=3}}},line=440,linkback="block_00078"},block_00068={{"text"},text={ja={{"「是的。常磐说得对。是我的想象力不足。我深感遗憾。」",{"rt2"},name={"智宏"}}}},line=368,linknext="block_00069",linkback="block_00067"},block_00081={{"fg",ex05="kan_noa0000",x=230,size="no",file="kan_noa0500",head="kan_noa",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="a0002"},{"text"},text={ja={{"「所以，既然只有和泉和天梨在，我就假设我能画出一些还算不错的设计。那我应该如何向新川君解释我能画设计的原因呢？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00301",ch="kan"}}},linknext="block_00082",delay={vl01={{"fg",ex05="kan_noa0000",x=230,size="no",file="kan_noa0500",head="kan_noa",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="a0003"}}},line=451,linkback="block_00080"},block_00062={{"text"},text={ja={{"当然，我没有提到关于cosplay的事情。关于新川曾经上过裁缝课堂的事情，我已经通过LIME向新川本人确认，并在昨天感谢他的同时询问是否可以告诉其他干部。",{"rt2"}}}},line=337,linknext="block_00063",linkback="block_00061"},block_00083={{"fg",ex05="kan_noa0010",x=230,size="no",file="kan_noa0510",head="kan_noa",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="a0028"},{"text"},text={ja={{"「只因为画得好就能画出设计吗？三视图呢？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00302",ch="kan"}}},line=461,linknext="block_00084",linkback="block_00082"},block_00075={{"fg",ex05="kan_nob0010",x=230,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0019"},{"text"},text={ja={{"「但这只是衣服设计，和画画技巧好坏又是另一回事哦」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00298",ch="kan"}}},line=411,linknext="block_00076",linkback="block_00074"},block_00000={{"savetitle",text="よく言えば家庭的"},{"bg",file="bg018a",path=":bg/",sync=0},{"fg",ex05="ame_noa0010",lv=2.2000000000000002,size="no",file="ame_noa0510",head="ame_noa",ex03="a0097",sync=0,path=":fg/ame/no/",id=13,ch="天梨",face="a0052",mode=3},{"extrans",time=2000},{"bgm",file="bgm06"},{"text"},text={ja={{"「啊」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00485",ch="ame"}}},linknext="block_00001",line=19},block_00031={{"text"},text={ja={{"「嗯，嗯当然！这也是为了学生会，也是为了我自己！」",{"rt2"},name={"智宏"}}}},line=158,linknext="block_00032",linkback="block_00030"},block_00043={{"fg",ex05="ame_z1b0010",lv=2.2999999999999998,size="z1",file="ame_z1b0510",head="ame_z1b",mode=3,path=":fg/ame/z1/",ch="天梨",id=13,face="b0023"},{"text"},text={ja={{"「我也会努力做好我能做的事情的。特别是当天的走秀，请放心交给我！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00500",ch="ame"}}},linknext="block_00044",delay={vl01={{"fg",ex05="ame_z1b0000",lv=2.2999999999999998,size="z1",file="ame_z1b0500",head="ame_z1b",mode=3,path=":fg/ame/z1/",ch="天梨",id=13,face="b0006"},{"fgact",id=13,ch="天梨",act="うなづく",lv=2.2999999999999998}}},line=222,linkback="block_00042"},block_00050={{"text"},text={ja={{"制服从正面、背后、侧面三个方向看的插图，袖口和下摆等设计部分都有文字说明。",{"rt2"}}}},line=271,linknext="block_00051",linkback="block_00049"},block_00033={{"text"},text={ja={{"「我就是想看到你的笑容」这样的话，或许能够直击女生的心。但是，这已经是我所能做到的极限了。",{"rt2"}}}},line=168,linknext="block_00034",linkback="block_00032"},block_00018={{"msgoff"},{"bg",x=-200,y=0,file="zbg018a",sync=0,ay=720,path=":bg/",ax=1280,by=-360,bx=-640},{"fg",ex05="ame_z1a0000",lv=2.2999999999999998,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,sync=0,path=":fg/ame/z1/",id=13,ch="天梨",face="a0055",resize=1},{"extrans"},{"fgact",id=13,ch="天梨",act="うんうん",lv=2.2999999999999998},{"text"},text={ja={{"「嘿嘿嘿，学长。昨天辛苦了。你真的很努力呢」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00490",ch="ame"}}},linknext="block_00019",delay={vl01={{"fg",ex05="ame_z1a0000",lv=2.2999999999999998,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",ch="天梨",id=13,face="a0052"}}},line=102,linkback="block_00017"},block_00105={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",ay=720,path=":bg/",ax=1280,file="zbg006a",bx=-640,by=-360,sync=0},{"fg",ex05="kan_z1b0000",x=230,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,sync=0,path=":fg/kan/z1/",id=10,lv=2.2999999999999998,ch="華乃",face="b0039",resize=1},{"fg",ex05="ame_z1a0000",x=-230,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,sync=0,path=":fg/ame/z1/",id=9,lv=2.2999999999999998,ch="天梨",face="a0034",resize=1},{"extrans",time=500},{"text"},text={ja={{"「其，其实是这样的，我想大家在华乃画设计图的时候可以一起吃点东西，所以我做了土豆沙拉过来了」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00512",ch="ame"}}},linknext="block_00106",delay={vl01={{"fg",ex05="ame_z1a0000",x=-230,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",id=9,ch="天梨",lv=2.2999999999999998,face="a0054"}}},line=601,linkback="block_00104"},block_00111={{"msgoff"},{"fg",mode=-2,sync=0},{"msgoff"},{"bg",id=7,lv=8,file="white",path=":bg/",sync=0,time=1000},{"bg",id=7,lv=8,sync=0,file="reco",path=":sys/",notone=1,time=1000},{"colortone",style="sepia"},{"bg",path=":ev/ev_ame_32/",time=400,file="ev_ame_32_05",face="1",set="ev_ame_32",sync=0},{"extrans",time=500},{"text"},text={ja={{"为什么天梨会带土豆沙拉过来。我对此原因有所了解。就是因为我前段时间大大地表扬过她。",{"rt2"}}}},line=630,linknext="block_00112",linkback="block_00110"},block_00102={{"fg",ex05="kan_noa0000",x=230,size="no",file="kan_noa0500",head="kan_noa",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="a0039"},{"text"},text={ja={{"「虽然这么说，但确实我们已经没有什么要做的了……我们三个人一起吃个饭然后散会吧？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00308",ch="kan"}}},linknext="block_00103",delay={vl01={{"fg",ex05="kan_noa0000",x=230,size="no",file="kan_noa0500",head="kan_noa",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="a0001"}}},line=573,linkback="block_00101"},block_00084={{"text"},text={ja={{"「刚才常磐说过，即使绘画技术本身并不高超，但如果有图纸知识的话……所以我们可以沿着这个思路走」",{"rt2"},name={"智宏"}}}},line=464,linknext="block_00085",linkback="block_00083"},block_00005={{"fg",ex05="ame_z1a0000",lv=2.2999999999999998,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",ch="天梨",id=13,face="a0053"},{"text"},text={ja={{"这已经是第三次和天梨约会了。我以为我已经习惯了……但当她看到我，高兴地挥手时，我意识到自己的脸红了。",{"rt2"}}}},line=40,linknext="block_00006",linkback="block_00004"},block_00049={{"text"},text={ja={{"华乃给我看的制服设计，真的只画了「制服」。可能是因为如果画上穿着人的脸会暴露身份。",{"rt2"}}}},line=269,linknext="block_00050",linkback="block_00048"},block_00107={{"text"},text={ja={{"啊？ 去同学家玩（虽然不算玩）还带土豆沙拉，这不是太过分心了吗……这是阳光型人物的标准选择吗？",{"rt2"}}}},line=607,linknext="block_00108",linkback="block_00106"},block_00054={{"fg",ex05="ame_nob0000",x=-230,size="no",file="ame_nob0500",head="ame_nob",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="b0034"},{"fgact",id=9,ch="天梨",act="いいえ",lv=2.2000000000000002},{"text"},text={ja={{"「啊——但是这个，我真不知道该选哪一个好呢！两个都好可爱，只能选一个真让人烦恼！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00502",ch="ame"}}},linknext="block_00055",delay={vl01={{"fg",ex05="ame_nob0010",x=-230,size="no",file="ame_nob0510",head="ame_nob",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="b0002"}}},line=293,linkback="block_00053"},block_00040={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",x=-200,y=0,file="zbg018a",sync=0,ay=720,path=":bg/",ax=1280,by=-360,bx=-640},{"cgdel",id=5,lv=5,sync=0,rule="cinema",time=400},{"extrans",time=500},{"text"},text={ja={{"我听到了背后的声音。即使稍微扭头去看，也看不到天梨的身影。我发现我的两侧被紧紧握住了。",{"rt2"}}}},line=208,linknext="block_00041",linkback="block_00039"},block_00051={{"text"},text={ja={{"这种东西经常可以在动画的设定资料集中看到……最近也常常看到Vtuber在首次亮相时会上传这种图片。",{"rt2"}}}},line=273,linknext="block_00052",linkback="block_00050"},block_00052={{"fg",ex05="kan_nob0010",x=230,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0002"},{"text"},text={ja={{"「只是不知道是否应该称之为实用性，关于功能性我完全是个外行，没有考虑过。我只把自己的特色「可爱」融入了设计中。」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00288",ch="kan"}}},linknext="block_00053",delay={vl01={{"fg",ex05="kan_nob0010",x=230,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0027"}}},line=280,linkback="block_00051"},block_00038={{"text"},text={ja={{"本想转身的我却停下了，在某个位置我的身体无法扭转。因为背脊处有东西碰到了。",{"rt2"}}}},line=187,linknext="block_00039",linkback="block_00037"},block_00125={{"text"},text={ja={{"天梨还保持着像弃犬般的模样。她轻轻地捏住我的衣角，以一种无助的眼神望着我。",{"rt2"}}}},line=719,linknext="block_00126",linkback="block_00124"},block_00114={{"fg",ex05="kan_z1b0000",x=230,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=10,ch="華乃",lv=2.2999999999999998,face="b0052"},{"text"},text={ja={{"「啊，嗯……附近有家很好吃的面包店，我想大家一起吃的话肯定会非常搭。我们一起去买吧」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00310",ch="kan"}}},linknext="block_00115",delay={vl01={{"fg",ex05="kan_z1b0000",x=230,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=10,ch="華乃",lv=2.2999999999999998,face="b0055"}}},line=649,linkback="block_00113"},block_00037={{"fg",ex05="ame_z1a0000",lv=2.2999999999999998,size="z1",file="ame_z1a0500",head="ame_z1a",mode=1,path=":fg/ame/z1/",ch="天梨",id=20,face="a0014"},{"text"},text={ja={{"「不、不是这个！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00498",ch="ame"}}},line=185,linknext="block_00038",linkback="block_00036"},block_00059={{"fg",ex05="kan_noa0000",x=230,size="no",file="kan_noa0500",head="kan_noa",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="a0018"},{"fg",ex05="ame_noa0000",x=-230,size="no",file="ame_noa0500",head="ame_noa",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="a0001"},{"text"},text={ja={{"「是新川吗？他坐在你后面，我对他的名字和脸有些印象……但我不确定。他怎么会画制版呢？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00291",ch="kan"}}},linknext="block_00060",delay={vl01={{"fg",ex05="kan_noa0000",x=230,size="no",file="kan_noa0500",head="kan_noa",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="a0002"}}},line=323,linkback="block_00058"},block_00015={{"bg",x=-640,y=0,file="zbg018a",ay=720,path=":bg/",by=-360,ax=1280,speed=120000,bx=-640,x2=640,y2=0},{"text"},text={ja={{"我和天梨并排开始走。然后像旁边的女孩一样引起人们关注的我的脸。",{"rt2"}}}},line=85,linknext="block_00016",linkback="block_00014"},block_00093={{"fg",ex05="kan_z1b0000",x=380,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=8,ch="華乃",lv=2.2999999999999998,face="b0038"},{"text"},text={ja={{"「诶……我从来没去过同班同学的家……啊。嗯，可以的。」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00306",ch="kan"}}},linknext="block_00094",delay={vl01={{"fg",ex05="kan_z1b0000",x=380,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=8,ch="華乃",lv=2.2999999999999998,face="b0012"}}},line=531,linkback="block_00092"},block_00069={{"fg",ex05="kan_z1b0000",x=230,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=10,hoho="h",ch="華乃",lv=2.2999999999999998,face="b0016h"},{"text"},text={ja={{"'自己的'，她说了'自己的身材'。这不是说她在'被迫'为'别人'缝制衣服吗？",{"rt2"}}}},line=372,linknext="block_00070",linkback="block_00068"},block_00024={{"text"},text={ja={{"虽然我无法像夸耀自己的成就那样，但是如果低调一点能让人们称赞我，那我至少应该说些积极的话。",{"rt2"}}}},line=124,linknext="block_00025",linkback="block_00023"},block_00022={{"fg",ex05="ame_z1b0000",lv=2.2999999999999998,size="z1",file="ame_z1b0500",head="ame_z1b",mode=3,path=":fg/ame/z1/",ch="天梨",id=13,face="b0010"},{"text"},text={ja={{"「但是如果没有学长在这里那里忙前忙后、发出指示的话，我们现在也不可能进展到这个地步吧？」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00492",ch="ame"}}},line=120,linknext="block_00023",linkback="block_00021"},block_00017={{"text"},text={ja={{"啊，但是前发是不是该剪剪了……总是挡在眼前……",{"rt2"}}}},line=89,linknext="block_00018",linkback="block_00016"},block_00025={{"text"},text={ja={{"「我之所以出人意料地努力……不，现在也还在努力，是因为……」",{"rt2"},name={"智宏"}}}},line=127,linknext="block_00026",linkback="block_00024"},block_00003={{"text"},text={ja={{"「嗯，嗯」",{"rt2"},name={"智宏"}}}},line=34,linknext="block_00004",linkback="block_00002"},block_00092={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",ay=720,path=":bg/",ax=1280,file="zbg006a",bx=-640,by=-360,sync=0},{"fg",ex05="kan_z1b0000",x=380,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,sync=0,path=":fg/kan/z1/",id=8,lv=2.2999999999999998,ch="華乃",face="b0008",resize=1},{"fg",ex05="ame_z1a0000",lv=1,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,sync=0,path=":fg/ame/z1/",id=9,x=0,face="a0052",ch="天梨",x2=0,resize=1},{"extrans",time=500},{"text"},text={ja={{"「呃——你赞美我爸爸的工作，我好开心！华乃酱，下次来我家玩吧！我有很多想给你看的家具！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00509",ch="ame"}}},linknext="block_00093",delay={vl01={{"fg",ex05="ame_z1a0000",lv=1,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",id=9,x=0,ch="天梨",x2=0,face="a0051"},{"fgact",id=9,ch="天梨",act="うんうん",lv=1}}},line=524,linkback="block_00091"},block_00106={{"fg",ex05="kan_z1b0000",x=230,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=10,ch="華乃",lv=2.2999999999999998,face="b0040"},{"text"},text={ja={{"看到从天梨包里拿出来的塑料盒子，常磐的眼睛瞪得圆了。说起来，我也是吓了一跳。",{"rt2"}}}},line=605,linknext="block_00107",linkback="block_00105"},block_00048={{"fg",ex05="kan_noa0010",x=230,size="no",file="kan_noa0510",head="kan_noa",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="a0001"},{"text"},text={ja={{"「因为必须是在学校里可以穿的设计，所以并没有特别讲究。」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00287",ch="kan"}}},line=267,linknext="block_00049",linkback="block_00047"},block_00010={{"text"},text={ja={{"「布料店是指伊贺田屋吗？我查过，他们不是十点才开门吗？现在应该刚开门……」",{"rt2"},name={"智宏"}}}},line=64,linknext="block_00011",linkback="block_00009"},block_00013={{"text"},text={ja={{"虽然这样说，但天梨似乎就在那个时间到达的，所以我并没有进一步追问。",{"rt2"}}}},line=76,linknext="block_00014",linkback="block_00012"},block_00065={{"fg",ex05="kan_z1a0000",x=230,size="z1",file="kan_z1a0500",head="kan_z1a",mode=3,path=":fg/kan/z1/",id=10,lv=2.2999999999999998,hoho="h",ch="華乃",face="a0042h",resize=1},{"fgact",id=10,ch="華乃",act="うんうん",lv=2.2999999999999998},{"text"},text={ja={{"「我、我、说、过，其他的学生coser也在自己制作服装呢。就算自己不能制作模型，也可以改造现有的东西，尽可能地动手做！」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00293",ch="kan"}}},linknext="block_00066",delay={vl01={{"fg",ex05="kan_z1a0010",x=230,size="z1",file="kan_z1a0510",head="kan_z1a",mode=3,path=":fg/kan/z1/",id=10,hoho="h",ch="華乃",lv=2.2999999999999998,face="a0003h"}}},line=351,linkback="block_00064"},block_00006={{"text"},text={ja={{"为什么？为什么女孩子只是站着就能这么可爱？她可爱到每个路过的人都要偷看一眼，但她只对我露出那样的笑容，这是不是bug？",{"rt2"}}}},line=42,linknext="block_00007",linkback="block_00005"},block_00146={{"se",stop=1,id=1,time=500},{"se",id=1,file="seドア(室内開き)"},{"fg",id=8,x=480,file="ame_nob0510",head="ame_nob",mode=3,path=":fg/ame/no/",ex05="ame_nob0010",lv=2.2000000000000002,ch="天梨",x2=380,face="b0054"},{"fg",id=13,x=160,file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",ex05="kan_nob0010",lv=2.2000000000000002,ch="華乃",x2=0,face="b0055"},{"fg",id=7,x=-160,file="shi_nob0500",head="shi_nob",mode=3,path=":fg/shi/no/",ex05="shi_nob0000",lv=2.2000000000000002,ch="詩桜",x2=-380,face="b0040"},{"fg",ex05="asu_nob0010",x=-480,size="no",file="asu_nob0510",head="asu_nob",mode=3,mx=-40,path=":fg/asu/no/",id=5,ch="あすみ",lv=2.2000000000000002,face="b0010"},{"text"},text={ja={{"「咦？你们都在这里呢？我是来看常磐学姐的情况……」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00179",ch="asu"}}},line=854,linknext="block_00147",linkback="block_00145"},block_00030={{"fg",ex05="ame_z1a0000",lv=2.2999999999999998,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",hoho="h",ch="天梨",id=13,face="a0008h"},{"text"},text={ja={{"「啊……啊……是为了我……」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00495",ch="ame"}}},linknext="block_00031",delay={vl01={{"fg",ex05="ame_z1a0010",lv=2.2999999999999998,size="z1",file="ame_z1a0510",head="ame_z1a",ex03="a0097",path=":fg/ame/z1/",id=13,hoho="h",ch="天梨",face="a0009h",mode=3}}},line=155,linkback="block_00029"},block_00028={{"fg",ex05="ame_z1b0000",lv=2.2999999999999998,size="z1",file="ame_z1b0500",head="ame_z1b",mode=3,path=":fg/ame/z1/",ch="天梨",id=13,face="b0010"},{"text"},text={ja={{"「我想如果这个计划能成功，天梨会很高兴的。所以我一直在努力。」",{"rt2"},name={"智宏"}}}},line=142,linknext="block_00029",linkback="block_00027"},label={top={label=1,block="block_00000"}},block_00026={{"fg",ex05="ame_z1b0000",lv=2.2999999999999998,size="z1",file="ame_z1b0500",head="ame_z1b",mode=3,path=":fg/ame/z1/",ch="天梨",id=13,face="b0055"},{"text"},text={ja={{"「你在努力是因为什么？」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00493",ch="ame"}}},line=132,linknext="block_00027",linkback="block_00025"},block_00023={{"text"},text={ja={{"'因为我最闲'这样说着，我想起了天梨这个女孩子每次都会随着我贬低自己而抬高我的情景。",{"rt2"}}}},line=122,linknext="block_00024",linkback="block_00022"},block_00123={{"se",id=1,file="seグサッ"},{"quake"},{"text"},text={ja={{"诶，现在我和天梨就两个人在这种氛围中！？现在正是我满心的爱意溢出的时候？ 等、等一下！这太突然了，我还没做好心理准备！",{"rt2"}}}},line=705,linknext="block_00124",linkback="block_00122"},block_00128={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",x=-400,file="zbg006a",sync=0,ay=720,path=":bg/",zoom=120,by=-360,bx=-640,ax=1280},{"fg",ex05="ame_z2a0000",lv=2.3999999999999999,size="z2",file="ame_z2a0500",head="ame_z2a",mode=3,sync=0,path=":fg/ame/z2/",id=9,ch="天梨",face="a0028",resize=1},{"extrans",time=500},{"se",id=1,file="seコミ_ぽよん"},{"text"},text={ja={{"这样做是不是太大胆了呢。一边使自己相信是为了让天梨放心，一边握紧了正捏着自己衣服下摆的手。",{"rt2"}}}},line=739,linknext="block_00129",linkback="block_00127"},block_00044={{"text"},text={ja={{"毕竟她虽然只是个读者模特，但也是带着「模特」名号的专业人士。像我这种宅男，从来没看过时装秀，所以开始期待起来了。",{"rt2"}}}},line=224,linknext="block_00045",linkback="block_00043"},block_00147={{"text"},text={ja={{"过了一会儿，锦也来看望我们了。今天的午餐似乎会特别热闹。",{"rt2"}}}},line=856,linknext="block_00148",linkback="block_00146"},block_00094={{"text"},text={ja={{"这里的「同班同学」应该是指女生吧。常磐来过我的家。虽然不是来玩的。",{"rt2"}}}},line=533,linknext="block_00095",linkback="block_00093"},block_00057={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0001"},{"fg",ex05="ame_noa0000",x=-230,size="no",file="ame_noa0500",head="ame_noa",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="a0055"},{"text"},text={ja={{"「两个设计都很棒，真难选一个……看来天梨也在犹豫该选哪个，是不是应该投票决定呢」",{"rt2"},name={"智宏"}}}},line=310,linknext="block_00058",linkback="block_00056"},block_00090={{"fg",ex05="ame_noa0000",x=-230,size="no",file="ame_noa0500",head="ame_noa",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="a0054"},{"text"},text={ja={{"「虽然他好像已经不再亲自画设计图了。自从成为管理职员后，他也不再亲自去海外了。」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00508",ch="ame"}}},linknext="block_00091",delay={vl01={{"fg",ex05="ame_noa0000",x=-230,size="no",file="ame_noa0500",head="ame_noa",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="a0053"}}},line=503,linkback="block_00089"},block_00144={{"fg",ex05="shi_nob0000",x=-380,size="no",file="shi_nob0500",head="shi_nob",mode=3,path=":fg/shi/no/",id=7,ch="詩桜",lv=2.2000000000000002,face="b0040"},{"text"},text={ja={{"「我到底是为了什么……？」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_01725",ch="shi"}}},line=836,linknext="block_00145",linkback="block_00143"},block_00108={{"fg",ex05="ame_z1a0000",x=-230,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",id=9,ch="天梨",lv=2.2999999999999998,face="a0009"},{"text"},text={ja={{"天梨似乎察觉到我和常磐愣住的反应，脸上明显地露出了困扰的表情。",{"rt2"}}}},line=611,linknext="block_00109",linkback="block_00107"},block_00103={{"fg",ex05="ame_noa0010",x=-230,size="no",file="ame_noa0510",head="ame_noa",ex03="a0097",path=":fg/ame/no/",id=9,lv=2.2000000000000002,ch="天梨",face="a0052",mode=3},{"fgact",id=9,ch="天梨",act="ジャンプ",lv=2.2000000000000002},{"text"},text={ja={{"「啊！如果可以借用厨房的话，我来做午餐吧？材料我们一起去买！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00511",ch="ame"}}},linknext="block_00104",delay={vl01={{"fg",ex05="ame_noa0010",x=-230,size="no",file="ame_noa0510",head="ame_noa",ex03="a0097",path=":fg/ame/no/",id=9,lv=2.2000000000000002,ch="天梨",face="a0054",mode=3}}},line=581,linkback="block_00102"},block_00143={{"text"},text={ja={{"「不过设计已经完成了。常磐的工作已经结束了」",{"rt2"},name={"智宏"}}}},line=828,linknext="block_00144",linkback="block_00142"},block_00142={{"fg",ex05="ame_nob0010",x=380,size="no",file="ame_nob0510",head="ame_nob",mode=3,path=":fg/ame/no/",id=8,ch="天梨",lv=2.2000000000000002,face="b0054"},{"fg",ex05="shi_nob0000",x=-380,size="no",file="shi_nob0500",head="shi_nob",mode=3,path=":fg/shi/no/",id=7,ch="詩桜",lv=2.2000000000000002,face="b0032"},{"text"},text={ja={{"常磐可能非常高兴她带来了商店街的章鱼烧，看起来很开心。今天的午餐是面包、章鱼烧和土豆沙拉……",{"rt2"}}}},line=825,linknext="block_00143",linkback="block_00141"},block_00110={{"quake"},{"text"},text={ja={{"「不，华乃可能不知道，但我很喜欢土豆沙拉！ 上次天梨做的让我尝了一下，真的超级好吃！」",{"rt2"},name={"智宏"}}}},line=621,linknext="block_00111",linkback="block_00109"},block_00139={{"fg",ex05="kan_nob0000",x=0,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=13,ch="華乃",lv=2.2000000000000002,face="b0002"},{"text"},text={ja={{"「啊？诗樱前辈，你知道我家在哪儿吗……？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00314",ch="kan"}}},line=804,linknext="block_00140",linkback="block_00138"},block_00136={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",file="bg006a",path=":bg/",sync=0},{"fg",ex05="ame_noa0000",x=380,size="no",file="ame_noa0500",head="ame_noa",mode=3,sync=0,path=":fg/ame/no/",id=8,lv=2.2000000000000002,ch="天梨",face="a0055",resize=1},{"extrans",time=500},{"se",id=1,file="seドア(室内開き)"},{"fg",ex05="kan_noa0000",x=0,size="no",file="kan_noa0500",head="kan_noa",mode=3,mx=-40,path=":fg/kan/no/",id=13,lv=2.2000000000000002,ch="華乃",face="a0039",resize=1},{"fg",ex05="shi_nob0010",x=-380,size="no",file="shi_nob0510",head="shi_nob",mode=3,mx=-40,path=":fg/shi/no/",id=7,ch="詩桜",lv=2.2000000000000002,face="b0032"},{"text"},text={ja={{"「是诗樱前辈」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00313",ch="kan"}}},line=788,linknext="block_00137",linkback="block_00135"},block_00007={{"text"},text={ja={{"尽管如此，我内心深处还是感到欣喜若狂，无法抑制自己对周围目光的优越感。我已经升级到和同班女生约会的地位了。",{"rt2"}}}},line=44,linknext="block_00008",linkback="block_00006"},block_00124={{"se",stop=1,id=1,time=500},{"msgoff"},{"cgdel",id=5,lv=5,sync=0,rule="cinema",time=400},{"bg",ay=720,x=-400,path=":bg/",ax=1280,file="zbg006a",by=-360,bx=-640,sync=0},{"fg",ex05="ame_z1a0000",lv=2.2999999999999998,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,sync=0,path=":fg/ame/z1/",hoho="h",ch="天梨",id=9,face="a0002h"},{"extrans",time=500},{"text"},text={ja={{"「会长，谢谢……」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00516",ch="ame"}}},line=717,linknext="block_00125",linkback="block_00123"},block_00135={{"fg",ex05="ame_z2a0000",lv=2.3999999999999999,size="z2",file="ame_z2a0500",head="ame_z2a",mode=3,path=":fg/ame/z2/",ch="天梨",id=9,face="a0055"},{"text"},text={ja={{"没能好好回复她。因为眼前这个女孩子太可爱了，让人眼花缭乱。有这么喜欢我、亲近我的孩子，怎么可能不可爱呢。",{"rt2"}}}},line=771,linknext="block_00136",linkback="block_00134"},block_00134={{"text"},text={ja={{"「谢谢」",{"rt2"},name={"智宏"}}}},line=767,linknext="block_00135",linkback="block_00133"},block_00078={{"fg",ex05="kan_nob0010",x=230,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0039"},{"text"},text={ja={{"「如果是专业的设计师，他们的作品甚至会进入艺术领域。你在网上搜索「设计图」就能明白了」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00300",ch="kan"}}},linknext="block_00079",delay={vl01={{"fg",ex05="kan_nob0010",x=230,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0055"}}},line=433,linkback="block_00077"},block_00131={{"fg",ex05="ame_z2a0000",lv=2.3999999999999999,size="z2",file="ame_z2a0500",head="ame_z2a",mode=3,path=":fg/ame/z2/",hoho="h",ch="天梨",id=9,face="a0022h"},{"text"},text={ja={{"那是我的真实感受。所以我没法看天梨的脸。然而，即便不直视她，我也能感受到天梨的表情瞬间鲜活了起来。",{"rt2"}}}},line=751,linknext="block_00132",linkback="block_00130"},block_00098={{"text"},text={ja={{"常磐非常讲义气，天梨看起来也不像是有什么阴暗面，所以彼此可能会觉得在彼此身边很安心。我不禁从父亲的角度看着这两个怕人的孩子。",{"rt2"}}}},line=544,linknext="block_00099",linkback="block_00097"},block_00130={{"text"},text={ja={{"「嗯。我很开心」",{"rt2"},name={"智宏"}}}},line=747,linknext="block_00131",linkback="block_00129"},block_00129={{"fg",ex05="ame_z2a0000",lv=2.3999999999999999,size="z2",file="ame_z2a0500",head="ame_z2a",mode=3,path=":fg/ame/z2/",hoho="h",ch="天梨",id=9,face="a0008h"},{"text"},text={ja={{"「啊！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00518",ch="ame"}}},line=744,linknext="block_00130",linkback="block_00128"},block_00127={{"text"},text={ja={{"而且她还把自己心里的想法说出来了。虽然我想可能就是这样……啊，真让人心疼。",{"rt2"}}}},line=728,linknext="block_00128",linkback="block_00126"},block_00145={{"text"},text={ja={{"「啊，我希望你能尽快开始制作裁剪纸样，所以如果你能在午餐后看看设计稿就太好了。」",{"rt2"},name={"智宏"}}}},line=839,linknext="block_00146",linkback="block_00144"},block_00122={{"se",id=1,file="seインターフォン"},{"msgoff"},{"fg",mode=-2,sync=0},{"bg",file="bg006a",path=":bg/",sync=0},{"bg",id=5,lv=5,file="cinema01",sync=0,rule="cinema",alpha=192,vague=40,path=":sys/",time=400},{"extrans",time=500},{"text"},text={ja={{"这时候有人来访了。这里是常磐的家，所以当然是常磐去应对。看来她的父母不在家。",{"rt2"}}}},line=700,linknext="block_00123",linkback="block_00121"},block_00113={{"msgoff"},{"msgoff"},{"colortone",style="reset"},{"bg",id=7,lv=8,file="white",path=":bg/",sync=0,time=1000},{"cgdel",id=7,lv=8,sync=0,time=1000},{"bg",ay=720,path=":bg/",ax=1280,file="zbg006a",bx=-640,by=-360,sync=0},{"fg",ex05="kan_z1b0000",x=230,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,sync=0,path=":fg/kan/z1/",id=10,ch="華乃",lv=2.2999999999999998,face="b0008"},{"fg",ex05="ame_z1a0000",x=-230,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,sync=0,path=":fg/ame/z1/",id=9,ch="天梨",lv=2.2999999999999998,face="a0008"},{"extrans",time=500},{"text"},text={ja={{"她一定是想象着我满足地吃这个，才会走进厨房的吧。即使这个场合不合适，看到这个土豆沙拉也不可能不感到高兴。",{"rt2"}}}},line=642,linknext="block_00114",linkback="block_00112"},block_00121={{"fg",ex05="kan_z1b0000",x=380,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=10,ch="華乃",lv=2.2999999999999998,face="b0055"},{"text"},text={ja={{"被这样一双像是弃犬的眼神盯着，真是可爱得让人无法抗拒啊。啊，真想摸摸她的头。",{"rt2"}}}},line=687,linknext="block_00122",linkback="block_00120"},block_00042={{"fg",ex05="ame_z1b0000",lv=2.2999999999999998,size="z1",file="ame_z1b0500",head="ame_z1b",mode=3,mx=40,path=":fg/ame/z1/",id=13,ch="天梨",face="b0055",resize=1},{"text"},text={ja={{"可能是觉得会妨碍交通，天梨很快就离开了，站在我的旁边。她没有显得羞涩，反而面带微笑。",{"rt2"}}}},line=214,linknext="block_00043",linkback="block_00041"},block_00004={{"text"},text={ja={{"「嗯」是什么意思？全世界有谁会像我这样用如此平淡的方式回应问候，还是个童贞呢？",{"rt2"}}}},line=36,linknext="block_00005",linkback="block_00003"},block_00118={{"text"},text={ja={{"天梨再次抱住华乃的手臂，华乃显得有些困惑。嗯……如果是天梨的那个小组的孩子们的话，可能会被她们笑话吧。我也要感谢你，华乃。",{"rt2"}}}},line=676,linknext="block_00119",linkback="block_00117"},block_00117={{"fg",ex05="kan_z1b0000",x=380,size="z1",file="kan_z1b0500",head="kan_z1b",mode=3,path=":fg/kan/z1/",id=10,ch="華乃",lv=2.2999999999999998,face="b0019"},{"text"},text={ja={{"「诶？ 哦，没、没什么。反倒是我应该感谢你，你才是做了土豆沙拉带过来的呢」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00312",ch="kan"}}},line=674,linknext="block_00118",linkback="block_00116"},block_00112={{"text"},text={ja={{"考虑到天梨因为高兴，不由自主地做了这个，我觉得她真的太让人心疼了。甜蜜得让人心头一紧。",{"rt2"}}}},line=632,linknext="block_00113",linkback="block_00111"},block_00077={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0034"},{"text"},text={ja={{"「并不是在谦虚，衣服设计需要的，和画画技巧好坏又是另一种感觉。可能更接近建筑图纸？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00299",ch="kan"}}},linknext="block_00078",delay={vl01={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0053"}}},line=426,linkback="block_00076"},block_00104={{"fg",ex05="kan_noa0000",x=230,size="no",file="kan_noa0500",head="kan_noa",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="a0010"},{"text"},text={ja={{"「啊？ 哦，自己做饭啊……如果要做的话我也可以帮忙……」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00309",ch="kan"}}},linknext="block_00105",delay={vl01={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0039"}}},line=588,linkback="block_00103"},block_00101={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0031"},{"text"},text={ja={{"「呃，嗯，就我个人来说，我想在今后的工作中，了解你们打算怎么隐藏我的爱好。只要我们能达成这样的沟通，那么我觉得你们的来访就已经有意义了。」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00307",ch="kan"}}},linknext="block_00102",delay={vl01={{"fg",ex05="kan_nob0010",x=230,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0053"}}},line=566,linkback="block_00100"},block_00012={{"text"},text={ja={{"大部分商店都是十点才开始营业的……有人会认为九点半就开始营业吗？",{"rt2"}}}},line=74,linknext="block_00013",linkback="block_00011"},block_00063={{"text"},text={ja={{"「话说回来，新川也说过，制版不是自学就能做出来的东西。虽然是「被迫」，但你能自己做出来真是了不起啊，常磐……」",{"rt2"},name={"智宏"}}}},line=340,linknext="block_00064",linkback="block_00062"},block_00036={{"text"},text={ja={{"「哎？啊，抱歉，我走得太快了……」",{"rt2"},name={"智宏"}}}},line=179,linknext="block_00037",linkback="block_00035"},block_00100={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0001"},{"text"},text={ja={{"但是，刚才我还在从父亲的角度看待他们，现在天梨却注意到了一个大问题。我们到底是为了什么而来的。",{"rt2"}}}},line=559,linknext="block_00101",linkback="block_00099"},block_00119={{"text"},text={ja={{"另一方面，我也很清楚这样努力过头的天梨可能和那个小组的孩子们不太合得来。但即便如此，我还是非常喜欢这种努力过头的女孩子。",{"rt2"}}}},line=678,linknext="block_00120",linkback="block_00118"},block_00096={{"text"},text={ja={{"可能是因为我们彼此能接受和不能接受的东西的感觉很相近。以前天梨曾经无意中透露出她的困扰，那方面关于道德和礼仪的问题。",{"rt2"}}}},line=540,linknext="block_00097",linkback="block_00095"},block_00126={{"fg",ex05="ame_z1a0000",lv=2.2999999999999998,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",ch="天梨",id=9,face="a0020"},{"text"},text={ja={{"「上次会长你夸奖我了，所以我又想让你吃……但是，我应该先问问你我能不能做再带过来的。」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00517",ch="ame"}}},linknext="block_00127",delay={vl01={{"fg",ex05="ame_z1a0000",lv=2.2999999999999998,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,path=":fg/ame/z1/",ch="天梨",id=9,face="a0028"}}},line=726,linkback="block_00125"},block_00089={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0002"},{"text"},text={ja={{"我告诉她他好像是海外家具的买手，常磐就向我投来一种「难道天梨是大小姐？」的视线。确实，她的生活看起来很不错。",{"rt2"}}}},line=496,linknext="block_00090",linkback="block_00088"},block_00087={{"fg",ex05="ame_nob0000",x=-230,size="no",file="ame_nob0500",head="ame_nob",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="b0006"},{"fgact",id=9,ch="天梨",act="うなづく",lv=2.2000000000000002},{"text"},text={ja={{"「那时他也画了从各种角度看的图纸！我记得！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00507",ch="ame"}}},line=485,linknext="block_00088",linkback="block_00086"},block_00085={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0050"},{"fg",ex05="ame_noa0000",x=-230,size="no",file="ame_noa0500",head="ame_noa",mode=3,path=":fg/ame/no/",id=9,ch="天梨",lv=2.2000000000000002,face="a0001"},{"text"},text={ja={{"「沿着哪个思路呢，那个。即使是硬编也好，我希望你能再给我一些理由来加强这个观点」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00303",ch="kan"}}},linknext="block_00086",delay={vl01={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0028"}}},line=472,linkback="block_00084"},block_00133={{"fg",ex05="ame_z2a0000",lv=2.3999999999999999,size="z2",file="ame_z2a0500",head="ame_z2a",mode=3,path=":fg/ame/z2/",ch="天梨",id=9,face="a0035"},{"text"},text={ja={{"「如果、如果会长会高兴的话，我还会再做的！下次一定事先和你商量好！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00520",ch="ame"}}},linknext="block_00134",delay={vl01={{"fg",ex05="ame_z2a0000",lv=2.3999999999999999,size="z2",file="ame_z2a0500",head="ame_z2a",mode=3,path=":fg/ame/z2/",ch="天梨",id=9,face="a0022"}}},line=764,linkback="block_00132"},block_00074={{"text"},text={ja={{"冷却下来的是刚刚开始兴奋的常磐，而使她冷静下来的正是天梨。看来被赞美自己的画很高兴，表情立刻变得明亮起来。",{"rt2"}}}},line=406,linknext="block_00075",linkback="block_00073"},block_00073={{"fg",ex05="kan_nob0010",x=230,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,lv=2.2000000000000002,ch="華乃",face="b0010",resize=1},{"text"},text={ja={{"「啊，谢谢。原来天梨喜欢这个啊。我也很喜欢袖子的线条。嗯，能在这里听到你的意见真好」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00297",ch="kan"}}},linknext="block_00074",delay={vl03={{"fgact",id=10,ch="華乃",act="うなづく",lv=2.2000000000000002}},vl01={{"fg",ex05="kan_nob0010",x=230,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0053"}},vl02={{"fg",ex05="kan_nob0010",x=230,size="no",file="kan_nob0510",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0054"}}},line=404,linkback="block_00072"},block_00021={{"text"},text={ja={{"「哎呀。计划是天梨提出的，制作方面则是常磐和新川等人全力以赴」",{"rt2"},name={"智宏"}}}},line=115,linknext="block_00022",linkback="block_00020"},block_00071={{"text"},text={ja={{"'别人的无法做'，她这么说了。这是我的错。每个人都有不应该触及的痛苦。如果再问下去，她可能就不会帮助我们准备文化节了。",{"rt2"}}}},line=381,linknext="block_00072",linkback="block_00070"},block_00058={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0039"},{"text"},text={ja={{"「那个，你刚说要画制版……那个画制版的人是学过服装设计的吗？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00290",ch="kan"}}},linknext="block_00059",delay={vl01={{"fg",ex05="kan_nob0000",x=230,size="no",file="kan_nob0500",head="kan_nob",mode=3,path=":fg/kan/no/",id=10,ch="華乃",lv=2.2000000000000002,face="b0001"}}},line=316,linkback="block_00057"},block_00097={{"text"},text={ja={{"说起来，彼此两个都害怕被别人背叛。常磐一直都是这样，而天梨最近才变成这样。也许彼此之间是因为同病相怜吧。",{"rt2"}}}},line=542,linknext="block_00098",linkback="block_00096"},block_00039={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",x=-200,y=0,file="zbg018a",sync=0,ay=720,path=":bg/",ax=1280,by=-360,bx=-640},{"bg",id=5,lv=1,file="cinema01",sync=0,rule="cinema",alpha=192,vague=40,path=":sys/",time=400},{"fg",ex05="ame_z2a0000",lv=2.3999999999999999,size="z2",file="ame_z2a0500",head="ame_z2a",mode=3,sync=0,path=":fg/ame/z2/",id=13,hoho="h",ch="天梨",face="a0032h",resize=1},{"extrans",time=500},{"text"},text={ja={{"「谢谢你，会长……好开心」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00499",ch="ame"}}},line=199,linknext="block_00040",linkback="block_00038"},block_00035={{"fg",id=13,lv=2.2999999999999998,ch="天梨",mode=-1,mx=-40},{"fg",ex05="ame_z1b0010",lv=2.2999999999999998,size="z1",file="ame_z1b0510",head="ame_z1b",mode=1,path=":fg/ame/z1/",ch="天梨",id=20,face="b0052"},{"text"},text={ja={{"「会长！稍等一下！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00497",ch="ame"}}},line=176,linknext="block_00036",linkback="block_00034"},block_00002={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",ay=720,path=":bg/",ax=1280,file="zbg018a",bx=-640,by=-360,sync=0},{"fg",ex05="ame_z1a0000",lv=2.2999999999999998,size="z1",file="ame_z1a0500",head="ame_z1a",mode=3,sync=0,path=":fg/ame/z1/",id=13,ch="天梨",face="a0054",resize=1},{"extrans",time=500},{"text"},text={ja={{"「凯哟！早安！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00486",ch="ame"}}},line=31,linknext="block_00003",linkback="block_00001"},block_00009={{"fg",ex05="ame_nob0010",lv=2.2000000000000002,size="no",file="ame_nob0510",head="ame_nob",mode=3,path=":fg/ame/no/",ch="天梨",id=13,face="b0052"},{"text"},text={ja={{"「我们约定的时间是五分钟前吧？我想去看看布料店，所以稍微早点来了」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00487",ch="ame"}}},linknext="block_00010",delay={vl01={{"fg",ex05="ame_nob0000",lv=2.2000000000000002,size="no",file="ame_nob0500",head="ame_nob",mode=3,path=":fg/ame/no/",ch="天梨",id=13,face="b0054"}}},line=61,linkback="block_00008"}}