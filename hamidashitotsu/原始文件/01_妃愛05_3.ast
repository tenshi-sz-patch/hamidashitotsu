astver = 2.0
ast = {block_00076={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,file="bg016a",path=":bg/"},{"fg",face="a0010",mode=3,lv=2.2000000000000002,sync=0,x=480,id=6,file="hiy_noa0200",head="hiy_noa",size="no",ex05="hiy_noa0000",ch="妃愛",path=":fg/hiy/no/"},{"fg",face="b0010",mode=3,sync=0,lv=2.2000000000000002,id=11,x=-160,ex05="shi_nob0010",file="shi_nob0210",head="shi_nob",size="no",resize=1,ch="詩桜",path=":fg/shi/no/"},{"fg",face="a0008",mode=3,sync=0,lv=2.2000000000000002,id=12,x=160,ex05="rir_noa0000",file="rir_noa0200",head="rir_noa",size="no",resize=1,ch="莉々子",path=":fg/rir/no/"},{"fg",face="a0010",mode=3,lv=2.2000000000000002,sync=0,x=-480,id=5,file="kan_noa0200",head="kan_noa",size="no",ex05="kan_noa0000",ch="華乃",path=":fg/kan/no/"},{"extrans",time=500},{"text"},linkback="block_00075",line=342,delay={vl01={{"fg",face="a0038",mode=3,lv=2.2000000000000002,sync=0,x=160,id=12,file="rir_noa0200",head="rir_noa",size="no",ex05="rir_noa0000",ch="莉々子",path=":fg/rir/no/"}}},text={vo={{"vo",file="fem_rir_00019",ch="rir"}},ja={{"「啊？我、我说了什么奇怪的事吗？不同地区使用的配料应该是不一样的……」",{"rt2"},name={"莉々子"}}}},linknext="block_00077"},block_00073={{"fg",face="a0009",mode=3,lv=2.2999999999999998,x=280,id=13,file="rir_z1a0200",head="rir_z1a",size="z1",ex05="rir_z1a0000",ch="莉々子",path=":fg/rir/z1/"},{"text"},linkback="block_00072",line=320,delay={vl01={{"fg",face="a0007",mode=3,lv=2.2999999999999998,x=280,id=13,file="rir_z1a0200",head="rir_z1a",size="z1",ex05="rir_z1a0000",ch="莉々子",path=":fg/rir/z1/"}}},text={vo={{"vo",file="fem_rir_00018",ch="rir"}},ja={{"「哈？猪肉汤里面应该放红薯才对吧。里薯是什么东西……」",{"rt2"},name={"莉々子"}}}},linknext="block_00074"},block_00094={{"fg",face="a0048",mode=3,lv=2.2999999999999998,x=0,id=13,file="hiy_z1a1000",head="hiy_z1a",size="z1",ex05="hiy_z1a0000",ch="妃愛",path=":fg/hiy/z1/"},{"fgact",act="いいえ",lv=2.2999999999999998,ch="妃愛",id=13},{"text"},line=453,linkback="block_00093",text={vo={{"vo",file="fem_hiy_00205",ch="hiy"}},ja={{"「嘿嘿嘿嘿嘿，真是太幸福了～每一天都充满了激动～和哥哥在一起的生活太舒适了～」",{"rt2"},name={"妃愛"}}}},linknext="block_00095"},block_00049={{"bg",time=400,face="1",file="ev_hiy_31_01",set="ev_hiy_31",sync=0,path=":ev/ev_hiy_31/"},{"text"},line=205,linkback="block_00048",text={vo={{"vo",file="fem_hiy_00196",ch="hiy"}},ja={{"「嗯——，因为到现在为止，如果不照顾哥哥我就会感到不安，所以我一直没能说出「把做家务的机会夺走了对不起」这样的话」",{"rt2"},name={"妃愛"}}}},linknext="block_00050"},block_00026={{"bg",time=400,face="1",ax=1280,set="ev_hiy_31",ay=720,by=-360,file="zev_hiy_31_05",bx=-640,y=70,x=-480,sync=0,path=":ev/ev_hiy_31/"},{"text"},line=119,linkback="block_00025",text={vo={{"vo",file="fem_hiy_00186",ch="hiy"}},ja={{"「不对哦，哥哥。其实是我一直都在请你让我帮忙的」",{"rt2"},name={"妃愛"}}}},linknext="block_00027"},block_00025={{"text"},line=114,linkback="block_00024",text={ja={{"「确实。我也有点不记得了。因为妃爱总是帮我做这些」",{"rt2"},name={"智宏"}}}},linknext="block_00026"},block_00069={{"fg",face="a0052",mode=3,lv=2.2999999999999998,x=0,id=13,file="rir_z1a0200",head="rir_z1a",size="z1",ex05="rir_z1a0000",ch="莉々子",path=":fg/rir/z1/"},{"fgact",act="びっくり",lv=2.2999999999999998,ch="莉々子",id=13},{"text"},linkback="block_00068",line=296,delay={vl01={{"fg",face="a0055",mode=3,lv=2.2999999999999998,x=0,id=13,file="rir_z1a0200",head="rir_z1a",size="z1",ex05="rir_z1a0000",ch="莉々子",path=":fg/rir/z1/"}}},text={vo={{"vo",file="fem_rir_00017",ch="rir"}},ja={{"「哎呀！一天内两次为社区做贡献，不愧是和泉会长啊。真是太伟大了。」",{"rt2"},name={"莉々子"}}}},linknext="block_00070"},block_00082={{"fg",face="a0039",mode=3,lv=2.2000000000000002,x=-480,id=5,file="kan_noa0210",head="kan_noa",size="no",ex05="kan_noa0010",ch="華乃",path=":fg/kan/no/"},{"text"},line=373,linkback="block_00081",text={vo={{"vo",file="fem_kan_00029",ch="kan"}},ja={{"「啊，这里还写着要放什么样的土豆。马铃薯是东北地区的，芋头是全国大多数地区的，而红薯是九州……」",{"rt2"},name={"華乃"}}}},linknext="block_00083"},block_00017={{"bg",time=400,face="1",file="ev_hiy_31_08",set="ev_hiy_31",sync=0,path=":ev/ev_hiy_31/"},{"text"},line=88,linkback="block_00016",text={vo={{"vo",file="fem_hiy_00183",ch="hiy"}},ja={{"「你竟然敢挑战我这个料理高手，真有胆量。那如果我输了，今晚就做哥哥喜欢吃的任何东西。如果削的土豆上还粘着皮，那就算无效哦。」",{"rt2"},name={"妃愛"}}}},linknext="block_00018"},block_00088={{"bg",y2=-360,x2=0,ay=720,speed=80000,by=-360,file="zbg099a",y=360,bx=-640,ax=1280,x=0,path=":bg/"},{"text"},line=411,linkback="block_00087",text={ja={{"尽管发生了这样的意外，但是猪肉汤会的帮忙还是受到了社区大家的热烈欢迎，多次收到了感谢。明年也希望能让我们的学生会参与进来呢。",{"rt2"}}}},linknext="block_00089"},block_00056={{"text"},line=231,linkback="block_00055",text={ja={{"「妃爱其实一点都不错，但除此之外，只有我能原谅妃爱，所以我现在就原谅你。在此基础上，请继续照顾我」",{"rt2"},name={"智宏"}}}},linknext="block_00057"},block_00043={{"bg",set="ev_hiy_31",time=500,file="ev_hiy_31_02",path=":ev/ev_hiy_31/"},{"text"},line=182,linkback="block_00042",text={vo={{"vo",file="fem_hiy_00193",ch="hiy"}},ja={{"「那么以后也请让我继续照顾哥哥吧。这是妃爱最大的快乐了」",{"rt2"},name={"妃愛"}}}},linknext="block_00044"},block_00039={{"text"},line=166,linkback="block_00038",text={ja={{"「妃爱。你过分高估了我。就像之前在志愿者集训营时说过的，如果不用做饭，我就会继续依赖妃爱，让你照顾下去。」",{"rt2"},name={"智宏"}}}},linknext="block_00040"},block_00050={{"text"},line=208,linkback="block_00049",text={vo={{"vo",file="fem_hiy_00197",ch="hiy"}},ja={{"「如果我不能照顾哥哥，我会哭的。如果哥哥什么都会了，可能会在毕业后开始独立生活呢」",{"rt2"},name={"妃愛"}}}},linknext="block_00051"},block_00008={{"text"},line=50,linkback="block_00007",text={ja={{"「话说回来，能吃到顶级声优做的猪骨汤，这是多么的奢侈啊。如果要定价的话，这个猪骨汤一碗至少得千元……」",{"rt2"},name={"智宏"}}}},linknext="block_00009"},block_00005={{"text"},line=28,linkback="block_00004",text={ja={{"我戴上围裙和三角巾，洗完手后拿起了刀。",{"rt2"}}}},linknext="block_00006"},block_00052={{"text"},line=216,linkback="block_00051",text={ja={{"「你是我世界上唯一的妹妹。只有你是我的妹妹，妃爱。如果有什么让你困扰的事，随时都可以来找我，我会一直回答你的」",{"rt2"},name={"智宏"}}}},linknext="block_00053"},block_00038={{"bg",time=400,face="1",ax=1280,set="ev_hiy_31",ay=720,by=-360,file="zev_hiy_31_08",bx=-640,y=70,x=-480,sync=0,path=":ev/ev_hiy_31/"},{"text"},line=163,linkback="block_00037",text={vo={{"vo",file="fem_hiy_00192",ch="hiy"}},ja={{"「每次听到哥哥说「谢谢」的时候，我都很开心。哥哥比我期待的还要珍视我……」",{"rt2"},name={"妃愛"}}}},linknext="block_00039"},block_00016={{"text"},line=83,linkback="block_00015",text={ja={{"「哦，和妃爱比赛啊。感觉比猜拳更有胜算。如果我输了，今晚的晚餐就做炒饭。」",{"rt2"},name={"智宏"}}}},linknext="block_00017"},block_00034={{"bg",time=400,face="1",ax=1280,set="ev_hiy_31",ay=720,by=-360,file="zev_hiy_31_07",bx=-640,y=70,x=-480,sync=0,path=":ev/ev_hiy_31/"},{"text"},line=149,linkback="block_00033",text={vo={{"vo",file="fem_hiy_00189",ch="hiy"}},ja={{"「哥哥你一直都是值得依赖的。只是我避免去依赖你，即使需要帮助也没有说出口而已」",{"rt2"},name={"妃愛"}}}},linknext="block_00035"},block_00019={{"text"},line=95,linkback="block_00018",text={ja={{"「难道刚才的打扫也是因为能和我一起进行学生会活动而感到开心？」",{"rt2"},name={"智宏"}}}},linknext="block_00020"},block_00007={{"bgm",stop=1},{"cgdel",id=-1},{"bg",time=1000,file="white",path=":bg/"},{"bg",y2=-350,x2=620,by=-360,ay=720,speed=5000,bx=-640,file="zev_hiy_31_01",y=140,ax=1280,set="ev_hiy_31",x=-570,path=":ev/ev_hiy_31/"},{"ex",func="wait",time=2000},{"bg",set="ev_hiy_31",file="ev_hiy_31_01",path=":ev/ev_hiy_31/"},{"bgm",file="bgm31"},{"text"},line=47,linkback="block_00006",text={ja={{"「妃爱做的猪骨汤，大受欢迎啊。大家都说好吃好吃。能用平时自己吃的料理取悦别人，确实让人很开心啊」",{"rt2"},name={"智宏"}}}},linknext="block_00008"},block_00084={{"fg",face="b0010",mode=3,lv=2.2000000000000002,x=-160,id=11,file="shi_nob0210",head="shi_nob",size="no",ex05="shi_nob0010",ch="詩桜",path=":fg/shi/no/"},{"text"},linkback="block_00083",line=387,delay={vl01={{"fg",face="b0039",mode=3,lv=2.2000000000000002,x=-160,id=11,file="shi_nob0210",head="shi_nob",size="no",ex05="shi_nob0010",ch="詩桜",path=":fg/shi/no/"}}},text={vo={{"vo",file="fem_shi_00137",ch="shi"}},ja={{"「那么，一直以为红薯是标准的莉莉子小姐，是九州的……」",{"rt2"},name={"詩桜"}}}},linknext="block_00085"},block_00010={{"text"},line=55,linkback="block_00009",text={ja={{"「诶，等等……是在生气吗？就算只是一碗汤，妃爱亲手做的料理，千元是不是太便宜了……？」",{"rt2"},name={"智宏"}}}},linknext="block_00011"},block_00012={{"bg",time=400,face="1",file="ev_hiy_31_03",set="ev_hiy_31",sync=0,path=":ev/ev_hiy_31/"},{"text"},line=67,linkback="block_00011",text={vo={{"vo",file="fem_hiy_00181",ch="hiy"}},ja={{"「和哥哥一起做饭。嘿嘿嘿嘿」",{"rt2"},name={"妃愛"}}}},linknext="block_00013"},block_00031={{"bg",time=400,face="1",ax=1280,set="ev_hiy_31",ay=720,by=-360,file="zev_hiy_31_06",bx=-640,y=70,x=-480,sync=0,path=":ev/ev_hiy_31/"},{"text"},line=139,linkback="block_00030",text={vo={{"vo",file="fem_hiy_00188",ch="hiy"}},ja={{"「但是现在，你没有叫别人，而是直接来帮助我了」",{"rt2"},name={"妃愛"}}}},linknext="block_00032"},block_00029={{"text"},line=131,linkback="block_00028",text={ja={{"因为妃爱突然道歉，我也不由得发出了奇怪的声音。我做饭的机会是什么意思呢。",{"rt2"}}}},linknext="block_00030"},block_00097={{"fg",face="b0032h",mode=3,lv=2.3999999999999999,id=13,x=0,ex05="hiy_z2b0000",hoho="h",head="hiy_z2b",size="z2",file="hiy_z2b1000",ch="妃愛",path=":fg/hiy/z2/"},{"text"},line=463,linkback="block_00096",text={ja={{"饭后想洗碗，结果被抢走了海绵。问有什么能帮忙的，结果她让我抱紧她，我就这么做了。",{"rt2"}}}},linknext="block_00098"},block_00086={{"fg",mx=40,mode=-1,id=12,ch="莉々子",lv=2.2000000000000002},{"fg",face="b0010",mode=3,lv=2.2000000000000002,x=480,id=6,file="hiy_nob0200",head="hiy_nob",size="no",ex05="hiy_nob0000",ch="妃愛",path=":fg/hiy/no/"},{"fg",face="b0010",mode=3,lv=2.2000000000000002,x=-160,id=11,file="shi_nob0210",head="shi_nob",size="no",ex05="shi_nob0010",ch="詩桜",path=":fg/shi/no/"},{"fg",face="a0010",mode=3,lv=2.2000000000000002,x=-480,id=5,file="kan_noa0210",head="kan_noa",size="no",ex05="kan_noa0010",ch="華乃",path=":fg/kan/no/"},{"text"},line=403,linkback="block_00085",text={ja={{"看来她真的只是路过这里。圣会长急忙离开公园，迅速离去。",{"rt2"}}}},linknext="block_00087"},label={top={label=1,block="block_00000"}},block_00072={{"fg",face="b0018",mode=3,lv=2.2999999999999998,x=-280,id=9,file="shi_z1b0210",head="shi_z1b",size="z1",ex05="shi_z1b0010",ch="詩桜",path=":fg/shi/z1/"},{"text"},line=312,linkback="block_00071",text={vo={{"vo",file="fem_shi_00135",ch="shi"}},ja={{"「在关东地区，貌似通常是放里薯的，红薯被认为太甜所以不受欢迎。」",{"rt2"},name={"詩桜"}}}},linknext="block_00073"},block_00098={{"bgm",stop=1,time=3000},{"se",time=3000,stop=1,id=-1},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",time=3000,file="black",path=":bg/"},{"cacheclear"},{"ex",func="wait",time=1000},{"bg",time=500,file="catch01",id=9,lv=9,path=":cg/"},{"bgm",file="bgm91",loop=0},{"ex",func="wait",time=6000},{"bgm",stop=1,time=3000},{"se",time=3000,stop=1,id=-1},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",time=3000,file="black",path=":bg/"},{"cacheclear"},{"ex",func="wait",time=1000},{"excall",file="01_妃愛06_1"},{"text"},line=469,linkback="block_00097"},block_00063={{"bg",time=500,ax=1280,set="ev_hiy_31",ay=720,x=-480,file="zev_hiy_31_09",y=70,by=-360,bx=-640,path=":ev/ev_hiy_31/"},{"text"},line=258,linkback="block_00062",text={ja={{"妃爱的脸立刻洋溢着笑容。从今天开始，每顿饭后，我要向她表达我的真心，直到她说「我知道了」。",{"rt2"}}}},linknext="block_00064"},block_00023={{"text"},line=106,linkback="block_00022",text={ja={{"我本以为她是因为能和恋人单独相处而感到高兴，但看来她作为妹妹享受「和哥哥一起」的感情更强烈。",{"rt2"}}}},linknext="block_00024"},block_00004={{"text"},line=26,linkback="block_00003",text={ja={{"「啊，那我来帮你。现在大家看起来都很忙」",{"rt2"},name={"智宏"}}}},linknext="block_00005"},block_00095={{"fg",face="b0055h",mode=3,id=13,lv=2.3999999999999999,file="hiy_z2b1010",x=0,ex05="hiy_z2b0010",hoho="h",head="hiy_z2b",size="z2",resize=1,ch="妃愛",path=":fg/hiy/z2/"},{"text"},line=457,linkback="block_00094",text={ja={{"我们兄弟俩就像新婚夫妇一样，互相喂食。",{"rt2"}}}},linknext="block_00096"},block_00093={{"text"},line=447,linkback="block_00092",text={ja={{"「不，这不是过奖。这就是我妈妈的味道。妃爱做的菜最合我口味」",{"rt2"},name={"智宏"}}}},linknext="block_00094"},block_00000={{"bg",path=":bg/",rule="rule_002r",file="bg016a",vague=40},{"bgm",file="bgm01"},{"text"},linknext="block_00001",text={ja={{"「我回来了……嗯？只有妃爱一个人？」",{"rt2"},name={"智宏"}}}},line=11},block_00036={{"text"},line=155,linkback="block_00035",text={vo={{"vo",file="fem_hiy_00190",ch="hiy"}},ja={{"「我听说察言观色是日本人特有的文化。但是，我现在觉得能在需要帮助的时候说出口的人更强大」",{"rt2"},name={"妃愛"}}}},linknext="block_00037"},block_00045={{"text"},line=190,linkback="block_00044",text={ja={{"「但是，现在我们谈过这样的话题，以后我就算真的想偷懒，只要妃爱说需要帮助，我也无法拒绝了呢」",{"rt2"},name={"智宏"}}}},linknext="block_00046"},block_00046={{"bg",time=400,face="1",file="ev_hiy_31_04",set="ev_hiy_31",sync=0,path=":ev/ev_hiy_31/"},{"text"},line=195,linkback="block_00045",text={vo={{"vo",file="fem_hiy_00195",ch="hiy"}},ja={{"「从现在开始，我可能会每天都要你帮忙。看来你已经陷入了我设下的陷阱了呢，呼呼呼」",{"rt2"},name={"妃愛"}}}},linknext="block_00047"},block_00059={{"text"},line=241,linkback="block_00058",text={ja={{"「刚才妃爱说，日本人喜欢察言观色的文化，可能我也有些依赖这一点吧」",{"rt2"},name={"智宏"}}}},linknext="block_00060"},block_00092={{"fg",face="b0035",mode=3,lv=2.2999999999999998,x=0,id=13,file="hiy_z1b1000",head="hiy_z1b",size="z1",ex05="hiy_z1b0000",ch="妃愛",path=":fg/hiy/z1/"},{"text"},line=444,linkback="block_00091",text={vo={{"vo",file="fem_hiy_00204",ch="hiy"}},ja={{"「过奖了～」",{"rt2"},name={"妃愛"}}}},linknext="block_00093"},block_00091={{"text"},line=439,linkback="block_00090",text={ja={{"「妃爱做的汤也很好喝。鸡蛋好飘香，比任何一间店的中式汤都好喝」",{"rt2"},name={"智宏"}}}},linknext="block_00092"},block_00090={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,file="zbg002c",bx=-640,ax=1280,ay=720,sync=0,path=":bg/"},{"fg",face="b0051",mode=3,sync=0,lv=2.2999999999999998,id=13,x=0,ex05="hiy_z1b0010",file="hiy_z1b1010",head="hiy_z1b",size="z1",resize=1,ch="妃愛",path=":fg/hiy/z1/"},{"extrans",vague=40,rule="rule_001"},{"fgact",act="ジャンプ",lv=2.2999999999999998,ch="妃愛",id=13},{"bgm",file="bgm28"},{"text"},linkback="block_00089",line=436,delay={vl01={{"fg",face="b0054",mode=3,lv=2.2999999999999998,x=0,id=13,file="hiy_z1b1010",head="hiy_z1b",size="z1",ex05="hiy_z1b0010",ch="妃愛",path=":fg/hiy/z1/"}}},text={vo={{"vo",file="fem_hiy_00203",ch="hiy"}},ja={{"「哥哥炒的饭真好吃！今天特别的好吃呢！是因为一直期待到现在的原因么」",{"rt2"},name={"妃愛"}}}},linknext="block_00091"},block_00035={{"text"},line=152,linkback="block_00034",text={ja={{"「不，那部分是我应该察觉的，而我却什么也没做就在房间里懒洋洋地躺着……」",{"rt2"},name={"智宏"}}}},linknext="block_00036"},block_00028={{"text"},line=129,linkback="block_00027",text={ja={{"「哈？」",{"rt2"},name={"智宏"}}}},linknext="block_00029"},block_00009={{"text"},line=52,linkback="block_00008",text={ja={{"由于没有回应我的话，我看向妃爱。她把刀放在案板上，明显在笑，小小地颤抖着。",{"rt2"}}}},linknext="block_00010"},block_00022={{"text"},line=104,linkback="block_00021",text={ja={{"甚至于，像这样的区域活动，妃爱从小就很少能参加。像现在这样两人一起削土豆的记忆，几乎没有。",{"rt2"}}}},linknext="block_00023"},block_00089={{"bgm",stop=1},{"cgdel",id=-1},{"bg",wait=250,time=1000,file="black",rule="rule_001",vague=40,path=":bg/"},{"text"},line=419,linkback="block_00088",text={ja={{"于是那天晚上，我和妃爱一起待在厨房，两人一起做饭。",{"rt2"}}}},linknext="block_00090"},block_00044={{"bg",time=400,face="1",file="ev_hiy_31_09",set="ev_hiy_31",sync=0,path=":ev/ev_hiy_31/"},{"text"},line=187,linkback="block_00043",text={vo={{"vo",file="fem_hiy_00194",ch="hiy"}},ja={{"「但是今天我发现一起做饭也很有趣，所以以后在厨房需要帮助的时候，我会向哥哥求助的」",{"rt2"},name={"妃愛"}}}},linknext="block_00045"},block_00085={{"fg",face="a0012h",mode=3,lv=2.2000000000000002,id=12,x=160,ex05="rir_noa0000",hoho="h",head="rir_noa",size="no",file="rir_noa0200",ch="莉々子",path=":fg/rir/no/"},{"fgact",act="いいえ",lv=2.2000000000000002,ch="莉々子",id=12},{"text"},linkback="block_00084",line=395,delay={vl01={{"fg",face="a0019h",mode=3,lv=2.2000000000000002,id=12,x=160,ex05="rir_noa0000",hoho="h",head="rir_noa",size="no",file="rir_noa0200",ch="莉々子",path=":fg/rir/no/"}},vl02={{"fg",face="a0033h",mode=3,lv=2.2000000000000002,id=12,x=160,ex05="rir_noa0000",hoho="h",head="rir_noa",size="no",file="rir_noa0200",ch="莉々子",path=":fg/rir/no/"}}},text={vo={{"vo",file="fem_rir_00021",ch="rir"}},ja={{"「啊，我，我突然想起有事。诗樱，这个猪……猪肉汤，真的非常感谢。非常好吃。那么，再见了」",{"rt2"},name={"莉々子"}}}},linknext="block_00086"},block_00083={{"fg",face="a0054",mode=3,lv=2.2000000000000002,x=480,id=6,file="hiy_noa0200",head="hiy_noa",size="no",ex05="hiy_noa0000",ch="妃愛",path=":fg/hiy/no/"},{"fg",face="a0009",mode=3,lv=2.2000000000000002,x=160,id=12,file="rir_noa0200",head="rir_noa",size="no",ex05="rir_noa0000",ch="莉々子",path=":fg/rir/no/"},{"text"},linkback="block_00082",line=381,delay={vl01={{"fg",face="a0052",mode=3,lv=2.2000000000000002,x=480,id=6,file="hiy_noa0210",head="hiy_noa",size="no",ex05="hiy_noa0010",ch="妃愛",path=":fg/hiy/no/"},{"fgact",act="うんうん",lv=2.2000000000000002,ch="妃愛",id=6}}},text={vo={{"vo",file="fem_hiy_00202",ch="hiy"}},ja={{"「我们家是东北的味道哦。说起来，土豆确实给人北方的感觉呢。」",{"rt2"},name={"妃愛"}}}},linknext="block_00084"},block_00003={{"fg",face="a0028",mode=1,lv=2.2000000000000002,id=20,file="hiy_noa0200",head="hiy_noa",size="no",ex05="hiy_noa0000",ch="妃愛",path=":fg/hiy/no/"},{"text"},linkback="block_00002",line=23,delay={vl01={{"fg",face="a0012",mode=1,lv=2.2000000000000002,id=20,file="hiy_noa0200",head="hiy_noa",size="no",ex05="hiy_noa0000",ch="妃愛",path=":fg/hiy/no/"}}},text={vo={{"vo",file="fem_hiy_00179",ch="hiy"}},ja={{"「面对这么多的东西，我有点吃力。如果有人能帮我一下就好了」",{"rt2"},name={"妃愛"}}}},linknext="block_00004"},block_00068={{"text"},line=289,linkback="block_00067",text={ja={{"「哦，其实今天我们在做两件志愿者活动……是在帮忙自治会。」",{"rt2"},name={"智宏"}}}},linknext="block_00069"},block_00041={{"text"},line=172,linkback="block_00040",text={ja={{"「现在也只是因为想和妃爱在一起才会帮忙。再说了，和妃爱一起削土豆的竞赛很有趣呢」",{"rt2"},name={"智宏"}}}},linknext="block_00042"},block_00030={{"text"},line=134,linkback="block_00029",text={ja={{"「不，妃爱，我确实一直把做饭的事情全都扔给你了，如果因此我要道歉的话，我能理解。但是，为什么妃爱你会说对不起呢？这点我不明白」",{"rt2"},name={"智宏"}}}},linknext="block_00031"},block_00066={{"text"},line=276,linkback="block_00065",text={ja={{"这张脸，我刚才才见过。他以一种窥视的姿态（可能）确认了我们的制服，确定是我们后就走进了公园。",{"rt2"}}}},linknext="block_00067"},block_00001={{"text"},line=13,linkback="block_00000",text={ja={{"把负责饮料的神田和饭田的两趟路程分量的大型塑料瓶总共8瓶交给他们后，我打算稍微休息一下，于是探头看了看准备食物的帐篷里。",{"rt2"}}}},linknext="block_00002"},block_00024={{"bg",time=500,ax=1280,set="ev_hiy_31",ay=720,x=-480,file="zev_hiy_31_04",y=70,by=-360,bx=-640,path=":ev/ev_hiy_31/"},{"text"},line=111,linkback="block_00023",text={vo={{"vo",file="fem_hiy_00185",ch="hiy"}},ja={{"「我和哥哥一起做饭是什么时候的事了呢？虽然有记忆中有哥哥帮忙的情景，但像这样「两人一起做饭」好像还是第一次呢。」",{"rt2"},name={"妃愛"}}}},linknext="block_00025"},block_00054={{"bg",time=400,face="1",file="ev_hiy_31_05",set="ev_hiy_31",sync=0,path=":ev/ev_hiy_31/"},{"text"},line=225,linkback="block_00053",text={vo={{"vo",file="fem_hiy_00199",ch="hiy"}},ja={{"「……但是现在，我对哥哥会一直陪在我身边有了自信，所以就想道歉一下，这是我之前就一直想说的」",{"rt2"},name={"妃愛"}}}},linknext="block_00055"},block_00079={{"fg",face="a0001",mode=3,lv=2.2000000000000002,x=-160,id=11,file="shi_noa0210",head="shi_noa",size="no",ex05="shi_noa0010",ch="詩桜",path=":fg/shi/no/"},{"text"},line=357,linkback="block_00078",text={vo={{"vo",file="fem_shi_00136",ch="shi"}},ja={{"「说起来，我去九州的时候，当地的人都称其为猪肉汤。」",{"rt2"},name={"詩桜"}}}},linknext="block_00080"},block_00051={{"text"},line=213,linkback="block_00050",text={vo={{"vo",file="fem_hiy_00198",ch="hiy"}},ja={{"「我真是个自私又过分的妹妹呢」",{"rt2"},name={"妃愛"}}}},linknext="block_00052"},block_00021={{"text"},line=102,linkback="block_00020",text={ja={{"现在想想，的确是这样……妃爱因为太忙，没能参加研讨会和支持足球队。",{"rt2"}}}},linknext="block_00022"},block_00077={{"text"},line=345,linkback="block_00076",text={ja={{"「该关注的问题不是这个，什么是『猪肉汤』呢？不应该说是『猪骨汤』吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00078"},block_00047={{"text"},line=197,linkback="block_00046",text={ja={{"但是她应该不会每天都这么说吧。当然，如果这样的妹妹每天都向我撒娇求助，我也会乐意帮忙的。",{"rt2"}}}},linknext="block_00048"},block_00062={{"text"},line=252,linkback="block_00061",text={ja={{"「我最喜欢的就是妃爱你做的食物。你总是准备着美味的饭菜，谢谢你」",{"rt2"},name={"智宏"}}}},linknext="block_00063"},block_00075={{"fg",face="b0010",mode=3,lv=2.2999999999999998,x=-280,id=9,file="shi_z1b0200",head="shi_z1b",size="z1",ex05="shi_z1b0000",ch="詩桜",path=":fg/shi/z1/"},{"fg",face="a0010",mode=3,lv=2.2999999999999998,x=280,id=13,file="rir_z1a0200",head="rir_z1a",size="z1",ex05="rir_z1a0000",ch="莉々子",path=":fg/rir/z1/"},{"text"},line=328,linkback="block_00074",text={ja={{"除了圣会长，场上的所有人都露出了吃惊的表情。看着我们的圣会长她也是一脸吃惊。",{"rt2"}}}},linknext="block_00076"},block_00074={{"text"},line=323,linkback="block_00073",text={ja={{"「猪肉汤？」",{"rt2"},name={"智宏"}}}},linknext="block_00075"},block_00071={{"fg",mx=-40,face="b0053",mode=3,lv=2.2999999999999998,id=9,x=-280,ex05="shi_z1b0000",file="shi_z1b0200",head="shi_z1b",size="z1",resize=1,ch="詩桜",path=":fg/shi/z1/"},{"fg",face="a0055",mode=3,x2=0,lv=2.2999999999999998,x=280,file="rir_z1a0200",head="rir_z1a",ex05="rir_z1a0000",id=13,ch="莉々子",path=":fg/rir/z1/"},{"text"},linkback="block_00070",line=307,delay={vl01={{"fg",face="b0034",mode=3,lv=2.2999999999999998,x=-280,id=9,file="shi_z1b0200",head="shi_z1b",size="z1",ex05="shi_z1b0000",ch="詩桜",path=":fg/shi/z1/"},{"fgact",act="おじぎ",lv=2.2999999999999998,ch="詩桜",id=9}}},text={vo={{"vo",file="fem_shi_00134",ch="shi"}},ja={{"「嗨，莉莉子，要不要来一碗？我的锅菜似乎不太受欢迎，剩下的还挺多的。」",{"rt2"},name={"詩桜"}}}},linknext="block_00072"},block_00070={{"text"},line=298,linkback="block_00069",text={ja={{"我本以为会因为上午没有告诉她而被责备，然而圣会长却直率地表示了赞赏。从她的语气来看，应该不是在讽刺或挖苦。",{"rt2"}}}},linknext="block_00071"},block_00081={{"fg",face="a0008h",mode=3,lv=2.2000000000000002,id=12,x=160,ex05="rir_noa0000",hoho="h",head="rir_noa",size="no",file="rir_noa0200",ch="莉々子",path=":fg/rir/no/"},{"fg",face="a0001",mode=3,lv=2.2000000000000002,x=480,id=6,file="hiy_noa0200",head="hiy_noa",size="no",ex05="hiy_noa0000",ch="妃愛",path=":fg/hiy/no/"},{"text"},line=368,linkback="block_00080",text={ja={{"听到华乃的解释后，圣会长更是瞪大了眼睛，用手捂住自己的嘴。看起来就像是做错了什么事的表情。",{"rt2"}}}},linknext="block_00082"},block_00018={{"bg",time=400,face="1",file="ev_hiy_31_05",set="ev_hiy_31",sync=0,path=":ev/ev_hiy_31/"},{"text"},line=92,linkback="block_00017",text={ja={{"妃爱一旦开始全力以赴，很快就开始领先一筹接着一筹。我为什么会想到能在料理上赢过这个平时都在做饭的妹妹呢？",{"rt2"}}}},linknext="block_00019"},block_00067={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",x=-200,ay=720,file="zbg016a",bx=-640,ax=1280,by=-360,sync=0,path=":bg/"},{"fg",face="a0010",mode=3,sync=0,lv=2.2999999999999998,id=13,x=0,ex05="rir_z1a0000",file="rir_z1a0200",head="rir_z1a",size="z1",resize=1,ch="莉々子",path=":fg/rir/z1/"},{"extrans",time=500},{"text"},line=286,linkback="block_00066",text={vo={{"vo",file="fem_rir_00016",ch="rir"}},ja={{"「和泉会长……？这是怎么回事？这里好热闹啊……」",{"rt2"},name={"莉々子"}}}},linknext="block_00068"},block_00080={{"fg",face="a0001",mode=3,lv=2.2000000000000002,x=-480,id=5,file="kan_noa0200",head="kan_noa",size="no",ex05="kan_noa0000",ch="華乃",path=":fg/kan/no/"},{"fg",face="a0010",mode=3,lv=2.2000000000000002,x=160,id=12,file="rir_noa0200",head="rir_noa",size="no",ex05="rir_noa0000",ch="莉々子",path=":fg/rir/no/"},{"text"},line=363,linkback="block_00079",text={vo={{"vo",file="fem_kan_00028",ch="kan"}},ja={{"「啊，我查到了。在九州的一部分地区，他们会读作『猪肉汤』。福冈县，佐贺县，长崎县……还有熊本县的一部分。」",{"rt2"},name={"華乃"}}}},linknext="block_00081"},block_00060={{"bg",time=400,face="1",file="ev_hiy_31_06",set="ev_hiy_31",sync=0,path=":ev/ev_hiy_31/"},{"text"},line=246,linkback="block_00059",text={vo={{"vo",file="fem_hiy_00201",ch="hiy"}},ja={{"「嗯？你希望我察觉到哪一点呢？」",{"rt2"},name={"妃愛"}}}},linknext="block_00061"},block_00015={{"bg",time=400,face="1",file="ev_hiy_31_07",set="ev_hiy_31",sync=0,path=":ev/ev_hiy_31/"},{"text"},line=80,linkback="block_00014",text={vo={{"vo",file="fem_hiy_00182",ch="hiy"}},ja={{"「对不起，明明你在帮我，我却停下手来。现在就开始削皮，一定要削掉。我会努力不输给哥哥的杀敌数的。」",{"rt2"},name={"妃愛"}}}},linknext="block_00016"},block_00065={{"bgm",stop=1},{"cgdel",id=-1},{"bg",wait=250,time=1000,file="black",rule="rule_001",vague=40,path=":bg/"},{"bg",path=":bg/",rule="rule_001",file="bg016a",vague=40},{"bgm",file="bgm01"},{"text"},line=272,linkback="block_00064",text={ja={{"我一边搬运切好的食材出帐篷，外面公园的出口处竟然站着一个完全出乎意料的人。",{"rt2"}}}},linknext="block_00066"},block_00014={{"text"},line=72,linkback="block_00013",text={ja={{"「虽然没有其他人在，但叫哥哥也没关系吧？」",{"rt2"},name={"智宏"}}}},linknext="block_00015"},block_00096={{"text"},line=459,linkback="block_00095",text={ja={{"但想到明天我可能会在无所事事的同时，享受妃爱的照顾。毕竟这就是我们彼此的常态。",{"rt2"}}}},linknext="block_00097"},block_00002={{"text"},line=15,linkback="block_00001",text={ja={{"在那里，我的妹妹一个人正在削土豆。说起来，常磐、天梨和诗樱前辈都在猪骨汤锅前，而锦正在安抚哭泣的孩子。",{"rt2"}}}},linknext="block_00003"},block_00064={{"text"},line=260,linkback="block_00063",text={ja={{"但是，当我削两个土豆的皮时，妃爱已经削了六个。就在这种时候，该轮到我做饭了。今晚我要做美味的炒饭。",{"rt2"}}}},linknext="block_00065"},block_00061={{"text"},line=249,linkback="block_00060",text={ja={{"「我以为我每天都在说「谢谢你」，但这还不够。我的话很不够，对不起」",{"rt2"},name={"智宏"}}}},linknext="block_00062"},block_00058={{"text"},line=238,linkback="block_00057",text={ja={{"我们在削土豆的时候说了些什么呢。皮削得很干净，我向妃爱炫耀了一下。",{"rt2"}}}},linknext="block_00059"},block_00057={{"bg",time=400,face="1",file="ev_hiy_31_03",set="ev_hiy_31",sync=0,path=":ev/ev_hiy_31/"},{"text"},line=236,linkback="block_00056",text={vo={{"vo",file="fem_hiy_00200",ch="hiy"}},ja={{"「我最爱你了。世界上最喜欢的哥哥。只属于我的哥哥」",{"rt2"},name={"妃愛"}}}},linknext="block_00058"},block_00055={{"text"},line=228,linkback="block_00054",text={ja={{"「啊，终于明白了。我知道了，你一直在责难自己，心里不安」",{"rt2"},name={"智宏"}}}},linknext="block_00056"},block_00037={{"text"},line=158,linkback="block_00036",text={vo={{"vo",file="fem_hiy_00191",ch="hiy"}},ja={{"「把哥哥关在房间里，我也意识到是我引导成这样的……因为我想让你依赖我」",{"rt2"},name={"妃愛"}}}},linknext="block_00038"},block_00033={{"text"},line=144,linkback="block_00032",text={ja={{"其实我一直都在偷懒玩游戏。回想起过去的行为，我无言以对。",{"rt2"}}}},linknext="block_00034"},block_00078={{"fg",face="a0009",mode=3,lv=2.2000000000000002,x=160,id=12,file="rir_noa0200",head="rir_noa",size="no",ex05="rir_noa0000",ch="莉々子",path=":fg/rir/no/"},{"fgact",act="いいえ",lv=2.2000000000000002,ch="莉々子",id=12},{"text"},linkback="block_00077",line=352,delay={vl01={{"fg",face="a0028",mode=3,lv=2.2000000000000002,x=160,id=12,file="rir_noa0200",head="rir_noa",size="no",ex05="rir_noa0000",ch="莉々子",path=":fg/rir/no/"}}},text={vo={{"vo",file="fem_rir_00020",ch="rir"}},ja={{"「哈啊？豚汁就是豚汁。什么叫『猪骨汤』，第一次听到这个词……」",{"rt2"},name={"莉々子"}}}},linknext="block_00079"},block_00048={{"text"},line=200,linkback="block_00047",text={ja={{"「但是为什么突然说起这个话题？」",{"rt2"},name={"智宏"}}}},linknext="block_00049"},block_00006={{"text"},line=30,linkback="block_00005",text={ja={{"虽然我削土豆的手艺不如妃爱，但既然要切成小块，也没那么在意。",{"rt2"}}}},linknext="block_00007"},block_00087={{"text"},line=405,linkback="block_00086",text={ja={{"说起来，记得在九州的志愿者训练营时，也遇到过圣会长……嗯？",{"rt2"}}}},linknext="block_00088"},block_00042={{"text"},line=175,linkback="block_00041",text={ja={{"「哥哥为了妃爱什么都愿意做，但如果妃爱不高兴，我就什么都不会做了哦」",{"rt2"},name={"智宏"}}}},linknext="block_00043"},block_00040={{"text"},line=169,linkback="block_00039",text={ja={{"「哥哥的性格可比妃爱想的还要懒散哦。自主性这种事简直是天方夜谭，如果没有必要，我可是连床都不会离开的」",{"rt2"},name={"智宏"}}}},linknext="block_00041"},block_00053={{"bg",time=400,face="1",file="ev_hiy_31_08",set="ev_hiy_31",sync=0,path=":ev/ev_hiy_31/"},{"text"},line=220,linkback="block_00052",text={ja={{"刚低下头的妃爱立刻抬起头看向我。但我们还需要继续削土豆，所以我们又将视线转向了案板。",{"rt2"}}}},linknext="block_00054"},block_00032={{"text"},line=142,linkback="block_00031",text={ja={{"「那、那个…如果妃爱你有困难的话，我随时都会……」",{"rt2"},name={"智宏"}}}},linknext="block_00033"},block_00013={{"text"},line=69,linkback="block_00012",text={ja={{"我还在想她为什么停下手来，原来妃爱对现在我们兄妹两人一起削土豆这个场景感到心动。",{"rt2"}}}},linknext="block_00014"},block_00027={{"bg",time=400,face="1",ax=1280,set="ev_hiy_31",ay=720,by=-360,file="zev_hiy_31_07",bx=-640,y=70,x=-480,sync=0,path=":ev/ev_hiy_31/"},{"text"},line=126,linkback="block_00026",text={vo={{"vo",file="fem_hiy_00187",ch="hiy"}},ja={{"「我们两个一起做饭。把哥哥这样的机会都占为己有，对不起呢」",{"rt2"},name={"妃愛"}}}},linknext="block_00028"},block_00020={{"bg",time=400,face="1",file="ev_hiy_31_06",set="ev_hiy_31",sync=0,path=":ev/ev_hiy_31/"},{"text"},line=100,linkback="block_00019",text={vo={{"vo",file="fem_hiy_00184",ch="hiy"}},ja={{"「你居然还有闲心跟我聊天。不过确实，这可能是我和哥哥第一次单独进行学生会活动。」",{"rt2"},name={"妃愛"}}}},linknext="block_00021"},block_00011={{"bg",time=400,face="1",file="ev_hiy_31_02",set="ev_hiy_31",sync=0,path=":ev/ev_hiy_31/"},{"text"},line=60,linkback="block_00010",text={vo={{"vo",file="fem_hiy_00180",ch="hiy"}},ja={{"「不是的。对不起对不起，不是这个意思，我只是太开心了，所以」",{"rt2"},name={"妃愛"}}}},linknext="block_00012"}}