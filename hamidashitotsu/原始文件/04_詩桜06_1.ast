astver = 2.0
ast = {block_00081={{"fg",mode=3,path=":fg/kan/no/",face="b0023",id=10,size="no",lv=2.2000000000000002,x=230,head="kan_nob",ex05="kan_nob0010",ch="華乃",file="kan_nob0210"},{"text"},line=469,text={vo={{"vo",ch="kan",file="fem_kan_00182_3"}},ja={{"「那么，关于这件事，我们稍后再开个会商量一下如何」",{"rt2"},name={"華乃"}}}},linknext="block_00082",linkback="block_00080"},block_00112={{"fg",mode=3,path=":fg/shi/z2/",face="a0001",id=10,size="z2",lv=2.3999999999999999,x=280,head="shi_z2a",ex05="shi_z2a0000",ch="詩桜",file="shi_z2a0200"},{"text"},line=593,text={vo={{"vo",ch="shi",file="fem_shi_00705"}},ja={{"「你有肩膀缩起来走路的习惯。挺起胸膛走会更好」",{"rt2"},name={"詩桜"}}}},linknext="block_00113",linkback="block_00111"},block_00034={{"fg",mode=1,path=":fg/hiy/z1/",face="a0010",size="z1",id=20,lv=2.2999999999999998,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"},{"text"},linknext="block_00035",delay={vl01={{"fg",mode=1,path=":fg/hiy/z1/",face="a0052",size="z1",id=20,lv=2.2999999999999998,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"}}},linkback="block_00033",text={vo={{"vo",ch="hiy",file="fem_hiy_01265"}},ja={{"「哎呀，里姐，怎么了？是久违的来接哥哥吗？」",{"rt2"},name={"妃愛"}}}},line=217},block_00010={{"text"},line=92,text={ja={{"但是对我来说，我想让诗樱能愉快地说出她的心情。毕竟，这是她如此热衷的爱好，为了照顾我，她已经封印了半年。",{"rt2"}}}},linknext="block_00011",linkback="block_00009"},block_00108={{"text"},line=579,text={ja={{"妃爱在舞台上走路也十分自信，同时又显得十分可爱。当她和我一起在街上走动时，她似乎更愿意隐藏那种像明星一样的举止。",{"rt2"}}}},linknext="block_00109",linkback="block_00107"},block_00021={{"fg",mode=3,path=":fg/hiy/z1/",face="a0016",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"},{"text"},linknext="block_00022",delay={vl01={{"fg",mode=3,path=":fg/hiy/z1/",face="b0042",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1b",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0200"},{"fgact",act="おじぎ",lv=2.2999999999999998,ch="妃愛",id=10}}},linkback="block_00020",text={vo={{"vo",ch="hiy",file="fem_hiy_01263"}},ja={{"「明明装得很淡定地送走了诗樱，但随着时间的推移，你肯定越来越不安。哈，我哥哥的器量就像小咖啡杯一样狭窄！」",{"rt2"},name={"妃愛"}}}},line=142},block_00046={{"fg",lv=2.2999999999999998,mx=-40,mode=-1,ch="里",id=9},{"se",id=1,file="seドア(室内閉じ)"},{"fg",mode=1,path=":fg/hiy/z1/",face="a0002",size="z1",id=20,lv=2.2999999999999998,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"},{"text"},line=281,text={vo={{"vo",ch="hiy",file="fem_hiy_01267"}},ja={{"「我被排挤出去了……」",{"rt2"},name={"妃愛"}}}},linknext="block_00047",linkback="block_00045"},block_00065={{"fg",mode=3,path=":fg/shi/z1/",face="a0019",size="z1",id=9,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"},{"text"},line=386,text={vo={{"vo",ch="shi",file="fem_shi_00690"}},ja={{"「她想在学生会室和我谈些事。」",{"rt2"},name={"詩桜"}}}},linknext="block_00066",linkback="block_00064"},block_00067={{"msgoff"},{"bg",path=":bg/",sync=0,file="bg009a"},{"fg",sync=0,mode=3,path=":fg/shi/no/",face="b0002",id=9,lv=2.2000000000000002,size="no",x=-230,head="shi_nob",ex05="shi_nob0010",resize=1,ch="詩桜",file="shi_nob0210"},{"fg",sync=0,mode=3,path=":fg/kan/no/",face="b0010",id=10,size="no",lv=2.2000000000000002,x=230,head="kan_nob",ex05="kan_nob0010",ch="華乃",file="kan_nob0210"},{"extrans",time=500},{"text"},line=398,text={vo={{"vo",ch="shi",file="fem_shi_00691"}},ja={{"「今天我被和泉老师叫过去。我不清楚内容是什么，但似乎是要严肃地谈些事。」",{"rt2"},name={"詩桜"}}}},linknext="block_00068",linkback="block_00066"},block_00132={{"text"},line=690,text={ja={{"「呃哇」",{"rt2"},name={"智宏"}}}},linknext="block_00133",linkback="block_00131"},block_00061={{"text"},line=357,text={ja={{"不，嗯……如果能和这两个人在这个家里一直幸福地生活下去，那真的太好了。我已经没有什么别的愿望了。",{"rt2"}}}},linknext="block_00062",linkback="block_00060"},block_00044={{"fg",mode=3,path=":fg/mir/z1/",face="a0055",id=9,size="z1",lv=2.2999999999999998,x=-280,head="mir_z1a",ex05="mir_z1a0000",ch="里",file="mir_z1a0300"},{"fgact",act="うんうん",lv=2.2999999999999998,ch="里",id=9},{"text"},line=267,text={vo={{"vo",ch="mir",file="fem_mir_00090"}},ja={{"「对对。如果不提前说的话，你们两个可能就会去寻找只有你们两个人的世界，对吧？」",{"rt2"},name={"里"}}}},linknext="block_00045",linkback="block_00043"},block_00098={{"text"},line=547,text={ja={{"诗樱前辈虽然自信满满，而且有与之相匹配的能力，但她绝不会让我看到她在练习中的未熟状态。",{"rt2"}}}},linknext="block_00099",linkback="block_00097"},block_00115={{"text"},line=603,text={ja={{"肯定是觉得这是个好机会，一边进行康复训练一边改正我。她在微妙地照顾我，我觉得很有趣。",{"rt2"}}}},linknext="block_00116",linkback="block_00114"},block_00029={{"fg",mode=3,path=":fg/shi/z1/",face="a0031",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"},{"text"},line=182,text={vo={{"vo",ch="shi",file="fem_shi_00681"}},ja={{"「我想要的并不是护身符，而是希望有一天能从你那里得到真正的礼物。」",{"rt2"},name={"詩桜"}}}},linknext="block_00030",linkback="block_00028"},block_00048={{"msgoff"},{"bg",ax=1280,ay=720,sync=0,by=-360,path=":bg/",bx=-640,file="zbg002a"},{"fg",sync=0,mode=3,path=":fg/shi/z1/",face="a0038",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"},{"fg",sync=0,mode=3,path=":fg/hiy/z1/",face="a0001",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"},{"extrans",time=500},{"text"},line=293,text={vo={{"vo",ch="shi",file="fem_shi_00683"}},ja={{"「希望不是因为我住址的问题」",{"rt2"},name={"詩桜"}}}},linknext="block_00049",linkback="block_00047"},block_00052={{"fg",mode=3,path=":fg/hiy/z1/",face="a0034",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"},{"text"},linknext="block_00053",delay={vl01={{"fg",mode=3,path=":fg/hiy/z1/",face="a0023",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"}}},linkback="block_00051",text={vo={{"vo",ch="hiy",file="fem_hiy_01268"}},ja={{"「哎呀，当哥哥受伤的时候，邻居们都说「有什么事就告诉我们」……他们也理解我工作很忙」",{"rt2"},name={"妃愛"}}}},line=316},block_00023={{"fg",mode=3,path=":fg/hiy/z1/",face="b0010",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1b",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0200"},{"quake",mode="mw"},{"text"},line=153,text={ja={{"我差点把嘴里的米饭喷出来。啊，真的吗？ 我完全没注意到……。",{"rt2"}}}},linknext="block_00024",linkback="block_00022"},block_00125={{"text"},line=645,text={ja={{"「实际上，我非常喜欢前辈的走路方式」",{"rt2"},name={"智宏"}}}},linknext="block_00126",linkback="block_00124"},block_00073={{"fg",mode=3,path=":fg/kan/no/",face="b0031",id=10,size="no",lv=2.2000000000000002,x=230,head="kan_nob",ex05="kan_nob0000",ch="華乃",file="kan_nob0200"},{"text"},line=431,text={vo={{"vo",ch="kan",file="fem_kan_00181"}},ja={{"「即使你说感谢，我只是做好了学生会主管赋予的任务而已，不是吗？」",{"rt2"},name={"華乃"}}}},linknext="block_00074",linkback="block_00072"},block_00001={{"fg",mode=3,path=":fg/shi/z1/",face="b0054",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0000",ch="詩桜",file="shi_z1b0200"},{"fgact",act="ジャンプ",lv=2.2999999999999998,ch="詩桜",id=9},{"text"},linknext="block_00002",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="b0022",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0000",ch="詩桜",file="shi_z1b0200"}},vl02={{"fg",mode=3,path=":fg/shi/z1/",face="b0028",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0000",ch="詩桜",file="shi_z1b0200"},{"fgact",act="おじぎ",lv=2.2999999999999998,ch="詩桜",id=9}}},linkback="block_00000",text={vo={{"vo",ch="shi",file="fem_shi_00669"}},ja={{"「哈哈哈，这个时候正是伊佐木鱼的旺季呢。六月的伊佐木鱼被称为梅雨伊佐木……不过一旦进入梅雨季节，就因为雨天不能去钓鱼了」",{"rt2"},name={"詩桜"}}}},line=32},block_00012={{"text"},line=96,text={ja={{"然而，在允许她重新开始喜欢的钓鱼之前，有大约一个月的艰苦斗争。过于保护我的诗樱总是把我放在自己前面，所以她很少去钓鱼。",{"rt2"}}}},linknext="block_00013",linkback="block_00011"},block_00076={{"fg",mode=3,path=":fg/shi/no/",face="b0034",id=9,size="no",lv=2.2000000000000002,x=-230,head="shi_nob",ex05="shi_nob0010",ch="詩桜",file="shi_nob0210"},{"text"},line=447,text={vo={{"vo",ch="shi",file="fem_shi_00695"}},ja={{"「我的稿件总是在最后一刻完成。真是让人不安」",{"rt2"},name={"詩桜"}}}},linknext="block_00077",linkback="block_00075"},block_00077={{"text"},line=449,text={ja={{"面对珍稀的苦笑的诗樱前辈，优秀并且受欢迎的画师华乃脸上洋溢着满意的笑容。",{"rt2"}}}},linknext="block_00078",linkback="block_00076"},block_00026={{"text"},line=168,text={ja={{"「不是那个……我并不想限制诗樱学姐的乐趣……」",{"rt2"},name={"智宏"}}}},linknext="block_00027",linkback="block_00025"},block_00020={{"text"},line=135,text={ja={{"如果说出来，诗樱可能会顾虑不去钓鱼了，所以不能说出来啊。尽管我这么想，但妃爱指出的确实是我的真心话，所以我无言以对。",{"rt2"}}}},linknext="block_00021",linkback="block_00019"},block_00009={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",ax=1280,ay=720,sync=0,speed=120000,path=":bg/",x2=640,y2=100,by=-360,bx=-640,x=-640,y=100,file="zbg002a"},{"extrans"},{"text"},line=90,text={ja={{"妃爱已经习惯了流畅地听诗樱的话，一边看新闻一边美味地品尝着煮菜。啊，妃爱已经完全习惯了对待诗樱的方式……",{"rt2"}}}},linknext="block_00010",linkback="block_00008"},block_00071={{"text"},line=420,text={ja={{"我想妃爱应该会告诉锦的。不过就算没有告诉她，她来学生会室后就会明白了。",{"rt2"}}}},linknext="block_00072",linkback="block_00070"},block_00060={{"text"},line=355,text={ja={{"我深深地感触到了自己的无能，同时也惊讶于诗樱和妃爱都如此认真地在为我的未来奋斗。",{"rt2"}}}},linknext="block_00061",linkback="block_00059"},block_00104={{"text"},line=571,text={ja={{"当然，她的外表很出色，但是我感觉到，当我和同样外表出色的其他人走在一起时，她会比其他人更吸引人们的目光。最初我并不知道这是为什么。",{"rt2"}}}},linknext="block_00105",linkback="block_00103"},block_00089={{"fg",mode=3,path=":fg/shi/z1/",face="a0039",size="z1",id=9,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"},{"text"},linknext="block_00090",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0019",size="z1",id=9,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"}}},linkback="block_00088",text={vo={{"vo",ch="shi",file="fem_shi_00698"}},ja={{"「那是因为我不懂专业问题，尽量注意措辞……但我并没有妥协哦」",{"rt2"},name={"詩桜"}}}},line=511},block_00043={{"fg",mode=3,path=":fg/shi/z1/",face="a0008",id=10,size="z1",lv=2.2999999999999998,x=280,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"},{"text"},linknext="block_00044",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0038",id=10,size="z1",lv=2.2999999999999998,x=280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"}},vl02={{"fg",mode=3,path=":fg/shi/z1/",face="a0010",id=10,size="z1",lv=2.2999999999999998,x=280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"}}},linkback="block_00042",text={vo={{"vo",ch="shi",file="fem_shi_00682"}},ja={{"「我也是吗？这个与学生会的事务无关……可以吗？」",{"rt2"},name={"詩桜"}}}},line=261},block_00037={{"text"},line=226,text={ja={{"之所以只考虑学生会的事情，是因为自从升到三年级后，里姐就不再是我的班主任了。在学校里，除了学生会，我们现在没有其他的交集。",{"rt2"}}}},linknext="block_00038",linkback="block_00036"},block_00068={{"fg",mode=3,path=":fg/kan/no/",face="b0008",id=10,size="no",lv=2.2000000000000002,x=230,head="kan_nob",ex05="kan_nob0010",ch="華乃",file="kan_nob0210"},{"fgact",act="びっくり",lv=2.2000000000000002,ch="華乃",id=10},{"text"},linknext="block_00069",delay={vl01={{"fg",mode=3,path=":fg/kan/no/",face="b0002",id=10,size="no",lv=2.2000000000000002,x=230,head="kan_nob",ex05="kan_nob0000",ch="華乃",file="kan_nob0200"}}},linkback="block_00067",text={vo={{"vo",ch="kan",file="fem_kan_00180"}},ja={{"「啊，这样啊！？那我们不能用学生会室了吗？我本来想让诗樱前辈看一下纸板剧的插图的……」",{"rt2"},name={"華乃"}}}},line=405},block_00110={{"fg",mode=3,path=":fg/shi/z2/",face="a0039",id=10,size="z2",lv=2.3999999999999999,x=280,head="shi_z2a",ex05="shi_z2a0000",ch="詩桜",file="shi_z2a0200"},{"text"},line=586,text={vo={{"vo",ch="shi",file="fem_shi_00704"}},ja={{"「虽然现在可能还有一些受伤的后遗症……」",{"rt2"},name={"詩桜"}}}},linknext="block_00111",linkback="block_00109"},block_00013={{"text"},line=98,text={ja={{"诗樱总是说自己不能离开家，不能让我一个人待着，我和妃爱劝说她。每当电视上播放钓鱼节目时，她总是羡慕地看着。",{"rt2"}}}},linknext="block_00014",linkback="block_00012"},block_00097={{"text"},line=545,text={ja={{"尽管我很想参加……",{"rt2"}}}},linknext="block_00098",linkback="block_00096"},block_00047={{"text"},line=283,text={ja={{"里姐就像台风一样离开了，我们连问都没机会问。早餐的气氛完全被打破了，这餐桌该怎么办啊。",{"rt2"}}}},linknext="block_00048",linkback="block_00046"},block_00038={{"fg",mode=1,path=":fg/hiy/z1/",face="b0039",size="z1",id=20,lv=2.2999999999999998,head="hiy_z1b",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0200"},{"text"},linknext="block_00039",delay={vl01={{"fg",mode=1,path=":fg/hiy/z1/",face="b0010",size="z1",id=20,lv=2.2999999999999998,head="hiy_z1b",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0200"}},vl02={{"fg",mode=1,path=":fg/hiy/z1/",face="b0034",size="z1",id=20,lv=2.2999999999999998,head="hiy_z1b",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0200"}}},linkback="block_00037",text={vo={{"vo",ch="hiy",file="fem_hiy_01266"}},ja={{"「找哥哥有事……是学生会的工作吗？那我有关系吗？虽然我现在不是正式的干部。」",{"rt2"},name={"妃愛"}}}},line=233},block_00000={{"savetitle",text="水仙の花"},{"msgoff"},{"bg",ax=1280,ay=720,sync=0,by=-360,path=":bg/",bx=-640,file="zbg002a"},{"fg",sync=0,mode=3,path=":fg/shi/z1/",face="b0053",id=9,lv=2.2999999999999998,size="z1",x=-280,head="shi_z1b",ex05="shi_z1b0010",resize=1,ch="詩桜",file="shi_z1b0210"},{"fg",sync=0,mode=3,path=":fg/hiy/z1/",face="a0052",id=10,lv=2.2999999999999998,size="z1",x=280,head="hiy_z1a",ex05="hiy_z1a0000",resize=1,ch="妃愛",file="hiy_z1a0200"},{"extrans",time=2000},{"bgm",file="bgm05"},{"text"},line=23,text={vo={{"vo",ch="hiy",file="fem_hiy_01261"}},ja={{"「哎呀，昨天我姐姐钓来的鱼真的好好吃哦！比百货商店买的刺身还要美味！」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",mode=3,path=":fg/hiy/z1/",face="a0055",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"}}},linknext="block_00001"},block_00051={{"fg",mode=3,path=":fg/shi/z1/",face="b0002",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"},{"text"},linknext="block_00052",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="b0028",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"}}},linkback="block_00050",text={vo={{"vo",ch="shi",file="fem_shi_00685"}},ja={{"「作为管理者，可能会因为居住人数等方面产生各种问题。实际上，我也很担心邻居们怎么看待这个问题」",{"rt2"},name={"詩桜"}}}},line=310},block_00111={{"text"},line=588,text={ja={{"我一边走着，一边看着前方的诗樱前辈，她一边看着前方，一边开始和我说话。",{"rt2"}}}},linknext="block_00112",linkback="block_00110"},block_00116={{"msgoff"},{"bg",ax=1280,ay=720,sync=0,path=":bg/",file="zbg010a",by=-360,bx=-640,zoom=120,x=-400},{"fg",sync=0,mode=3,path=":fg/shi/z1/",face="a0020",id=10,size="z1",lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0010",resize=1,ch="詩桜",file="shi_z1a0210"},{"extrans",time=500},{"text"},line=612,text={ja={{"「和前辈在一起时，我觉得我可以挺起胸膛」",{"rt2"},name={"智宏"}}}},linknext="block_00117",linkback="block_00115"},block_00100={{"fg",mode=3,path=":fg/shi/z1/",face="a0019h",id=9,size="z1",lv=2.2999999999999998,hoho="h",head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"},{"text"},line=555,text={vo={{"vo",ch="shi",file="fem_shi_00703"}},ja={{"「我希望在你面前显得自信满满。请你理解我」",{"rt2"},name={"詩桜"}}}},linknext="block_00101",linkback="block_00099"},block_00101={{"text"},line=557,text={ja={{"由于她害羞地笑着的侧脸实在太美，当她让我原谅的时候，我就迅速答应了。",{"rt2"}}}},linknext="block_00102",linkback="block_00100"},block_00057={{"fg",mode=3,path=":fg/shi/z1/",face="a0001",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"},{"text"},linknext="block_00058",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0027",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"}}},linkback="block_00056",text={vo={{"vo",ch="shi",file="fem_shi_00688"}},ja={{"「我并不想因此给他的内申或者老师对我的评价带来负面影响，所以我在想，最好是能在我们毕业前保持现状。」",{"rt2"},name={"詩桜"}}}},line=344},block_00015={{"msgoff"},{"bg",ax=1280,ay=720,sync=0,by=-360,path=":bg/",bx=-640,file="zbg002a"},{"fg",sync=0,mode=3,path=":fg/shi/z1/",face="b0001",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"},{"fg",sync=0,mode=3,path=":fg/hiy/z1/",face="a0001",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"},{"extrans"},{"text"},linknext="block_00016",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="b0018",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"}}},linkback="block_00014",text={vo={{"vo",ch="shi",file="fem_shi_00676"}},ja={{"「我也想带和泉君去，但让他在岩石上走动，我还是有点担心……」",{"rt2"},name={"詩桜"}}}},line=112},block_00075={{"fg",mode=3,path=":fg/kan/no/",face="b0055",id=10,size="no",lv=2.2000000000000002,x=230,head="kan_nob",ex05="kan_nob0010",ch="華乃",file="kan_nob0210"},{"fgact",act="うなづく",lv=2.2000000000000002,ch="華乃",id=10},{"text"},line=442,text={vo={{"vo",ch="kan",file="fem_kan_00182"}},ja={{"「嘛，因为我是专业的，对吗？」",{"rt2"},name={"華乃"}}}},linknext="block_00076",linkback="block_00074"},block_00134={{"text"},line=697,text={ja={{"她也许以为这样就是在溺爱我。如果你要对我温柔一些，就请你向妃爱学习。",{"rt2"}}}},linknext="block_00135",linkback="block_00133"},block_00079={{"fg",mode=3,path=":fg/kan/no/",face="b0038",id=10,size="no",lv=2.2000000000000002,x=230,head="kan_nob",ex05="kan_nob0000",ch="華乃",file="kan_nob0200"},{"fg",mode=3,path=":fg/shi/no/",face="b0053",id=9,size="no",lv=2.2000000000000002,x=-230,head="shi_nob",ex05="shi_nob0010",ch="詩桜",file="shi_nob0210"},{"text"},linknext="block_00080",delay={vl01={{"fg",mode=3,path=":fg/kan/no/",face="b0001",id=10,size="no",lv=2.2000000000000002,x=230,head="kan_nob",ex05="kan_nob0000",ch="華乃",file="kan_nob0200"}}},linkback="block_00078",text={vo={{"vo",ch="kan",file="fem_kan_00182_2"}},ja={{"「只是诗樱前辈，从对话的内容来看，我有些地方真的很想增加一些插画……到了暑假应该会有些闲暇，增加一些插画页数可以吗？」",{"rt2"},name={"華乃"}}}},line=458},block_00128={{"fg",mode=3,path=":fg/shi/z1/",face="b0001",size="z1",id=10,lv=2.2999999999999998,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"},{"text"},linknext="block_00129",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="b0022",size="z1",id=10,lv=2.2999999999999998,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"}},vl02={{"fg",mode=3,path=":fg/shi/z1/",face="b0003",size="z1",id=10,lv=2.2999999999999998,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"}}},linkback="block_00127",text={vo={{"vo",ch="shi",file="fem_shi_00711"}},ja={{"「改正姿势也需要肌肉力量。你可以做点运动，如果是武道，那就更好了。不过，一定要在不被溺爱的环境下进行。」",{"rt2"},name={"詩桜"}}}},line=662},block_00119={{"fg",mode=3,path=":fg/shi/z1/",face="a0053",size="z1",id=10,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"},{"fgact",act="うなづく",lv=2.2999999999999998,ch="詩桜",id=10},{"text"},linknext="block_00120",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0054",size="z1",id=10,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"}}},linkback="block_00118",text={vo={{"vo",ch="shi",file="fem_shi_00708"}},ja={{"「是吗，我明白了。我喜欢你。我打算把余生都献给你，服侍你」",{"rt2"},name={"詩桜"}}}},line=627},block_00090={{"fg",mode=3,path=":fg/shi/z1/",face="b0052",size="z1",id=9,lv=2.2999999999999998,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"},{"text"},linknext="block_00091",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="b0055",size="z1",id=9,lv=2.2999999999999998,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"}}},linkback="block_00089",text={vo={{"vo",ch="shi",file="fem_shi_00699"}},ja={{"「看着常磐的工作进度，我时常会感觉她习惯于团队作业。一向独自创作的我，觉得这一切既新鲜又有趣」",{"rt2"},name={"詩桜"}}}},line=517},block_00080={{"fg",mode=3,path=":fg/shi/no/",face="b0010",id=9,size="no",lv=2.2000000000000002,x=-230,head="shi_nob",ex05="shi_nob0000",ch="詩桜",file="shi_nob0200"},{"text"},linknext="block_00081",delay={vl01={{"fg",mode=3,path=":fg/shi/no/",face="b0015",id=9,size="no",lv=2.2000000000000002,x=-230,head="shi_nob",ex05="shi_nob0000",ch="詩桜",file="shi_nob0200"}}},linkback="block_00079",text={vo={{"vo",ch="shi",file="fem_shi_00695_2"}},ja={{"「不仅按时交付，还要做额外的工作吗？当然，我不介意」",{"rt2"},name={"詩桜"}}}},line=464},block_00006={{"fg",mode=3,path=":fg/shi/z1/",face="b0022",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"},{"text"},linknext="block_00007",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="b0032",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"}},vl02={{"fg",mode=3,path=":fg/shi/z1/",face="b0006",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"}}},linkback="block_00005",text={vo={{"vo",ch="shi",file="fem_shi_00674"}},ja={{"「如果妃爱会开心的话，我想再去钓鱼。下次要不要试试石鲷呢？三裂港稍微往前一点的海域里，有一个可以抵达深海的好岩场」",{"rt2"},name={"詩桜"}}}},line=73},block_00066={{"text"},line=388,text={ja={{"里姐给我发送的LIME信息也提到了给忘说地点的这桩事，还附带了一句话：「真是糟糕啊！？」你的脑筋才糟糕。",{"rt2"}}}},linknext="block_00067",linkback="block_00065"},block_00041={{"text"},line=244,text={ja={{"我自作聪明地猜测对话内容，结果完全误解了。这是怎么回事，跟学生会无关的事情找我……难道是家里的事？",{"rt2"}}}},linknext="block_00042",linkback="block_00040"},block_00105={{"text"},line=573,text={ja={{"我在恢复过来之后才明白原因。她走路的姿势很优雅。她昂首挺胸，脚步稳健，步伐从容，行走得堂堂正正。",{"rt2"}}}},linknext="block_00106",linkback="block_00104"},block_00082={{"text"},line=471,text={ja={{"但即便如此，最近这两人关系似乎挺好的。",{"rt2"}}}},linknext="block_00083",linkback="block_00081"},block_00008={{"text"},line=82,text={ja={{"你说得好激动啊……",{"rt2"}}}},linknext="block_00009",linkback="block_00007"},block_00035={{"fg",mode=3,path=":fg/mir/no/",face="a0053",size="no",id=13,lv=2.2000000000000002,head="mir_noa",ex05="mir_noa0010",ch="里",file="mir_noa0310"},{"text"},line=222,text={vo={{"vo",ch="mir",file="fem_mir_00087"}},ja={{"「嗯？不是，今天我并不是来接人的，只是有事要告诉智宏，所以才过来的。」",{"rt2"},name={"里"}}}},linknext="block_00036",linkback="block_00034"},block_00085={{"se",id=1,file="se扉開く(スライド)教室"},{"cgdel",id=-1},{"bg",time=1000,vague=40,rule="rule_001",path=":bg/",wait=250,file="black"},{"msgoff"},{"bg",ax=1280,ay=720,sync=0,by=-360,path=":bg/",bx=-640,file="zbg010a"},{"fg",sync=0,mode=3,path=":fg/shi/z1/",face="a0001",id=9,size="z1",lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0000",resize=1,ch="詩桜",file="shi_z1a0200"},{"extrans",rule="rule_001",vague=40},{"text"},linknext="block_00086",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0032",size="z1",id=9,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"}}},linkback="block_00084",text={vo={{"vo",ch="shi",file="fem_shi_00697"}},ja={{"「常磐真是能干的女士……每月底之前总是能交上一幅画，而且基本上不存在修改的需要」",{"rt2"},name={"詩桜"}}}},line=498},label={top={label=1,block="block_00000"}},block_00019={{"fg",mode=3,path=":fg/hiy/z1/",face="a0043",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"},{"text"},linknext="block_00020",delay={vl01={{"fg",mode=3,path=":fg/hiy/z1/",face="a0003",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"},{"fgact",act="うんうん",lv=2.2999999999999998,ch="妃愛",id=10}}},linkback="block_00018",text={vo={{"vo",ch="hiy",file="fem_hiy_01262"}},ja={{"「就算是我这个深爱的哥哥，也真是太狼狈了。诗樱姐姐诗樱姐姐。这家伙担心在海上或船上无法和你说话呢。」",{"rt2"},name={"妃愛"}}}},line=133},block_00017={{"fg",mode=3,path=":fg/shi/z1/",face="a0028",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"},{"text"},linknext="block_00018",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0019",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"}}},linkback="block_00016",text={vo={{"vo",ch="shi",file="fem_shi_00677"}},ja={{"「对啊，把你这种不习惯炎热天气的人带去钓鱼可能不太好。等换个季节再邀请你吧。」",{"rt2"},name={"詩桜"}}}},line=121},block_00078={{"text"},line=451,text={ja={{"常磐最自豪的一点就是「从不拖稿」，在创作者圈子里，这才是最值得尊敬的事情。",{"rt2"}}}},linknext="block_00079",linkback="block_00077"},block_00118={{"text"},line=620,text={ja={{"「因为能和这么美丽的人在一起让我感到自豪」",{"rt2"},name={"智宏"}}}},linknext="block_00119",linkback="block_00117"},block_00030={{"text"},line=185,text={ja={{"「好，好的。我会努力的。」",{"rt2"},name={"智宏"}}}},linknext="block_00031",linkback="block_00029"},block_00036={{"text"},line=224,text={ja={{"对，里姐在三个月前就已经停止接送我了。所以她现在出现在这里，我猜测可能是有关学生会的事情。",{"rt2"}}}},linknext="block_00037",linkback="block_00035"},block_00106={{"text"},line=575,text={ja={{"而且她的视线始终保持稳定。她始终看着前方，不去注意旁边的事情。她仿佛不把世间的琐事放在眼里，周围的杂音对她来说根本无足轻重。",{"rt2"}}}},linknext="block_00107",linkback="block_00105"},block_00099={{"text"},line=550,text={ja={{"「我却想看诗樱前辈害羞的样子……」",{"rt2"},name={"智宏"}}}},linknext="block_00100",linkback="block_00098"},block_00055={{"fg",mode=3,path=":fg/shi/z1/",face="b0023",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"},{"text"},line=332,text={vo={{"vo",ch="shi",file="fem_shi_00686"}},ja={{"「如果能得到妃爱的许可，我正在考虑……明年内……」",{"rt2"},name={"詩桜"}}}},linknext="block_00056",linkback="block_00054"},block_00074={{"fg",mode=3,path=":fg/shi/no/",face="b0022",id=9,size="no",lv=2.2000000000000002,x=-230,head="shi_nob",ex05="shi_nob0000",ch="詩桜",file="shi_nob0200"},{"text"},line=436,text={vo={{"vo",ch="shi",file="fem_shi_00694"}},ja={{"「你按照每月一张的进度，从未耽搁过提交」",{"rt2"},name={"詩桜"}}}},linknext="block_00075",linkback="block_00073"},block_00059={{"text"},line=353,text={ja={{"太了不起了，她们甚至想到了户口本的事情，当我卧床不起时，她们在为我周围的环境做群众工作。我只是一个人无忧无虑地在复习考试。",{"rt2"}}}},linknext="block_00060",linkback="block_00058"},block_00062={{"text"},line=359,text={ja={{"但，但是，这个年龄就有了「家庭」，还是感觉有点太早了……不过，今年我也得做好心理准备！",{"rt2"}}}},linknext="block_00063",linkback="block_00061"},block_00027={{"fg",mode=3,path=":fg/hiy/z1/",face="a0043",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"},{"text"},linknext="block_00028",delay={vl01={{"fg",mode=3,path=":fg/hiy/z1/",face="a0026",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"}}},linkback="block_00026",text={vo={{"vo",ch="hiy",file="fem_hiy_01264"}},ja={{"「你这个犹豫不决的哥哥，还真是不像个男人。」",{"rt2"},name={"妃愛"}}}},line=174},block_00045={{"fg",mode=3,path=":fg/mir/z1/",face="a0025",id=9,size="z1",lv=2.2999999999999998,x=-280,head="mir_z1a",ex05="mir_z1a0000",ch="里",file="mir_z1a0300"},{"text"},linknext="block_00046",delay={vl01={{"fg",mode=3,path=":fg/mir/z1/",face="a0052",id=9,size="z1",lv=2.2999999999999998,x=-280,head="mir_z1a",ex05="mir_z1a0000",ch="里",file="mir_z1a0300"}}},linkback="block_00044",text={vo={{"vo",ch="mir",file="fem_mir_00091"}},ja={{"「嘛，你们两个人一起亲密地吃午餐，在我看来完全没问题哦。具体的事情我们在午餐时再谈吧。」",{"rt2"},name={"里"}}}},line=273},block_00002={{"fg",mode=3,path=":fg/shi/z1/",face="a0022",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"},{"text"},linknext="block_00003",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0031",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"}}},linkback="block_00001",text={vo={{"vo",ch="shi",file="fem_shi_00670"}},ja={{"「好久没钓鱼了，真是愉快。昨天天气晴朗，云都没有一片。坐在夏日三裂港的堤防上，眼前就是无边无际的太平洋，仿佛和天空融为一体」",{"rt2"},name={"詩桜"}}}},line=40},block_00092={{"fg",mode=3,path=":fg/shi/z1/",face="b0016",size="z1",id=9,lv=2.2999999999999998,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"},{"fgact",act="いいえ",lv=2.2999999999999998,ch="詩桜",id=9},{"text"},line=526,text={vo={{"vo",ch="shi",file="fem_shi_00700"}},ja={{"「我拒绝。这有点尴尬」",{"rt2"},name={"詩桜"}}}},linknext="block_00093",linkback="block_00091"},block_00120={{"fg",mode=3,path=":fg/shi/z1/",face="a0022",size="z1",id=10,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"},{"text"},linknext="block_00121",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0006",size="z1",id=10,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"}}},linkback="block_00119",text={vo={{"vo",ch="shi",file="fem_shi_00709"}},ja={{"「我希望你能自信满满地变成一个配得上我这位恋人的男性。」",{"rt2"},name={"詩桜"}}}},line=633},block_00058={{"fg",mode=3,path=":fg/hiy/z1/",face="a0010",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"},{"text"},linknext="block_00059",delay={vl01={{"fg",mode=3,path=":fg/hiy/z1/",face="a0054",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"}},vl02={{"fg",mode=3,path=":fg/hiy/z1/",face="a0052",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1a",ex05="hiy_z1a0000",ch="妃愛",file="hiy_z1a0200"}}},linkback="block_00057",text={vo={{"vo",ch="hiy",file="fem_hiy_01271"}},ja={{"「诗樱姐……如果姐姐已经想得这么周全了，那我也会按照这个方向去做的。如果里姐有关于家庭的问题，我们再来开家庭会议吧。」",{"rt2"},name={"妃愛"}}}},line=351},block_00126={{"fg",mode=3,path=":fg/shi/z1/",face="a0052",size="z1",id=10,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"},{"fgact",act="うなづく",lv=2.2999999999999998,ch="詩桜",id=10},{"text"},linknext="block_00127",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0055",size="z1",id=10,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"}}},linkback="block_00125",text={vo={{"vo",ch="shi",file="fem_shi_00710"}},ja={{"「哦，那你就学习学习吧，如果需要练习，我会陪你的」",{"rt2"},name={"詩桜"}}}},line=652},block_00025={{"fg",mode=3,path=":fg/shi/z1/",face="a0052",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"},{"text"},linknext="block_00026",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0055",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"}}},linkback="block_00024",text={vo={{"vo",ch="shi",file="fem_shi_00680"}},ja={{"「除非你同行，否则我会尽量不上船。这样可以吗？」",{"rt2"},name={"詩桜"}}}},line=165},block_00113={{"fg",mode=3,path=":fg/shi/z2/",face="a0039",id=10,size="z2",lv=2.3999999999999999,x=280,head="shi_z2a",ex05="shi_z2a0010",ch="詩桜",file="shi_z2a0210"},{"text"},linknext="block_00114",delay={vl01={{"fg",mode=3,path=":fg/shi/z2/",face="a0020",id=10,size="z2",lv=2.3999999999999999,x=280,head="shi_z2a",ex05="shi_z2a0010",ch="詩桜",file="shi_z2a0210"}}},linkback="block_00112",text={vo={{"vo",ch="shi",file="fem_shi_00706"}},ja={{"「还有，你走路时身体前倾也很不好，会加重驼背。哎，我知道可能是受伤的影响」",{"rt2"},name={"詩桜"}}}},line=599},block_00131={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",ax=1280,ay=720,sync=0,speed=120000,path=":bg/",x2=640,y2=100,zoom=120,by=-360,bx=-640,x=-640,y=100,file="zbg010a"},{"extrans"},{"quake"},{"se",id=1,file="seコミ_つねる"},{"text"},line=687,text={ja={{"前辈说自己是失去獠牙的虎，但是当我背脊弯曲走路时，前辈就用手臂狠狠地按住我的脊椎，让我纠正姿势。手上的力量很大。",{"rt2"}}}},linknext="block_00132",linkback="block_00130"},block_00069={{"fg",mode=1,path=":fg/ame/no/",face="b0009",size="no",id=20,lv=2.2000000000000002,head="ame_nob",ex05="ame_nob0000",ch="天梨",file="ame_nob0200"},{"text"},linknext="block_00070",delay={vl01={{"fg",mode=1,path=":fg/ame/no/",face="b0047",size="no",id=20,lv=2.2000000000000002,head="ame_nob",ex05="ame_nob0000",ch="天梨",file="ame_nob0200"}}},linkback="block_00068",text={vo={{"vo",ch="ame",file="fem_ame_00062"}},ja={{"「诶—！现在去学餐看看还有没有座位吧……呜嗯，我更希望能在休息前告诉我这件事啊」",{"rt2"},name={"天梨"}}}},line=411},block_00022={{"fg",mode=3,path=":fg/shi/z1/",face="b0023",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0000",ch="詩桜",file="shi_z1b0200"},{"text"},line=147,text={vo={{"vo",ch="shi",file="fem_shi_00678"}},ja={{"「出门的时候，我会把戒指戴在左手无名指上。」",{"rt2"},name={"詩桜"}}}},linknext="block_00023",linkback="block_00021"},block_00102={{"cgdel",id=-1},{"bg",time=1000,vague=40,rule="rule_001",path=":bg/",wait=250,file="black"},{"msgoff"},{"bg",ax=1280,ay=720,by=-360,sync=0,bx=-640,path=":bg/",zoom=120,file="zbg010a"},{"fg",sync=0,mode=3,path=":fg/shi/z2/",face="a0001",id=10,lv=2.3999999999999999,size="z2",x=280,head="shi_z2a",ex05="shi_z2a0000",resize=1,ch="詩桜",file="shi_z2a0200"},{"extrans",rule="rule_001",vague=40},{"text"},line=567,text={ja={{"哈啊，像这样并肩行走，让我又一次认识到诗樱前辈的出色外貌……",{"rt2"}}}},linknext="block_00103",linkback="block_00101"},block_00087={{"text"},line=502,text={ja={{"但可能是因为常磐经常做确认，我记不起诗樱前辈对提交的画提出修改的情况。最多就是讨论过颜色的搭配。",{"rt2"}}}},linknext="block_00088",linkback="block_00086"},block_00011={{"text"},line=94,text={ja={{"她一定很喜欢钓鱼。昨天早上四点，夏日的阳光还未升起，她就骑着摩托车兴高采烈地出发了。",{"rt2"}}}},linknext="block_00012",linkback="block_00010"},block_00028={{"text"},line=177,text={ja={{"「好的。谢谢你，我很高兴能听到诗樱学姐这样的话。我相信你。」",{"rt2"},name={"智宏"}}}},linknext="block_00029",linkback="block_00027"},block_00103={{"text"},line=569,text={ja={{"无论是在学校里行走，还是在外面的街头闲逛，她总是引人注目。站在她旁边，我能真切地感受到周围的目光。",{"rt2"}}}},linknext="block_00104",linkback="block_00102"},block_00124={{"text"},line=642,text={ja={{"这么美丽的女人，愿意陪伴我一生，让我成为一个独立的男人，自然令人羡慕。我也以我自己和我的恋人感到骄傲。",{"rt2"}}}},linknext="block_00125",linkback="block_00123"},block_00004={{"fg",mode=3,path=":fg/shi/z1/",face="b0054",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0000",ch="詩桜",file="shi_z1b0200"},{"text"},linknext="block_00005",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="b0052",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0000",ch="詩桜",file="shi_z1b0200"}}},linkback="block_00003",text={vo={{"vo",ch="shi",file="fem_shi_00672"}},ja={{"「但是现在我有了家，有了在家等我的人，我可以和她分享这份喜悦。由妃爱处理的鱼，和妃爱一起品尝的味道，真是无比美妙」",{"rt2"},name={"詩桜"}}}},line=59},block_00016={{"text"},line=115,text={ja={{"「啊？ 嗯，不是，我是……」",{"rt2"},name={"智宏"}}}},linknext="block_00017",linkback="block_00015"},block_00007={{"fg",mode=3,path=":fg/shi/z1/",face="a0039",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"},{"text"},linknext="block_00008",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0002",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"}},vl02={{"fg",mode=3,path=":fg/shi/z1/",face="a0028",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"}}},linkback="block_00006",text={vo={{"vo",ch="shi",file="fem_shi_00675"}},ja={{"「那里确实是个绝佳的钓鱼地点，但如果得意忘形地钓上三个小时，脚下就会被海水浸湿了。尽管只是海水，但如果浸到膝盖，走路回家就会觉得很烦……」",{"rt2"},name={"詩桜"}}}},line=80},block_00095={{"text"},line=537,text={ja={{"「你已经写完稿子了，诗樱前辈你准备的是……啊，朗读训练！？那个，我也可以参加吗！？你一直是和妃爱一起练习的对吧？」",{"rt2"},name={"智宏"}}}},linknext="block_00096",linkback="block_00094"},block_00018={{"text"},line=124,text={ja={{"「啊，不是那个，其实我对夏日的海很感兴趣的，就是怎么说呢……」",{"rt2"},name={"智宏"}}}},linknext="block_00019",linkback="block_00017"},block_00005={{"fg",mode=3,path=":fg/shi/z1/",face="b0031",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0000",ch="詩桜",file="shi_z1b0200"},{"text"},linknext="block_00006",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="b0010",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"}},vl02={{"fg",mode=3,path=":fg/shi/z1/",face="b0035",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"}}},linkback="block_00004",text={vo={{"vo",ch="shi",file="fem_shi_00673"}},ja={{"「有了我所爱的人，我的快乐升华为幸福……独食鲷鱼不香，这就是那句话的意思吧。我真是幸运……能和泉家一起围坐在餐桌上真是幸福」",{"rt2"},name={"詩桜"}}}},line=66},block_00135={{"bgm",stop=1},{"cgdel",id=-1},{"bg",time=1000,vague=40,rule="rule_001",path=":bg/",wait=250,file="black"},{"excall",file="04_詩桜06_2"},{"text"},line=702,linkback="block_00134"},block_00040={{"text"},line=242,text={ja={{"「哎呀」",{"rt2"},name={"智宏"}}}},linknext="block_00041",linkback="block_00039"},block_00130={{"fg",mode=3,path=":fg/shi/z1/",face="b0008",size="z1",id=10,lv=2.2999999999999998,head="shi_z1b",ex05="shi_z1b0000",ch="詩桜",file="shi_z1b0200"},{"fgact",act="びっくり",lv=2.2999999999999998,ch="詩桜",id=10},{"text"},linknext="block_00131",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="b0023",size="z1",id=10,lv=2.2999999999999998,head="shi_z1b",ex05="shi_z1b0000",ch="詩桜",file="shi_z1b0200"}}},linkback="block_00129",text={vo={{"vo",ch="shi",file="fem_shi_00712"}},ja={{"「等等，我不是很温柔吗。我不是一直宠你吗。对你来说，我已经变成了完全失去獠牙的虎了。」",{"rt2"},name={"詩桜"}}}},line=672},block_00129={{"text"},line=665,text={ja={{"「在恋人面前严格已经够了……」",{"rt2"},name={"智宏"}}}},linknext="block_00130",linkback="block_00128"},block_00084={{"text"},line=480,text={ja={{"里姐正在认真讨论问题，如果在她面前吃饭似乎也不妥……在剩下五分钟的时间里决定准备吃便当，于是我和诗樱前辈一起离开了座位。",{"rt2"}}}},linknext="block_00085",linkback="block_00083"},block_00123={{"text"},line=640,text={ja={{"或许是因为她平时就这样，或许是她在教室里的表现太过于体贴，我并未感受到嫉妒的目光。然而，我确实感受到了羡慕的目光。",{"rt2"}}}},linknext="block_00124",linkback="block_00122"},block_00122={{"text"},line=638,text={ja={{"我们就在走廊上这么说。她真的完全不在意周围的人。",{"rt2"}}}},linknext="block_00123",linkback="block_00121"},block_00121={{"text"},line=636,text={ja={{"「好的」",{"rt2"},name={"智宏"}}}},linknext="block_00122",linkback="block_00120"},block_00091={{"text"},line=520,text={ja={{"「如果你告诉常磐这些，她一定会很开心的」",{"rt2"},name={"智宏"}}}},linknext="block_00092",linkback="block_00090"},block_00063={{"bgm",stop=1},{"cgdel",id=-1},{"bg",time=1000,vague=40,rule="rule_001",path=":bg/",wait=250,file="black"},{"msgoff"},{"bg",ax=1280,ay=720,by=-360,sync=0,bx=-640,path=":bg/",file="zbg009a",x=400},{"fg",sync=0,mode=3,path=":fg/shi/z1/",face="a0001",size="z1",id=9,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"},{"extrans",rule="rule_001",vague=40},{"bgm",file="bgm06"},{"text"},line=378,text={vo={{"vo",ch="shi",file="fem_shi_00689"}},ja={{"「收到了和泉老师的信息。她说今早显然忘了告诉我们地点。」",{"rt2"},name={"詩桜"}}}},linknext="block_00064",linkback="block_00062"},block_00117={{"fg",mode=3,path=":fg/shi/z1/",face="a0002",size="z1",id=10,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"},{"text"},line=617,text={vo={{"vo",ch="shi",file="fem_shi_00707"}},ja={{"「那是为什么呢」",{"rt2"},name={"詩桜"}}}},linknext="block_00118",linkback="block_00116"},block_00070={{"fg",mode=3,path=":fg/shi/no/",face="b0034",id=9,size="no",lv=2.2000000000000002,x=-230,head="shi_nob",ex05="shi_nob0010",ch="詩桜",file="shi_nob0210"},{"fgact",act="いいえ",lv=2.2000000000000002,ch="詩桜",id=9},{"text"},linknext="block_00071",delay={vl01={{"fg",mode=3,path=":fg/shi/no/",face="b0019",id=9,size="no",lv=2.2000000000000002,x=-230,head="shi_nob",ex05="shi_nob0010",ch="詩桜",file="shi_nob0210"}}},linkback="block_00069",text={vo={{"vo",ch="shi",file="fem_shi_00692"}},ja={{"「啊，不，因为我会使用会议室，所以你们在学生会室吃饭是没问题的。只是我和他不能一起吃而已。」",{"rt2"},name={"詩桜"}}}},line=418},block_00114={{"text"},line=601,text={ja={{"抱着我的伤势作为借口，诗樱前辈开始矫正我的走路方式。她肯定已经注意到了。",{"rt2"}}}},linknext="block_00115",linkback="block_00113"},block_00109={{"text"},line=581,text={ja={{"如此优雅的步伐和妃爱、天梨相比也毫不逊色的诗樱前辈，她是在哪里学到这样行走的姿态的呢？这是个谜。",{"rt2"}}}},linknext="block_00110",linkback="block_00108"},block_00093={{"text"},line=528,text={ja={{"虽然不是因为之前的争执而怀恨在心，但诗樱对常磐似乎有种竞争心态。虽然确实承认了，但并不是很坦诚。",{"rt2"}}}},linknext="block_00094",linkback="block_00092"},block_00003={{"fg",mode=3,path=":fg/shi/z1/",face="a0055",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"},{"text"},linknext="block_00004",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0019",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"}},vl02={{"fg",mode=3,path=":fg/shi/z1/",face="a0052",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"}}},linkback="block_00002",text={vo={{"vo",ch="shi",file="fem_shi_00671"}},ja={{"「而且昨天，我发现了钓鱼新的乐趣。以前我总是独享所钓之鱼。用自己的手钓起来，然后自己动手烹制，最后自己品尝，这就是钓鱼的乐趣」",{"rt2"},name={"詩桜"}}}},line=49},block_00107={{"text"},line=577,text={ja={{"天梨引人注目的原因也可能是同样的。作为一个让人注目的人物，她一定是通过辛勤的努力才能将站立和走路的姿势掌握得如此熟练。",{"rt2"}}}},linknext="block_00108",linkback="block_00106"},block_00096={{"fg",mode=3,path=":fg/shi/z1/",face="a0050",size="z1",id=9,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"},{"fgact",act="いいえ",lv=2.2999999999999998,ch="詩桜",id=9},{"text"},line=543,text={vo={{"vo",ch="shi",file="fem_shi_00702"}},ja={{"「我拒绝。感觉还是很害羞」",{"rt2"},name={"詩桜"}}}},linknext="block_00097",linkback="block_00095"},block_00127={{"text"},line=655,text={ja={{"「走路的方式，我应该在哪里学习呢？」",{"rt2"},name={"智宏"}}}},linknext="block_00128",linkback="block_00126"},block_00088={{"text"},line=505,text={ja={{"「诗樱前辈不过分插手，也许有利于事情的顺利进行吧」",{"rt2"},name={"智宏"}}}},linknext="block_00089",linkback="block_00087"},block_00072={{"fg",mode=3,path=":fg/shi/no/",face="b0053",id=9,size="no",lv=2.2000000000000002,x=-230,head="shi_nob",ex05="shi_nob0000",ch="詩桜",file="shi_nob0200"},{"text"},linknext="block_00073",delay={vl01={{"fg",mode=3,path=":fg/shi/no/",face="b0055",id=9,size="no",lv=2.2000000000000002,x=-230,head="shi_nob",ex05="shi_nob0000",ch="詩桜",file="shi_nob0200"}}},linkback="block_00071",text={vo={{"vo",ch="shi",file="fem_shi_00693"}},ja={{"「我会在课后欣赏常磐同学的画……总是很感谢你，我很欣赏」",{"rt2"},name={"詩桜"}}}},line=426},block_00014={{"text"},line=100,text={ja={{"然而到了那天，诗樱的眼睛闪烁着光芒，像个孩子一样兴奋。有了这样的曲折经历，我想愉悦地听诗樱的话……",{"rt2"}}}},linknext="block_00015",linkback="block_00013"},block_00094={{"fg",mode=3,path=":fg/shi/z1/",face="a0003",size="z1",id=9,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"},{"text"},linknext="block_00095",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0027",size="z1",id=9,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"}}},linkback="block_00093",text={vo={{"vo",ch="shi",file="fem_shi_00701"}},ja={{"「今天完成的话就是第9张了，按照原计划剩下3张……但常磐说她想增加画数。所以在她推进的同时，我也必须准备好自己的部分」",{"rt2"},name={"詩桜"}}}},line=534},block_00064={{"text"},line=381,text={ja={{"「里姐，您总是忘了说一些关键的事情……」",{"rt2"},name={"智宏"}}}},linknext="block_00065",linkback="block_00063"},block_00083={{"fg",mode=3,path=":fg/shi/no/",face="b0052",id=9,size="no",lv=2.2000000000000002,x=-230,head="shi_nob",ex05="shi_nob0010",ch="詩桜",file="shi_nob0210"},{"text"},linknext="block_00084",delay={vl01={{"fg",mode=3,path=":fg/shi/no/",face="a0001",id=9,size="no",lv=2.2000000000000002,x=-230,head="shi_noa",ex05="shi_noa0010",ch="詩桜",file="shi_noa0210"}},vl02={{"fg",mode=3,path=":fg/shi/no/",face="a0053",id=9,size="no",lv=2.2000000000000002,x=-230,head="shi_noa",ex05="shi_noa0010",ch="詩桜",file="shi_noa0210"}}},linkback="block_00082",text={vo={{"vo",ch="shi",file="fem_shi_00696"}},ja={{"「那我就先走一步了。据谈话的严肃程度来看，似乎没有太多吃饭的时间了……好，开始走吧」",{"rt2"},name={"詩桜"}}}},line=478},block_00086={{"text"},line=500,text={ja={{"目前为止，诗樱前辈和常磐在创作过程中还没有发生冲突。一开始，我和其他三人总是因为可能存在的图像差异而感到恐慌，总是战战兢兢。",{"rt2"}}}},linknext="block_00087",linkback="block_00085"},block_00056={{"fg",mode=3,path=":fg/shi/z1/",face="a0002",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"},{"text"},linknext="block_00057",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0028",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0010",ch="詩桜",file="shi_z1a0210"}}},linkback="block_00055",text={vo={{"vo",ch="shi",file="fem_shi_00687"}},ja={{"「之前是因为他需要护理而住在一起，但如果还没有特别的正当理由，我并不确定学校会怎样看待我们这样的同学共居……」",{"rt2"},name={"詩桜"}}}},line=338},block_00054={{"fg",mode=3,path=":fg/hiy/z1/",face="b0010",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1b",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0200"},{"text"},line=327,text={vo={{"vo",ch="hiy",file="fem_hiy_01270"}},ja={{"「话说回来，如果真的出现这样的麻烦，为什么不把户口迁到这里，彻底搬过来呢？」",{"rt2"},name={"妃愛"}}}},linknext="block_00055",linkback="block_00053"},block_00053={{"fg",mode=3,path=":fg/hiy/z1/",face="b0019",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1b",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0200"},{"text"},linknext="block_00054",delay={vl01={{"fg",mode=3,path=":fg/hiy/z1/",face="b0034",id=10,size="z1",lv=2.2999999999999998,x=280,head="hiy_z1b",ex05="hiy_z1b0000",ch="妃愛",file="hiy_z1b0200"}}},linkback="block_00052",text={vo={{"vo",ch="hiy",file="fem_hiy_01269"}},ja={{"「我已经解释过，「学校的前辈来帮忙了」，所以我想邻居们应该不会介意」",{"rt2"},name={"妃愛"}}}},line=322},block_00050={{"fg",mode=3,path=":fg/shi/z1/",face="a0001",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"},{"text"},linknext="block_00051",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="a0018",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"}}},linkback="block_00049",text={vo={{"vo",ch="shi",file="fem_shi_00684"}},ja={{"「那边的房间我还在租着，工作时也会用到，生活上倒是有实际情况……但实际上住的地方是这个家」",{"rt2"},name={"詩桜"}}}},line=302},block_00049={{"text"},line=296,text={ja={{"「前辈的住址？是吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00050",linkback="block_00048"},block_00039={{"fg",mode=3,path=":fg/mir/no/",face="a0010",size="no",id=13,lv=2.2000000000000002,head="mir_noa",ex05="mir_noa0010",ch="里",file="mir_noa0310"},{"fgact",act="いいえ",lv=2.2000000000000002,ch="里",id=13},{"text"},line=239,text={vo={{"vo",ch="mir",file="fem_mir_00088"}},ja={{"「不，与学生会无关。这是另外的事情。」",{"rt2"},name={"里"}}}},linknext="block_00040",linkback="block_00038"},block_00133={{"fg",mode=1,path=":fg/shi/z1/",face="a0015",size="z1",id=20,lv=2.2999999999999998,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"},{"text"},line=695,text={vo={{"vo",ch="shi",file="fem_shi_00713"}},ja={{"「别发出这么懦弱的声音，你是我的恋人呀。」",{"rt2"},name={"詩桜"}}}},linknext="block_00134",linkback="block_00132"},block_00042={{"msgoff"},{"bg",ax=1280,ay=720,by=-360,sync=0,bx=-640,path=":bg/",file="zbg002a",x=400},{"fg",sync=0,mode=3,path=":fg/mir/z1/",face="a0052",id=9,lv=2.2999999999999998,size="z1",x=-280,head="mir_z1a",ex05="mir_z1a0010",resize=1,ch="里",file="mir_z1a0310"},{"fg",sync=0,mode=3,path=":fg/shi/z1/",face="a0001",id=10,size="z1",lv=2.2999999999999998,x=280,head="shi_z1a",ex05="shi_z1a0000",ch="詩桜",file="shi_z1a0200"},{"extrans",time=500},{"text"},line=254,text={vo={{"vo",ch="mir",file="fem_mir_00089"}},ja={{"「还有诗樱。你也陪我一起过来吧，就在今天的午休时间。」",{"rt2"},name={"里"}}}},linknext="block_00043",linkback="block_00041"},block_00033={{"text"},line=211,text={ja={{"正当我不知道接下来该说什么的时候，和泉家的早晨团聚被热带低压气围包围。我希望这股热情如火的明亮能把这个场合的所有尴尬气氛都吹散。",{"rt2"}}}},linknext="block_00034",linkback="block_00032"},block_00032={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",path=":bg/",sync=0,file="bg002a"},{"extrans",time=500},{"se",id=1,file="seドア(室内開き)"},{"fg",mx=-40,path=":fg/mir/no/",face="a0041",id=13,size="no",lv=2.2000000000000002,head="mir_noa",ex05="mir_noa0000",mode=3,ch="里",file="mir_noa0300"},{"text"},line=207,text={vo={{"vo",ch="mir",file="fem_mir_00086"}},ja={{"「嗨，早上好！对不起，我正在吃早餐，打扰了。」",{"rt2"},name={"里"}}}},linknext="block_00033",linkback="block_00031"},block_00031={{"text"},line=187,text={ja={{"我在妹妹面前被逼说了什么？ 我羞得无法直视她们两个人的脸。",{"rt2"}}}},linknext="block_00032",linkback="block_00030"},block_00024={{"fg",mode=3,path=":fg/shi/z1/",face="b0039",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"},{"text"},linknext="block_00025",delay={vl01={{"fg",mode=3,path=":fg/shi/z1/",face="b0053",id=9,size="z1",lv=2.2999999999999998,x=-280,head="shi_z1b",ex05="shi_z1b0010",ch="詩桜",file="shi_z1b0210"}}},linkback="block_00023",text={vo={{"vo",ch="shi",file="fem_shi_00679"}},ja={{"「我会遵守作为垂钓者的礼节，但对于那些出于非钓鱼目的来搭话的人，或者那些爱指点江山的人，我会尽快拉开距离。因为我不想背叛你。」",{"rt2"},name={"詩桜"}}}},line=159}}