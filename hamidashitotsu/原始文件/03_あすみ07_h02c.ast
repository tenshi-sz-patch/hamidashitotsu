astver = 2.0
ast = {block_00032={{"fg",file="asu_z1a1011",size="z1",id=13,mode=3,head="asu_z1a",ch="あすみ",lv=2.2999999999999998,x=0,ex05="asu_z1a0011",path=":fg/asu/z1/",face="a0007"},{"text"},text={ja={{"「嗯。在某种程度上，可以说已经习惯了……虽然不应该习惯，但在遇见前辈之前，一直都是过着懒散的生活并进行曲作……」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10872"}}},delay={vl02={{"fg",file="asu_z1a1011",size="z1",id=13,mode=3,head="asu_z1a",ch="あすみ",lv=2.2999999999999998,face="a0011h",x=0,ex05="asu_z1a0011",path=":fg/asu/z1/",hoho="h"}},vl01={{"fg",file="asu_z1a1011",size="z1",id=13,mode=3,head="asu_z1a",ch="あすみ",lv=2.2999999999999998,face="a0038h",x=0,ex05="asu_z1a0011",path=":fg/asu/z1/",hoho="h"}}},linknext="block_00033",linkback="block_00031",line=163},block_00047={{"se",stop=1,time=500,id=1},{"fg",file="asu_z2b1011",size="z2",id=13,mode=3,head="asu_z2b",ch="あすみ",lv=2.3999999999999999,face="b0007h",x=0,ex05="asu_z2b0011",path=":fg/asu/z2/",hoho="h"},{"text"},text={ja={{"「如果能比现在更加放肆一点，我想请求前辈。因为我想更加依赖前辈」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10879"}}},delay={vl01={{"fg",file="asu_z2b1011",size="z2",id=13,mode=3,head="asu_z2b",ch="あすみ",lv=2.3999999999999999,face="b0023h",x=0,ex05="asu_z2b0011",path=":fg/asu/z2/",hoho="h"}}},linknext="block_00048",linkback="block_00046",line=236},block_00002={{"fg",file="asu_noa1001",size="no",id=13,mode=3,head="asu_noa",ch="あすみ",lv=2.2000000000000002,x=0,ex05="asu_noa0001",path=":fg/asu/no/",face="a0008"},{"fgact",id=13,ch="あすみ",act="いいえ",lv=2.2000000000000002},{"text"},text={ja={{"「对，对不起，我没感到冷。只是打了个喷嚏而已，没关系的」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10859"}}},delay={vl01={{"fg",file="asu_noa1001",size="no",id=13,mode=3,head="asu_noa",ch="あすみ",lv=2.2000000000000002,x=0,ex05="asu_noa0001",path=":fg/asu/no/",face="a0022"}}},linknext="block_00003",linkback="block_00001",line=29},block_00049={{"text"},linknext="block_00050",linkback="block_00048",text={ja={{"「嗯。在去学校和回家的路上以及午休时间里，你可以跟我说说进展」",{"rt2"},name={"智宏"}}}},line=245},block_00015={{"text"},linknext="block_00016",linkback="block_00014",text={ja={{"「嗯？在加入学生会之前，我们只交谈了两天吧？温柔……吗？」",{"rt2"},name={"智宏"}}}},line=83},block_00029={{"text"},linknext="block_00030",linkback="block_00028",text={ja={{"「哈哈」",{"rt2"},name={"智宏"}}}},line=151},block_00051={{"text"},linknext="block_00052",linkback="block_00050",text={ja={{"「式酱的cosplay真是超级可爱啊」",{"rt2"},name={"智宏"}}}},line=253},block_00021={{"text"},linknext="block_00022",linkback="block_00020",text={ja={{"「原来如此……嗯，可能是这样吧。那时候，能对别人施以善意真是太好了」",{"rt2"},name={"智宏"}}}},line=109},block_00018={{"text"},linknext="block_00019",linkback="block_00017",text={ja={{"「啊，那个……只是对亚澄友善，或者说是一时的善意，就像给老奶奶让座的感觉……」",{"rt2"},name={"智宏"}}}},line=95},block_00017={{"bg",by=-360,file="zbg016c",speed=150000,ax=1280,y2=100,x2=640,y=100,x=-640,bx=-640,path=":bg/",ay=720},{"fg",file="asu_nob1001",size="no",id=20,mode=1,head="asu_nob",ch="あすみ",lv=2.2000000000000002,resize=1,ex05="asu_nob0001",path=":fg/asu/no/",face="b0015"},{"text"},linknext="block_00018",linkback="block_00016",text={ja={{"「在便利店里让我吃了肉包」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10864"}}},line=92},block_00024={{"text"},linknext="block_00025",linkback="block_00023",text={ja={{"「唔，我也是，在我们第一次交谈的那天就觉得亚澄很可爱……有点像是男孩遇见女孩的期待。像是命中注定的相遇」",{"rt2"},name={"智宏"}}}},line=122},block_00008={{"text"},linknext="block_00009",linkback="block_00007",text={ja={{"「是呢……」",{"rt2"},name={"智宏"}}}},line=54},block_00022={{"fg",file="asu_nob1001",size="no",id=20,mode=1,head="asu_nob",ch="あすみ",lv=2.2000000000000002,ex05="asu_nob0001",path=":fg/asu/no/",face="b0023"},{"text"},linknext="block_00023",linkback="block_00021",text={ja={{"「是的。那份善意拯救了我。对男性，我第一次产生了这个人很棒的感觉」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10867"}}},line=114},block_00010={{"fg",file="asu_z1b1011",size="z1",id=13,mode=3,head="asu_z1b",ch="あすみ",lv=2.2999999999999998,x=0,ex05="asu_z1b0011",path=":fg/asu/z1/",face="b0023"},{"text"},linknext="block_00011",linkback="block_00009",text={ja={{"「前辈总是很珍惜我呢」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10861"}}},line=61},block_00003={{"text"},linknext="block_00004",linkback="block_00002",text={ja={{"虽然说没关系，但亚澄是以喉咙为生的工作，就算是一次喷嚏也会让人担心。",{"rt2"}}}},line=31},block_00037={{"fg",file="asu_z1b1001",size="z1",id=13,mode=3,head="asu_z1b",ch="あすみ",lv=2.2999999999999998,x=0,ex05="asu_z1b0001",path=":fg/asu/z1/",face="b0020"},{"text"},linknext="block_00038",linkback="block_00036",text={ja={{"「而且当集中力消失时，稍微望向窗外，世界变得明亮，就像是被阳光晒得发干一样……」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10875"}}},line=184},block_00027={{"fg",file="asu_z1b1011",size="z1",id=13,mode=3,head="asu_z1b",ch="あすみ",lv=2.2999999999999998,x=0,ex05="asu_z1b0011",path=":fg/asu/z1/",face="b0032"},{"fgact",id=13,ch="あすみ",act="おじぎ",lv=2.2999999999999998},{"text"},linknext="block_00028",linkback="block_00026",text={ja={{"「所以，即使知道你会感到寒冷，我还是会跟着你过来，因为我想要被前辈对待得温柔」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10870"}}},line=142},block_00040={{"fg",file="asu_z1b1001",size="z1",id=13,mode=3,head="asu_z1b",ch="あすみ",lv=2.2999999999999998,x=0,ex05="asu_z1b0001",path=":fg/asu/z1/",face="b0007"},{"fgact",id=13,ch="あすみ",act="いいえ",lv=2.2999999999999998},{"text"},linknext="block_00041",linkback="block_00039",text={ja={{"「不、不，不能让前辈做那种事情！」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10876"}}},line=196},block_00046={{"fg",file="asu_z2b1001",size="z2",id=13,mode=3,head="asu_z2b",ch="あすみ",lv=2.3999999999999999,face="b0018h",x=0,ex05="asu_z2b0001",path=":fg/asu/z2/",hoho="h"},{"text"},linknext="block_00047",linkback="block_00045",text={ja={{"「如果我……能够依赖前辈的话……」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10878"}}},line=228},block_00053={{"text"},linknext="block_00054",linkback="block_00052",text={ja={{"夸奖着亚澄的肌肤微微泛红。如果身体变暖了就好了。",{"rt2"}}}},line=261},block_00054={{"bgm",stop=1,time=3000},{"se",stop=1,id=-1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",path=":bg/",time=3000,file="black"},{"cacheclear"},{"ex",time=1000,func="wait"},{"bg",time=500,file="catch03",path=":cg/",id=9,lv=9},{"bgm",loop=0,file="bgm93"},{"ex",time=6000,func="wait"},{"bgm",stop=1,time=3000},{"se",stop=1,id=-1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",path=":bg/",time=3000,file="black"},{"cacheclear"},{"ex",time=1000,func="wait"},{"excall",file="03_あすみ08"},{"text"},linkback="block_00053",line=269},block_00016={{"text"},linknext="block_00017",linkback="block_00015",text={ja={{"反而感觉有些冷淡。对宗教和无店铺销售持警惕态度。",{"rt2"}}}},line=85},label={top={block="block_00000",label=1}},block_00052={{"fg",file="asu_z2a1001",size="z2",id=13,mode=3,head="asu_z2a",ch="あすみ",lv=2.3999999999999999,face="a0046h",x=0,ex05="asu_z2a0001",path=":fg/asu/z2/",hoho="h"},{"fgact",id=13,ch="あすみ",act="びっくり",lv=2.3999999999999999},{"text"},linknext="block_00053",linkback="block_00051",text={ja={{"「前辈……」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10881"}}},line=259},block_00035={{"fg",file="asu_z1a1011",size="z1",id=13,mode=3,head="asu_z1a",ch="あすみ",lv=2.2999999999999998,x=0,ex05="asu_z1a0011",path=":fg/asu/z1/",face="a0053"},{"text"},text={ja={{"「现在能够继续上学，不可能白天去创作曲子是理所当然的……之前我就是在半夜进行工作的……」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10873"}}},delay={vl01={{"fg",file="asu_z1a1001",size="z1",id=13,mode=3,head="asu_z1a",ch="あすみ",lv=2.2999999999999998,x=0,ex05="asu_z1a0001",path=":fg/asu/z1/",face="a0002"},{"fgact",id=13,ch="あすみ",act="おじぎ",lv=2.2999999999999998}}},linknext="block_00036",linkback="block_00034",line=174},block_00012={{"fg",file="asu_z2b1001",size="z2",lv=2.3999999999999999,id=13,mode=3,head="asu_z2b",ch="あすみ",resize=1,my=40,x=0,ex05="asu_z2b0001",path=":fg/asu/z2/",face="b0054"},{"text"},linknext="block_00013",linkback="block_00011",text={ja={{"「能和前辈呼吸同样的空气，我很幸福」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10862"}}},line=70},block_00050={{"fg",file="asu_z2a1001",size="z2",id=13,mode=3,head="asu_z2a",ch="あすみ",lv=2.3999999999999999,face="a0032h",x=0,ex05="asu_z2a0001",path=":fg/asu/z2/",hoho="h"},{"fgact",id=13,ch="あすみ",act="おじぎ",lv=2.3999999999999999},{"text"},linknext="block_00051",linkback="block_00049",text={ja={{"我轻轻地抚摸着亚澄的头，她不同寻常地靠近我，甚至蹭了蹭我的脸颊。因为太可爱了，最后我还是抱住了她",{"rt2"}}}},line=250},block_00048={{"fg",file="asu_z2b1011",size="z2",id=13,mode=3,head="asu_z2b",ch="あすみ",lv=2.3999999999999999,face="b0038h",x=0,ex05="asu_z2b0011",path=":fg/asu/z2/",hoho="h"},{"text"},text={ja={{"「但是，因为还没有勇气展示出那个邋遢的自己，所以从今天开始又要再努力两周……直到完成曲子为止，我会一个人加油的」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10880"}}},delay={vl01={{"fg",file="asu_z2a1011",size="z2",id=13,mode=3,head="asu_z2a",ch="あすみ",lv=2.3999999999999999,face="a0003h",x=0,ex05="asu_z2a0011",path=":fg/asu/z2/",hoho="h"}}},linknext="block_00049",linkback="block_00047",line=242},block_00013={{"text"},linknext="block_00014",linkback="block_00012",text={ja={{"刚觉得我们靠近了一步，亚澄和我的呼吸交织在一起，变得复杂起来。冬天的呼吸能看见形状，真是麻烦。",{"rt2"}}}},line=72},block_00045={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,sync=0,bx=-640,file="zbg016c",zoom=120,ay=720,path=":bg/",ax=1280},{"fg",sync=0,file="asu_z2b1001",size="z2",lv=2.3999999999999999,id=13,mode=3,head="asu_z2b",ch="あすみ",ex05="asu_z2b0001",face="b0028h",x=0,resize=1,path=":fg/asu/z2/",hoho="h"},{"extrans",time=500},{"se",id=1,file="se抱きつく"},{"text"},linknext="block_00046",linkback="block_00044",text={ja={{"她软弱地依偎在我身上。她握着我的外套，手指都变得苍白，看到她用这样僵硬的手在创作音乐，我真想尽快让她回去休息。",{"rt2"}}}},line=223},block_00044={{"fg",file="asu_z1b1001",size="z1",id=13,mode=3,head="asu_z1b",ch="あすみ",lv=2.2999999999999998,face="b0008h",x=0,ex05="asu_z1b0001",path=":fg/asu/z1/",hoho="h"},{"fgact",id=13,ch="あすみ",act="いいえ",lv=2.2999999999999998},{"text"},linknext="block_00045",linkback="block_00043",text={ja={{"「…………！！！」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10877"}}},line=211},block_00043={{"text"},linknext="block_00044",linkback="block_00042",text={ja={{"「等到我们一起生活的时候再考虑吧。那还是很久以后的事情」",{"rt2"},name={"智宏"}}}},line=204},block_00041={{"text"},linknext="block_00042",linkback="block_00040",text={ja={{"「但是我喜欢对亚澄好」",{"rt2"},name={"智宏"}}}},line=199},block_00009={{"text"},linknext="block_00010",linkback="block_00008",text={ja={{"我意识到自己比以前更过分地过保护了。但是，要说不把这么小、这么纤细的女孩当宝贝，那就太勉强了，所以我毫不犹豫地保护她。",{"rt2"}}}},line=56},block_00004={{"text"},linknext="block_00005",linkback="block_00003",text={ja={{"本来打算在家门口分别的，但亚澄说要「送行」，所以一起走到了这里。在寒冷的冬天空气中走了这么远。",{"rt2"}}}},line=33},block_00038={{"text"},linknext="block_00039",linkback="block_00037",text={ja={{"「只要亚澄不勉强自己就好。别让自己倒下来就行。亚澄已经是专业的了，有自己的曲作方式，我们这些外行人不该多嘴」",{"rt2"},name={"智宏"}}}},line=187},block_00039={{"text"},linknext="block_00040",linkback="block_00038",text={ja={{"「但如果有对曲作有帮助的地方，请告诉我。我也可以做饭，去买东西。我在另一个房间里闲逛，你可以不用在意我」",{"rt2"},name={"智宏"}}}},line=190},block_00005={{"text"},linknext="block_00006",linkback="block_00004",text={ja={{"虽然中途说了「到这里就可以了」，但亚澄却说要「买晚饭」，所以不能让她回去。公交车站就在便利店前面。",{"rt2"}}}},line=35},block_00033={{"text"},linknext="block_00034",linkback="block_00032",text={ja={{"说到那里，亚澄红透了脸颊，更加地羞得低下了头。",{"rt2"}}}},line=165},block_00036={{"fg",file="asu_z1a1001",size="z1",id=13,mode=3,head="asu_z1a",ch="あすみ",lv=2.2999999999999998,x=0,ex05="asu_z1a0001",path=":fg/asu/z1/",face="a0032"},{"text"},linknext="block_00037",linkback="block_00035",text={ja={{"「全世界的声音停下来后，我才能够专注于音乐创作……像是独自坐在没有声音的房间椅子上的感觉……」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10874"}}},line=179},block_00028={{"fg",file="asu_z1b1011",size="z1",id=13,mode=3,head="asu_z1b",ch="あすみ",lv=2.2999999999999998,x=0,ex05="asu_z1b0011",path=":fg/asu/z1/",face="b0019"},{"text"},text={ja={{"「明明知道被前辈担心了会跟着过来的我是个坏恋人。对不起，但是被珍惜着让我感到非常开心」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10871"}}},delay={vl01={{"fg",file="asu_z1b1011",size="z1",id=13,mode=3,head="asu_z1b",ch="あすみ",lv=2.2999999999999998,x=0,ex05="asu_z1b0011",path=":fg/asu/z1/",face="b0055"}}},linknext="block_00029",linkback="block_00027",line=148},block_00025={{"text"},linknext="block_00026",linkback="block_00024",text={ja={{"不行啊。我们两个人在这样浪漫的氛围中，居然还会对「命中注定」这个词感到害羞。我想成为能够轻松说出这种装腔作势的话的男人。",{"rt2"}}}},line=126},block_00007={{"fg",file="asu_z1b1011",size="z1",id=13,mode=3,head="asu_z1b",ch="あすみ",lv=2.2999999999999998,x=0,ex05="asu_z1b0011",path=":fg/asu/z1/",face="b0035"},{"text"},linknext="block_00008",linkback="block_00006",text={ja={{"「现在放进口袋里的话，暖手宝在到家之前就会变热了呢」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10860"}}},line=51},block_00006={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,sync=0,file="zbg016c",bx=-640,ay=720,path=":bg/",ax=1280},{"fg",sync=0,file="asu_z1a1001",size="z1",id=13,mode=3,head="asu_z1a",ch="あすみ",lv=2.2999999999999998,resize=1,x=0,ex05="asu_z1a0001",path=":fg/asu/z1/",face="a0022"},{"extrans",time=500},{"text"},linknext="block_00007",linkback="block_00005",text={ja={{"「啊，包里有暖手宝！亚澄，给你……！」",{"rt2"},name={"智宏"}}}},line=45},block_00014={{"fg",file="asu_z2b1001",size="z2",id=13,mode=3,head="asu_z2b",ch="あすみ",lv=2.3999999999999999,face="b0004h",x=0,ex05="asu_z2b0001",path=":fg/asu/z2/",hoho="h"},{"text"},text={ja={{"「在加入学生会之前，前辈就对我很温柔了。所以……」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10863"}}},delay={vl01={{"fg",file="asu_z2b1001",size="z2",id=13,mode=3,head="asu_z2b",ch="あすみ",lv=2.3999999999999999,face="b0038h",x=0,ex05="asu_z2b0001",path=":fg/asu/z2/",hoho="h"}}},linknext="block_00015",linkback="block_00013",line=80},block_00034={{"text"},linknext="block_00035",linkback="block_00033",text={ja={{"听你说出那种让我害羞的话倒是没问题，但坦白说自己过着懒散的生活却觉得很尴尬呢。不要紧的哦。",{"rt2"}}}},line=167},block_00042={{"text"},linknext="block_00043",linkback="block_00041",text={ja={{"刚才我被亚澄说了个过去的故事，这次却让亚澄说不出话来了。她困惑地低下了头。",{"rt2"}}}},line=201},block_00011={{"text"},linknext="block_00012",linkback="block_00010",text={ja={{"「亚澄离开学生会后，被当作天使一样对待着。让她呼吸这个下界肮脏的空气，我感到很担心。」",{"rt2"},name={"智宏"}}}},line=64},block_00000={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,path=":bg/",file="bg016c"},{"fg",sync=0,file="asu_noa1001",size="no",id=13,mode=3,head="asu_noa",ch="あすみ",lv=2.2000000000000002,x=0,ex05="asu_noa0001",path=":fg/asu/no/",face="a0011"},{"extrans",rule="rule_002r",vague=40},{"fgact",id=13,ch="あすみ",act="びっくり",lv=2.2000000000000002},{"bgm",file="bgm08"},{"text"},linknext="block_00001",text={ja={{"「嚏！」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10858"}}},line=19},block_00031={{"text"},linknext="block_00032",linkback="block_00030",text={ja={{"「作曲还好吗？生活变成了夜猫子的作息，不会累吗？」",{"rt2"},name={"智宏"}}}},line=156},block_00030={{"text"},linknext="block_00031",linkback="block_00029",text={ja={{"从今天开始直到以后，「因为冷就回去吧」这句话已经不灵了。只能尽全力变得过度保护了。",{"rt2"}}}},line=153},block_00026={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",by=-360,sync=0,file="zbg016c",bx=-640,ay=720,path=":bg/",ax=1280},{"fg",sync=0,file="asu_z1b1001",size="z1",id=13,mode=3,head="asu_z1b",ch="あすみ",lv=2.2999999999999998,resize=1,x=0,ex05="asu_z1b0001",path=":fg/asu/z1/",face="b0022"},{"extrans"},{"text"},linknext="block_00027",linkback="block_00025",text={ja={{"「知道了那个人觉得很棒，甚至听了我的歌，我终于感受到了「善良的人」所带来的命运」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10869"}}},line=136},block_00023={{"fg",file="asu_nob1001",size="no",id=20,mode=1,head="asu_nob",ch="あすみ",lv=2.2000000000000002,ex05="asu_nob0001",path=":fg/asu/no/",face="b0054"},{"text"},linknext="block_00024",linkback="block_00022",text={ja={{"「与那个棒的人交往的现在的关系，让我感受到了命中注定的东西」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10868"}}},line=119},block_00019={{"fg",file="asu_noa1001",size="no",id=20,mode=1,head="asu_noa",ch="あすみ",lv=2.2000000000000002,ex05="asu_noa0001",path=":fg/asu/no/",face="a0022"},{"text"},linknext="block_00020",linkback="block_00018",text={ja={{"「所以我才觉得安心。对不是锦亚澄也不是锦的我如此友善，不是真心实意的好意吗？」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10865"}}},line=100},block_00020={{"fg",file="asu_noa1001",size="no",id=20,mode=1,head="asu_noa",ch="あすみ",lv=2.2000000000000002,ex05="asu_noa0001",path=":fg/asu/no/",face="a0003"},{"text"},text={ja={{"「前辈离开便利店时，没有追上我。他也不知道我是后辈。所以，我很开心」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_10866"}}},delay={vl01={{"fg",file="asu_noa1001",size="no",id=20,mode=1,head="asu_noa",ch="あすみ",lv=2.2000000000000002,ex05="asu_noa0001",path=":fg/asu/no/",face="a0054"}}},linknext="block_00021",linkback="block_00019",line=106},block_00001={{"text"},linknext="block_00002",linkback="block_00000",text={ja={{"「啊啊！你明明这么冷还跑出来……你，你，你没事吧？我给你穿上我的外套回家吧？我送你到家，然后……」",{"rt2"},name={"智宏"}}}},line=22}}