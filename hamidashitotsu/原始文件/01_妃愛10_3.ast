astver = 2.0
ast = {block_00084={{"fg",face="a0032",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"text"},delay={vl01={{"fg",face="a0055",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"}}},line=524,text={vo={{"vo",file="fem_asu_00096",ch="asu"}},ja={{"「我也是，自从加入学生会后，每天都很期待上学……我真的很喜欢现在的这个学生会。」",{"rt2"},name={"あすみ"}}}},linkback="block_00083",linknext="block_00085"},block_00009={{"fg",face="a0054",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"fgact",ch="あすみ",lv=2.2000000000000002,id=10,act="うんうん"},{"text"},delay={vl02={{"fg",face="a0055",mode=3,id=10,file="asu_noa0210",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0010"}},vl01={{"fg",face="a0053",mode=3,id=10,file="asu_noa0210",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0010"}}},line=72,text={vo={{"vo",file="fem_asu_00080",ch="asu"}},ja={{"「啊，我懂那种感觉……我也是夏假没什么特别的计划，能和大家一起参加学生会活动真的很期待」",{"rt2"},name={"あすみ"}}}},linkback="block_00008",linknext="block_00010"},block_00038={{"text"},line=235,text={ja={{"我不禁想，为什么她会对我产生憧憬……但就像她说的，可能只是因为我恰好是出现在她周围的那个「年长的男性」。",{"rt2"}}}},linkback="block_00037",linknext="block_00039"},block_00037={{"text"},line=233,text={ja={{"为什么！？那个，因为……啊！？神田小姐那么开朗又明亮，绝对是男生喜欢的类型……啊啊啊！？",{"rt2"}}}},linkback="block_00036",linknext="block_00038"},block_00063={{"fg",face="a0046",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fg",face="a0040",mode=3,id=10,file="asu_noa0210",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0010"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="びっくり"},{"text"},delay={vl02={{"fg",face="a0047",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}},vl01={{"fg",face="a0042",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=397,text={vo={{"vo",file="fem_kna_00076",ch="kna"}},ja={{"「我可不是故意让他看见的啊！你要相信我！那只是个意外！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00062",linknext="block_00064"},block_00008={{"fg",face="a0054",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl01={{"fg",face="a0041",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=64,text={vo={{"vo",file="fem_kna_00061",ch="kna"}},ja={{"「我现在嘛，参加学生会活动本身就很开心呢！平时没有其他安排，假期能这样做真的很开心！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00007",linknext="block_00009"},block_00013={{"text"},line=84,text={ja={{"我并没有特别需要隐藏的理由，所以我打算在妃爱回来之前，找那些光彩照人的后辈们聊聊天。",{"rt2"}}}},linkback="block_00012",linknext="block_00014"},block_00024={{"fg",face="a0014",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="いいえ"},{"text"},delay={vl01={{"fg",face="a0018",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=144,text={vo={{"vo",file="fem_kna_00064",ch="kna"}},ja={{"「我觉得笑着把这事过去，也不会让和泉会长感到困扰！毕竟被看到也不是什么大不了的事」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00023",linknext="block_00025"},block_00027={{"fg",face="a0047",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="いいえ"},{"text"},delay={vl01={{"fg",face="a0012",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=165,text={vo={{"vo",file="fem_kna_00065",ch="kna"}},ja={{"「没、没有！如果做那种事情，反而会让他反感！而且当时和泉会长的妹妹妃爱也在场，我怎么可能做出那么大胆的举动！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00026",linknext="block_00028"},block_00022={{"fg",face="a0045",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fg",face="a0008",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="びっくり"},{"text"},delay={vl02={{"fg",face="a0046",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}},vl01={{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="いいえ"}}},line=131,text={vo={{"vo",file="fem_kna_00063",ch="kna"}},ja={{"「没有没有！有学长在场，我怎么可能做那种事情！真的只是不小心啦！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00021",linknext="block_00023"},block_00034={{"fg",face="a0038",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl01={{"fg",face="a0052",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=212,text={vo={{"vo",file="fem_kna_00070",ch="kna"}},ja={{"「但是，嗯……怎么说呢。包括对和泉会长的憧憬和心跳感觉在内的「学生会很有趣」这种感觉吧。」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00033",linknext="block_00035"},block_00081={{"fg",face="a0012",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"text"},line=505,text={vo={{"vo",file="fem_ina_00087",ch="ina"}},ja={{"「柑凪，你看，亚澄都困扰了。」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00080",linknext="block_00082"},block_00080={{"fg",face="a0041",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fg",face="a0002",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"text"},line=500,text={vo={{"vo",file="fem_kna_00084",ch="kna"}},ja={{"「我不会等你做好准备，但如果我们同时准备好，因为你是先遇到会长的，所以你有优先权！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00079",linknext="block_00081"},block_00048={{"fg",face="b0040",mode=3,id=10,file="asu_nob0200",head="asu_nob",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"},{"text"},line=292,text={ja={{"女孩子们真的很直接，即使是我这种人都会避免触及的问题，她们也会毫不犹豫地提出来。虽然我知道锦对我的感情并不是那种类型。",{"rt2"}}}},linkback="block_00047",linknext="block_00049"},block_00042={{"fg",face="a0045",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"fg",face="a0052",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="飯田",lv=2.2000000000000002,id=9,act="びっくり"},{"text"},line=262,text={vo={{"vo",file="fem_ina_00078",ch="ina"}},ja={{"「你能理解吗！？」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00041",linknext="block_00043"},block_00021={{"fg",face="a0020",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"text"},line=122,text={vo={{"vo",file="fem_ina_00075",ch="ina"}},ja={{"「就、就是……和泉会长」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00020",linknext="block_00022"},block_00055={{"fg",face="b0052",mode=3,id=10,file="asu_nob0200",head="asu_nob",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"},{"fg",face="a0010",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"fgact",ch="あすみ",lv=2.2000000000000002,id=10,act="うなづく"},{"text"},delay={vl01={{"fg",face="b0055",mode=3,id=10,file="asu_nob0200",head="asu_nob",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"}}},line=346,text={vo={{"vo",file="fem_asu_00091",ch="asu"}},ja={{"「对！真的，就是这样……神田你说得对。所以，我能理解那种感觉。」",{"rt2"},name={"あすみ"}}}},linkback="block_00054",linknext="block_00056"},block_00018={{"text"},line=106,text={ja={{"如果我发出声音，她们可能会过来。我比妃爱更小心翼翼地隐藏起来，就像成为了学校墙壁的一部分，消除了自己的气息。",{"rt2"}}}},linkback="block_00017",linknext="block_00019"},block_00053={{"fg",face="a0052",mode=3,id=10,file="asu_noa0210",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0010"},{"text"},delay={vl01={{"fg",face="a0013",mode=3,id=10,file="asu_noa0210",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0010"}},vl03={{"fg",face="a0009",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"fgact",ch="あすみ",lv=2.2000000000000002,id=10,act="びっくり"}},vl02={{"fg",face="a0048",mode=3,id=10,file="asu_noa0210",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0010"}}},line=329,text={vo={{"vo",file="fem_asu_00090",ch="asu"}},ja={{"「我原本只活在自己的小世界里，但是遇到学长后，我的世界就打开了……他就像是给我勇气的象征，带我走出来……啊，等等？我刚刚说了什么伤人的话吗？是我吗？」",{"rt2"},name={"あすみ"}}}},linkback="block_00052",linknext="block_00054"},block_00030={{"fg",face="a0004",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fg",face="a0002",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"text"},delay={vl02={{"fg",face="a0055",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}},vl01={{"fg",face="a0022",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=187,text={vo={{"vo",file="fem_kna_00067",ch="kna"}},ja={{"「我一直想要有个能聊得来的年纪比我大的男性前辈……现在，我总算遇到这样一个人了。」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00029",linknext="block_00031"},block_00046={{"fg",face="a0010",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},line=280,text={vo={{"vo",file="fem_kna_00072",ch="kna"}},ja={{"「说是仰慕，更像是亚澄你喜欢和泉会长吧」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00045",linknext="block_00047"},block_00100={{"text"},line=613,text={ja={{"我反思了一下，总是让妃爱和我一起回家可能也不太好。既然她有了这么好的同班同学，我应该也让她像那些孩子们一样，过一个愉快的放学后生活。",{"rt2"}}}},linkback="block_00099",linknext="block_00101"},block_00007={{"msgoff"},{"bg",file="bg010b",path=":bg/",sync=0},{"fg",face="a0023",mode=3,sync=0,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"fg",face="a0010",mode=3,sync=0,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"fg",face="a0052",mode=3,sync=0,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"extrans",time=500},{"text"},delay={vl02={{"fgact",ch="あすみ",lv=2.2000000000000002,id=10,act="おじぎ"}},vl01={{"fg",face="a0034",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"}}},line=58,text={vo={{"vo",file="fem_asu_00079",ch="asu"}},ja={{"「放假期间还期待学生会活动？内容是志愿者活动，可能……并不是那种你会觉得有趣的工作哦」",{"rt2"},name={"あすみ"}}}},linkback="block_00006",linknext="block_00008"},block_00061={{"fg",face="a0002",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"text"},delay={vl02={{"fg",face="a0018",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"}},vl01={{"fgact",ch="飯田",lv=2.2000000000000002,id=9,act="うなづく"}}},line=382,text={vo={{"vo",file="fem_ina_00081",ch="ina"}},ja={{"「但是，嗯，我也……平时能和我说话的男生只有和泉会长，所以在他身边的时候会有点紧张，可能吧」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00060",linknext="block_00062"},block_00078={{"fg",face="a0009",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"fgact",ch="あすみ",lv=2.2000000000000002,id=10,act="びっくり"},{"text"},line=488,text={vo={{"vo",file="fem_asu_00095",ch="asu"}},ja={{"「啊，啊啊啊……！？」",{"rt2"},name={"あすみ"}}}},linkback="block_00077",linknext="block_00079"},block_00051={{"fg",face="b0038",mode=3,id=10,file="asu_nob0210",head="asu_nob",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0010"},{"text"},delay={vl02={{"fg",face="b0031",mode=3,id=10,file="asu_nob0200",head="asu_nob",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"}},vl01={{"fg",face="b0013",mode=3,id=10,file="asu_nob0200",head="asu_nob",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"}}},line=311,text={vo={{"vo",file="fem_asu_00088",ch="asu"}},ja={{"「或者说，我自己并不太理解恋爱感情……对于学长的感情，更像是在和她交谈时感到安心，或者说和他在一起时感到舒适……」",{"rt2"},name={"あすみ"}}}},linkback="block_00050",linknext="block_00052"},block_00043={{"se",file="seコミ_金属",id=1},{"quake",mode="mw"},{"text"},line=267,text={ja={{"能理解吗！？",{"rt2"}}}},linkback="block_00042",linknext="block_00044"},block_00000={{"msgoff"},{"bg",file="zbg010b",bx=-640,by=-360,sync=0,path=":bg/",ax=1280,ay=720},{"fg",face="b0008",mode=3,sync=0,id=13,file="hiy_z1b0200",head="hiy_z1b",lv=2.2999999999999998,ex05="hiy_z1b0000",ch="妃愛",path=":fg/hiy/z1/",size="z1",resize=1},{"extrans",vague=40,rule="rule_001"},{"bgm",file="bgm08"},{"fgact",ch="妃愛",lv=2.2999999999999998,id=13,act="びっくり"},{"text"},line=20,delay={vl01={{"fg",face="b0002",mode=3,file="hiy_z1b0200",head="hiy_z1b",id=13,lv=2.2999999999999998,ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1b0000"}}},text={vo={{"vo",file="fem_hiy_00437",ch="hiy"}},ja={{"「哎？啊，对不起，等一下我接个电话。」",{"rt2"},name={"妃愛"}}}},linknext="block_00001"},block_00075={{"fg",face="a0054",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},line=471,text={vo={{"vo",file="fem_kna_00081",ch="kna"}},ja={{"「但是，我们又不能破坏学生会的人际关系，而且会长的妹妹妃爱是我的朋友，所以现阶段主动提出交往是不可能的。」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00074",linknext="block_00076"},block_00068={{"text"},line=426,text={ja={{"神田的语气让人分不清她是认真的还是在开玩笑，想起那时她的内裤颜色，我也不禁心跳加速。",{"rt2"}}}},linkback="block_00067",linknext="block_00069"},block_00099={{"fg",face="a0054",mode=3,file="hiy_z1a0200",head="hiy_z1a",id=13,lv=2.2999999999999998,ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1a0000"},{"text"},delay={vl01={{"fg",face="a0053",mode=3,file="hiy_z1a0200",head="hiy_z1a",id=13,lv=2.2999999999999998,ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1a0000"}}},line=611,text={vo={{"vo",file="fem_hiy_00442",ch="hiy"}},ja={{"「在放学后和学生会的朋友们一起吃汉堡，这就是理想的校园生活啊。我以为我永远都无法体验到这样的事情呢。」",{"rt2"},name={"妃愛"}}}},linkback="block_00098",linknext="block_00100"},block_00056={{"text"},line=348,text={ja={{"虽然神田的话简洁地表达了锦内心深处复杂的感情，但锦本人对此似乎很满意。",{"rt2"}}}},linkback="block_00055",linknext="block_00057"},label={top={block="block_00000",label=1}},block_00017={{"fg",face="a0040",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},line=104,text={ja={{"如果只有神田同学……不，即使只有神田同学在场，我也想避免谈论内裤的话题。更何况还有锦同学在场，我绝对不想因为谈论内裤的事情而露出让人觉得恶心的表情。",{"rt2"}}}},linkback="block_00016",linknext="block_00018"},block_00083={{"fg",face="a0032",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl01={{"fg",face="a0022",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="うんうん"}}},line=518,text={vo={{"vo",file="fem_kna_00086",ch="kna"}},ja={{"「真高兴能加入学生会。每天都很期待上学！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00082",linknext="block_00084"},block_00082={{"fg",face="a0035",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl01={{"fg",face="a0053",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=511,text={vo={{"vo",file="fem_kna_00085",ch="kna"}},ja={{"「哈哈哈，对不起对不起。但是这种对话，我一直都想试试看。」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00081",linknext="block_00083"},block_00025={{"fg",face="a0038",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"text"},delay={vl01={{"fg",face="a0002",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"}}},line=150,text={vo={{"vo",file="fem_asu_00082",ch="asu"}},ja={{"「首先，为什么要故意让人看到……？正常来说，这不会觉得害羞吗？」",{"rt2"},name={"あすみ"}}}},linkback="block_00024",linknext="block_00026"},block_00108={{"fg",face="a0008",mode=3,id=10,file="hiy_noa0200",head="hiy_noa",x=230,lv=2.2000000000000002,ch="妃愛",path=":fg/hiy/no/",size="no",ex05="hiy_noa0000"},{"text"},delay={vl01={{"fg",face="a0054",mode=3,id=10,file="hiy_noa0200",head="hiy_noa",x=230,lv=2.2000000000000002,ch="妃愛",path=":fg/hiy/no/",size="no",ex05="hiy_noa0000"}}},line=659,text={vo={{"vo",file="fem_hiy_00445",ch="hiy"}},ja={{"「啊，对、对了！优子已经没事了！打了点滴后，热度就明显下降了。」",{"rt2"},name={"妃愛"}}}},linkback="block_00107",linknext="block_00109"},block_00094={{"fg",face="b0002",mode=3,file="hiy_z1b0200",head="hiy_z1b",id=13,lv=2.2999999999999998,ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1b0000"},{"text"},delay={vl01={{"fg",face="b0043",mode=3,file="hiy_z1b0200",head="hiy_z1b",id=13,lv=2.2999999999999998,ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1b0000"}}},line=589,text={vo={{"vo",file="fem_hiy_00440",ch="hiy"}},ja={{"「先到先得，如果大家都做好了决定，那就看谁先遇到的。我比任何人都早遇到哥哥。」",{"rt2"},name={"妃愛"}}}},linkback="block_00093",linknext="block_00095"},block_00106={{"fg",face="a0045",mode=3,file="hiy_z1a0200",head="hiy_z1a",id=13,lv=2.2999999999999998,ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1a0000"},{"fgact",ch="妃愛",lv=2.2999999999999998,id=13,act="びっくり"},{"quake",mode="mw"},{"text"},line=643,text={ja={{"「对不起！」",{"rt2"},name={"智宏"}}}},linkback="block_00105",linknext="block_00107"},block_00105={{"fg",face="a0050",mode=3,mx=-40,id=7,lv=2.2999999999999998,file="mir_z1a0300",head="mir_z1a",x=-430,ex05="mir_z1a0000",ch="里",path=":fg/mir/z1/",size="z1",resize=1},{"text"},line=635,text={vo={{"vo",file="fem_mir_00059",ch="mir"}},ja={{"「话说回来，妃爱说电话结束后会联系我，我在停车场等了好久，怎么这么晚？」",{"rt2"},name={"里"}}}},linkback="block_00104",linknext="block_00106"},block_00002={{"text"},line=24,text={ja={{"妃爱似乎很担心悠子的身体状况，一副急切想接电话的样子，而且还要调整今天的日程。我示意她可以去楼梯那边接。",{"rt2"}}}},linkback="block_00001",linknext="block_00003"},block_00036={{"cgdel",id=-1},{"bg",file="black",time=250,path=":bg/",vague=40,rule=280},{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="zbg010b",bx=-640,by=-360,sync=0,path=":bg/",ax=1280,ay=720},{"bg",lv=5,time=400,rule="cinema",file="cinema01",alpha=192,sync=0,path=":sys/",vague=40,id=5},{"extrans",vague=40,time=250,rule=280},{"text"},line=231,text={ja={{"我正在努力抑制自己激动的呼吸，已经到了连呼吸声一点也不能发出的状况了。这是怎么回事？我到底在听什么？",{"rt2"}}}},linkback="block_00035",linknext="block_00037"},block_00104={{"text"},line=630,text={ja={{"妃爱握着我衣服边角的手，力量大得就像是在拧干湿毛巾。她紧紧地握着，像个小孩子一样。",{"rt2"}}}},linkback="block_00103",linknext="block_00105"},block_00103={{"fg",face="a0018",mode=3,file="hiy_z1a0200",head="hiy_z1a",id=13,lv=2.2999999999999998,ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1a0000"},{"fgact",ch="妃愛",lv=2.2999999999999998,id=13,act="おじぎ"},{"text"},line=628,text={vo={{"vo",file="fem_hiy_00444",ch="hiy"}},ja={{"「如果回去了就不能抱抱了～……」",{"rt2"},name={"妃愛"}}}},linkback="block_00102",linknext="block_00104"},block_00041={{"cgdel",id=-1},{"bg",file="black",time=250,path=":bg/",vague=40,rule=280},{"msgoff"},{"bg",file="bg010b",path=":bg/",sync=0},{"fg",face="a0002",mode=3,sync=0,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"fg",face="a0053",mode=3,sync=0,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"fg",face="a0053",mode=3,sync=0,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"cgdel",lv=5,time=400,sync=0,id=5,rule="cinema"},{"extrans",vague=40,time=250,rule=280},{"text"},delay={vl01={{"fg",face="a0055",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"}}},line=255,text={vo={{"vo",file="fem_asu_00085",ch="asu"}},ja={{"「我也许能理解神田同学对学生会和前辈们的感情」",{"rt2"},name={"あすみ"}}}},linkback="block_00040",linknext="block_00042"},block_00062={{"fg",face="a0047",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"fgact",ch="飯田",lv=2.2000000000000002,id=9,act="いいえ"},{"text"},line=388,text={vo={{"vo",file="fem_ina_00082",ch="ina"}},ja={{"「但是我可做不到像柑凪那样，给和泉会长看我的内裤……」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00061",linknext="block_00063"},block_00101={{"fg",face="b0029h",mode=3,id=13,file="hiy_z1b0200",head="hiy_z1b",lv=2.2999999999999998,hoho="h",ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1b0000"},{"text"},delay={vl01={{"fg",face="b0002h",mode=3,id=13,file="hiy_z1b0200",head="hiy_z1b",lv=2.2999999999999998,hoho="h",ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1b0000"}}},line=619,text={vo={{"vo",file="fem_hiy_00443",ch="hiy"}},ja={{"「哥哥，我能稍微抱抱你吗？从我这边来。」",{"rt2"},name={"妃愛"}}}},linkback="block_00100",linknext="block_00102"},block_00058={{"fg",face="a0055",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="うんうん"},{"text"},delay={vl02={{"fg",face="a0006",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}},vl01={{"fg",face="a0035",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=361,text={vo={{"vo",file="fem_kna_00074",ch="kna"}},ja={{"「我也有同样的感觉！感觉谈恋爱有点浪费，我更想享受学生会的活动，和和泉会长聊天！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00057",linknext="block_00059"},block_00047={{"fg",face="a0040",mode=3,id=10,file="asu_noa0210",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0010"},{"fg",face="a0034",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"fgact",ch="飯田",lv=2.2000000000000002,id=9,act="うなづく"},{"text"},line=288,text={vo={{"vo",file="fem_ina_00079",ch="ina"}},ja={{"「我也这么觉得」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00046",linknext="block_00048"},block_00109={{"fg",face="a0052",mode=3,id=9,file="mir_noa0310",head="mir_noa",x=-230,lv=2.2000000000000002,ch="里",path=":fg/mir/no/",size="no",ex05="mir_noa0010"},{"text"},delay={vl01={{"fg",face="a0002",mode=3,id=9,file="mir_noa0300",head="mir_noa",x=-230,lv=2.2000000000000002,ch="里",path=":fg/mir/no/",size="no",ex05="mir_noa0000"}}},line=665,text={vo={{"vo",file="fem_mir_00060",ch="mir"}},ja={{"「哦，那就好！等等，是感冒吗？你们是声优呀，希望不是喉咙出问题。妃爱你也要小心哦？」",{"rt2"},name={"里"}}}},linkback="block_00108",linknext="block_00110"},block_00097={{"fg",face="b0053",mode=3,file="hiy_z1b0210",head="hiy_z1b",id=13,lv=2.2999999999999998,ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1b0010"},{"fgact",ch="妃愛",lv=2.2999999999999998,id=13,act="うんうん"},{"text"},delay={vl01={{"fg",face="b0032",mode=3,file="hiy_z1b0200",head="hiy_z1b",id=13,lv=2.2999999999999998,ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1b0000"}}},line=602,text={vo={{"vo",file="fem_hiy_00441",ch="hiy"}},ja={{"「因为哥哥很温柔，所以我已经没事了。只是有点寂寞，因为不能和大家一起吃汉堡。」",{"rt2"},name={"妃愛"}}}},linkback="block_00096",linknext="block_00098"},block_00096={{"text"},line=595,text={ja={{"「没事的，妃爱。」",{"rt2"},name={"智宏"}}}},linkback="block_00095",linknext="block_00097"},block_00086={{"fg",face="a0010",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl02={{"fg",face="a0054",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="うなづく"}},vl01={{"fg",face="a0052",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=539,text={vo={{"vo",file="fem_kna_00087",ch="kna"}},ja={{"「啊，怎么突然这么说？我每天都超级开心的，感觉我的校园生活终于开始了！我会继续努力参加学生会活动的！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00085",linknext="block_00087"},block_00107={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="bg010b",path=":bg/",sync=0},{"fg",face="a0050",mode=3,sync=0,id=9,lv=2.2000000000000002,file="mir_noa0300",head="mir_noa",x=-230,ex05="mir_noa0000",ch="里",path=":fg/mir/no/",size="no",resize=1},{"fg",face="a0020",mode=3,sync=0,id=10,lv=2.2000000000000002,file="hiy_noa0200",head="hiy_noa",x=230,ex05="hiy_noa0000",ch="妃愛",path=":fg/hiy/no/",size="no",resize=1},{"extrans",time=500},{"text"},line=653,text={ja={{"看到突然出现的里，即使是撒娇模式的妃爱也不禁慌了起来。真是太好了，她并没有抱住我。",{"rt2"}}}},linkback="block_00106",linknext="block_00108"},block_00079={{"fg",face="a0052",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl01={{"fg",face="a0006",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=494,text={vo={{"vo",file="fem_kna_00083",ch="kna"}},ja={{"「如果我们同时无法抑制自己的感情，我会让给你优先权的，亚澄！虽然我认为恋爱中晚来的总比早来的好，但我也认为恋爱中应该有义气和人情！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00078",linknext="block_00080"},block_00026={{"fg",face="a0046",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"fgact",ch="飯田",lv=2.2000000000000002,id=9,act="びっくり"},{"text"},delay={vl02={{"fg",face="a0014",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"}},vl01={{"fg",face="a0020",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"}}},line=158,text={vo={{"vo",file="fem_ina_00077",ch="ina"}},ja={{"「所，所以……柑凪啊，你喜欢和泉会长……想引起他的注意，之类的」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00025",linknext="block_00027"},block_00003={{"text"},line=26,text={ja={{"在楼梯旁边的话，从我现在站的位置就能看得到。如果有坏人靠近妃爱，我可以立刻冲过去，这样我们彼此都能安心。",{"rt2"}}}},linkback="block_00002",linknext="block_00004"},block_00028={{"fg",face="b0008",mode=3,id=10,file="asu_nob0200",head="asu_nob",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"},{"fgact",ch="あすみ",lv=2.2000000000000002,id=10,act="びっくり"},{"text"},line=171,text={vo={{"vo",file="fem_asu_00083",ch="asu"}},ja={{"「神田，你喜欢和泉前辈吗？」",{"rt2"},name={"あすみ"}}}},linkback="block_00027",linknext="block_00029"},block_00093={{"fg",face="a0042",mode=3,file="hiy_z1a0200",head="hiy_z1a",id=13,lv=2.2999999999999998,ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1a0000"},{"text"},delay={vl01={{"fg",face="a0043",mode=3,file="hiy_z1a0200",head="hiy_z1a",id=13,lv=2.2999999999999998,ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1a0000"}}},line=583,text={vo={{"vo",file="fem_hiy_00439",ch="hiy"}},ja={{"「我也是大家的朋友。所以我应该有平等的权利。」",{"rt2"},name={"妃愛"}}}},linkback="block_00092",linknext="block_00094"},block_00092={{"se",stop=1,id=1,time=500},{"text"},line=577,text={ja={{"然后我注意到，比那些亲近我的后辈更可爱的，已经成为我的恋人的妹妹正握着我的衣服下摆。",{"rt2"}}}},linkback="block_00091",linknext="block_00093"},block_00054={{"fg",face="a0052",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="びっくり"},{"text"},delay={vl02={{"fg",face="a0041",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="うんうん"}},vl01={{"fg",face="a0054",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=338,text={vo={{"vo",file="fem_kna_00073",ch="kna"}},ja={{"「完全没有！你是在说遇到和泉会长并加入学生会后，每天的校园生活都变得有趣了对吧？我们也是一样一样的！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00053",linknext="block_00055"},block_00091={{"se",file="se抱きつく",id=1},{"cgdel",id=-1},{"bg",file="black",time=250,path=":bg/",vague=40,rule=280},{"msgoff"},{"bg",file="zbg010b",bx=-640,by=-360,sync=0,path=":bg/",ax=1280,ay=720},{"fg",face="b0017",mode=3,sync=0,file="hiy_z1b0200",head="hiy_z1b",id=13,lv=2.2999999999999998,ch="妃愛",path=":fg/hiy/z1/",size="z1",ex05="hiy_z1b0000"},{"extrans",vague=40,time=250,rule=280},{"text"},line=573,text={vo={{"vo",file="fem_hiy_00438",ch="hiy"}},ja={{"「我在这儿呢。」",{"rt2"},name={"妃愛"}}}},linkback="block_00090",linknext="block_00092"},block_00090={{"text"},line=558,text={ja={{"三个人都好可爱啊……这就是对自己亲近的后辈的感觉，真是太可爱了。今天我真想变成一棵树，静静地看着她们。",{"rt2"}}}},linkback="block_00089",linknext="block_00091"},block_00102={{"text"},line=622,text={ja={{"「等、等等，这里毕竟是学校内部，而且出入口的视线非常开阔……这样会被误认为是恋人关系的啊。」",{"rt2"},name={"智宏"}}}},linkback="block_00101",linknext="block_00103"},block_00088={{"fg",mode=-1,mx=40,ch="神田",id=13,lv=2.2000000000000002},{"fg",mode=-1,mx=40,ch="あすみ",id=10,lv=2.2000000000000002},{"fg",mode=-1,mx=40,ch="飯田",id=9,lv=2.2000000000000002},{"text"},line=554,text={ja={{"三个人似乎要在千玉逛逛。她们一边用可爱的声音聊天，一边消失在通向车站的方向。",{"rt2"}}}},linkback="block_00087",linknext="block_00089"},block_00004={{"fg",mode=-1,mx=40,ch="妃愛",id=13,lv=2.2999999999999998},{"text"},line=32,text={ja={{"我告诉里姐我会通过LIME告诉她，妃爱说了谢谢之后就小跑着走了。可能是因为朋友的身体状况，她异常地一本正经。",{"rt2"}}}},linkback="block_00003",linknext="block_00005"},block_00087={{"fg",face="a0034",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"text"},delay={vl02={{"fg",face="a0012",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"}},vl01={{"fg",face="a0002",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"}}},line=546,text={vo={{"vo",file="fem_ina_00088",ch="ina"}},ja={{"「话说如果要聊天的话，我们去店里坐着聊吧！我们在电梯口聊到什么时候呀？马上就要天黑了呢。」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00086",linknext="block_00088"},block_00049={{"fg",face="b0034",mode=3,id=10,file="asu_nob0200",head="asu_nob",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"},{"fgact",ch="あすみ",lv=2.2000000000000002,id=10,act="うなづく"},{"text"},delay={vl01={{"fg",face="a0020",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"}}},line=299,text={vo={{"vo",file="fem_asu_00087",ch="asu"}},ja={{"「当然我非常喜欢，但是我想我们两个人的想法，想要交往的那种恋爱感情，又是另一回事……」",{"rt2"},name={"あすみ"}}}},linkback="block_00048",linknext="block_00050"},block_00095={{"text"},line=592,text={ja={{"「嗯。从妃爱出生到现在，我一直是妃爱的哥哥。从现在开始，永远、永永远远都是只属于妃爱的哥哥。」",{"rt2"},name={"智宏"}}}},linkback="block_00094",linknext="block_00096"},block_00076={{"fg",face="a0002",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"fg",face="a0002",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"text"},line=476,text={ja={{"嗯……即使这里的某个人向我告白，我也有我心爱的妹妹在，所以我必须拒绝。虽然这只是一个奢侈的假设……！",{"rt2"}}}},linkback="block_00075",linknext="block_00077"},block_00110={{"bgm",stop=1},{"cgdel",id=-1},{"bg",file="black",wait=500,time=1000,path=":bg/",vague=40,rule="rule_002"},{"excall",file="01_妃愛10_4"},{"text"},linkback="block_00109",line=670},block_00098={{"text"},line=605,text={ja={{"「啊，是我让你答应和我一起回家，做了不好的事情。下次你可以邀请那三个人。」",{"rt2"},name={"智宏"}}}},linkback="block_00097",linknext="block_00099"},block_00016={{"fg",face="a0010",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"fg",face="a0045",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",act="いいえ",lv=2.2000000000000002,time=80,ch="神田",id=13},{"text"},line=100,text={ja={{"但是当她们的话题转向神田同学的内裤曝光事件时，我把已经到了喉咙口的「嘿」吞了回去。真是危险。",{"rt2"}}}},linkback="block_00015",linknext="block_00017"},block_00014={{"text"},line=86,text={ja={{"然而，我从没想过会有一天，我可以如此自然地向女孩子搭话。这是多么的奢侈……",{"rt2"}}}},linkback="block_00013",linknext="block_00015"},block_00010={{"fg",face="a0054",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl01={{"fg",face="a0052",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=78,text={vo={{"vo",file="fem_kna_00062",ch="kna"}},ja={{"「亚澄酱，你也好喜欢学生会活动啊！我也觉得课后的时间最开心！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00009",linknext="block_00011"},block_00077={{"fg",face="a0006",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl01={{"fg",face="a0054",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=482,text={vo={{"vo",file="fem_kna_00082",ch="kna"}},ja={{"「但是如果这个学生会解散了，到那时如果感情无法抑制，那就看谁动作快了！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00076",linknext="block_00078"},block_00085={{"fg",face="a0022",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"text"},delay={vl02={{"fg",face="a0052",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"}},vl01={{"fg",face="a0055",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"}}},line=531,text={vo={{"vo",file="fem_asu_00097",ch="asu"}},ja={{"「所以，听到神田你说喜欢学生会，我很开心。你说在这里感觉很舒服，我也很开心。以后也请多多关照了。」",{"rt2"},name={"あすみ"}}}},linkback="block_00084",linknext="block_00086"},block_00011={{"text"},line=80,text={ja={{"锦、神田、饭田他们……每个人都相处得很融洽。大家一起下课走回家的情景，真的十分像光鲜亮丽的女校生。希望我的妹妹也能加入他们。",{"rt2"}}}},linkback="block_00010",linknext="block_00012"},block_00001={{"text"},line=22,text={ja={{"里姐说要开车送妃爱，我在和大家告别后向电梯口走去时，接到了那位经理的联络。",{"rt2"}}}},linkback="block_00000",linknext="block_00002"},block_00031={{"fg",face="a0010",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"text"},line=192,text={vo={{"vo",file="fem_asu_00084",ch="asu"}},ja={{"「那不就是对和泉前辈心动了吗？」",{"rt2"},name={"あすみ"}}}},linkback="block_00030",linknext="block_00032"},block_00033={{"fg",face="a0054",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl01={{"fg",face="a0009",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=206,text={vo={{"vo",file="fem_kna_00069",ch="kna"}},ja={{"「但是现在比起恋爱，我更享受学生会的活动，所以并不是想要交往什么的。如果因为告白被拒绝，然后被大家讨厌了，那我会很难过的。」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00032",linknext="block_00034"},block_00074={{"fg",face="a0026",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},line=466,text={vo={{"vo",file="fem_kna_00080",ch="kna"}},ja={{"「伊伊奈你是那种如果对方一直催促的话，会不知不觉地点头答应的类型吧！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00073",linknext="block_00075"},block_00029={{"fg",face="a0046",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="びっくり"},{"text"},delay={vl01={{"fg",face="a0019",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="いいえ"}}},line=179,text={vo={{"vo",file="fem_kna_00066",ch="kna"}},ja={{"「哎呀，让亚澄产生了误解……不对，我不是喜欢他那种意思，我只是跟伊伊奈说，在和泉会长一起的时候很开心。」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00028",linknext="block_00030"},block_00035={{"fg",face="a0054",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl02={{"fg",face="a0041",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="うんうん"}},vl01={{"fg",face="a0006",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=220,text={vo={{"vo",file="fem_kna_00071",ch="kna"}},ja={{"「课外活动……虽然不是课外活动，但和大家一起做些什么，或者和男性前辈聊天时的心跳感觉，现在这一切都非常有趣！每天都过得非常充实。」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00034",linknext="block_00036"},block_00057={{"fg",face="b0022",mode=3,id=10,file="asu_nob0200",head="asu_nob",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"},{"text"},line=353,text={vo={{"vo",file="fem_asu_00092",ch="asu"}},ja={{"「现在我更想要在学长身边过着舒适的每一天，而不仅仅是想要和他在一起。」",{"rt2"},name={"あすみ"}}}},linkback="block_00056",linknext="block_00058"},block_00067={{"fg",face="a0054",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl01={{"fg",face="a0041",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="うんうん"}}},line=424,text={vo={{"vo",file="fem_kna_00078",ch="kna"}},ja={{"「哈哈哈！那下次我是不是应该故意让他看见呢？」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00066",linknext="block_00068"},block_00072={{"fg",face="a0015",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="うんうん"},{"text"},delay={vl01={{"fg",face="a0054",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=454,text={vo={{"vo",file="fem_kna_00079",ch="kna"}},ja={{"「我肯定会答应和他交往！如果会长说他喜欢我的话！伊伊奈呢？」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00071",linknext="block_00073"},block_00015={{"fg",face="a0007",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"text"},delay={vl01={{"fg",face="a0012",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"}},vl03={{"fgact",ch="飯田",lv=2.2000000000000002,id=9,act="おじぎ"}},vl02={{"fg",face="a0046",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"}}},line=94,text={vo={{"vo",file="fem_ina_00073",ch="ina"}},ja={{"「柑凪酱，虽然参加学生会很开心，但像今天这样过于兴奋就不好了。和泉会长在场，你的、你的内裤都露出来了」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00014",linknext="block_00016"},block_00020={{"fg",face="a0002",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"text"},delay={vl01={{"fg",face="a0040",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"}}},line=117,text={vo={{"vo",file="fem_asu_00081",ch="asu"}},ja={{"「故意露出来……给谁看呢？」",{"rt2"},name={"あすみ"}}}},linkback="block_00019",linknext="block_00021"},block_00070={{"fg",face="b0009h",mode=3,id=10,lv=2.2000000000000002,file="asu_nob0200",head="asu_nob",x=230,hoho="h",ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"},{"fgact",ch="あすみ",lv=2.2000000000000002,id=10,act="びっくり"},{"text"},delay={vl02={{"fg",face="b0029h",mode=3,id=10,lv=2.2000000000000002,file="asu_nob0210",head="asu_nob",x=230,hoho="h",ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0010"}},vl01={{"fg",face="b0038h",mode=3,id=10,lv=2.2000000000000002,file="asu_nob0200",head="asu_nob",x=230,hoho="h",ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"}}},line=441,text={vo={{"vo",file="fem_asu_00094",ch="asu"}},ja={{"「那、那个……！我从没想过这样的情况，所以不太清楚，但我想我应该不会拒绝……」",{"rt2"},name={"あすみ"}}}},linkback="block_00069",linknext="block_00071"},block_00005={{"fg",face="a0052",mode=1,file="kna_noa0200",head="kna_noa",id=20,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl01={{"fg",face="a0054",mode=1,file="kna_noa0200",head="kna_noa",id=20,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=40,text={vo={{"vo",file="fem_kna_00060",ch="kna"}},ja={{"「耶——！学生会活动在暑假也有真好！超级期待呢！」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00004",linknext="block_00006"},block_00069={{"fg",face="a0012",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"fgact",ch="飯田",lv=2.2000000000000002,id=9,act="びっくり"},{"text"},delay={vl01={{"fg",face="a0010",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"}}},line=433,text={vo={{"vo",file="fem_ina_00084",ch="ina"}},ja={{"「呃……但是……从你们两个的话来看，柑凪和亚澄，如果和泉会长向你们告白，你会答应和他交往吗？」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00068",linknext="block_00070"},block_00066={{"fg",face="a0055",mode=3,id=10,file="asu_noa0200",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0000"},{"fgact",ch="あすみ",lv=2.2000000000000002,id=10,act="うなづく"},{"text"},line=417,text={vo={{"vo",file="fem_asu_00093",ch="asu"}},ja={{"「确实挺放荡的。」",{"rt2"},name={"あすみ"}}}},linkback="block_00065",linknext="block_00067"},block_00012={{"text"},line=82,text={ja={{"她们并没有注意到在附近默默站立的妃爱，锦和神田笑得格外欢乐。而妃爱似乎更想专注于手机上的事情，保持着尽可能低调的态度。",{"rt2"}}}},linkback="block_00011",linknext="block_00013"},block_00071={{"fg",face="a0012",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"fg",face="a0010",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},line=447,text={vo={{"vo",file="fem_ina_00085",ch="ina"}},ja={{"「如果不拒绝的话，那就是说你会和他交往了吧……」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00070",linknext="block_00072"},block_00064={{"fg",face="a0006",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl01={{"fg",face="a0052",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}}},line=403,text={vo={{"vo",file="fem_kna_00077",ch="kna"}},ja={{"「……不过，既然他看到了，我还是很好奇他的反应。我在想，他会不会对我的内裤感兴趣呢？这让我有点紧张。」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00063",linknext="block_00065"},block_00089={{"text"},line=556,text={ja={{"我不知不觉听进了那如梦如幻的对话……我对她们做了坏事。希望她们能原谅我，在无法逃脱的情况下出于无可抗拒的原因。",{"rt2"}}}},linkback="block_00088",linknext="block_00090"},block_00060={{"fg",face="a0045",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"fg",face="a0053",mode=3,id=10,file="asu_noa0210",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0010"},{"fgact",ch="飯田",lv=2.2000000000000002,id=9,act="びっくり"},{"text"},delay={vl02={{"fg",face="a0038",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"}},vl01={{"fg",face="a0046",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"}}},line=375,text={vo={{"vo",file="fem_ina_00080",ch="ina"}},ja={{"「啊！？嗯，呃……我，我该怎么说呢」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00059",linknext="block_00061"},block_00059={{"fg",face="a0048",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},line=366,text={vo={{"vo",file="fem_kna_00075",ch="kna"}},ja={{"「那么，伊伊奈怎么看？」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00058",linknext="block_00060"},block_00073={{"fg",face="a0046",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"text"},delay={vl01={{"fg",face="a0028",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"fgact",ch="飯田",lv=2.2000000000000002,id=9,act="いいえ"}}},line=461,text={vo={{"vo",file="fem_ina_00086",ch="ina"}},ja={{"「啊，嗯……呃，我，我不知道该怎么回答……」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00072",linknext="block_00074"},block_00044={{"text"},line=269,text={ja={{"我和饭田同学的心情似乎产生了共鸣。我在想，究竟是哪一点让锦同学能理解神田同学的心情呢。",{"rt2"}}}},linkback="block_00043",linknext="block_00045"},block_00019={{"fg",face="a0050",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"text"},line=111,text={vo={{"vo",file="fem_ina_00074",ch="ina"}},ja={{"「难道……难道你是故意露出来的吗？」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00018",linknext="block_00020"},block_00052={{"fg",face="b0023",mode=3,id=10,file="asu_nob0200",head="asu_nob",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"},{"text"},delay={vl02={{"fg",face="b0013",mode=3,id=10,file="asu_nob0200",head="asu_nob",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"}},vl01={{"fg",face="b0032",mode=3,id=10,file="asu_nob0200",head="asu_nob",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"}},vl03={{"fg",face="b0004",mode=3,id=10,file="asu_nob0200",head="asu_nob",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_nob0000"}},vl04={{"fg",face="a0055",mode=3,id=10,file="asu_noa0210",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0010"}}},line=320,text={vo={{"vo",file="fem_asu_00089",ch="asu"}},ja={{"「对我来说，学长就像一个让人感到舒适的地方，那里有我喜欢的人们，是他让我们能够相遇……可以说是把我从房间里带出来的人。」",{"rt2"},name={"あすみ"}}}},linkback="block_00051",linknext="block_00053"},block_00032={{"fg",face="a0052",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},delay={vl02={{"fg",face="a0010",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"}},vl01={{"fg",face="a0045",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"fgact",ch="神田",lv=2.2000000000000002,id=13,act="びっくり"}}},line=200,text={vo={{"vo",file="fem_kna_00068",ch="kna"}},ja={{"「确实在他身边会有些心跳加速。不会？因为我现在跟的这位性别为男的前辈，就只有和泉会长一个人嘛。」",{"rt2"},name={"神田 & 柑凪"}}}},linkback="block_00031",linknext="block_00033"},block_00050={{"fg",face="a0001",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"fg",face="a0001",mode=3,id=13,file="kna_noa0200",head="kna_noa",x=-90,lv=2.2000000000000002,ch="神田",path=":fg/kna/no/",size="no",ex05="kna_noa0000"},{"text"},line=304,text={ja={{"好像是。我记得当我劝阻锦不要退出学生会时，说过「我很喜欢你」。",{"rt2"}}}},linkback="block_00049",linknext="block_00051"},block_00039={{"text"},line=237,text={ja={{"虽然她自己也说了，那份感情并不是恋爱。但是，也许，如果我主动一点，我们之间还真的有可能发展成恋人关系呢。",{"rt2"}}}},linkback="block_00038",linknext="block_00040"},block_00045={{"fg",face="a0032",mode=3,id=10,file="asu_noa0210",head="asu_noa",x=230,lv=2.2000000000000002,ch="あすみ",path=":fg/asu/no/",size="no",ex05="asu_noa0010"},{"fg",face="a0012",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"text"},line=275,text={vo={{"vo",file="fem_asu_00086",ch="asu"}},ja={{"「我向来都对学长抱有一种——像是仰慕的感情……」",{"rt2"},name={"あすみ"}}}},linkback="block_00044",linknext="block_00046"},block_00040={{"text"},line=239,text={ja={{"无论如何，这都是我不应该听到的对话。不过既然是她，即使知道我听到了这些话，也可能会毫不在意。",{"rt2"}}}},linkback="block_00039",linknext="block_00041"},block_00023={{"fg",face="a0002",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"text"},delay={vl01={{"fg",face="a0043",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"}}},line=137,text={vo={{"vo",file="fem_ina_00076",ch="ina"}},ja={{"「但是，你并没有表现得很慌张……也没有表现出不喜欢」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00022",linknext="block_00024"},block_00065={{"fg",face="a0042",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"},{"fgact",ch="飯田",lv=2.2000000000000002,id=9,act="うんうん"},{"text"},delay={vl01={{"fg",face="a0043",mode=3,id=9,file="ina_noa0200",head="ina_noa",x=-320,lv=2.2000000000000002,ch="飯田",path=":fg/ina/no/",size="no",ex05="ina_noa0000"}}},line=410,text={vo={{"vo",file="fem_ina_00083",ch="ina"}},ja={{"「你这个坏蛋！真是放荡！」",{"rt2"},name={"飯田 & 伊々奈"}}}},linkback="block_00064",linknext="block_00066"},block_00006={{"text"},line=42,text={ja={{"在这个时候，从楼梯上方传来了充满活力的声音。可能是知道这个时间没有人，所以音量相当大。",{"rt2"}}}},linkback="block_00005",linknext="block_00007"}}