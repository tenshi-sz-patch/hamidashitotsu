astver = 2.0
ast = {block_00110={{"fg",size="z2",face="b0028",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",mode=3},{"text"},line=575,linkback="block_00109",text={vo={{"vo",file="fem_shi_00645",ch="shi"}},ja={{"「在我道歉并让你知道所有事情之前，我和你的谈话、旅行、所有和你一起度过的时光，都伴随着不能坦白的心酸」",{"rt2"},name={"詩桜"}}}},linknext="block_00111"},block_00142={{"text"},line=727,linkback="block_00141",text={ja={{"「啊，是的，是我耽误了。我应该在妃爱之前就回家。」",{"rt2"},name={"智宏"}}}},linknext="block_00143"},block_00135={{"text"},line=698,linkback="block_00134",text={ja={{"「说到这个，我也在和诗樱前辈旅行的时候，就已经被魅力增强3000倍了。」",{"rt2"},name={"智宏"}}}},linknext="block_00136"},block_00039={{"fg",size="z1",face="b0039",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00600",ch="shi"}},ja={{"「我并不想穿荷叶边的衣服，也不是想成为新闻主播，但我确实有过想做偶像的经历。我甚至参加了面试，进入了最后一轮」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="b0027",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=3}}},line=204,linkback="block_00038",linknext="block_00040"},block_00033={{"fg",size="z1",face="a0039",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00597",ch="shi"}},ja={{"「就算你说我在演戏。我喜欢小日向这件事是发自内心的。并没有在做什么表演」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="a0016",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=3}}},line=178,linkback="block_00032",linknext="block_00034"},block_00132={{"text"},line=683,linkback="block_00131",text={ja={{"「真的很抱歉。」",{"rt2"},name={"智宏"}}}},linknext="block_00133"},block_00076={{"text"},line=397,linkback="block_00075",text={ja={{"「啊，那个……里姐跟我说过。她没想到我会这么努力，还说我的努力让她非常高兴。」",{"rt2"},name={"智宏"}}}},linknext="block_00077"},block_00029={{"se",file="seコミ_どよーん",id=1},{"text"},line=153,linkback="block_00028",text={ja={{"我的抱怨被她的兴奋所掩盖，被无视了。我真的很受伤。",{"rt2"}}}},linknext="block_00030"},block_00124={{"fg",size="z2",face="b0027",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00654",ch="shi"}},ja={{"「我也没有别的男人气质啊……你周围有很多好女人，比如常磐。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0003",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",mode=3}},vl02={{"fg",size="z2",face="b0017",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",mode=3}}},line=646,linkback="block_00123",linknext="block_00125"},block_00087={{"fg",size="z1",face="a0053",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=3},{"text"},line=450,linkback="block_00086",text={vo={{"vo",file="fem_shi_00630",ch="shi"}},ja={{"「其实，我确实对那次事故有些想要做出补偿的想法。我也不知道会有什么后遗症，所以我已经做好了准备，用一生的时间去照顾你，作为恋人，也愿意付出我所有。」",{"rt2"},name={"詩桜"}}}},linknext="block_00088"},block_00090={{"text"},line=465,linkback="block_00089",text={ja={{"「确实呢……一般人不会有那么自责的想法。即使是学生会的事情也……」",{"rt2"},name={"智宏"}}}},linknext="block_00091"},block_00025={{"fg",size="z2",face="b0022",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00594",ch="shi"}},ja={{"「我非常喜欢小日向。我不想和你分开，也不想让小日向有所顾忌，所以决定在和泉家生活，就当是一种特权吧，这样更轻松」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0053",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3}}},line=131,linkback="block_00024",linknext="block_00026"},block_00004={{"text"},line=27,linkback="block_00003",text={ja={{"看样子诗樱前辈开始关注时间了，一边看着手机一边喃喃自语。看向窗外，天色已经开始暗下来。",{"rt2"}}}},linknext="block_00005"},block_00089={{"fg",size="z1",face="b0002",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00632",ch="shi"}},ja={{"「我对轻视了你，或者将你视为附属于里姐的确有一些愧疚……但我并没有想过因此而去尽一生来赔偿。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="b0023",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=3}}},line=462,linkback="block_00088",linknext="block_00090"},block_00035={{"text"},line=187,linkback="block_00034",text={ja={{"「刚才那个即将达到高潮的诗樱学姐很可爱哦」",{"rt2"},name={"智宏"}}}},linknext="block_00036"},block_00005={{"fg",size="no",face="a0038",id=20,ch="詩桜",lv=2.2000000000000002,head="shi_noa",path=":fg/shi/no/",file="shi_noa1100",ex05="shi_noa0000",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00582",ch="shi"}},ja={{"「虽然你伤愈全好让我更加高兴，但不能再合法地在你房间里过夜真是遗憾」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="no",face="a0019",id=20,ch="詩桜",lv=2.2000000000000002,head="shi_noa",path=":fg/shi/no/",file="shi_noa1100",ex05="shi_noa0000",mode=1}}},line=33,linkback="block_00004",linknext="block_00006"},block_00063={{"text"},line=325,linkback="block_00062",text={ja={{"「好的。我很抱歉。在这一点上，我确实没有任何的借口。」",{"rt2"},name={"智宏"}}}},linknext="block_00064"},block_00061={{"text"},line=316,linkback="block_00060",text={ja={{"「我对不起，质疑了诗樱前辈的爱。前辈的感情绝对是真实的。而且我也觉得，那个妹妹是七万亿分之一的奇迹，是我得到的宝贵礼物。」",{"rt2"},name={"智宏"}}}},linknext="block_00062"},block_00020={{"fg",size="z2",face="a0020",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00591",ch="shi"}},ja={{"「自从那次事故发生后，她过度地害怕失去你……让这样的小日向和你分开生活，我实在无法想象」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0028",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3}}},line=107,linkback="block_00019",linknext="block_00021"},block_00139={{"text"},line=711,linkback="block_00138",text={ja={{"同时，我希望自己以后能够免受同样的焦虑，要增加自信。我要努力成为配得上她的恋人。",{"rt2"}}}},linknext="block_00140"},block_00064={{"msgoff"},{"bg",zoom=120,bx=-640,by=-360,sync=0,file="zbg025b",ay=720,ax=1280,path=":bg/"},{"fg",size="z2",sync=0,face="a0010",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3},{"extrans",time=500},{"fgact",ch="詩桜",id=13,lv=2.3999999999999999,act="いいえ"},{"text"},text={vo={{"vo",file="fem_shi_00616",ch="shi"}},ja={{"「不，你还是有一些借口吧。你本来是可以每月从和泉老师那里领取零花钱的，全部交给妃爱来管理，对吧？」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0053",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3}}},line=337,linkback="block_00063",linknext="block_00065"},block_00136={{"fg",size="z1",face="a0052h",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",mode=3,path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",hoho="h"},{"text"},text={vo={{"vo",file="fem_shi_00660",ch="shi"}},ja={{"「那么我们就是以普通恋爱的3000倍强度相爱的了。明白这一点，你应该也不再感到不安了吧。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="a0032h",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",mode=3,path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",hoho="h"}}},line=704,linkback="block_00135",linknext="block_00137"},block_00000={{"savetitle",text="汝の敵を愛せよ"},{"bg",x=100,y=-360,ay=720,ax=1280,time=2000,by=-360,y2=360,x2=100,speed=80000,bx=-640,path=":bg/",file="zbg025b"},{"bgm",file="bgm08"},{"text"},line=16,text={ja={{"我们彼此交替洗澡，等诗樱前辈回来后，就在床上享受她的膝枕，度过了宁静的时光。",{"rt2"}}}},linknext="block_00001"},block_00001={{"text"},line=18,linkback="block_00000",text={ja={{"温柔而慢慢地抚摸我的头的诗樱前辈，仿佛充满了如女神般的母性。",{"rt2"}}}},linknext="block_00002"},block_00112={{"fg",size="z2",face="b0052h",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",mode=3,path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",hoho="h"},{"text"},line=590,linkback="block_00111",text={vo={{"vo",file="fem_shi_00647",ch="shi"}},ja={{"「用轻小说的方式来说，我像是被施了魔法，以三千倍的爱意和你相处着」",{"rt2"},name={"詩桜"}}}},linknext="block_00113"},block_00128={{"fg",size="z1",face="a0052",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=3},{"text"},line=667,linkback="block_00127",text={vo={{"vo",file="fem_shi_00656",ch="shi"}},ja={{"「你是不是因为我总是忙于工作，家里的事情都交给你处理，所以感到有些对不起我呢？」",{"rt2"},name={"詩桜"}}}},linknext="block_00129"},block_00011={{"fg",size="no",face="a0026",id=20,ch="詩桜",lv=2.2000000000000002,head="shi_noa",path=":fg/shi/no/",file="shi_noa1100",ex05="shi_noa0000",mode=1},{"text"},line=62,linkback="block_00010",text={vo={{"vo",file="fem_shi_00586",ch="shi"}},ja={{"「而且虽然有些不恰当，但你害羞的脸真的很可爱……呵呵」",{"rt2"},name={"詩桜"}}}},linknext="block_00012"},block_00154={{"text"},line=782,linkback="block_00153",text={ja={{"「因为学姐的脸很美，我就看呆了」",{"rt2"},name={"智宏"}}}},linknext="block_00155"},block_00109={{"msgoff"},{"colortone",style="reset"},{"bg",path=":bg/",sync=1,lv=8,id=7,file="white",time=1000},{"cgdel",lv=8,id=7,time=1000},{"msgoff"},{"bg",zoom=120,bx=-640,by=-360,sync=0,file="zbg025b",ay=720,ax=1280,path=":bg/"},{"fg",size="z2",sync=0,face="b0002",id=13,resize=1,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"extrans"},{"text"},text={vo={{"vo",file="fem_shi_00644",ch="shi"}},ja={{"「你对我的恋情源头的不安，我绝无怨言。因为我的这份感情，是在你看不到的地方慢慢培养出来的」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0038",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3}}},line=570,linkback="block_00108",linknext="block_00110"},block_00149={{"msgoff"},{"bg",zoom=120,bx=-640,by=-360,sync=0,file="zbg018c",ay=720,ax=1280,path=":bg/"},{"fg",size="z2",sync=0,face="a0009",id=13,resize=1,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1000",ex05="shi_z2a0000",mode=3},{"extrans"},{"fgact",ch="詩桜",id=13,lv=2.3999999999999999,act="びっくり"},{"text"},line=764,linkback="block_00148",text={vo={{"vo",file="fem_shi_00663",ch="shi"}},ja={{"「啊，糟了。如果妃爱正在节食的话，我该怎么办呢」",{"rt2"},name={"詩桜"}}}},linknext="block_00150"},block_00080={{"fg",size="z2",face="b0020",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00628",ch="shi"}},ja={{"「我对轻视你的人格，施加压力等行为的确有一种罪疚感，但我最大的罪过，是只把你看作妃爱的附属品。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0002",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3}}},line=421,linkback="block_00079",linknext="block_00081"},block_00108={{"text"},line=555,linkback="block_00107",text={ja={{"听了前辈的话后回想起那段时间，我感到很羞耻…不过，前辈，我穿着女仆服装，和诗樱前辈一起工作，我真的很高兴。",{"rt2"}}}},linknext="block_00109"},block_00151={{"fg",size="z2",face="b0006",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1010",ex05="shi_z2b0010",mode=3},{"text"},line=772,linkback="block_00150",text={vo={{"vo",file="fem_shi_00664",ch="shi"}},ja={{"「让妃爱吃的任务我希望你能让给我」",{"rt2"},name={"詩桜"}}}},linknext="block_00152"},block_00085={{"fg",size="z1",face="b0010",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=3},{"text"},line=440,linkback="block_00084",text={ja={{"她愣住了，但又马上笑了出来，眼神变得狡黠。",{"rt2"}}}},linknext="block_00086"},block_00038={{"text"},line=198,linkback="block_00037",text={ja={{"「话说回来，你说这些是不是说明，诗樱学姐也有想变得可爱的愿望呢。比如想试试戴上蝴蝶结，穿上荷叶边的衣服什么的」",{"rt2"},name={"智宏"}}}},linknext="block_00039"},block_00140={{"text"},line=713,linkback="block_00139",text={ja={{"尽管我们的相遇很糟糕，事故也很糟糕，但总的来说，我和诗樱前辈正在享受一段美好的恋情。对这份幸运表示感谢……不，应该赞美那天采取行动的自己。",{"rt2"}}}},linknext="block_00141"},block_00070={{"fg",size="z2",face="b0020",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00620",ch="shi"}},ja={{"「尽管如此，在我心中，我依然感觉对你有罪。因为我对你负担过重。我会通过帮助你养成来偿还这个罪过。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0003",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3}}},line=367,linkback="block_00069",linknext="block_00071"},block_00071={{"text"},line=370,linkback="block_00070",text={ja={{"「也不能算是诗樱前辈的罪过吧。我说过很多次，事实是我用妃爱的钱玩了抽奖。」",{"rt2"},name={"智宏"}}}},linknext="block_00072"},block_00006={{"fg",size="no",face="b0055",id=20,ch="詩桜",lv=2.2000000000000002,head="shi_nob",path=":fg/shi/no/",file="shi_nob1100",ex05="shi_nob0000",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00583",ch="shi"}},ja={{"「在照顾你的期间，我和小日向一起合作，互相帮助，提升了彼此的信任和理解。甚至还得到了你的感谢。虽然她还没对我完全亲近……」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="no",face="b0038",id=20,ch="詩桜",lv=2.2000000000000002,head="shi_nob",path=":fg/shi/no/",file="shi_nob1100",ex05="shi_nob0000",mode=1}}},line=39,linkback="block_00005",linknext="block_00007"},block_00098={{"text"},line=500,linkback="block_00097",text={ja={{"听到这个，我又不禁心跳加速。能成为学姐首次的那个人，我感到很荣幸。",{"rt2"}}}},linknext="block_00099"},block_00008={{"text"},line=48,linkback="block_00007",text={ja={{"「啊，身体不自由确实很窘迫，但是包括里姐在内的你们三位的全心全意照顾，我真的觉得很舒服」",{"rt2"},name={"智宏"}}}},linknext="block_00009"},block_00160={{"text"},line=810,linkback="block_00159",text={ja={{"即使不全力以赴，那巨大的干劲。也可靠得过头，让我不禁笑了出来。",{"rt2"}}}},linknext="block_00161"},block_00057={{"fg",size="z2",face="a0023",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00611",ch="shi"}},ja={{"「我也很喜欢锦同学，因为她是真正的人才。但是，让妃爱在我心中变得特殊的，是因为她的艺术生涯的长期坚持」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0003",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=1}}},line=291,linkback="block_00056",linknext="block_00058"},block_00058={{"fg",size="z2",face="a0027",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00612",ch="shi"}},ja={{"「妃爱多年来始终保持的清廉的态度，不是一朝一夕能够得到的。她完全囊括了我所期望的偶像形象」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0022",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=1}}},line=299,linkback="block_00057",linknext="block_00059"},block_00158={{"text"},line=801,linkback="block_00157",text={ja={{"明天是工作日。如果学生会工作拖延，只能直接回家，那我可能会很痛苦。年轻的我，只能苦笑。",{"rt2"}}}},linknext="block_00159"},block_00009={{"text"},line=51,linkback="block_00008",text={ja={{"「那个，我让你们也受了一些委屈，这点我真的很抱歉……」",{"rt2"},name={"智宏"}}}},linknext="block_00010"},block_00048={{"bg",x=640,y=-200,ay=720,ax=1280,by=-360,y2=-200,x2=-640,speed=120000,bx=-640,path=":bg/",file="zbg025b"},{"fg",size="z1",face="a0053",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=1},{"text"},line=245,linkback="block_00047",text={vo={{"vo",file="fem_shi_00604",ch="shi"}},ja={{"「所以，我喜欢那些面对演艺工作、全力以赴的女性。因为她们在我曾经放弃的世界里奋斗着」",{"rt2"},name={"詩桜"}}}},linknext="block_00049"},block_00079={{"fg",size="z2",face="b0038",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00627",ch="shi"}},ja={{"「看到和泉老师的样子，感到罪疚的我也不禁觉得有些罪恶感。毕竟那是在已经开始喜欢你之后，而不是无视你的那一段时间。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0028",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"fgact",ch="詩桜",id=13,lv=2.3999999999999999,act="おじぎ"}}},line=415,linkback="block_00078",linknext="block_00080"},block_00075={{"fg",size="z2",face="a0052",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00624",ch="shi"}},ja={{"「所以你一旦被赋予目标和价值就开始成长，对和泉老师来说，应该是一个令人愉快的意外。他们肯定没想到你会有这样的干劲。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0054",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3}}},line=394,linkback="block_00074",linknext="block_00076"},block_00017={{"fg",size="z2",face="b0053",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00589",ch="shi"}},ja={{"「作为将来要嫁入和泉家的人，我还是想记住家庭的规则。而且虽然对小日向有些抱歉，但我再也无法离开你独自生活了」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0001",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3}}},line=92,linkback="block_00016",linknext="block_00018"},block_00107={{"text"},line=553,linkback="block_00106",text={ja={{"那四天里，我只是专注于工作，全力努力，完全没有注意到前辈那样的目光在看我。",{"rt2"}}}},linknext="block_00108"},block_00045={{"fg",size="z1",face="b0053",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1110",ex05="shi_z1b0010",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00603",ch="shi"}},ja={{"「那时候，我已经作为商业作家出道并获得了奖项，所以公司也很支持我……但是由于合同问题，我不得不放弃。我觉得那样做太过束缚」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="b0039",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1110",ex05="shi_z1b0010",mode=3},{"fgact",ch="詩桜",id=13,lv=2.2999999999999998,act="おじぎ"}}},line=234,linkback="block_00044",linknext="block_00046"},block_00043={{"fg",size="z1",face="a0006",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00602",ch="shi"}},ja={{"「做偶像是一种非常珍贵的体验。我可以近距离观察到从欢喜到厌恶的生动感情。我去面试并不是什么奇怪的事」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="a0031",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=3}},vl02={{"fg",size="z1",face="a0023",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=3}}},line=225,linkback="block_00042",linknext="block_00044"},block_00040={{"quake",mode="mw"},{"se",file="seコミ_胸を張る",id=1},{"text"},line=210,linkback="block_00039",text={ja={{"「你在开玩笑吧！？」",{"rt2"},name={"智宏"}}}},linknext="block_00041"},block_00103={{"fg",size="z1",face="a0028",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00640",ch="shi"}},ja={{"「虽然胸怀罪恶感，我仍然和你相处，这可是非常消耗人的。不知道为什么，不是理智而是无法抗拒的，我开始觉得你很可爱」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="a0034",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=1}}},line=534,linkback="block_00102",linknext="block_00104"},block_00026={{"fg",size="z2",face="a0032",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00595",ch="shi"}},ja={{"「最近因为练习纸芝居，经常有机会和小日向单独待在一起……能近距离看到现役顶级声优的脸，这种兴奋……」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0035",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=3}}},line=137,linkback="block_00025",linknext="block_00027"},block_00137={{"text"},line=707,linkback="block_00136",text={ja={{"「是的。谢谢你诗樱前辈，愿意认真对待我那些无聊的担忧。」",{"rt2"},name={"智宏"}}}},linknext="block_00138"},block_00050={{"fg",size="z1",face="b0022",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00606",ch="shi"}},ja={{"「那样的女性，在表面上无动于衷，但在水面下却如鸭子划水一般挣扎，这样的场景我更喜欢。外在美丽，内在却充满泥土气息。这难道不是最好的吗」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="b0032",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=1}}},line=257,linkback="block_00049",linknext="block_00051"},block_00115={{"fg",size="z2",face="a0019h",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",mode=3,path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",hoho="h"},{"text"},text={vo={{"vo",file="fem_shi_00648",ch="shi"}},ja={{"「因为我一直在烦恼着你，这可以说是我自作自受……但换个完全，这样也让我们能有美好的恋情，所以结果来说应该是好的吧」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0055h",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",mode=3,path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",hoho="h"}}},line=601,linkback="block_00114",linknext="block_00116"},block_00101={{"fg",size="z1",face="b0028",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00639",ch="shi"}},ja={{"「从那之后的两天非常艰难。我刚才说不是因为想做补偿所以才和你在一起的，但在培养感情的过程中，罪恶感确实牵涉其中」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="b0018",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=1}}},line=523,linkback="block_00100",linknext="block_00102"},block_00096={{"text"},line=493,linkback="block_00095",text={ja={{"「相反，你还经常让我让你坐在我的膝盖上……那是在我们交往之前，所以我还是挺紧张的」",{"rt2"},name={"智宏"}}}},linknext="block_00097"},block_00024={{"text"},line=125,linkback="block_00023",text={ja={{"「嗯，我明白了……你真的很关心妃爱的情况。作为妃爱的哥哥，我在此表示感谢」",{"rt2"},name={"智宏"}}}},linknext="block_00025"},block_00099={{"fg",size="z1",face="a0055",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00637",ch="shi"}},ja={{"「不过，我对你在华道部的支持和做义工的行为都很赞赏。我对你并无反感，反倒开始用好意的目光看你了」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="a0022",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=3}}},line=508,linkback="block_00098",linknext="block_00100"},block_00023={{"fg",size="z2",face="b0053",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00593",ch="shi"}},ja={{"「那么，我就大胆一点吧。只要我坦然面对，小日向也会觉得「自己也不用顾忌」而心情变得明朗起来」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0032",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3}}},line=122,linkback="block_00022",linknext="block_00024"},block_00065={{"text"},line=340,linkback="block_00064",text={ja={{"「无论如何，我总是向妃爱撒娇，只是把生活费和零花钱一起交给我……我向她撒娇需要的金额远远超过零花钱的金额。」",{"rt2"},name={"智宏"}}}},linknext="block_00066"},block_00044={{"text"},line=227,linkback="block_00043",text={ja={{"现在想想，她有如此出色的面容和身材，完全可以作为一个艺人活动。而且她是一个行动派。",{"rt2"}}}},linknext="block_00045"},block_00032={{"text"},line=172,linkback="block_00031",text={ja={{"「但是，你对妃爱的爱情表现不是过于夸张了吗？虽然你可能不能突然改变态度，但在我面前就不用演戏了吧？」",{"rt2"},name={"智宏"}}}},linknext="block_00033"},block_00034={{"fg",size="z1",face="a0001",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00598",ch="shi"}},ja={{"「我对可爱的女性很感兴趣。虽然我不认为我的外表比别人差，但我明白我的容貌并不属于那种被称为可爱的类型」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="a0039",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=3}}},line=184,linkback="block_00033",linknext="block_00035"},block_00067={{"fg",size="z2",face="b0039",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00618",ch="shi"}},ja={{"「对于这个意愿，虽然我或许有所偏见，但我认为你\"有\"。还有一件很重要的事情，就是你是否将这个意愿付诸行动。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0053",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3}}},line=351,linkback="block_00066",linknext="block_00068"},block_00072={{"fg",size="z2",face="b0016",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",mode=3},{"text"},line=377,linkback="block_00071",text={vo={{"vo",file="fem_shi_00621",ch="shi"}},ja={{"「我也说过好几次，对我来说重要的是你的意愿。是你一直咬紧牙关想要改变什么，还是只在房间里胡吃薯片。」",{"rt2"},name={"詩桜"}}}},linknext="block_00073"},block_00105={{"fg",size="z1",face="a0054",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=1},{"text"},line=545,linkback="block_00104",text={vo={{"vo",file="fem_shi_00642",ch="shi"}},ja={{"「那个我无法向其道歉的人，就在眼前，汗流浃背地为我而努力，一直叫着前辈前辈的。这种感觉也不乏生出怜爱心情」",{"rt2"},name={"詩桜"}}}},linknext="block_00106"},block_00118={{"text"},line=617,linkback="block_00117",text={ja={{"「那么，是不是我可以绑起诗樱前辈了呢……」",{"rt2"},name={"智宏"}}}},linknext="block_00119"},block_00002={{"text"},line=20,linkback="block_00001",text={ja={{"但是随着体力恢复，我开始有些色心起来，偷偷摸了摸她的大腿，结果被她狠狠地掐了一下脸颊。果然她并不是女神。前辈就是前辈。",{"rt2"}}}},linknext="block_00003"},block_00111={{"fg",size="z2",face="b0019h",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",mode=3,path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",hoho="h"},{"text"},text={vo={{"vo",file="fem_shi_00646",ch="shi"}},ja={{"「我是在经历后才知道，心酸这种感觉，能让快乐和爱意倍增。而且，这种心酸的程度，每天都在以幂次递增」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0032h",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",mode=3,path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",hoho="h"}}},line=583,linkback="block_00110",linknext="block_00112"},block_00003={{"fg",size="no",face="a0001",id=20,ch="詩桜",lv=2.2000000000000002,head="shi_noa",path=":fg/shi/no/",file="shi_noa1100",ex05="shi_noa0000",mode=1},{"text"},line=25,linkback="block_00002",text={vo={{"vo",file="fem_shi_00581",ch="shi"}},ja={{"「应该差不多是小日向要回来的时间了……」",{"rt2"},name={"詩桜"}}}},linknext="block_00004"},block_00091={{"text"},line=468,linkback="block_00090",text={ja={{"「如果真的说起对学校感到对不起的话，那是另一回事，我自己并没有因为学姐造成困扰……是的，对你学姐。当然，也对里姐。」",{"rt2"},name={"智宏"}}}},linknext="block_00092"},block_00051={{"fg",size="z2",face="b0053",id=20,resize=1,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00607",ch="shi"}},ja={{"「虽然ひよりん并非偶像，但她在类似的世界里，勇往直前、全力以赴。并且，她总是带着那纯真无邪的笑容」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0003",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=1}}},line=263,linkback="block_00050",linknext="block_00052"},block_00013={{"fg",size="no",face="b0006",id=20,ch="詩桜",lv=2.2000000000000002,head="shi_nob",path=":fg/shi/no/",file="shi_nob1100",ex05="shi_nob0000",mode=1},{"text"},line=69,linkback="block_00012",text={vo={{"vo",file="fem_shi_00587",ch="shi"}},ja={{"「如果你希望的话，我也可以陪你玩这种游戏」",{"rt2"},name={"詩桜"}}}},linknext="block_00014"},block_00148={{"text"},line=753,linkback="block_00147",text={ja={{"我真幸福呀。能和诗樱学姐这样像恋人一样相处，真是太好了。即使我们像恋人一样握着手，她那直视前方、美丽得无法形容的侧脸也让我感到幸福。",{"rt2"}}}},linknext="block_00149"},block_00010={{"fg",size="no",face="b0010",id=20,ch="詩桜",lv=2.2000000000000002,head="shi_nob",path=":fg/shi/no/",file="shi_nob1110",ex05="shi_nob0010",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00585",ch="shi"}},ja={{"「委屈……我没有什么特别的印象。小日向应该也是一样。在照顾你的过程中，没有任何让我们犹豫的行为」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="no",face="b0053",id=20,ch="詩桜",lv=2.2000000000000002,head="shi_nob",path=":fg/shi/no/",file="shi_nob1110",ex05="shi_nob0010",mode=1}}},line=57,linkback="block_00009",linknext="block_00011"},block_00054={{"fg",size="z2",face="b0054",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00610",ch="shi"}},ja={{"「在同一屋檐下度过的这半年里，我确信她周围没有异性的影子。ひよりん就是「真正」的偶像……」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0006",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",mode=1}}},line=281,linkback="block_00053",linknext="block_00055"},block_00130={{"fg",size="z1",face="a0023",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=3},{"text"},line=674,linkback="block_00129",text={ja={{"我只负责照顾资正……这样想着，但诗樱前辈并没有对此做出任何回应，只是微微地笑了笑。",{"rt2"}}}},linknext="block_00131"},block_00037={{"text"},line=195,linkback="block_00036",text={ja={{"「嗯，确实……我觉得学姐不仅仅是可爱，而是有一张非常美丽的脸。如果没有听说你是作家，我还以为你想成为新闻主播呢」",{"rt2"},name={"智宏"}}}},linknext="block_00038"},block_00146={{"text"},line=749,linkback="block_00145",text={ja={{"虽然只是为了尽到对妃爱的义务，但诗樱学姐似乎无法抑制对我的深情。在等公交的时候，她主动握住了我的手。",{"rt2"}}}},linknext="block_00147"},block_00055={{"text"},line=283,linkback="block_00054",text={ja={{"说得好热情啊……",{"rt2"}}}},linknext="block_00056"},label={top={block="block_00000",label=1}},block_00014={{"text"},line=72,linkback="block_00013",text={ja={{"「我真的没有这种兴趣……只是感激你的好意。是的」",{"rt2"},name={"智宏"}}}},linknext="block_00015"},block_00068={{"fg",size="z2",face="b0052",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00619",ch="shi"}},ja={{"「如果你无法准备足够的金额来偿还妃爱，那我将代为偿还。如果你不想欠我，那就努力赚钱还给我吧，亲爱的。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0054",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3}}},line=357,linkback="block_00067",linknext="block_00069"},block_00022={{"fg",size="z2",face="a0018",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3},{"text"},line=116,linkback="block_00021",text={vo={{"vo",file="fem_shi_00592",ch="shi"}},ja={{"「小日向有时候会担心，自己的存在是不是成为了我和你恋爱的障碍」",{"rt2"},name={"詩桜"}}}},linknext="block_00023"},block_00016={{"msgoff"},{"bg",zoom=120,bx=-640,by=-360,sync=0,file="zbg025b",ay=720,ax=1280,path=":bg/"},{"fg",size="z2",sync=0,face="b0039",id=13,resize=1,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"extrans"},{"text"},line=86,linkback="block_00015",text={ja={{"「话说回来，诗樱学姐，这个房间比现在的房间更方便使用对吧？我已经恢复了，也可以回到这里来生活……」",{"rt2"},name={"智宏"}}}},linknext="block_00017"},block_00161={{"bgm",stop=1,time=3000},{"se",stop=1,id=-1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",file="black",path=":bg/",time=3000},{"cacheclear"},{"ex",func="wait",time=1000},{"bg",path=":cg/",file="catch04",id=9,lv=9,time=500},{"bgm",file="bgm94",loop=0},{"ex",func="wait",time=6000},{"bgm",stop=1,time=3000},{"se",stop=1,id=-1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",file="black",path=":bg/",time=3000},{"cacheclear"},{"ex",func="wait",time=1000},{"excall",file="04_詩桜06_1"},{"text"},line=816,linkback="block_00160"},block_00036={{"fg",size="z1",face="b0043h",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",mode=3,path=":fg/shi/z1/",file="shi_z1b1110",ex05="shi_z1b0010",hoho="h"},{"text"},line=192,linkback="block_00035",text={vo={{"vo",file="fem_shi_00599",ch="shi"}},ja={{"「我要杀了你」",{"rt2"},name={"詩桜"}}}},linknext="block_00037"},block_00031={{"fg",size="z1",face="b0010",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=3},{"text"},line=169,linkback="block_00030",text={ja={{"「你说过你是真心支持我的，并且你喜欢女孩子这一点，从圣会长的社交网站上也得到了证实」",{"rt2"},name={"智宏"}}}},linknext="block_00032"},block_00159={{"fg",size="z2",face="b0054h",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",mode=3,path=":fg/shi/z2/",file="shi_z2b1010",ex05="shi_z2b0010",hoho="h"},{"text"},text={vo={{"vo",file="fem_shi_00668",ch="shi"}},ja={{"「没关系的。我也有那种感觉。明天要全力完成学生会的工作」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0015h",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",mode=3,path=":fg/shi/z2/",file="shi_z2b1010",ex05="shi_z2b0010",hoho="h"}}},line=808,linkback="block_00158",linknext="block_00160"},block_00157={{"fg",size="z2",face="a0006h",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",mode=3,path=":fg/shi/z2/",file="shi_z2a1000",ex05="shi_z2a0000",hoho="h"},{"fgact",ch="詩桜",id=13,lv=2.3999999999999999,act="うなづく"},{"se",file="se抱きつく",id=1},{"text"},line=799,linkback="block_00156",text={ja={{"诗樱学姐这么说着，更大胆地缠住了我们紧握的手指。因为手背被轻轻地挠了一下，我果然按照诗樱学姐的话稍微有些欲求不满。",{"rt2"}}}},linknext="block_00158"},block_00113={{"text"},line=593,linkback="block_00112",text={ja={{"「我可是第一次听说爱意增加到3000倍这回事……我倒是听过感度增加3000倍……」",{"rt2"},name={"智宏"}}}},linknext="block_00114"},block_00049={{"fg",size="z1",face="a0054",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00605",ch="shi"}},ja={{"「尤其是那些外表可爱的女性，我特别喜欢。因为这是我从未从天上得到，也无法获得的东西。充满可爱之处的女性值得尊敬」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="a0023",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=1}}},line=251,linkback="block_00048",linknext="block_00050"},block_00155={{"fg",size="z2",face="a0020h",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",mode=3,path=":fg/shi/z2/",file="shi_z2a1000",ex05="shi_z2a0000",hoho="h"},{"text"},text={vo={{"vo",file="fem_shi_00666",ch="shi"}},ja={{"「明明就要回家了，你还真会让人产生那种感觉。今晚，看来我又要一个人痛苦地扭动了」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0027h",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",mode=3,path=":fg/shi/z2/",file="shi_z2a1000",ex05="shi_z2a0000",hoho="h"}}},line=788,linkback="block_00154",linknext="block_00156"},block_00030={{"msgoff"},{"bg",bx=-640,by=-360,sync=0,file="zbg025b",ay=720,ax=1280,path=":bg/"},{"fg",size="z1",sync=0,face="b0036",id=13,resize=1,ch="詩桜",ex02="b0098",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=3},{"extrans",time=500},{"se",stop=1,id=1,time=1000},{"text"},line=164,linkback="block_00029",text={ja={{"「那个，我之前也说过，我以为学姐对妃爱的喜欢是有些过度演出的。就是为了让我当上学生会长，你故意装作不知道」",{"rt2"},name={"智宏"}}}},linknext="block_00031"},block_00153={{"fg",size="z2",face="b0043",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1010",ex05="shi_z2b0010",mode=3},{"text"},line=779,linkback="block_00152",text={vo={{"vo",file="fem_shi_00665",ch="shi"}},ja={{"「你在凝视什么呢」",{"rt2"},name={"詩桜"}}}},linknext="block_00154"},block_00053={{"fg",size="z2",face="a0052",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00609",ch="shi"}},ja={{"「实际上见过她之后，我确认了她的可爱是真实的，无论是外貌还是性格，都与她在媒体上展示的形象一致……不，甚至超出了那些。这一切我都亲眼看到了」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0031",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=1}}},line=275,linkback="block_00052",linknext="block_00054"},block_00147={{"text"},line=751,linkback="block_00146",text={ja={{"一旦回到家，就无法再见到恋人的脸庞。正因为如此，在这短暂的时间里，她向我展示了不同于平常的纯真。",{"rt2"}}}},linknext="block_00148"},block_00152={{"text"},line=774,linkback="block_00151",text={ja={{"虽然是为了妃爱，但诗樱学姐终于把脸转向了我。她的侧脸很美，但正面看也非常美。",{"rt2"}}}},linknext="block_00153"},block_00150={{"text"},line=767,linkback="block_00149",text={ja={{"「那时候就让她吃一口，剩下的我们两个人平分吧」",{"rt2"},name={"智宏"}}}},linknext="block_00151"},block_00145={{"cgdel",id=-1},{"bg",file="black",rule="rule_001",wait=250,path=":bg/",vague=40,time=1000},{"bg",x=-640,rule="rule_001",ay=720,vague=40,by=-360,bx=-640,ax=1280,y2=300,x2=640,speed=120000,y=300,path=":bg/",file="zbg018c"},{"text"},line=747,linkback="block_00144",text={ja={{"我和诗樱学姐都很喜欢妃爱，等公交的时候我们买了她喜欢的布丁。",{"rt2"}}}},linknext="block_00146"},block_00144={{"text"},line=735,linkback="block_00143",text={ja={{"原来如此，就是这个啊。我稍微理解了诗樱学姐的感受。确实，这份爱意是因为对妃爱的深深眷恋而得到的加强。",{"rt2"}}}},linknext="block_00145"},block_00019={{"fg",size="z2",face="a0038",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00590",ch="shi"}},ja={{"「那样对小日向太可怜了。她一直在顾忌我，但其实直到你的伤愈合，她都一直想和你一起睡觉的」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0002",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3}}},line=101,linkback="block_00018",linknext="block_00020"},block_00102={{"text"},line=526,linkback="block_00101",text={ja={{"「你对我…对我产生了感情吗……在那个打工的时间里，居然有这么大的心情变化……」",{"rt2"},name={"智宏"}}}},linknext="block_00103"},block_00143={{"fg",size="no",face="b0006",id=13,ch="詩桜",lv=2.2000000000000002,head="shi_nob",path=":fg/shi/no/",file="shi_nob1100",ex05="shi_nob0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00662",ch="shi"}},ja={{"「刚才，你在想象妃爱伤心的样子时，同时感到了爱意和痛苦吧。这就是魅力增强3000倍的感觉。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="no",face="b0055",id=13,ch="詩桜",lv=2.2000000000000002,head="shi_nob",path=":fg/shi/no/",file="shi_nob1100",ex05="shi_nob0000",mode=3}}},line=733,linkback="block_00142",linknext="block_00144"},block_00042={{"fg",size="z1",face="a0008",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00601",ch="shi"}},ja={{"「你为什么一脸像被蜥蜴咬了一样的表情……你应该知道我渴望各种体验，为此甚至愿意付出自己的努力」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="a0010",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=3}}},line=218,linkback="block_00041",linknext="block_00043"},block_00141={{"msgoff"},{"bg",file="bg025b",path=":bg/",sync=0},{"fg",size="no",sync=0,face="a0039",id=13,resize=1,ch="詩桜",lv=2.2000000000000002,head="shi_noa",path=":fg/shi/no/",file="shi_noa1110",ex05="shi_noa0010",mode=3},{"extrans",time=500},{"text"},text={vo={{"vo",file="fem_shi_00661",ch="shi"}},ja={{"「那么，我们该赶快回你家了，否则妃爱会先回来。我不想让她感到孤单。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="no",face="a0001",id=13,ch="詩桜",lv=2.2000000000000002,head="shi_noa",path=":fg/shi/no/",file="shi_noa1110",ex05="shi_noa0010",mode=3}}},line=724,linkback="block_00140",linknext="block_00142"},block_00097={{"fg",size="z1",face="a0032h",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",mode=3,path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",hoho="h"},{"text"},line=498,linkback="block_00096",text={vo={{"vo",file="fem_shi_00636",ch="shi"}},ja={{"「我也是紧张得心跳加速。这是我第一次主动让男性接触到我的身体」",{"rt2"},name={"詩桜"}}}},linknext="block_00098"},block_00117={{"fg",size="z2",face="a0002",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00650",ch="shi"}},ja={{"「所以，如果你觉得我需要道歉，可以随时告诉我。我真的很抱歉」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0028",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3},{"fgact",ch="詩桜",id=13,lv=2.3999999999999999,act="おじぎ"}}},line=614,linkback="block_00116",linknext="block_00118"},block_00081={{"text"},line=423,linkback="block_00080",text={ja={{"我从没打算责难诗樱学姐，但现在感觉自己最弱的一面被戳中了。",{"rt2"}}}},linknext="block_00082"},block_00127={{"text"},line=662,linkback="block_00126",text={ja={{"「妃爱？妃爱有罪恶感……是吗？是针对谁的呢？」",{"rt2"},name={"智宏"}}}},linknext="block_00128"},block_00138={{"text"},line=709,linkback="block_00137",text={ja={{"如果她只是因为罪恶感才和我在一起，那就糟糕了（虽然我还是不会分手），但那只是因素，而不是原因。听到她这么说，我就满足了。",{"rt2"}}}},linknext="block_00139"},block_00062={{"fg",size="z2",face="a0055",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00615",ch="shi"}},ja={{"「嗯，你对妃爱的感情也是真实的。这一点我承认……所以，我不希望你在金钱上产生负担。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0001",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=1}}},line=322,linkback="block_00061",linknext="block_00063"},block_00133={{"fg",size="z1",face="b0020",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00658",ch="shi"}},ja={{"「你再道歉我就会觉得心痛，请别再说了。我想说的是，因为你的事故，我承受了永远无法消除的罪恶感。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="b0002",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=3}}},line=689,linkback="block_00132",linknext="block_00134"},block_00119={{"fg",size="z2",face="a0050",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3},{"fgact",ch="詩桜",id=13,lv=2.3999999999999999,act="いいえ"},{"text"},line=623,linkback="block_00118",text={vo={{"vo",file="fem_shi_00651",ch="shi"}},ja={{"「那个可以等一等」",{"rt2"},name={"詩桜"}}}},linknext="block_00120"},block_00121={{"text"},line=628,linkback="block_00120",text={ja={{"「但是，因为魅惑魔法的原因，诗樱前辈喜欢上我，对您来说不是不情愿的吗？比如，如果没有罪恶感的话，本来就不会和我在一起……」",{"rt2"},name={"智宏"}}}},linknext="block_00122"},block_00083={{"text"},line=427,linkback="block_00082",text={ja={{"因此我并没有否定诗樱学姐的罪疚感……反而，既然不用否定，我趁机问了一直心中的疑惑。",{"rt2"}}}},linknext="block_00084"},block_00123={{"fg",size="z2",face="b0022",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"text"},line=639,linkback="block_00122",text={vo={{"vo",file="fem_shi_00653",ch="shi"}},ja={{"「如果在结束橘子咖啡厅的兼职工作后我们还能保持那样的好关系，即使没有加成，我迟早也会喜欢上你的吧。」",{"rt2"},name={"詩桜"}}}},linknext="block_00124"},block_00129={{"text"},line=670,linkback="block_00128",text={ja={{"「啊，我们确实有时会谈到这个。但是，无论怎么看，应该是我感到对不起妃爱才对。家务事和家庭经济都是妃爱在负责」",{"rt2"},name={"智宏"}}}},linknext="block_00130"},block_00015={{"fg",size="no",face="b0039",id=20,ch="詩桜",lv=2.2000000000000002,head="shi_nob",path=":fg/shi/no/",file="shi_nob1100",ex05="shi_nob0000",mode=1},{"text"},line=77,linkback="block_00014",text={vo={{"vo",file="fem_shi_00588",ch="shi"}},ja={{"「嗯，我还是希望避免在你的房间里做这种异常的行为。我们必须要注意小日向的眼光」",{"rt2"},name={"詩桜"}}}},linknext="block_00016"},block_00126={{"msgoff"},{"bg",bx=-640,by=-360,sync=0,file="zbg025b",ay=720,ax=1280,path=":bg/"},{"fg",size="z1",sync=0,face="b0001",id=13,resize=1,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1110",ex05="shi_z1b0010",mode=3},{"extrans",time=500},{"text"},text={vo={{"vo",file="fem_shi_00655",ch="shi"}},ja={{"「关于抱着罪恶感，让爱意越发汹涌的痛苦，我和妃爱经常聊。因为这个，我更喜欢她了。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="b0032",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1110",ex05="shi_z1b0010",mode=3}}},line=659,linkback="block_00125",linknext="block_00127"},block_00027={{"text"},line=140,linkback="block_00026",text={ja={{"「等等，你们两个是单独一起练习吗？我也想听听诗樱学姐的表演，为什么不能让我参加呢」",{"rt2"},name={"智宏"}}}},linknext="block_00028"},block_00125={{"text"},line=648,linkback="block_00124",text={ja={{"诗樱前辈真的很嫉妒常磐呢。虽然每次提起来都后悔，但诗樱前辈的嫉妒很少见，所以我并不觉得讨厌。",{"rt2"}}}},linknext="block_00126"},block_00122={{"fg",size="z2",face="b0032",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00652",ch="shi"}},ja={{"「魅惑魔法不是起因，而是加成。把1变成10并不是特别困难，但始终困难的是从0变成1。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0053",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3}}},line=634,linkback="block_00121",linknext="block_00123"},block_00007={{"fg",size="no",face="b0052",id=20,ch="詩桜",lv=2.2000000000000002,head="shi_nob",path=":fg/shi/no/",file="shi_nob1100",ex05="shi_nob0000",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00584",ch="shi"}},ja={{"「在这之上，我能和你共享睡眠和饮食，有一个晚上能够陪你睡觉的幸福环境。对你来说可能只是痛苦的记忆……」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="no",face="b0002",id=20,ch="詩桜",lv=2.2000000000000002,head="shi_nob",path=":fg/shi/no/",file="shi_nob1100",ex05="shi_nob0000",mode=1}}},line=45,linkback="block_00006",linknext="block_00008"},block_00120={{"text"},line=625,linkback="block_00119",text={ja={{"经过了两个小时的舔阴以及热乎乎的关东煮，似乎有些承受不住。诗樱前辈表示想道歉，但被拒绝了。",{"rt2"}}}},linknext="block_00121"},block_00088={{"fg",size="z1",face="a0027",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00631",ch="shi"}},ja={{"「但是，如果问我在和你交往之前是否有赎罪的意识，我可以说完全没有。想想看，我们开始交往的时候，事故还没发生，对吧？」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="a0052",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=3}}},line=456,linkback="block_00087",linknext="block_00089"},block_00106={{"fg",size="z1",face="b0023",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00643",ch="shi"}},ja={{"「在听了ひよりん的解释后的这两天，大约工作了将近20小时，我一直看着这后辈的勇敢形象。想要拥抱他，对他说对不起」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="b0032",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=1}}},line=551,linkback="block_00105",linknext="block_00107"},block_00077={{"fg",size="z2",face="a0028",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00625",ch="shi"}},ja={{"「她也同样有一种罪疚感。她有过几次自嘲地埋怨，害怕是不是利用了你。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0038",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3}}},line=403,linkback="block_00076",linknext="block_00078"},block_00100={{"msgoff"},{"bg",path=":bg/",sync=1,lv=8,id=7,file="white",time=1000},{"bg",notone=1,path=":sys/",lv=8,id=7,file="reco",time=1000},{"colortone",style="sepia"},{"bg",file="ev_sio_01a",set="ev_sio_01",path=":ev/ev_sio_01/"},{"fg",size="z1",face="b0052",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00638",ch="shi"}},ja={{"「我一直觉得你是个很有骨气的男生，然后我又因为个人问题受你照顾了。加上，对于我疑惑的部分，从小日向那里也有解释」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="b0001",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=1}}},line=517,linkback="block_00099",linknext="block_00101"},block_00046={{"text"},line=236,linkback="block_00045",text={ja={{"如果一开始就有一技之长，对于推广方来说肯定会更加方便……不过，她的谈话能力和协作性还有待商榷，成功与否还很难预测。",{"rt2"}}}},linknext="block_00047"},block_00116={{"fg",size="z2",face="a0018",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00649",ch="shi"}},ja={{"「但是说实话，这份恋情的热情只有我一个人感受到，对你来说可能完全没那么感慨吧。毕竟曾经严苛的对你的我，某天突然就开始满脸通红地对你撒娇了」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0029",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1100",ex05="shi_z2a0000",mode=3}}},line=607,linkback="block_00115",linknext="block_00117"},block_00047={{"text"},line=238,linkback="block_00046",text={ja={{"哈哈，原来是想成为偶像啊。诗樱学姐做偶像……天啊，这让我觉得她更加迷人了。能独占一个有着能进入最后选拔的美貌的女性……",{"rt2"}}}},linknext="block_00048"},block_00156={{"fg",size="z2",face="a0025h",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",mode=3,path=":fg/shi/z2/",file="shi_z2a1000",ex05="shi_z2a0000",hoho="h"},{"text"},line=793,linkback="block_00155",text={vo={{"vo",file="fem_shi_00667",ch="shi"}},ja={{"「你也该有那种感觉，过一个痛苦的夜吧。明天再见面」",{"rt2"},name={"詩桜"}}}},linknext="block_00157"},block_00104={{"fg",size="z1",face="a0018",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00641",ch="shi"}},ja={{"「如果你对我怀有恶意，那情况就会完全不同了……但很明显你没有恶意，你甚至还为了我和我的恩人不断工作」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="a0019",id=20,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=1}}},line=540,linkback="block_00103",linknext="block_00105"},block_00012={{"text"},line=64,linkback="block_00011",text={ja={{"虽然是我自己提起的话题，但被说出我想要忘记的回忆，羞愧得用手捂住了脸。被看到比色情还要羞耻的样子，我可能都无法出嫁了。",{"rt2"}}}},linknext="block_00013"},block_00074={{"fg",size="z2",face="a0001",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=3},{"text"},line=388,linkback="block_00073",text={vo={{"vo",file="fem_shi_00623",ch="shi"}},ja={{"「本来如果彼此两个想让你当会长，对我来说抬高你的估价更有利，我没有理由怀疑彼此的评价。」",{"rt2"},name={"詩桜"}}}},linknext="block_00075"},block_00095={{"fg",size="z1",face="a0039",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00635",ch="shi"}},ja={{"「除非你从一开始就希望和我交往……啊，你对我的身体似乎很感兴趣，是曾经让我坐在你的膝盖上了呢」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="a0008",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=3},{"fgact",ch="詩桜",id=13,lv=2.2999999999999998,act="おじぎ"}}},line=490,linkback="block_00094",linknext="block_00096"},block_00094={{"fg",size="z1",face="a0001",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00634",ch="shi"}},ja={{"「即便如此，我并没有你所担忧的那种，为了弥补而和你交往的想法。因为和我交往对你来说没有任何好处，作为赎罪的方式是行不通的。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="a0027",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1110",ex05="shi_z1a0010",mode=3}}},line=483,linkback="block_00093",linknext="block_00095"},block_00056={{"text"},line=285,linkback="block_00055",text={ja={{"原来如此，我和妃爱的相遇可能只是偶然。但我确实深深理解了诗樱前辈那份对于'真实'的热情。",{"rt2"}}}},linknext="block_00057"},block_00093={{"fg",size="z1",face="b0039",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1110",ex05="shi_z1b0010",mode=3},{"fgact",ch="詩桜",id=13,lv=2.2999999999999998,act="いいえ"},{"text"},text={vo={{"vo",file="fem_shi_00633",ch="shi"}},ja={{"「反过来说，我至今并没有对学校有任何感到罪疚。我唯一觉得对不起的，是对你和你的幕僚们，因为我丢下了学生会的事情。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="b0001",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1110",ex05="shi_z1b0010",mode=3}}},line=477,linkback="block_00092",linknext="block_00094"},block_00092={{"text"},line=470,linkback="block_00091",text={ja={{"做学生会长这件事的构想并不是由你提出的嘛。呃，我并没有想责难你的意思哦。",{"rt2"}}}},linknext="block_00093"},block_00086={{"fg",size="z1",face="b0025",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=3},{"text"},line=445,linkback="block_00085",text={vo={{"vo",file="fem_shi_00629",ch="shi"}},ja={{"「怎么了？难道你在担心我是为了赎罪才和你在一起吗？」",{"rt2"},name={"詩桜"}}}},linknext="block_00087"},block_00084={{"msgoff"},{"bg",bx=-640,by=-360,sync=0,file="zbg025b",ay=720,ax=1280,path=":bg/"},{"fg",size="z1",sync=0,face="b0002",id=13,resize=1,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",mode=3},{"extrans",time=500},{"text"},line=436,linkback="block_00083",text={ja={{"「诗樱学姐，你愿意和我交往……是不是也受那种罪疚感的影响？」",{"rt2"},name={"智宏"}}}},linknext="block_00085"},block_00131={{"fg",size="z1",face="a0001",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00657",ch="shi"}},ja={{"「我向你道歉，终于消除了罪恶感，正打算坦然地表达好感……然后你就发生了事故，就在我们开始平等相处的时候。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="a0038",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1a",path=":fg/shi/z1/",file="shi_z1a1100",ex05="shi_z1a0000",mode=3}}},line=680,linkback="block_00130",linknext="block_00132"},block_00082={{"text"},line=425,linkback="block_00081",text={ja={{"因为我对里姐很感激，她没有把我当作妃爱的附属品。否定诗樱学姐的罪感，就等于否定对里姐的感激。",{"rt2"}}}},linknext="block_00083"},block_00078={{"fg",size="z2",face="b0022",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3},{"text"},line=408,linkback="block_00077",text={vo={{"vo",file="fem_shi_00626",ch="shi"}},ja={{"「本应被利用的你，自己找到了价值，甚至展示出成长，对她来说，这是一种解脱，作为家人，也是一种无比的喜悦。」",{"rt2"},name={"詩桜"}}}},linknext="block_00079"},block_00073={{"fg",size="z2",face="a0039",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=3},{"text"},text={vo={{"vo",file="fem_shi_00622",ch="shi"}},ja={{"「最初听你的话的时候，我完全信了和泉老师和那个经理不会去编造能够立马查证出来的事情，认定你是张口就是妹妹的人，过着无所事事的日子。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0027",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=3}}},line=383,linkback="block_00072",linknext="block_00074"},block_00041={{"text"},line=212,linkback="block_00040",text={ja={{"什么？诗樱学姐想成为偶像！？那种会唱歌、跳舞，和粉丝握手的偶像吗？就连面无表情都如此出色的诗樱学姐！？",{"rt2"}}}},linknext="block_00042"},block_00018={{"text"},line=95,linkback="block_00017",text={ja={{"「那么，你有没有考虑过……比如我们两个人以这个房间为主来生活？」",{"rt2"},name={"智宏"}}}},linknext="block_00019"},block_00069={{"fgact",ch="詩桜",id=13,lv=2.3999999999999999,act="うんうん"},{"text"},line=361,linkback="block_00068",text={ja={{"诗樱前辈用温柔的眼神抚摸着我的头。感觉让我依赖的对象从妃爱变成了诗樱前辈。但是前辈不像妃爱那般温柔，我还是得自立。",{"rt2"}}}},linknext="block_00070"},block_00066={{"fg",size="z2",face="b0001",id=13,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",mode=3},{"text"},line=345,linkback="block_00065",text={vo={{"vo",file="fem_shi_00617",ch="shi"}},ja={{"「呵呵，我已经从妃爱那里听到了关于她纵容你的原因。对我来说最重要的是，你是否有意复偿。」",{"rt2"},name={"詩桜"}}}},linknext="block_00067"},block_00134={{"fg",size="z1",face="b0019h",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",mode=3,path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",hoho="h"},{"text"},text={vo={{"vo",file="fem_shi_00659",ch="shi"}},ja={{"「也就是说，我现在就像是一直处于被魅力增强3000倍的状态。不用再隐藏我的感情，反而比以前更轻松了。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z1",face="b0055h",id=13,ch="詩桜",lv=2.2999999999999998,head="shi_z1b",mode=3,path=":fg/shi/z1/",file="shi_z1b1100",ex05="shi_z1b0000",hoho="h"}}},line=695,linkback="block_00133",linknext="block_00135"},block_00114={{"text"},line=595,linkback="block_00113",text={ja={{"不过连感度增加3000倍的说法在轻小说里也很少听说。那应该是在别的媒体上吧。",{"rt2"}}}},linknext="block_00115"},block_00059={{"fg",size="z2",face="b0032",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00613",ch="shi"}},ja={{"「只需保持和妃爱目前的距离，我就没有必要自己成为偶像。她所讲述的工作故事，我都会以获得金句的心情饶有兴趣地倾听。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0053",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",mode=1}}},line=305,linkback="block_00058",linknext="block_00060"},block_00052={{"fg",size="z2",face="a0039",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00608",ch="shi"}},ja={{"「声优这个行业与偶像有相似之处，但也有所不同。在听到和泉老师的讲述之前，我甚至都没有意识到这个行业的存在。我越是了解她，就越被她吸引」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0053",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=1}}},line=269,linkback="block_00051",linknext="block_00053"},block_00060={{"fg",size="z2",face="b0052",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",mode=1},{"text"},text={vo={{"vo",file="fem_shi_00614",ch="shi"}},ja={{"「你妹妹妃爱是占整个地球人口……不，是千年难遇的人才，是七万亿分之一的珍贵存在。」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="b0031",id=20,ch="詩桜",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1110",ex05="shi_z2b0010",mode=1}}},line=313,linkback="block_00059",linknext="block_00061"},block_00028={{"fg",size="z2",face="a0040",id=13,ch="詩桜",ex02="a0098",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=3},{"fgact",ch="詩桜",id=13,lv=2.3999999999999999,act="quake"},{"text"},text={vo={{"vo",file="fem_shi_00596",ch="shi"}},ja={{"「哎呀呀呀！实际上能看到小日向穿睡衣、刚醒来、甚至刚洗完澡的样子，你过去的生活该有多么令人羡慕啊……！」",{"rt2"},name={"詩桜"}}}},delay={vl01={{"fg",size="z2",face="a0009",id=13,ch="詩桜",ex02="a0098",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=3}},vl02={{"fg",size="z2",face="a0046",id=13,ch="詩桜",ex02="a0098",lv=2.3999999999999999,head="shi_z2a",path=":fg/shi/z2/",file="shi_z2a1110",ex05="shi_z2a0010",mode=3}},vl03={{"fg",size="z2",face="b0036",id=13,ch="詩桜",ex02="b0098",lv=2.3999999999999999,head="shi_z2b",path=":fg/shi/z2/",file="shi_z2b1100",ex05="shi_z2b0000",mode=3}}},line=149,linkback="block_00027",linknext="block_00029"},block_00021={{"text"},line=109,linkback="block_00020",text={ja={{"诗樱学姐虽然对我隐瞒，但据里姐说，妃爱一本正经地一个人坐在食堂的椅子上，这样的事情似乎也发生过。确实，我不能离家出走。",{"rt2"}}}},linknext="block_00022"}}