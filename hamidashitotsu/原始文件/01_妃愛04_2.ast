astver = 2.0
ast = {block_00047={{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0000",head="hiy_z2b",id=13,size="z2",lv=2.3999999999999999,face="b0055",file="hiy_z2b0200",path=":fg/hiy/z2/"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00145"}},ja={{"「虽然外面的世界很可怕，但当感觉自己好像被什么力量从背后推出去时，只能回忆起现在的自己。因为在我被推出去的地方，也有幸福存在。」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0000",head="hiy_z2b",id=13,size="z2",lv=2.3999999999999999,face="b0052",file="hiy_z2b0200",path=":fg/hiy/z2/"}}},line=239,linknext="block_00048",linkback="block_00046"},block_00089={{"text"},line=419,text={ja={{"只要能见到那些同学们和妹妹，就足够成为我去学生会室的理由了。明天我们要做什么……新人们能承担的工作有哪些。",{"rt2"}}}},linknext="block_00090",linkback="block_00088"},block_00067={{"bg",file="ev_hiy_03b",set="ev_hiy_03",sync=0,face="1",time=400,path=":ev/ev_hiy_03/"},{"text"},line=342,text={vo={{"vo",ch="hiy",file="fem_hiy_00151"}},ja={{"「听到这个消息的那天，我就开始期待着可能会和哥哥一起创作作品，心里充满了激动」",{"rt2"},name={"妃愛"}}}},linknext="block_00068",linkback="block_00066"},block_00091={{"text"},line=429,text={ja={{"「妃爱田小姐」",{"rt2"},name={"智宏"}}}},linknext="block_00092",linkback="block_00090"},block_00015={{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0000",head="hiy_z2b",id=13,size="z2",lv=2.3999999999999999,face="b0032h",hoho="h",file="hiy_z2b0200",path=":fg/hiy/z2/"},{"text"},line=93,text={vo={{"vo",ch="hiy",file="fem_hiy_00133"}},ja={{"「我被爱着……被爱着……好幸福……太幸福了……」",{"rt2"},name={"妃愛"}}}},linknext="block_00016",linkback="block_00014"},block_00054={{"text"},line=272,text={ja={{"「啊？就算你刚刚一直在吃糖浆麦芽糖吗？唔，从妃爱的角度看，你再吃应该没问题吧？」",{"rt2"},name={"智宏"}}}},linknext="block_00055",linkback="block_00053"},block_00011={{"fg",mode=3,ch="妃愛",ex05="hiy_noa0010",head="hiy_noa",id=13,size="no",lv=2.2000000000000002,x=0,face="a0051",file="hiy_noa0210",path=":fg/hiy/no/"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00131"}},ja={{"「稍微有点心动呢。被疼爱这个表达很好，我想被哥哥疼爱」",{"rt2"},name={"妃愛"}}}},delay={vl02={{"fg",mode=3,ch="妃愛",ex05="hiy_nob0000",head="hiy_nob",id=13,size="no",lv=2.2000000000000002,x=0,face="b0032",file="hiy_nob0200",path=":fg/hiy/no/"}},vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_noa0000",head="hiy_noa",id=13,size="no",lv=2.2000000000000002,x=0,face="a0035",file="hiy_noa0200",path=":fg/hiy/no/"}}},line=68,linknext="block_00012",linkback="block_00010"},block_00024={{"text"},line=131,text={ja={{"像是和常磐聊二次元的话题，或者和锦讨论Vtuber之类的。在那两个人面前，诗樱学姐对我也变得更加温柔了。",{"rt2"}}}},linknext="block_00025",linkback="block_00023"},block_00043={{"text"},line=219,text={ja={{"「呜哈啊」",{"rt2"},name={"智宏"}}}},linknext="block_00044",linkback="block_00042"},block_00000={{"bg",path=":bg/",file="black"},{"text"},text={ja={{"因为回家的时间稍微晚了一些，诗樱学姐帮我们叫了出租车。依次送锦和常磐回家后，我们两兄妹才回到了自己的家。",{"rt2"}}}},linknext="block_00001",line=8},block_00060={{"se",id=1,stop=1,time=1000},{"bgm",stop=1},{"cgdel",id=-1},{"bg",file="black",rule="rule_002",vague=40,path=":bg/",time=1000,wait=500},{"bg",file="zbg002c",vague=40,by=-360,ay=720,speed=150000,x2=-640,bx=-640,rule="rule_002r",y2=100,x=640,y=100,ax=1280,path=":bg/"},{"bgm",file="bgm23"},{"text"},line=316,text={ja={{"和资正玩了一会儿后，我们两个人边吃着妃爱做的梅子泡饭，边愉快地回顾了今天的事情。",{"rt2"}}}},linknext="block_00061",linkback="block_00059"},block_00026={{"text"},line=137,text={ja={{"「只是感觉，从正好适宜肌肤的温水中出来感觉有点可惜……因为它给人一种如梦般的舒服感觉。」",{"rt2"},name={"智宏"}}}},linknext="block_00027",linkback="block_00025"},block_00052={{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0010",head="hiy_z1b",id=13,size="z1",lv=2.2999999999999998,face="b0041",file="hiy_z1b0210",path=":fg/hiy/z1/"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00146"}},ja={{"「伤感的心情，就他自己而言也是珍贵的东西，你应该好好品味它！不过嗯，如果能让哥哥有元气，那就太好了。」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_z1a0000",head="hiy_z1a",id=13,size="z1",lv=2.2999999999999998,face="a0054",file="hiy_z1a0200",path=":fg/hiy/z1/"}}},line=262,linknext="block_00053",linkback="block_00051"},block_00061={{"text"},line=318,text={ja={{"真是幸福啊，这个时光。即使未来会有怎样的变化，只要能回到这个家，和妃爱一起吃晚饭的这个时间，希望它永远不会改变。",{"rt2"}}}},linknext="block_00062",linkback="block_00060"},block_00044={{"text"},line=221,text={ja={{"我不禁想象起了刚刚自我介绍的那些后辈们穿着浴巾的样子。虽然有些罪恶感，但这不是我的错……对吧？",{"rt2"}}}},linknext="block_00045",linkback="block_00043"},block_00039={{"text"},line=197,text={ja={{"「嗯，我知道，我也明白。但是我的心里还是有些难过，所以只想在妃爱面前撒撒娇。」",{"rt2"},name={"智宏"}}}},linknext="block_00040",linkback="block_00038"},block_00045={{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0000",head="hiy_z2b",id=13,size="z2",lv=2.3999999999999999,face="b0019",file="hiy_z2b0200",path=":fg/hiy/z2/"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00143"}},ja={{"「半年前，我也曾觉得就这样平淡地过下去也不错。只要能和哥哥一直这样生活下去，无论多少年都没有任何改变……」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0000",head="hiy_z2b",id=13,size="z2",lv=2.3999999999999999,face="b0032",file="hiy_z2b0200",path=":fg/hiy/z2/"}}},line=227,linknext="block_00046",linkback="block_00044"},block_00051={{"msgoff"},{"fg",mode=-2,sync=0},{"cgdel",id=5,rule="cinema",lv=5,sync=0,time=400},{"bg",file="zbg004c",bx=-640,by=-360,sync=0,ax=1280,ay=720,path=":bg/"},{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0000",head="hiy_z1b",sync=0,resize=1,id=13,size="z1",lv=2.2999999999999998,face="b0053",file="hiy_z1b0200",path=":fg/hiy/z1/"},{"extrans",time=500},{"text"},line=256,text={ja={{"「谢谢你，妃爱，我现在感觉特别有精神。那种伤感已经没有了。」",{"rt2"},name={"智宏"}}}},linknext="block_00052",linkback="block_00050"},block_00074={{"text"},line=366,text={ja={{"这确实是个严重问题。在兄妹之间的表演中展现出恋爱的表情。",{"rt2"}}}},linknext="block_00075",linkback="block_00073"},block_00055={{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0010",head="hiy_z1b",id=13,size="z1",lv=2.2999999999999998,face="b0006",file="hiy_z1b0210",path=":fg/hiy/z1/"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00148"}},ja={{"「所以我想要吃点像茶泡饭这样简单的食物。我想要和哥哥一起吃晚饭。」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0000",head="hiy_z1b",id=13,size="z1",lv=2.2999999999999998,face="b0041",file="hiy_z1b0200",path=":fg/hiy/z1/"}}},line=278,linknext="block_00056",linkback="block_00054"},block_00040={{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0000",head="hiy_z1b",id=13,size="z1",lv=2.2999999999999998,face="b0054",file="hiy_z1b0200",path=":fg/hiy/z1/"},{"text"},line=202,text={vo={{"vo",ch="hiy",file="fem_hiy_00141"}},ja={{"「摇摇晃晃的～」",{"rt2"},name={"妃愛"}}}},linknext="block_00041",linkback="block_00039"},block_00082={{"text"},line=397,text={ja={{"如果过于期待，万一因为无法避免的原因无法实现，我怕会让妃爱感到抱歉。所以要适可而止。",{"rt2"}}}},linknext="block_00083",linkback="block_00081"},block_00062={{"text"},line=320,text={ja={{"不，正因为这个时间一直未曾改变，所以无论家外发生什么事情，我都能愉快地度过。我要珍惜和妃爱一起用餐的时光。",{"rt2"}}}},linknext="block_00063",linkback="block_00061"},block_00059={{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0010",head="hiy_z1b",id=13,size="z1",lv=2.2999999999999998,face="b0009",file="hiy_z1b0210",path=":fg/hiy/z1/"},{"fgact",act="びっくり",ch="妃愛",id=13,lv=2.2999999999999998},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00149"}},ja={{"「比起我们自己要吃什么，首先应该让资正看到我们的脸！对不起，让你一个人在家待了这么久，真是对不起。」",{"rt2"},name={"妃愛"}}}},delay={vl02={{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0010",head="hiy_z1b",id=13,size="z1",lv=2.2999999999999998,face="b0047",file="hiy_z1b0210",path=":fg/hiy/z1/"}},vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0010",head="hiy_z1b",id=13,size="z1",lv=2.2999999999999998,face="b0046",file="hiy_z1b0210",path=":fg/hiy/z1/"}}},line=302,linknext="block_00060",linkback="block_00058"},block_00028={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",file="zbg004c",bx=-640,by=-360,sync=0,ax=1280,ay=720,path=":bg/"},{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0000",head="hiy_z1b",sync=0,resize=1,id=13,size="z1",lv=2.2999999999999998,face="b0009",file="hiy_z1b0200",path=":fg/hiy/z1/"},{"extrans"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00137"}},ja={{"「比喻不恰当。你只需要说出你喜欢温水就可以了。」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0000",head="hiy_z1b",id=13,size="z1",lv=2.2999999999999998,face="b0047",file="hiy_z1b0200",path=":fg/hiy/z1/"}}},line=152,linknext="block_00029",linkback="block_00027"},block_00092={{"bgm",time=3000,stop=1},{"se",id=-1,stop=1,time=3000},{"vostop",time=3000,stop=1},{"ntclear"},{"cgdel",id=-1},{"bg",file="black",time=3000,path=":bg/"},{"cacheclear"},{"ex",time=1000,func="wait"},{"bg",id=9,file="catch01",lv=9,time=500,path=":cg/"},{"bgm",loop=0,file="bgm91"},{"ex",time=6000,func="wait"},{"bgm",time=3000,stop=1},{"se",id=-1,stop=1,time=3000},{"vostop",time=3000,stop=1},{"ntclear"},{"cgdel",id=-1},{"bg",file="black",time=3000,path=":bg/"},{"cacheclear"},{"ex",time=1000,func="wait"},{"excall",file="01_妃愛05_1"},{"text"},line=435,linkback="block_00091"},label={top={label=1,block="block_00000"}},block_00088={{"text"},line=417,text={ja={{"虽然这不是学生会的工作，但至少我们现在有了一个目标……看来我们不会陷入只是在放学后闲聊打发时间的局面了。",{"rt2"}}}},linknext="block_00089",linkback="block_00087"},block_00090={{"bg",file="ev_hiy_03k",set="ev_hiy_03",sync=0,face="1",time=400,path=":ev/ev_hiy_03/"},{"text"},line=426,text={vo={{"vo",ch="hiy",file="fem_hiy_00163"}},ja={{"「那么到底哥哥你更喜欢神田小姐还是饭田小姐呢？」",{"rt2"},name={"妃愛"}}}},linknext="block_00091",linkback="block_00089"},block_00087={{"text"},line=415,text={ja={{"虽然她并不是正式的干部，但我们还是需要有人来引导大家。现在有理由让她来学生会室，真是太好了。",{"rt2"}}}},linknext="block_00088",linkback="block_00086"},block_00086={{"text"},line=413,text={ja={{"「话说回来，她可能会为了咨询小说的事情而来到学校。也许还有很多机会见面呢？」",{"rt2"},name={"智宏"}}}},linknext="block_00087",linkback="block_00085"},block_00050={{"text"},line=245,text={ja={{"虽然有些地方要依赖诗樱学姐，但我也要尽可能地利用自己可以协作的部分来贡献。",{"rt2"}}}},linknext="block_00051",linkback="block_00049"},block_00085={{"bg",file="ev_hiy_03c",set="ev_hiy_03",sync=0,face="1",time=400,path=":ev/ev_hiy_03/"},{"text"},line=410,text={vo={{"vo",ch="hiy",file="fem_hiy_00162"}},ja={{"「只是诗樱前辈三月份就要毕业了，这让人有点遗憾呢」",{"rt2"},name={"妃愛"}}}},linknext="block_00086",linkback="block_00084"},block_00019={{"text"},line=113,text={ja={{"「我们的人才和环境都太优秀了，真是太感激不尽了。只是因为听到诗樱前辈的话，对建立新的人际关系还是感到有些焦虑。」",{"rt2"},name={"智宏"}}}},linknext="block_00020",linkback="block_00018"},block_00078={{"text"},line=382,text={ja={{"「如果真的能实现，那我就能久违地看到妃爱的现场表演了，真是让人期待」",{"rt2"},name={"智宏"}}}},linknext="block_00079",linkback="block_00077"},block_00083={{"bg",set="ev_hiy_03",file="ev_hiy_03b",time=500,path=":ev/ev_hiy_03/"},{"text"},line=402,text={vo={{"vo",ch="hiy",file="fem_hiy_00160"}},ja={{"「即使这样，谈论充满梦想的事情还是很开心的！这也能激发大家的动力，如果不行，就找下一个机会就好了」",{"rt2"},name={"妃愛"}}}},linknext="block_00084",linkback="block_00082"},block_00012={{"text"},line=71,text={ja={{"「从你出生那一刻起，我就一直很珍视你」",{"rt2"},name={"智宏"}}}},linknext="block_00013",linkback="block_00011"},block_00084={{"text"},line=405,text={vo={{"vo",ch="hiy",file="fem_hiy_00161"}},ja={{"「只要能和大家一起热闹地度过，再和哥哥度过放学后的时间，我就已经很满足了。现在我只是帮忙做点事情，所以也很轻松」",{"rt2"},name={"妃愛"}}}},linknext="block_00085",linkback="block_00083"},block_00002={{"text"},line=23,text={ja={{"「嗯」",{"rt2"},name={"智宏"}}}},linknext="block_00003",linkback="block_00001"},block_00003={{"text"},line=25,text={ja={{"还没来得及脱鞋，妃爱就开始向我发问。本以为她会突然扑过来抱住我，但是我可爱的妹妹的态度却显得有些冷淡。",{"rt2"}}}},linknext="block_00004",linkback="block_00002"},block_00065={{"bg",file="ev_hiy_03c",set="ev_hiy_03",sync=0,face="1",time=400,path=":ev/ev_hiy_03/"},{"text"},line=334,text={vo={{"vo",ch="hiy",file="fem_hiy_00150"}},ja={{"「关于诗樱学姐的小说的事情，对不起我没告诉你～」",{"rt2"},name={"妃愛"}}}},linknext="block_00066",linkback="block_00064"},block_00006={{"fg",mode=3,ch="妃愛",ex05="hiy_nob0000",head="hiy_nob",id=13,size="no",lv=2.2000000000000002,x=0,face="b0003",file="hiy_nob0200",path=":fg/hiy/no/"},{"fgact",act="いいえ",ch="妃愛",id=13,lv=2.2000000000000002},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00128"}},ja={{"「我才不会被你的甜言蜜语骗过。来说吧，你肯定喜欢饭田那样的人吧」",{"rt2"},name={"妃愛"}}}},delay={vl02={{"fg",mode=3,ch="妃愛",ex05="hiy_nob0000",head="hiy_nob",id=13,size="no",lv=2.2000000000000002,x=0,face="b0043",file="hiy_nob0200",path=":fg/hiy/no/"},{"fgact",act="うんうん",ch="妃愛",id=13,lv=2.2000000000000002}},vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_nob0000",head="hiy_nob",id=13,size="no",lv=2.2000000000000002,x=0,face="b0033",file="hiy_nob0200",path=":fg/hiy/no/"}}},line=42,linknext="block_00007",linkback="block_00005"},block_00081={{"text"},line=395,text={ja={{"「嗯……嗯，应该是这样的」",{"rt2"},name={"智宏"}}}},linknext="block_00082",linkback="block_00080"},block_00046={{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0000",head="hiy_z2b",id=13,size="z2",lv=2.3999999999999999,face="b0053",file="hiy_z2b0200",path=":fg/hiy/z2/"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00144"}},ja={{"「但不论我怎么想，随着我们所处环境的变化，我最想得到的东西已经得到了……我已经触及到那曾经放弃的幸福。」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0010",head="hiy_z2b",id=13,size="z2",lv=2.3999999999999999,face="b0022",file="hiy_z2b0210",path=":fg/hiy/z2/"}}},line=233,linknext="block_00047",linkback="block_00045"},block_00048={{"text"},line=241,text={ja={{"啊，多么鼓舞人心的话。没错，离开了像温水般宽容的自家房间后，我得到了和这个妹妹一辈子都不分开的未来。",{"rt2"}}}},linknext="block_00049",linkback="block_00047"},block_00013={{"fg",mode=3,ch="妃愛",ex05="hiy_nob0010",head="hiy_nob",id=13,size="no",lv=2.2000000000000002,x=0,face="b0048",file="hiy_nob0210",path=":fg/hiy/no/"},{"fgact",act="いいえ",ch="妃愛",id=13,lv=2.2000000000000002},{"text"},line=77,text={vo={{"vo",ch="hiy",file="fem_hiy_00132"}},ja={{"「呵呵呵，哥哥～，疼爱我～」",{"rt2"},name={"妃愛"}}}},linknext="block_00014",linkback="block_00012"},block_00079={{"bg",file="zev_hiy_03b",sync=0,time=400,ax=1280,y=40,ay=720,by=-360,bx=-640,x=-390,face="1",set="ev_hiy_03",path=":ev/ev_hiy_03/"},{"text"},line=387,text={vo={{"vo",ch="hiy",file="fem_hiy_00158"}},ja={{"「说到现场表演，好像有两种解释呢。虽然你明白我的意思。但可不能带你去拍摄现场呢」",{"rt2"},name={"妃愛"}}}},linknext="block_00080",linkback="block_00078"},block_00010={{"text"},line=61,text={ja={{"「如果我在开始交往之前就用性的眼光看我的妹妹，那我就是个有问题的人了。妃爱不是性的对象，而是我一直想要疼爱的人。」",{"rt2"},name={"智宏"}}}},linknext="block_00011",linkback="block_00009"},block_00034={{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0000",head="hiy_z1b",id=13,size="z1",lv=2.2999999999999998,face="b0053",file="hiy_z1b0200",path=":fg/hiy/z1/"},{"text"},line=180,text={ja={{"「我知道之后还会有更多有趣的事等着我。因为常磐、锦、诗樱学姐、天梨……还有妃爱都在一起。」",{"rt2"},name={"智宏"}}}},linknext="block_00035",linkback="block_00033"},block_00021={{"bg",zoom=100,by=-360,ay=720,speed=150000,x2=640,bx=-640,ax=1280,y2=100,x=-640,y=100,file="zbg004c",path=":bg/"},{"text"},line=123,text={ja={{"「我只是因为是妃爱才说，嗯，就是这样。不过，确实对常磐、锦、天梨和诗樱学姐我都没有什么担心。」",{"rt2"},name={"智宏"}}}},linknext="block_00022",linkback="block_00020"},block_00077={{"text"},line=379,text={vo={{"vo",ch="hiy",file="fem_hiy_00157"}},ja={{"「只是因为这次是诗樱前辈的请求，而且哥哥也参与其中，所以我接受了。但我想我不会继续接受这样的工作。虽然我不能完全断定」",{"rt2"},name={"妃愛"}}}},linknext="block_00078",linkback="block_00076"},block_00004={{"fg",mode=3,ch="妃愛",ex05="hiy_nob0010",head="hiy_nob",id=13,size="no",lv=2.2000000000000002,x=0,face="b0050",file="hiy_nob0210",path=":fg/hiy/no/"},{"text"},line=30,text={vo={{"vo",ch="hiy",file="fem_hiy_00127"}},ja={{"「你更喜欢哪个」",{"rt2"},name={"妃愛"}}}},linknext="block_00005",linkback="block_00003"},block_00017={{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0000",head="hiy_z2b",id=13,size="z2",lv=2.3999999999999999,face="b0048h",hoho="h",file="hiy_z2b0200",path=":fg/hiy/z2/"},{"fgact",act="いいえ",ch="妃愛",id=13,lv=2.3999999999999999},{"text"},line=102,text={vo={{"vo",ch="hiy",file="fem_hiy_00134"}},ja={{"「哥哥在夸我……我能帮到哥哥的忙……真觉得学生会活动好棒。环境就连我也觉得舒服到不能自已～」",{"rt2"},name={"妃愛"}}}},linknext="block_00018",linkback="block_00016"},block_00049={{"text"},line=243,text={ja={{"即使从上周开始走出学生会室，也一定有更好的未来在等待着。已经开始有了可以参与妃爱主演的电影作品的喜悦。",{"rt2"}}}},linknext="block_00050",linkback="block_00048"},block_00075={{"bg",file="zev_hiy_03d",set="ev_hiy_03",time=500,ax=1280,by=-360,bx=-640,x=-390,y=40,ay=720,path=":ev/ev_hiy_03/"},{"text"},line=371,text={ja={{"「话说回来，我真没想到妃爱会接到真人剧的邀请……」",{"rt2"},name={"智宏"}}}},linknext="block_00076",linkback="block_00074"},block_00036={{"text"},line=186,text={ja={{"今天我被诗樱学姐赞扬了，这是很罕见的，让我感觉到自己从六月开始并没有太大的变化。",{"rt2"}}}},linknext="block_00037",linkback="block_00035"},block_00073={{"bg",file="ev_hiy_03d",set="ev_hiy_03",sync=0,face="1",time=400,path=":ev/ev_hiy_03/"},{"text"},line=364,text={vo={{"vo",ch="hiy",file="fem_hiy_00155"}},ja={{"「我怕会露出恋爱中的少女的表情呢」",{"rt2"},name={"妃愛"}}}},linknext="block_00074",linkback="block_00072"},block_00033={{"text"},line=175,text={ja={{"「的确很开心。无论是温泉还是海滩，甚至满身泥巴搬沙袋也很有趣，只要和大家在一起。」",{"rt2"},name={"智宏"}}}},linknext="block_00034",linkback="block_00032"},block_00071={{"text"},line=356,text={vo={{"vo",ch="hiy",file="fem_hiy_00154"}},ja={{"「如果能把这个故事拍成电影，还能得到邀请的话，我会很高兴的……只是我担心在演戏的时候会不自觉地想起哥哥的脸」",{"rt2"},name={"妃愛"}}}},linknext="block_00072",linkback="block_00070"},block_00070={{"bg",file="ev_hiy_03a",set="ev_hiy_03",sync=0,face="1",time=400,path=":ev/ev_hiy_03/"},{"text"},line=353,text={vo={{"vo",ch="hiy",file="fem_hiy_00153"}},ja={{"「虽然现在的流行趋势，或者是同一时间段的热门话题是否能被大热门作品一网打尽，这种事情确实有运气的成分在里面。但是还是希望诗樱前辈的书能卖得好呢」",{"rt2"},name={"妃愛"}}}},linknext="block_00071",linkback="block_00069"},block_00069={{"text"},line=348,text={ja={{"「你几乎每天都满心期待地抱着我。我怎么可能注意到呢」",{"rt2"},name={"智宏"}}}},linknext="block_00070",linkback="block_00068"},block_00068={{"text"},line=345,text={vo={{"vo",ch="hiy",file="fem_hiy_00152"}},ja={{"「我这么满心期待地抱着他，会不会让哥哥觉得我有点奇怪呢，我有点担心……」",{"rt2"},name={"妃愛"}}}},linknext="block_00069",linkback="block_00067"},block_00022={{"text"},line=126,text={ja={{"「但是，正如学姐所说，我现在有种在舒适的小天地里过得太舒服的感觉……就像是有种亲近感或者说封闭性。」",{"rt2"},name={"智宏"}}}},linknext="block_00023",linkback="block_00021"},block_00066={{"text"},line=337,text={ja={{"「哎，那也没办法……毕竟是被前辈这么请求的嘛」",{"rt2"},name={"智宏"}}}},linknext="block_00067",linkback="block_00065"},block_00029={{"text"},line=155,text={ja={{"「真直接」",{"rt2"},name={"智宏"}}}},linknext="block_00030",linkback="block_00028"},block_00064={{"text"},line=329,text={ja={{"这已经不只是温水浴了，简直就是热乎乎的温泉啊。我多次抚摸着蹭过来的妹妹的头。",{"rt2"}}}},linknext="block_00065",linkback="block_00063"},block_00009={{"fg",mode=3,ch="妃愛",ex05="hiy_noa0000",head="hiy_noa",id=13,size="no",lv=2.2000000000000002,x=0,face="a0003",file="hiy_noa0200",path=":fg/hiy/no/"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00130"}},ja={{"「你不是喜欢外表像诗樱学姐，性格像锦那样的女生吗？我既不苗条也不是天使呀」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_noa0000",head="hiy_noa",id=13,size="no",lv=2.2000000000000002,x=0,face="a0028",file="hiy_noa0200",path=":fg/hiy/no/"}}},line=58,linknext="block_00010",linkback="block_00008"},block_00037={{"text"},line=188,text={ja={{"我觉得舒适的地方，只是从自己的房间变成了学生会室。看来这种感觉会持续到再来年毕业前后。",{"rt2"}}}},linknext="block_00038",linkback="block_00036"},block_00041={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",file="zbg004c",zoom=120,by=-360,bx=-640,sync=0,ax=1280,ay=720,path=":bg/"},{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0000",head="hiy_z2b",sync=0,resize=1,id=13,size="z2",lv=2.3999999999999999,face="b0054",file="hiy_z2b0200",path=":fg/hiy/z2/"},{"extrans",time=500},{"text"},line=211,text={ja={{"妃爱紧紧地抱住了我。我想，她可能是想要抱住我，但是被压制住的手无法伸展开来。",{"rt2"}}}},linknext="block_00042",linkback="block_00040"},block_00063={{"bg",set="ev_hiy_03",file="ev_hiy_03a",path=":ev/ev_hiy_03/"},{"text"},line=327,text={ja={{"然后在上床之后，我们彼此紧紧地抱在了一起。",{"rt2"}}}},linknext="block_00064",linkback="block_00062"},block_00031={{"text"},line=165,text={ja={{"「别这么说。想象一下，我，我会去脸红的。」",{"rt2"},name={"智宏"}}}},linknext="block_00032",linkback="block_00030"},block_00058={{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0010",head="hiy_z1b",id=13,size="z1",lv=2.2999999999999998,face="b0045",file="hiy_z1b0210",path=":fg/hiy/z1/"},{"se",id=1,file="seガーン"},{"text"},line=294,text={ja={{"「啊啊啊啊对不起，资正，回家后我一直没有理你！」",{"rt2"},name={"智宏"}}}},linknext="block_00059",linkback="block_00057"},block_00057={{"fg",mode=1,ch="資正",head="suk_noa",id=20,size="no",lv=2.2000000000000002,face="a0003",file="suk_noa0000",path=":fg/suk/no/"},{"se",id=1,file="seドア(室内開き)"},{"text"},line=287,text={vo={{"vo",ch="suk",file="etc_suk_00003"}},ja={{"「呜～」",{"rt2"},name={"資正"}}}},linknext="block_00058",linkback="block_00056"},block_00053={{"fg",mode=3,ch="妃愛",ex05="hiy_z1a0000",head="hiy_z1a",id=13,size="z1",lv=2.2999999999999998,face="a0038",file="hiy_z1a0200",path=":fg/hiy/z1/"},{"fgact",act="おじぎ",ch="妃愛",id=13,lv=2.2999999999999998},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00147"}},ja={{"「虽然刚才还觉得胃已经满满的，可是和哥哥聊完天后突然间就饿了……想吃点什么？」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_z1a0000",head="hiy_z1a",id=13,size="z1",lv=2.2999999999999998,face="a0053",file="hiy_z1a0200",path=":fg/hiy/z1/"}}},line=269,linknext="block_00054",linkback="block_00052"},block_00056={{"text"},line=281,text={ja={{"「啊，嗯，如果是这样的话。我也会陪着妃爱一起。嗯，我们吃吧，我想吃。」",{"rt2"},name={"智宏"}}}},linknext="block_00057",linkback="block_00055"},block_00030={{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0000",head="hiy_z1b",id=13,size="z1",lv=2.2999999999999998,face="b0053",file="hiy_z1b0200",path=":fg/hiy/z1/"},{"fgact",act="おじぎ",ch="妃愛",id=13,lv=2.2999999999999998},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00138"}},ja={{"「哥哥在加入学生会之前就喜欢泡温水浴。这半年间，原本一个人的浴室现在多了几个好朋友而已。」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0000",head="hiy_z1b",id=13,size="z1",lv=2.2999999999999998,face="b0055",file="hiy_z1b0200",path=":fg/hiy/z1/"}}},line=162,linknext="block_00031",linkback="block_00029"},block_00007={{"text"},line=45,text={ja={{"「我说了，是妃爱」",{"rt2"},name={"智宏"}}}},linknext="block_00008",linkback="block_00006"},block_00076={{"bg",file="zev_hiy_03e",sync=0,time=400,ax=1280,y=40,ay=720,by=-360,bx=-640,x=-390,face="1",set="ev_hiy_03",path=":ev/ev_hiy_03/"},{"text"},line=376,text={vo={{"vo",ch="hiy",file="fem_hiy_00156"}},ja={{"「是吗？我并没有抵触感。虽然去公司打招呼或者向帮助过我的人、将来可能帮助我的人问候这些事情很麻烦」",{"rt2"},name={"妃愛"}}}},linknext="block_00077",linkback="block_00075"},block_00018={{"se",id=1,stop=1,time=500},{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0000",head="hiy_z2b",id=13,size="z2",lv=2.3999999999999999,face="b0013",file="hiy_z2b0200",path=":fg/hiy/z2/"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00135"}},ja={{"「神田同学和饭田同学都是和常磐或锦一样的好人……感觉这一年又会变得很愉快呢。」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0000",head="hiy_z2b",id=13,size="z2",lv=2.3999999999999999,face="b0054",file="hiy_z2b0200",path=":fg/hiy/z2/"}}},line=110,linknext="block_00019",linkback="block_00017"},block_00020={{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0000",head="hiy_z2b",id=13,size="z2",lv=2.3999999999999999,face="b0010",file="hiy_z2b0200",path=":fg/hiy/z2/"},{"text"},line=118,text={vo={{"vo",ch="hiy",file="fem_hiy_00136"}},ja={{"「呐？你又开始担心如果现在开心的日常被打破的话该怎么办了吗？」",{"rt2"},name={"妃愛"}}}},linknext="block_00021",linkback="block_00019"},block_00001={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",sync=0,file="bg004c",path=":bg/"},{"fg",mode=3,ch="妃愛",ex05="hiy_noa0000",head="hiy_noa",sync=0,id=13,size="no",lv=2.2000000000000002,x=0,face="a0043",file="hiy_noa0200",path=":fg/hiy/no/"},{"extrans",rule="rule_002r",vague=40},{"bgm",file="bgm01"},{"text"},line=20,text={vo={{"vo",ch="hiy",file="fem_hiy_00126"}},ja={{"「那么，哥哥。今天发生了这么多事情」",{"rt2"},name={"妃愛"}}}},linknext="block_00002",linkback="block_00000"},block_00080={{"bg",file="zev_hiy_03c",sync=0,time=400,ax=1280,y=40,ay=720,by=-360,bx=-640,x=-390,face="1",set="ev_hiy_03",path=":ev/ev_hiy_03/"},{"text"},line=392,text={vo={{"vo",ch="hiy",file="fem_hiy_00159"}},ja={{"「只是当时为了不破坏气氛，我没有说出来，电影的演员阵容其实是非常棘手的，涉及到很多大人们的考量，所以期望值大约只有30%吧」",{"rt2"},name={"妃愛"}}}},linknext="block_00081",linkback="block_00079"},block_00014={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",file="zbg004c",zoom=120,by=-360,bx=-640,sync=0,ax=1280,ay=720,path=":bg/"},{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0000",head="hiy_z2b",sync=0,resize=1,id=13,size="z2",lv=2.3999999999999999,face="b0048",file="hiy_z2b0200",path=":fg/hiy/z2/"},{"extrans",time=500},{"se",id=1,file="se抱きつく"},{"text"},line=88,text={ja={{"她毫无防备地放松了胳膊，转向我，于是我毫不犹豫地紧紧抱住了她。从胸口传来了一阵令人不安的笑声。",{"rt2"}}}},linknext="block_00015",linkback="block_00013"},block_00042={{"fg",mode=3,ch="妃愛",ex05="hiy_z2b0000",head="hiy_z2b",id=13,size="z2",lv=2.3999999999999999,face="b0013",file="hiy_z2b0200",path=":fg/hiy/z2/"},{"text"},line=216,text={vo={{"vo",ch="hiy",file="fem_hiy_00142"}},ja={{"「如果你觉得和大家一起泡温泉很开心，那么想象一下如果神田和饭田也加入我们，应该会更加开心吧。」",{"rt2"},name={"妃愛"}}}},linknext="block_00043",linkback="block_00041"},block_00032={{"fg",mode=3,ch="妃愛",ex05="hiy_z1a0000",head="hiy_z1a",id=13,size="z1",lv=2.2999999999999998,face="a0043",file="hiy_z1a0200",path=":fg/hiy/z1/"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00139"}},ja={{"「哥哥实际上和大家一起洗过澡了。和大家一起泡温泉很有趣吧？不是指泡澡本身，而是旅行的意义。」",{"rt2"},name={"妃愛"}}}},delay={vl02={{"fg",mode=3,ch="妃愛",ex05="hiy_z1a0000",head="hiy_z1a",id=13,size="z1",lv=2.2999999999999998,face="a0025",file="hiy_z1a0200",path=":fg/hiy/z1/"}},vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_z1a0000",head="hiy_z1a",id=13,size="z1",lv=2.2999999999999998,face="a0053",file="hiy_z1a0200",path=":fg/hiy/z1/"}}},line=172,linknext="block_00033",linkback="block_00031"},block_00035={{"text"},line=184,text={ja={{"「现在的感觉就像回程中的火车上，望着窗外的风景，只是那一瞬间有些感伤罢了。」",{"rt2"},name={"智宏"}}}},linknext="block_00036",linkback="block_00034"},block_00072={{"text"},line=359,text={ja={{"「为什么要担心？」",{"rt2"},name={"智宏"}}}},linknext="block_00073",linkback="block_00071"},block_00038={{"fg",mode=3,ch="妃愛",ex05="hiy_z1b0000",head="hiy_z1b",id=13,size="z1",lv=2.2999999999999998,face="b0031",file="hiy_z1b0200",path=":fg/hiy/z1/"},{"fgact",act="いいえ",ch="妃愛",id=13,lv=2.2999999999999998},{"text"},line=194,text={vo={{"vo",ch="hiy",file="fem_hiy_00140"}},ja={{"「哥哥你这种感伤的心情，其实每个人心中都藏着，所以不必自卑。」",{"rt2"},name={"妃愛"}}}},linknext="block_00039",linkback="block_00037"},block_00027={{"text"},line=140,text={ja={{"「就好像我用了两年的手机坏了，虽然我可以再买同样的型号，但既然要花同样的钱，那就买最新的型号吧。」",{"rt2"},name={"智宏"}}}},linknext="block_00028",linkback="block_00026"},block_00025={{"text"},line=134,text={ja={{"「生活变得更加热闹了，工作也更容易进行了，就像妃爱刚刚说的那样，我真的很期待接下来一年的生活……是的，我现在的心情其实非常积极。」",{"rt2"},name={"智宏"}}}},linknext="block_00026",linkback="block_00024"},block_00016={{"text"},line=96,text={ja={{"「辛苦一整天了。多亏了妃爱，大家的聊天可以如此顺利。你在这里真是帮大忙了。」",{"rt2"},name={"智宏"}}}},linknext="block_00017",linkback="block_00015"},block_00023={{"text"},line=129,text={ja={{"「两个新人都很好，我觉得他们会很快适应生徒会的……但另一方面，也会有越来越多的事情不能像以前那样做了。」",{"rt2"},name={"智宏"}}}},linknext="block_00024",linkback="block_00022"},block_00008={{"fg",mode=3,ch="妃愛",ex05="hiy_noa0010",head="hiy_noa",id=13,size="no",lv=2.2000000000000002,x=0,face="a0043",file="hiy_noa0210",path=":fg/hiy/no/"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_00129"}},ja={{"「哎呀～虽然我知道哥哥对我很好，但我也清楚我并不是你的理想型呢～」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",mode=3,ch="妃愛",ex05="hiy_noa0000",head="hiy_noa",id=13,size="no",lv=2.2000000000000002,x=0,face="a0018",file="hiy_noa0200",path=":fg/hiy/no/"},{"fgact",act="おじぎ",ch="妃愛",id=13,lv=2.2000000000000002}}},line=52,linknext="block_00009",linkback="block_00007"},block_00005={{"text"},line=33,text={ja={{"「妃爱」",{"rt2"},name={"智宏"}}}},linknext="block_00006",linkback="block_00004"}}