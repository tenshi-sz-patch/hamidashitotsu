astver = 2.0
ast = {block_00029={{"text"},line=226,linkback="block_00028",linknext="block_00030",text={ja={{"从受里姐姐之托让妃爱来上学开始，再到被诗樱前辈劝说，把让妃爱交到朋友作为目标。",{"rt2"}}}}},block_00088={{"fg",file="hiy_nob0200",ch="妃愛",lv=2.2000000000000002,mode=1,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0027",id=20,ex05="hiy_nob0000"},{"text"},linkback="block_00087",delay={vl01={{"fg",file="hiy_nob0200",ch="妃愛",lv=2.2000000000000002,mode=1,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0053",id=20,ex05="hiy_nob0000"}}},line=631,linknext="block_00089",text={ja={{"「嗯，原来如此。这可能是个好机会。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00686",ch="hiy"}}}},block_00082={{"text"},line=589,linkback="block_00081",linknext="block_00083",text={ja={{"「嗯，我可不打算硬逼不感兴趣的人认识这个。本来就只是学生的帮手而已……」",{"rt2"},name={"智宏"}}}}},block_00093={{"bgm",file="bgm15"},{"fg",file="hiy_nob0200",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0053",x=160,id=12,ex05="hiy_nob0000"},{"text"},linkback="block_00092",delay={vl01={{"fg",file="hiy_nob0200",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0032",x=160,id=12,ex05="hiy_nob0000"},{"fgact",lv=2.2000000000000002,ch="妃愛",act="いいえ",id=12}},vl02={{"fg",file="hiy_nob0200",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0054",x=160,id=12,ex05="hiy_nob0000"},{"fgact",lv=2.2000000000000002,ch="妃愛",act="おじぎ",id=12}}},line=670,linknext="block_00094",text={ja={{"「哥哥……不，前任学生会的前辈们，这一年你们辛苦了。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00688",ch="hiy"}}}},block_00090={{"fg",file="hiy_noa0200",ch="妃愛",lv=2.2000000000000002,mode=1,size="no",path=":fg/hiy/no/",head="hiy_noa",face="a0054",id=20,ex05="hiy_noa0000"},{"fg",ch="あすみ",lv=2.2999999999999998,mode=-1,id=8,mx=40},{"text"},line=643,linkback="block_00089",linknext="block_00091",text={ja={{"「锦，你过来这边。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00687",ch="hiy"}}}},block_00027={{"fg",file="hiy_nob0200",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0053",x=-160,id=11,ex05="hiy_nob0000"},{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=3,size="no",path=":fg/ina/no/",head="ina_noa",face="a0053",x=-480,id=5,ex05="ina_noa0000"},{"fg",file="asu_noa0210",ch="あすみ",lv=2.2000000000000002,mode=3,size="no",path=":fg/asu/no/",head="asu_noa",face="a0053",x=160,id=12,ex05="asu_noa0010"},{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=3,size="no",path=":fg/kna/no/",head="kna_noa",face="a0053",x=480,id=6,ex05="kna_noa0000"},{"text"},line=222,linkback="block_00026",linknext="block_00028",text={ja={{"如果接任的是陌生人，我可能会更寂寞。但我很庆幸接任者是妃爱，我的喜悦超过了寂寞。",{"rt2"}}}}},block_00007={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0052",x=0,id=13,ex05="hiy_z1b0000"},{"text"},linkback="block_00006",delay={vl01={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0031",x=0,id=13,ex05="hiy_z1b0000"}},vl02={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0054",x=0,id=13,ex05="hiy_z1b0000"}}},line=79,linknext="block_00008",text={ja={{"「通过志愿者活动与社区人民的交流，通过支持各种俱乐部活动与他们的交往……以及与同在学生会的伙伴们不可替代的团结……」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00675",ch="hiy"}}}},block_00001={{"fg",file="mir_z1a0200",ch="里",lv=2.2999999999999998,mode=3,size="z1",path=":fg/mir/z1/",head="mir_z1a",face="a0006",x=0,id=13,ex05="mir_z1a0000"},{"text"},linkback="block_00000",delay={vl01={{"fg",file="mir_z1a0200",ch="里",lv=2.2999999999999998,mode=3,size="z1",path=":fg/mir/z1/",head="mir_z1a",face="a0054",x=0,id=13,ex05="mir_z1a0000"}},vl02={{"fg",file="mir_z1a0200",ch="里",lv=2.2999999999999998,mode=3,size="z1",path=":fg/mir/z1/",head="mir_z1a",face="a0052",x=0,id=13,ex05="mir_z1a0000"}}},line=30,linknext="block_00002",text={ja={{"「嗯，至少不是零投票，这点让我很开心！我希望大家能把她们当作大麻学园自主性的象征，热烈欢迎她们！好了，一起来做个自我介绍吧」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00071",ch="mir"}}}},block_00157={{"bg",file="ev_hiy_35_01",face="1",time=400,path=":ev/ev_hiy_35/",sync=0,set="ev_hiy_35"},{"text"},line=1043,linkback="block_00156",linknext="block_00158",text={ja={{"「不，这次的复杂并不是让人烦躁的那种复杂。而是让我想起了很多事情，就是这种意思的复杂。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00704",ch="hiy"}}}},block_00022={{"fg",file="kna_z1a0200",ch="神田",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kna/z1/",head="kna_z1a",face="a0006",x=430,id=8,ex05="kna_z1a0000"},{"text"},line=182,linkback="block_00021",linknext="block_00023",text={ja={{"「另外，我也很喜欢上任会长！」",{"rt2"},name={"神田 & 柑凪"}}},vo={{"vo",file="fem_kna_00093",ch="kna"}}}},block_00095={{"fg",file="hiy_nob0200",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0006",x=160,id=12,ex05="hiy_nob0000"},{"fgact",lv=2.2000000000000002,ch="妃愛",act="うなづく",id=12},{"text"},linkback="block_00094",delay={vl01={{"fg",file="hiy_nob0210",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0032",x=160,id=12,ex05="hiy_nob0010"}},vl02={{"fg",file="hiy_nob0210",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0041",x=160,id=12,ex05="hiy_nob0010"}}},line=686,linknext="block_00096",text={ja={{"「接下来就交给我吧！不过，如果你们有空过来玩的话，顺便帮帮我吧！」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00690",ch="hiy"}}}},block_00035={{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=3,size="no",path=":fg/kna/no/",head="kna_noa",face="a0040",x=480,id=6,ex05="kna_noa0000"},{"fgact",lv=2.2000000000000002,ch="神田",act="びっくり",id=6},{"text"},linkback="block_00034",delay={vl03={{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=3,size="no",path=":fg/kna/no/",head="kna_noa",face="a0052",x=480,id=6,ex05="kna_noa0000"}},vl04={{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=3,size="no",path=":fg/kna/no/",head="kna_noa",face="a0041",x=480,id=6,ex05="kna_noa0000"}},vl01={{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=3,size="no",path=":fg/kna/no/",head="kna_noa",face="a0046",x=480,id=6,ex05="kna_noa0000"},{"fgact",lv=2.2000000000000002,ch="神田",act="いいえ",id=6}},vl02={{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=3,size="no",path=":fg/kna/no/",head="kna_noa",face="a0054",x=480,id=6,ex05="kna_noa0000"}}},line=283,linknext="block_00036",text={ja={{"「啊，和泉会长……不对，是和泉前辈！我刚才的致辞做得好吗？请夸奖我一下！」",{"rt2"},name={"神田 & 柑凪"}}},vo={{"vo",file="fem_kna_00094",ch="kna"}}}},block_00166={{"text"},line=1075,linkback="block_00165",linknext="block_00167",text={ja={{"虽然门槛很高，也不建议大众效仿，但如果有机会写自传，我会在最后一行写上这样的话：「和妹妹的恋爱是最棒的」。",{"rt2"}}}}},block_00013={{"text"},line=113,linkback="block_00012",linknext="block_00014",text={ja={{"今天作为哥哥，我感到的震撼甚至超过了感慨。妃爱真是太棒了。虽然长得那么可爱，但却真正地显得如此坚毅。",{"rt2"}}}}},block_00152={{"text"},line=1027,linkback="block_00151",linknext="block_00153",text={ja={{"「不过话说回来，当妃爱还是个童星的时候，如果她向陌生人喊着'爸爸' '妈妈'，我们的父母们看到也会有和我现在一样的感觉吧。」",{"rt2"},name={"智宏"}}}}},block_00044={{"fg",file="asu_nob0200",ch="あすみ",lv=2.2000000000000002,mode=3,size="no",path=":fg/asu/no/",head="asu_nob",face="b0054",x=-280,id=11,ex05="asu_nob0000"},{"text"},linkback="block_00043",delay={vl01={{"fg",file="asu_nob0210",ch="あすみ",lv=2.2000000000000002,mode=3,size="no",path=":fg/asu/no/",head="asu_nob",face="b0006",x=-280,id=11,ex05="asu_nob0010"}}},line=328,linknext="block_00045",text={ja={{"「现在已经彻底从章鱼烧聚会变成了烧饼聚会……下次我们试试中式包子聚会吧。」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00115",ch="asu"}}}},block_00017={{"fg",ch="妃愛",lv=2.2999999999999998,mode=-1,id=13,mx=-40},{"text"},line=136,linkback="block_00016",linknext="block_00018",text={ja={{"然而妃爱向来出色的表态在她多余的注解之下显得平凡了许多。很明显这部分完全没有必要吧？ 不过不知为何，妃爱看起来很满意。",{"rt2"}}}}},block_00041={{"text"},line=309,linkback="block_00040",linknext="block_00042",text={ja={{"「你说的「以来」，其实中间只有一个并没有被称赞的学生会长吧。」",{"rt2"},name={"智宏"}}}}},block_00134={{"fg",hoho="h",ch="あすみ",resize=1,lv=2.2000000000000002,mode=1,size="no",path=":fg/asu/no/",head="asu_noa",face="a0010h",id=20,ex05="asu_noa0000",file="asu_noa0200"},{"fg",hoho="h",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0032h",id=12,ex05="hiy_z1b0000",file="hiy_z1b0200"},{"text"},linkback="block_00133",delay={vl01={{"fg",hoho="h",ch="あすみ",lv=2.2000000000000002,mode=1,size="no",path=":fg/asu/no/",head="asu_nob",face="b0034h",id=20,ex05="asu_nob0000",file="asu_nob0200"}}},line=942,linknext="block_00135",text={ja={{"「如果、如果大家都要的话……我、我也可以接受……吧？」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00123",ch="asu"}}}},block_00064={{"se",file="se扉開く(スライド)教室",id=1},{"fg",file="kan_z1a0200",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1a",face="a0008",x=0,id=9,x2=-280,ex05="kan_z1a0000"},{"fg",file="ame_z1a0200",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1a",face="a0008",x=430,id=10,x2=280,ex05="ame_z1a0000"},{"fg",file="shi_z1b0510",ch="詩桜",resize=1,lv=2.2999999999999998,mode=3,size="z1",mx=-40,path=":fg/shi/z1/",head="shi_z1b",face="b0015",x=-430,id=7,ex05="shi_z1b0010"},{"text"},linkback="block_00063",delay={vl01={{"fg",file="shi_z1b0500",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1b",face="b0025",x=-430,id=7,ex05="shi_z1b0000"}},vl02={{"fg",file="shi_z1b0500",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1b",face="b0031",x=-430,id=7,ex05="shi_z1b0000"}}},line=488,linknext="block_00065",text={ja={{"「嘿，你们，我也来观摩学生大会了。与去年那个紧张得僵硬的某人不同，你们发表的致辞非常镇定自若，真好。」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00216",ch="shi"}}}},block_00002={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="zbg013a",ax=1280,by=-360,x=-200,bx=-640,ay=720,sync=0,path=":bg/"},{"fg",file="hiy_z1a0200",ch="妃愛",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1a",face="a0052",x=0,id=13,sync=0,ex05="hiy_z1a0000"},{"extrans",time=500},{"text"},linkback="block_00001",delay={vl01={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0032",x=0,id=13,ex05="hiy_z1b0000"}},vl02={{"fgact",lv=2.2999999999999998,ch="妃愛",act="おじぎ",id=13}}},line=59,linknext="block_00003",text={ja={{"「这次，我和泉妃爱被选为学生会长。大家，多多指教」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00673",ch="hiy"}}}},block_00099={{"text"},line=718,linkback="block_00098",linknext="block_00100",text={ja={{"辛苦了，我自己。从现在开始，作为妃爱的哥哥兼丈夫，我要悠闲地走向一个无波澜的幸福生活。",{"rt2"}}}}},block_00073={{"fg",file="shi_z1b0510",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1b",face="b0055",x=-430,id=7,ex05="shi_z1b0010"},{"text"},linkback="block_00072",delay={vl01={{"fg",file="shi_z1b0510",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1b",face="b0053",x=-430,id=7,ex05="shi_z1b0010"}}},line=541,linknext="block_00074",text={ja={{"「既然是个好机会，和泉老师建议我以旁观者的身份来看一下学生大会。所以我就来看了。」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00219",ch="shi"}}}},block_00026={{"text"},line=215,linkback="block_00025",linknext="block_00027",text={ja={{"虽然这样的就职发言让人有些疑惑，但看着台上四人齐齐低下头，我突然感到了安心，同时也有一丝寂寞。",{"rt2"}}}}},block_00156={{"text"},line=1038,linkback="block_00155",linknext="block_00157",text={ja={{"「这个妹妹无论做什么都那么复杂。究竟是为了什么才来的，现在就回去吧。」",{"rt2"},name={"智宏"}}}}},block_00147={{"text"},line=1015,linkback="block_00146",linknext="block_00148",text={ja={{"「哥哥你怎么想？我在演技中叫别人哥哥，你完全不在意吗？」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00702",ch="hiy"}}}},block_00051={{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=1,size="no",path=":fg/kna/no/",head="kna_noa",face="a0054",id=20,ex05="kna_noa0000"},{"text"},line=383,linkback="block_00050",linknext="block_00052",text={ja={{"「也许是感到麻烦，有时候会直接称呼她为「小西」。」",{"rt2"},name={"神田 & 柑凪"}}},vo={{"vo",file="fem_kna_00095",ch="kna"}}}},block_00045={{"text"},line=330,linkback="block_00044",linknext="block_00046",text={ja={{"当每个人都拿到自己喜欢的江户川烧饼时，妃爱突然在向她常坐的座位走去的途中停下了脚步。",{"rt2"}}}}},block_00009={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0052",x=0,id=13,ex05="hiy_z1b0000"},{"text"},line=89,linkback="block_00008",linknext="block_00010",text={ja={{"「具体来说，我将倾听大家的声音，与老师们进行协调，与附近学校和社区居民交流，并将每一次体验与大家共享。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00677",ch="hiy"}}}},block_00011={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="bg013a",sync=0,path=":bg/"},{"fg",file="hiy_nob0200",ch="妃愛",resize=1,lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0053",id=13,sync=0,ex05="hiy_nob0000"},{"bg",file="cinema01",lv=1,sync=0,path=":sys/",time=400,id=5,rule="cinema",vague=40,alpha=192},{"extrans",time=500},{"se",file="se拍手2",id=1},{"text"},line=107,linkback="block_00010",linknext="block_00012",text={ja={{"她的演讲内容比我想象中来得更有力量，一开始是几秒钟的沉默，然后立刻引发了热烈的掌声。去年我发言的时候，好像并没有这样的掌声。",{"rt2"}}}}},block_00153={{"text"},line=1030,linkback="block_00152",linknext="block_00154",text={ja={{"「所以我不能说只有我才会感到不舒服。这条路是妃爱的家人都走过来的。」",{"rt2"},name={"智宏"}}}}},block_00000={{"savetitle",text="しあわせのひよこ"},{"msgoff"},{"bg",file="zbg013a",ax=1280,by=-360,x=600,bx=-640,ay=720,sync=0,path=":bg/"},{"fg",file="mir_z1a0210",ch="里",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/mir/z1/",head="mir_z1a",face="a0052",x=0,id=13,sync=0,ex05="mir_z1a0010"},{"extrans",time=2000},{"bgm",file="bgm06"},{"text"},delay={vl01={{"fg",file="mir_z1a0210",ch="里",lv=2.2999999999999998,mode=3,size="z1",path=":fg/mir/z1/",head="mir_z1a",face="a0009",x=0,id=13,ex05="mir_z1a0010"}}},line=23,linknext="block_00001",text={ja={{"「嗯，接下来我要宣布学生会选举的结果，和去年一样，每个职位只有一个人参选」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00070",ch="mir"}}}},block_00052={{"text"},line=386,linkback="block_00051",linknext="block_00053",text={ja={{"「谁……？」",{"rt2"},name={"智宏"}}}}},block_00067={{"fg",file="kan_z1a0210",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1a",face="a0045",x=0,id=9,ex05="kan_z1a0010"},{"fgact",lv=2.2999999999999998,ch="華乃",act="ジャンプ",id=9},{"text"},line=507,linkback="block_00066",linknext="block_00068",text={ja={{"「你这家伙，到底站在谁那边！？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00117",ch="kan"}}}},block_00003={{"text"},line=61,linkback="block_00002",linknext="block_00004",text={ja={{"这情景让我回想起一年前。唯一的区别就是，去年是我上台，而今年换成了妃爱。",{"rt2"}}}}},block_00103={{"fg",hoho="h",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0021h",x=280,id=10,ex05="kan_z1b0000",file="kan_z1b0200"},{"fgact",lv=2.2999999999999998,ch="華乃",act="びっくり",id=10},{"text"},linkback="block_00102",delay={vl03={{"fg",hoho="h",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0030h",x=280,id=10,ex05="kan_z1b0000",file="kan_z1b0200"}},vl01={{"fg",hoho="h",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0005h",x=280,id=10,ex05="kan_z1b0000",file="kan_z1b0200"}},vl02={{"fgact",lv=2.2999999999999998,ch="華乃",act="いいえ",id=10}}},line=756,linknext="block_00104",text={ja={{"「啊……等等，啊，这太糟了，等等，呜……这是什么？ 呜哇，呜咕，等等我不行了，我在哭……」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00120",ch="kan"}}}},block_00018={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="zbg013a",ax=1280,by=-360,x=-200,bx=-640,ay=720,sync=0,path=":bg/"},{"fg",file="ina_z1a0200",ch="飯田",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/ina/z1/",head="ina_z1a",face="a0053",x=-430,id=7,sync=0,ex05="ina_z1a0000"},{"fg",file="asu_z1a0200",ch="あすみ",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1a",face="a0053",x=0,id=13,sync=0,ex05="asu_z1a0000"},{"fg",file="kna_z1a0200",ch="神田",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/kna/z1/",head="kna_z1a",face="a0015",x=430,id=8,sync=0,ex05="kna_z1a0000"},{"extrans",vague=40,rule="rule_001"},{"fgact",lv=2.2999999999999998,ch="あすみ",act="おじぎ",id=13},{"text"},linkback="block_00017",delay={vl01={{"fg",file="asu_z1a0210",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1a",face="a0052",x=0,id=13,ex05="asu_z1a0010"}}},line=152,linknext="block_00019",text={ja={{"「副会长锦，在上一届学生会中我担任了书记。这次我决定参选，原因是想要支持我的朋友和泉。」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00112",ch="asu"}}}},block_00124={{"text"},line=877,linkback="block_00123",linknext="block_00125",text={ja={{"如果是普通的兄妹可能会退缩，但是我们是特别亲近……好、好吧！",{"rt2"}}}}},block_00025={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="bg013a",sync=0,path=":bg/"},{"fg",file="hiy_nob0200",ch="妃愛",resize=1,lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0032",x=-160,id=11,sync=0,ex05="hiy_nob0000"},{"fg",file="ina_noa0200",ch="飯田",resize=1,lv=2.2000000000000002,mode=3,size="no",path=":fg/ina/no/",head="ina_noa",face="a0032",x=-480,id=5,sync=0,ex05="ina_noa0000"},{"fg",file="asu_noa0210",ch="あすみ",resize=1,lv=2.2000000000000002,mode=3,size="no",path=":fg/asu/no/",head="asu_noa",face="a0032",x=160,id=12,sync=0,ex05="asu_noa0010"},{"fg",file="kna_noa0200",ch="神田",resize=1,lv=2.2000000000000002,mode=3,size="no",path=":fg/kna/no/",head="kna_noa",face="a0032",x=480,id=6,sync=0,ex05="kna_noa0000"},{"bg",file="cinema01",lv=1,sync=0,path=":sys/",time=400,id=5,rule="cinema",vague=40,alpha=192},{"extrans",time=500},{"fgact",lv=2.2000000000000002,ch="妃愛",act="おじぎ",id=11},{"fgact",lv=2.2000000000000002,ch="あすみ",act="おじぎ",id=12},{"fgact",lv=2.2000000000000002,ch="飯田",act="おじぎ",id=5},{"fgact",lv=2.2000000000000002,ch="神田",act="おじぎ",id=6},{"text"},line=213,linkback="block_00024",linknext="block_00026",text={ja={{"妃爱的发言中，提到上任会长还能算作是「需要超越的目标」，但其他三人提起他就完全没有必要了吧？难道是故意找茬？",{"rt2"}}}}},block_00053={{"fg",file="mir_noa0210",ch="里",lv=2.2000000000000002,mode=1,size="no",path=":fg/mir/no/",head="mir_noa",face="a0054",id=20,ex05="mir_noa0010"},{"text"},linkback="block_00052",delay={vl01={{"fg",file="mir_noa0200",ch="里",lv=2.2000000000000002,mode=1,size="no",path=":fg/mir/no/",head="mir_noa",face="a0020",id=20,ex05="mir_noa0000"}},vl02={{"fg",file="mir_noa0200",ch="里",lv=2.2000000000000002,mode=1,size="no",path=":fg/mir/no/",head="mir_noa",face="a0031",id=20,ex05="mir_noa0000"}}},line=393,linknext="block_00054",text={ja={{"「啊，大家比以前更亲近了呢。虽然担心没有朋友会怎么样，但事实上大家都更自由了。」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00074",ch="mir"}}}},block_00127={{"text"},line=901,linkback="block_00126",linknext="block_00128",text={ja={{"她似乎是一半期待一半放弃的心情，结果真的发生了，妃爱高兴得像是被激动到了一样发出了喜悦的声音。",{"rt2"}}}}},block_00006={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0053",x=0,id=13,ex05="hiy_z1b0000"},{"text"},linkback="block_00005",delay={vl01={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0032",x=0,id=13,ex05="hiy_z1b0000"}},vl02={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0055",x=0,id=13,ex05="hiy_z1b0000"}}},line=72,linknext="block_00007",text={ja={{"「我曾作为临时副会长参加过半年的学生会活动。那时的各种活动让我有了再次参加学生会的想法，那是非常充实的体验。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00674",ch="hiy"}}}},block_00033={{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=3,size="no",path=":fg/ina/no/",head="ina_noa",face="a0052",x=-480,id=5,ex05="ina_noa0000"},{"fg",file="asu_nob0200",ch="あすみ",lv=2.2000000000000002,mode=3,size="no",path=":fg/asu/no/",head="asu_nob",face="b0052",x=-160,id=11,ex05="asu_nob0000"},{"fg",file="hiy_noa0200",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_noa",face="a0052",x=160,id=12,ex05="hiy_noa0000"},{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=3,size="no",path=":fg/kna/no/",head="kna_noa",face="a0052",x=480,id=6,ex05="kna_noa0000"},{"fgact",lv=2.2000000000000002,ch="妃愛",act="びっくり",id=12},{"text"},linkback="block_00032",delay={vl01={{"fg",file="hiy_noa0210",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_noa",face="a0055",x=160,id=12,ex05="hiy_noa0010"}}},line=264,linknext="block_00034",text={ja={{"「啊，哥哥！你来了，谢谢你！」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00680",ch="hiy"}}}},block_00039={{"text"},line=295,linkback="block_00038",linknext="block_00040",text={ja={{"太好了……我可以继续和这些孩子们在一起……",{"rt2"}}}}},block_00116={{"text"},line=850,linkback="block_00115",linknext="block_00117",text={ja={{"其实我很想抱她，但在大家面前我实在做不出来。那就只能这样了。",{"rt2"}}}}},block_00065={{"se",stop=1,id=1,time=500},{"fg",file="kan_z1b0210",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0009",x=0,id=9,ex05="kan_z1b0010"},{"fg",file="ame_z1b0200",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1b",face="b0053",x=430,id=10,ex05="ame_z1b0000"},{"fgact",lv=2.2999999999999998,ch="華乃",act="びっくり",id=9},{"text"},linkback="block_00064",delay={vl01={{"fg",file="kan_z1b0200",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0038",x=0,id=9,ex05="kan_z1b0000"}}},line=498,linknext="block_00066",text={ja={{"「等等……紧张得瑟瑟发抖，你说的是谁？我可没有！？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00116",ch="kan"}}}},block_00083={{"text"},line=592,linkback="block_00082",linknext="block_00084",text={ja={{"「啊，但是如果这样能让大家的辛劳得到回报，那么至少让他们知道我们部份的工作也好吧？」",{"rt2"},name={"智宏"}}}}},block_00130={{"text"},line=919,linkback="block_00129",linknext="block_00131",text={ja={{"但是她并没有把这当做玩笑来处理，反而露出了恋人般的表情。在大家面前露出这种表情可不行啊。",{"rt2"}}}}},block_00091={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="bg011a",sync=0,path=":bg/"},{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=3,size="no",path=":fg/ina/no/",head="ina_noa",face="a0001",x=-480,id=5,sync=0,ex05="ina_noa0000"},{"fg",file="asu_nob0210",ch="あすみ",resize=1,lv=2.2000000000000002,mode=3,size="no",path=":fg/asu/no/",head="asu_nob",face="b0010",x=-160,id=11,sync=0,ex05="asu_nob0010"},{"fg",file="hiy_noa0200",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_noa",face="a0006",x=160,id=12,sync=0,ex05="hiy_noa0000"},{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=3,size="no",path=":fg/kna/no/",head="kna_noa",face="a0053",x=480,id=6,sync=0,ex05="kna_noa0000"},{"extrans",time=500},{"bgm",stop=1},{"text"},line=657,linkback="block_00090",linknext="block_00092",text={ja={{"锦被妃爱拉过去。然后，在里姐和诗樱的注视下，二年级和三年级的学生们面对面站了起来。",{"rt2"}}}}},block_00161={{"text"},line=1058,linkback="block_00160",linknext="block_00162",text={ja={{"不能说出口。但看着电影回顾自己的人生，我就忍不住想说出来。我深深地爱着这个作为恋人的妹妹。",{"rt2"}}}}},block_00040={{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=3,size="no",path=":fg/ina/no/",head="ina_noa",face="a0054",x=-520,id=5,x2=-480,ex05="ina_noa0000"},{"fg",file="asu_nob0200",ch="あすみ",lv=2.2000000000000002,mode=3,size="no",path=":fg/asu/no/",head="asu_nob",face="b0004",x=-280,id=11,x2=-160,ex05="asu_nob0000"},{"fg",file="hiy_noa0200",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_noa",face="a0055",x=0,id=12,x2=160,ex05="hiy_noa0000"},{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=3,size="no",path=":fg/kna/no/",head="kna_noa",face="a0052",x=280,id=6,x2=480,ex05="kna_noa0000"},{"fg",file="mir_noa0210",ch="里",resize=1,lv=2.2000000000000002,mode=3,size="no",mx=40,path=":fg/mir/no/",head="mir_noa",face="a0054",x=520,id=4,ex05="mir_noa0010"},{"text"},linkback="block_00039",delay={vl01={{"fg",file="mir_noa0210",ch="里",lv=2.2000000000000002,mode=3,size="no",path=":fg/mir/no/",head="mir_noa",face="a0052",x=520,id=4,ex05="mir_noa0010"}}},line=306,linknext="block_00041",text={ja={{"「哇，没想到能听到这么精彩的就职演讲。说不定你会成为继诗樱以后的伟大学生会长呢？」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00072",ch="mir"}}}},block_00058={{"se",stop=1,id=1,time=500},{"fg",file="ame_z1a0200",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1a",face="a0052",x=280,id=10,ex05="ame_z1a0000"},{"text"},linkback="block_00057",delay={vl01={{"fg",file="ame_z1b0200",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1b",face="b0052",x=280,id=10,ex05="ame_z1b0000"}}},line=441,linknext="block_00059",text={ja={{"「熟面孔的团队来做学生会感觉挺好的吧？学生大会的发言很棒，辛苦啦！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00042",ch="ame"}}}},block_00136={{"fg",file="shi_nob0500",ch="詩桜",lv=2.2000000000000002,mode=1,size="no",path=":fg/shi/no/",head="shi_nob",face="b0046",id=20,ex05="shi_nob0000"},{"text"},linkback="block_00135",delay={vl01={{"fg",file="shi_noa0500",ch="詩桜",lv=2.2000000000000002,mode=1,size="no",path=":fg/shi/no/",head="shi_noa",face="a0007",id=20,ex05="shi_noa0000"}},vl02={{"fg",file="shi_noa0510",ch="詩桜",lv=2.2000000000000002,mode=1,size="no",path=":fg/shi/no/",head="shi_noa",face="a0054",id=20,ex05="shi_noa0010"}}},line=956,linknext="block_00137",text={ja={{"「等、等一下！如果前任会长和泉君能抱妃爱的话，那作为前前任会长的我也有这个资格吧！？可以吧！」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00226",ch="shi"}}}},block_00108={{"fg",file="ame_z1b0200",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1b",face="b0035",x=0,id=13,ex05="ame_z1b0000"},{"fgact",lv=2.2999999999999998,ch="天梨",act="いいえ",id=13},{"text"},linkback="block_00107",delay={vl01={{"fg",file="ame_z1b0200",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1b",face="b0052",x=0,id=13,ex05="ame_z1b0000"}},vl02={{"fg",file="ame_z1a0210",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1a",face="a0022",x=0,id=13,ex03="a0097",ex05="ame_z1a0010"}}},line=807,linknext="block_00109",text={ja={{"「啊，真是的，小可爱和伊奈都太可爱了！我们一起去购物吧！我还会告诉你们在Instagram上很火的咖啡店哦！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00045",ch="ame"}}}},block_00048={{"fg",file="hiy_z1b0210",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0015",x=280,id=12,ex05="hiy_z1b0010"},{"fgact",lv=2.2999999999999998,ch="妃愛",act="ジャンプ",id=12},{"text"},linkback="block_00047",delay={vl01={{"fg",file="hiy_z1a0210",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1a",face="a0041",x=280,id=12,ex05="hiy_z1a0010"}}},line=366,linknext="block_00049",text={ja={{"「哦，不愧是副会长，支持得很到位呢。我会依赖你的，在这里面你在学生会的经验最丰富，毕竟你是小锦啊。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00683",ch="hiy"}}}},block_00004={{"text"},line=63,linkback="block_00003",linknext="block_00005",text={ja={{"看到新任学生会长上台的样子，女生们发出了欢迎的声音，男生们则发出了遗憾的叹息。",{"rt2"}}}}},block_00030={{"text"},line=228,linkback="block_00029",linknext="block_00031",text={ja={{"现在，我看到的结果超出了我原本设定的目标。那个妃爱已经交到了朋友，作为学生代表主动站出来。",{"rt2"}}}}},block_00121={{"fg",file="hiy_z1a0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1a",face="a0043",id=12,ex05="hiy_z1a0000"},{"text"},linkback="block_00120",delay={vl01={{"fg",file="hiy_z1a0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1a",face="a0041",id=12,ex05="hiy_z1a0000"}}},line=871,linknext="block_00122",text={ja={{"「有点青春气息的感觉，抱一下怎么样？」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00695",ch="hiy"}}}},block_00031={{"text"},line=230,linkback="block_00030",linknext="block_00032",text={ja={{"我可以放心地把后续工作交给她。我要把前任给我的宝贵建议传递下去：「尽情享受吧」。现在我能理解一直在看着我的里姐姐的心情了。",{"rt2"}}}}},block_00015={{"text"},line=117,linkback="block_00014",linknext="block_00016",text={ja={{"啊，等等……去年妃爱是后来才迟到地走进学生会室的吧？ 也就是说她没听到我的发言吗？ 真让人失落。",{"rt2"}}}}},block_00028={{"text"},line=224,linkback="block_00027",linknext="block_00029",text={ja={{"尤其是因为我知道妃爱参选的原因，我更加感受到这一年，甚至更早从一年半前开始的学生会活动的意义。",{"rt2"}}}}},block_00055={{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=1,size="no",path=":fg/kna/no/",head="kna_noa",face="a0015",id=20,ex05="kna_noa0000"},{"text"},linkback="block_00054",delay={vl01={{"fg",file="kna_z1a0200",ch="神田",resize=1,lv=2.2999999999999998,mode=1,size="z1",path=":fg/kna/z1/",head="kna_z1a",face="a0054",id=20,ex05="kna_z1a0000"}},vl02={{"fg",file="kna_z1a0200",ch="神田",lv=2.2999999999999998,mode=1,size="z1",path=":fg/kna/z1/",head="kna_z1a",face="a0041",id=20,ex05="kna_z1a0000"}}},line=414,linknext="block_00056",text={ja={{"「对对对！就算你走了了也不要忘记我们哦！一起吃午饭吧～」",{"rt2"},name={"神田 & 柑凪"}}},vo={{"vo",file="fem_kna_00096",ch="kna"}}}},block_00043={{"fg",file="hiy_nob0210",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0052",x=0,id=12,ex05="hiy_nob0010"},{"fgact",lv=2.2000000000000002,ch="妃愛",act="ジャンプ",id=12},{"text"},linkback="block_00042",delay={vl01={{"fg",file="hiy_nob0210",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0055",x=0,id=12,ex05="hiy_nob0010"}}},line=322,linknext="block_00044",text={ja={{"「哇哦，赞啊，是江户川烧饼！我要吃南瓜馅的！」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00681",ch="hiy"}}}},block_00061={{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=1,size="no",path=":fg/ina/no/",head="ina_noa",face="a0013",id=20,ex05="ina_noa0000"},{"text"},linkback="block_00060",delay={vl01={{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=1,size="no",path=":fg/ina/no/",head="ina_noa",face="a0054",id=20,ex05="ina_noa0000"}},vl02={{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=1,size="no",path=":fg/ina/no/",head="ina_noa",face="a0041",id=20,ex05="ina_noa0000"}}},line=466,linknext="block_00062",text={ja={{"「你们两位也喝茶吗？要来杯红茶吗？实际上，我们把零花钱凑在一起，在百货店一楼的红茶店买了茶包。」",{"rt2"},name={"飯田 & 伊々奈"}}},vo={{"vo",file="fem_ina_00098",ch="ina"}}}},block_00122={{"text"},line=873,linkback="block_00121",linknext="block_00123",text={ja={{"但是妃爱提出了更进一步的要求，或者说是她最想我做的事情。如果做到那个程度，不就有点过分了吗？",{"rt2"}}}}},block_00105={{"fg",hoho="h",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0029h",x=280,id=10,ex05="kan_z1b0000",file="kan_z1b0200"},{"fgact",lv=2.2999999999999998,ch="華乃",act="ジャンプ",id=10},{"text"},linkback="block_00104",delay={vl03={{"fg",hoho="h",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0030h",x=280,id=10,ex05="kan_z1b0000",file="kan_z1b0200"}},vl01={{"fg",hoho="h",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0021h",x=280,id=10,ex05="kan_z1b0000",file="kan_z1b0200"}},vl02={{"fg",hoho="h",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0037h",x=280,id=10,ex05="kan_z1b0000",file="kan_z1b0200"}}},line=775,linknext="block_00106",text={ja={{"「呜呜……啊啊啊啊啊！我也，我也真的很喜欢锦！她是我最喜欢的后辈！虽然是后辈，但我们也是朋友！」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00122",ch="kan"}}}},block_00158={{"bg",file="zev_hiy_35_03",y=100,ay=720,set="ev_hiy_35",path=":ev/ev_hiy_35/",time=500,bx=-640,by=-360,ax=1280},{"text"},line=1049,linkback="block_00157",linknext="block_00159",text={ja={{"说到这里，妃爱在扶手上叠起了手。她的身体似乎带着一些热度，感觉比平时要暖和一些。",{"rt2"}}}}},block_00070={{"fg",file="ame_z1b0200",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1b",face="b0010",x=430,id=10,ex05="ame_z1b0000"},{"text"},line=522,linkback="block_00069",linknext="block_00071",text={ja={{"「哎？刚才是华乃你说的吗？」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00043",ch="ame"}}}},block_00145={{"text"},line=1008,linkback="block_00144",linknext="block_00146",text={ja={{"「虽然在演技中我不会掺杂私情，但如果从观众的视角看到自己对着别人叫哥哥，那就是另一回事了。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00701",ch="hiy"}}}},block_00005={{"text"},line=65,linkback="block_00004",linknext="block_00006",text={ja={{"如果我知道妃爱要参选的话，我也会想要参加学生会。谁会让可爱的妹妹接触到异性呢。",{"rt2"}}}}},block_00071={{"fg",file="shi_z1b0510",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1b",face="b0053",x=-430,id=7,ex05="shi_z1b0010"},{"text"},linkback="block_00070",delay={vl01={{"fg",file="shi_z1b0510",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1b",face="b0055",x=-430,id=7,ex05="shi_z1b0010"}}},line=528,linknext="block_00072",text={ja={{"「从明年开始我打算取得教师资格……虽然并不打算做老师，但我觉得拿上教师证还是有好处的。」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00217",ch="shi"}}}},block_00140={{"bgm",stop=1,time=3000},{"se",stop=1,id=-1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",file="white",path=":bg/",time=3000},{"cacheclear"},{"ex",func="wait",time=0},{"bg",file="bg099a",path=":bg/",time=2000},{"text"},line=979,linkback="block_00139",linknext="block_00141",text={ja={{"自从那场会长交接后，我这个主角就淡出了舞台，专心致志于考试学习，过着像隐居者一样悠然自得、安逸的日子。",{"rt2"}}}}},block_00038={{"text"},line=293,linkback="block_00037",linknext="block_00039",text={ja={{"虽然有点厚颜无耻，但我想如果是这些孩子们应该会温柔地接待我。带着一半期待一半不安的心情走进去，结果得到了比幕末时期的美国还要优待的待遇。",{"rt2"}}}}},block_00056={{"text"},line=416,linkback="block_00055",linknext="block_00057",text={ja={{"她们的温柔让我情不自禁，我转过脸去悄悄擦拭眼角。妃爱果然有眼光，选了这么好的朋友。",{"rt2"}}}}},block_00168={{"text"},line=1080,linkback="block_00167",linknext="block_00169",text={ja={{"「哈啊～。现在真的很幸福～」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00707",ch="hiy"}}}},block_00076={{"fg",file="hiy_noa0200",ch="妃愛",lv=2.2000000000000002,mode=1,size="no",path=":fg/hiy/no/",head="hiy_noa",face="a0034",id=20,ex05="hiy_noa0000"},{"text"},linkback="block_00075",delay={vl01={{"fg",file="hiy_noa0210",ch="妃愛",lv=2.2000000000000002,mode=1,size="no",path=":fg/hiy/no/",head="hiy_noa",face="a0052",id=20,ex05="hiy_noa0010"}}},line=557,linknext="block_00077",text={ja={{"「哎呀，我只是说了一些听起来很有道理的话，但具体要做什么，其实我也没有计划。我只是想确定一下我们船的航向而已。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00685",ch="hiy"}}}},block_00046={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="zbg011a",by=-360,ax=1280,bx=-640,ay=720,sync=0,path=":bg/"},{"fg",file="hiy_z1a0200",ch="妃愛",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1a",face="a0006",id=12,sync=0,ex05="hiy_z1a0000"},{"extrans",time=500},{"fgact",lv=2.2999999999999998,ch="妃愛",act="うんうん",id=12},{"text"},linkback="block_00045",delay={vl01={{"fg",file="hiy_z1a0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1a",face="a0054",id=12,ex05="hiy_z1a0000"}}},line=344,linknext="block_00047",text={ja={{"「哥哥，你是要像以前一样坐在中间吗？」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00682",ch="hiy"}}}},block_00021={{"fg",file="kna_z1a0200",ch="神田",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kna/z1/",head="kna_z1a",face="a0052",x=430,id=8,ex05="kna_z1a0000"},{"fgact",lv=2.2999999999999998,ch="神田",act="おじぎ",id=8},{"text"},linkback="block_00020",delay={vl01={{"fg",file="kna_z1a0200",ch="神田",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kna/z1/",head="kna_z1a",face="a0054",x=430,id=8,ex05="kna_z1a0000"}},vl02={{"fgact",lv=2.2999999999999998,ch="神田",act="おじぎ",id=8}}},line=177,linknext="block_00022",text={ja={{"「我是负责公关和会计的神田！新任会长表示想要重视宣传和公关工作，所以我会铭记自己肩负的重大职责，尽职尽责地工作！」",{"rt2"},name={"神田 & 柑凪"}}},vo={{"vo",file="fem_kna_00092",ch="kna"}}}},block_00118={{"fg",file="hiy_z1b0210",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0008",id=12,ex05="hiy_z1b0010"},{"fgact",lv=2.2999999999999998,ch="妃愛",act="うなづく",id=12},{"text"},linkback="block_00117",delay={vl01={{"fg",file="hiy_z1b0210",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0053",id=12,ex05="hiy_z1b0010"}}},line=860,linknext="block_00119",text={ja={{"「啊，钥匙。没想到哥哥还留着呢。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00694",ch="hiy"}}}},block_00109={{"fg",file="shi_noa0510",ch="詩桜",resize=1,lv=2.2000000000000002,mode=1,size="no",path=":fg/shi/no/",head="shi_noa",face="a0031",id=20,ex05="shi_noa0010"},{"text"},linkback="block_00108",delay={vl01={{"fg",file="shi_noa0510",ch="詩桜",lv=2.2000000000000002,mode=1,size="no",path=":fg/shi/no/",head="shi_noa",face="a0053",id=20,ex05="shi_noa0010"}}},line=813,linknext="block_00110",text={ja={{"「和泉老师，你哭没关系，但不要倚着我的肩膀哭，去那些孩子们中间怎么样？你有这个资格。」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00225",ch="shi"}}}},block_00008={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0031",x=0,id=13,ex05="hiy_z1b0000"},{"text"},line=84,linkback="block_00007",linknext="block_00009",text={ja={{"「我在半年的学生会活动中学到的是人与人之间联系的重要性。我打算以这个理念为基础来运营新的学生会。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00676",ch="hiy"}}}},block_00087={{"fg",file="ame_z1b0200",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1b",face="b0052",x=0,id=13,ex05="ame_z1b0000"},{"text"},linkback="block_00086",delay={vl01={{"fg",file="ame_z1b0200",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1b",face="b0006",x=0,id=13,ex05="ame_z1b0000"}}},line=625,linknext="block_00088",text={ja={{"「志愿者训练营，真的很有趣！我觉得那时大家的友情都加深了！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00044",ch="ame"}}}},block_00096={{"fg",file="hiy_nob0200",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0055",x=160,id=12,ex05="hiy_nob0000"},{"text"},linkback="block_00095",delay={vl01={{"fg",file="hiy_nob0200",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0031",x=160,id=12,ex05="hiy_nob0000"},{"fgact",lv=2.2000000000000002,ch="妃愛",act="おじぎ",id=12}}},line=693,linknext="block_00097",text={ja={{"「哥哥，常磐，天梨前辈，诗樱前辈。谢谢你们。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00691",ch="hiy"}}}},block_00010={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0006",x=0,id=13,ex05="hiy_z1b0000"},{"text"},linkback="block_00009",delay={vl01={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0054",x=0,id=13,ex05="hiy_z1b0000"}}},line=95,linknext="block_00011",text={ja={{"「作为新一任的学生代表，我想把这个大马学院变成一个「人与人紧密相连」的校园！」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00678",ch="hiy"}}}},label={top={block="block_00000",label=1}},block_00170={{"bgm",stop=1,time=3000},{"se",stop=1,id=-1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",file="white",path=":bg/",time=3000},{"cacheclear"},{"ex",func="wait",time=0},{"bgm",file="bgm01",unlock=1},{"bgm",file="bgm02",unlock=1},{"bgm",file="bgm03",unlock=1},{"bgm",file="bgm04",unlock=1},{"bgm",file="bgm05",unlock=1},{"bgm",file="bgm06",unlock=1},{"bgm",file="bgm07",unlock=1},{"bgm",file="bgm08",unlock=1},{"bgm",file="bgm09",unlock=1},{"bgm",file="bgm10",unlock=1},{"bgm",file="bgm11",unlock=1},{"bgm",file="bgm12",unlock=1},{"bgm",file="bgm13",unlock=1},{"bgm",file="bgm14",unlock=1},{"bgm",file="bgm15",unlock=1},{"bgm",file="bgm16",unlock=1},{"bgm",file="bgm17",unlock=1},{"bgm",file="bgm18",unlock=1},{"bgm",file="bgm19",unlock=1},{"bgm",file="bgm20",unlock=1},{"bgm",file="bgm21",unlock=1},{"bgm",file="bgm22",unlock=1},{"bgm",file="bgm23",unlock=1},{"bgm",file="bgm24",unlock=1},{"bgm",file="bgm25",unlock=1},{"bgm",file="bgm26",unlock=1},{"bgm",file="bgm27",unlock=1},{"bgm",file="bgm28",unlock=1},{"bgm",file="bgm29",unlock=1},{"bgm",file="bgm30",unlock=1},{"bgm",file="bgm31",unlock=1},{"bgm",file="bgm32",unlock=1},{"bgm",file="bgm33",unlock=1},{"bgm",file="bgm34",unlock=1},{"bgm",file="bgm35",unlock=1},{"scene",file="scene04"},{"staffroll",ch="妃愛"},{"bg",file="black",sync=0,path=":bg/"},{"bg",file="exdlg_hiy",lv=5,ay=540,sync=0,path=":cg/",ax=960,id=1,by=-180,cond="g.hiy==0",bx=-320},{"extrans",time=250},{"ex",func="wait",time=20000},{"eval",exp="g.hiyend=1"},{"eval",exp="g.hiy=1"},{"bgm",stop=1,time=3000},{"se",stop=1,id=-1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",file="black",path=":bg/",time=3000},{"cacheclear"},{"ex",func="wait",time=1000},{"タイトル"},{"text"},line=1142,linkback="block_00169"},block_00106={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="zbg011a",ax=1280,by=-360,x=-400,bx=-640,ay=720,sync=0,path=":bg/"},{"fg",file="ina_z1a0200",ch="飯田",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/ina/z1/",head="ina_z1a",face="a0013",x=-430,id=7,sync=0,ex05="ina_z1a0000"},{"fg",file="ame_z1a0200",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1a",face="a0053",x=0,id=13,sync=0,ex05="ame_z1a0000"},{"fg",file="kna_z1a0200",ch="神田",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/kna/z1/",head="kna_z1a",face="a0052",x=430,id=8,sync=0,ex05="kna_z1a0000"},{"extrans",vague=40,rule="rule_001"},{"text"},linkback="block_00105",delay={vl01={{"fg",file="kna_z1a0200",ch="神田",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kna/z1/",head="kna_z1a",face="a0006",x=430,id=8,ex05="kna_z1a0000"},{"fgact",lv=2.2999999999999998,ch="神田",act="うんうん",id=8}}},line=792,linknext="block_00107",text={ja={{"「天梨前辈，你真的好酷，我们一直都在借鉴你的穿搭！你是我们引以为傲的前辈！」",{"rt2"},name={"神田 & 柑凪"}}},vo={{"vo",file="fem_kna_00098",ch="kna"}}}},block_00169={{"text"},line=1082,linkback="block_00168",linknext="block_00170",text={ja={{"看吧。",{"rt2"}}}}},block_00060={{"fg",file="kna_noa0200",ch="神田",resize=1,lv=2.2000000000000002,mode=1,size="no",path=":fg/kna/no/",head="kna_noa",face="a0004",id=20,ex05="kna_noa0000"},{"text"},linkback="block_00059",delay={vl01={{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=1,size="no",path=":fg/kna/no/",head="kna_noa",face="a0055",id=20,ex05="kna_noa0000"}},vl02={{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=1,size="no",path=":fg/kna/no/",head="kna_noa",face="a0019",id=20,ex05="kna_noa0000"}}},line=459,linknext="block_00061",text={ja={{"「常磐学姐！谢谢你上次耐心教我如何使用推特！我之前只用过ZukBak。」",{"rt2"},name={"神田 & 柑凪"}}},vo={{"vo",file="fem_kna_00097",ch="kna"}}}},block_00165={{"text"},line=1073,linkback="block_00164",linknext="block_00166",text={ja={{"尽管我会继续把这份感情藏在心底，但我这个平凡人过着平凡生活也能做到，「兄妹恋情」已经开始被我视为理所当然。",{"rt2"}}}}},block_00042={{"fg",file="mir_noa0200",ch="里",lv=2.2000000000000002,mode=3,size="no",path=":fg/mir/no/",head="mir_noa",face="a0055",x=520,id=4,ex05="mir_noa0000"},{"text"},linkback="block_00041",delay={vl01={{"fg",file="mir_noa0200",ch="里",lv=2.2000000000000002,mode=3,size="no",path=":fg/mir/no/",head="mir_noa",face="a0015",x=520,id=4,ex05="mir_noa0000"}}},line=315,linknext="block_00043",text={ja={{"「那就让我们来吃大阪烧！我买了好多红豆馅，先到先得哦！」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00073",ch="mir"}}}},block_00063={{"text"},line=474,linkback="block_00062",linknext="block_00064",text={ja={{"当常磐和天梨也来到时，她们也像我一样受到了二年级学生们的热烈欢迎。没关系，我并没有想着只有我才能享受这种待遇。",{"rt2"}}}}},block_00075={{"fg",file="shi_z1a0510",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1a",face="a0053",x=-430,id=7,ex05="shi_z1a0010"},{"text"},linkback="block_00074",delay={vl01={{"fg",file="shi_z1a0510",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1a",face="a0031",x=-430,id=7,ex05="shi_z1a0010"}}},line=551,linknext="block_00076",text={ja={{"「我很有兴趣听了妃爱的政见发表演说。一个强调人与人之间联系的学生会长，对于把官员赶出去的我来说，这是个令人不舒服的话题。」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00220",ch="shi"}}}},block_00104={{"fg",hoho="h",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0021h",x=280,id=10,ex05="kan_z1b0000",file="kan_z1b0200"},{"fg",file="asu_z1b0210",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1b",face="b0023",x=-280,id=9,ex05="asu_z1b0010"},{"text"},linkback="block_00103",delay={vl03={{"fg",hoho="h",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0030h",x=280,id=10,ex05="kan_z1b0000",file="kan_z1b0200"}},vl01={{"fg",hoho="h",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0037h",x=280,id=10,ex05="kan_z1b0000",file="kan_z1b0200"}},vl02={{"fg",hoho="h",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0005h",x=280,id=10,ex05="kan_z1b0000",file="kan_z1b0200"}}},line=765,linknext="block_00105",text={ja={{"「我要哭、要哭……我要哭了！ 我真的很喜欢锦！ 尽管我不太擅长和女孩子交朋友，但和锦聊天很轻松……我们很投缘……」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00121",ch="kan"}}}},block_00163={{"bg",file="ev_hiy_35_04",face="1",time=400,path=":ev/ev_hiy_35/",sync=0,set="ev_hiy_35"},{"text"},line=1068,linkback="block_00162",linknext="block_00164",text={ja={{"「兄妹判定」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00706",ch="hiy"}}}},block_00120={{"text"},line=865,linkback="block_00119",linknext="block_00121",text={ja={{"顺便递交钥匙的时候，我抚摸了一下她的头。这种程度应该算是兄妹间微笑可鞠的亲昵行为吧。",{"rt2"}}}}},block_00074={{"fg",file="kan_z1b0200",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0020",x=0,id=9,ex05="kan_z1b0000"},{"text"},line=545,linkback="block_00073",linknext="block_00075",text={ja={{"这完全是一件合情合理的事情。要说起来，比起已经退休的我们，她未来可能会成为更实际的利害关系人。无话可说的常磐满脸不甘地低头看着地板。",{"rt2"}}}}},block_00072={{"fg",file="shi_z1b0510",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1b",face="b0003",x=-430,id=7,ex05="shi_z1b0010"},{"fg",file="ame_z1b0200",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1b",face="b0053",x=430,id=10,ex05="ame_z1b0000"},{"text"},linkback="block_00071",delay={vl01={{"fg",file="shi_z1b0500",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1b",face="b0032",x=-430,id=7,ex05="shi_z1b0000"}}},line=535,linknext="block_00073",text={ja={{"「如果要去做教育实习，那就得再等两年了。所以我认为现在就应该就此事进行咨询一下。这就是我今天要来找和泉老师的原因。」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00218",ch="shi"}}}},block_00162={{"text"},line=1060,linkback="block_00161",linknext="block_00163",text={ja={{"原来如此，真是复杂啊。我轻轻地移开叠在一起的妃爱的手，然后重新以恋人的方式握住了她的手。",{"rt2"}}}}},block_00131={{"text"},line=921,linkback="block_00130",linknext="block_00132",text={ja={{"尽管妃爱之前对叫我哥哥感到非常害羞，但最近只要在学生会成员面前，只要情绪高涨就会叫我哥哥。我可不负责之后被大家取笑。",{"rt2"}}}}},block_00069={{"fg",file="kan_z1b0210",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0018",x=0,id=9,ex05="kan_z1b0010"},{"text"},linkback="block_00068",delay={vl01={{"fg",file="kan_z1b0200",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0050",x=0,id=9,ex05="kan_z1b0000"}},vl02={{"fg",file="kan_z1b0200",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0043",x=0,id=9,ex05="kan_z1b0000"}}},line=517,linknext="block_00070",text={ja={{"「总、总之，已经毕业的诗樱学姐来这里有什么事？已经没有小说取材什么的了吧？来这里是为了散发你学姐的气场吗？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00118",ch="kan"}}}},block_00132={{"fg",file="kna_noa0200",ch="神田",resize=1,lv=2.2000000000000002,mode=1,size="no",path=":fg/kna/no/",head="kna_noa",face="a0045",id=20,ex05="kna_noa0000"},{"text"},linkback="block_00131",delay={vl01={{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=1,size="no",path=":fg/kna/no/",head="kna_noa",face="a0052",id=20,ex05="kna_noa0000"}}},line=927,linknext="block_00133",text={ja={{"「啊，我也想被那样！学长，你能不能也对我说加油然后抱抱我？」",{"rt2"},name={"神田 & 柑凪"}}},vo={{"vo",file="fem_kna_00099",ch="kna"}}}},block_00155={{"text"},line=1035,linkback="block_00154",linknext="block_00156",text={ja={{"「不过抛开这个不谈，电影本身的内容就像是回顾我的人生一样，对我来说也是一种复杂的心境。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00703",ch="hiy"}}}},block_00019={{"fg",file="asu_z1b0200",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1b",face="b0055",x=0,id=13,ex05="asu_z1b0000"},{"text"},linkback="block_00018",delay={vl01={{"fg",file="asu_z1b0200",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1b",face="b0006",x=0,id=13,ex05="asu_z1b0000"}},vl02={{"fgact",lv=2.2999999999999998,ch="あすみ",act="おじぎ",id=13}}},line=159,linknext="block_00020",text={ja={{"「我在上任会长那里受到了很多照顾，所以为了回报他的恩情，我也将全力支持新任会长和泉，她是他的妹妹。」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00113",ch="asu"}}}},block_00154={{"text"},line=1032,linkback="block_00153",linknext="block_00155",text={ja={{"说到过去的事情，似乎让妃爱想起了一些事情。「智宏你真的成长了呢」她赞扬道。这是母亲视角吗？",{"rt2"}}}}},block_00151={{"text"},line=1024,linkback="block_00150",linknext="block_00152",text={ja={{"但是这次因为模特就是我，她可能会用很接近平时的语气叫我哥哥，这一点让我感到非常不安。",{"rt2"}}}}},block_00150={{"text"},line=1022,linkback="block_00149",linknext="block_00151",text={ja={{"大体上，无论在动画中妃爱怎么称呼陌生人为'哥哥'，那都是和平时的妃爱的声音是分开的，所以我并不在意。",{"rt2"}}}}},block_00133={{"fg",file="ina_noa0200",ch="飯田",resize=1,lv=2.2000000000000002,mode=1,size="no",path=":fg/ina/no/",head="ina_noa",face="a0008",id=20,ex05="ina_noa0000"},{"text"},linkback="block_00132",delay={vl03={{"fg",hoho="h",ch="飯田",lv=2.2000000000000002,mode=1,size="no",path=":fg/ina/no/",head="ina_noa",face="a0054h",id=20,ex05="ina_noa0000",file="ina_noa0200"}},vl01={{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=1,size="no",path=":fg/ina/no/",head="ina_noa",face="a0012",id=20,ex05="ina_noa0000"}},vl02={{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=1,size="no",path=":fg/ina/no/",head="ina_noa",face="a0018",id=20,ex05="ina_noa0000"}}},line=935,linknext="block_00134",text={ja={{"「啊～！？柑凪酱，那是因为妃爱是他的妹妹……啊，但、但如果也能得到的话，我也……好、好吧好吧！」",{"rt2"},name={"飯田 & 伊々奈"}}},vo={{"vo",file="fem_ina_00100",ch="ina"}}}},block_00059={{"fg",file="asu_noa0210",ch="あすみ",resize=1,lv=2.2000000000000002,mode=1,size="no",path=":fg/asu/no/",head="asu_noa",face="a0008",id=20,ex05="asu_noa0010"},{"fg",file="kan_z1a0200",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1a",face="a0053",x=-280,id=9,ex05="kan_z1a0000"},{"fg",file="ame_z1b0200",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1b",face="b0053",x=280,id=10,ex05="ame_z1b0000"},{"text"},linkback="block_00058",delay={vl01={{"fg",file="asu_noa0210",ch="あすみ",lv=2.2000000000000002,mode=1,size="no",path=":fg/asu/no/",head="asu_noa",face="a0053",id=20,ex05="asu_noa0010"}},vl02={{"fg",file="asu_nob0200",ch="あすみ",lv=2.2000000000000002,mode=1,size="no",path=":fg/asu/no/",head="asu_nob",face="b0054",id=20,ex05="asu_nob0000"}}},line=450,linknext="block_00060",text={ja={{"「啊，常磐学姐，天梨学姐！你们虽然在忙着复习考试，却还过来了？谢谢你们！」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00118",ch="asu"}}}},block_00020={{"text"},line=161,linkback="block_00019",linknext="block_00021",text={ja={{"锦的发言也非常得体。但无论怎么看，提到上任会长的部分完全没有必要吧？为什么锦也一脸满足的样子？",{"rt2"}}}}},block_00049={{"fg",file="hiy_z1a0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1a",face="a0054",x=280,id=12,ex05="hiy_z1a0000"},{"text"},line=371,linkback="block_00048",linknext="block_00050",text={ja={{"「小锦是……？」",{"rt2"},name={"智宏"}}}}},block_00148={{"text"},line=1018,linkback="block_00147",linknext="block_00149",text={ja={{"「复杂。」",{"rt2"},name={"智宏"}}}}},block_00146={{"bg",file="ev_hiy_35_02",face="1",time=400,path=":ev/ev_hiy_35/",sync=0,set="ev_hiy_35"},{"text"},line=1012,linkback="block_00145",linknext="block_00147",text={ja={{"妃爱边说「复杂」边抱着头呻吟。即使来到试映会，也被她说出这样的话。",{"rt2"}}}}},block_00144={{"text"},line=1005,linkback="block_00143",linknext="block_00145",text={ja={{"「因为你看，如果只是给一个具象的角色注入生命还好，但这次是用我的脸、我的声音去叫别人哥哥。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00700",ch="hiy"}}}},block_00143={{"text"},line=1002,linkback="block_00142",linknext="block_00144",text={ja={{"「为什么？」",{"rt2"},name={"智宏"}}}}},block_00142={{"bg",file="ev_hiy_35_01",set="ev_hiy_35",path=":ev/ev_hiy_35/",time=500},{"text"},line=999,linkback="block_00141",linknext="block_00143",text={ja={{"「其实这部电影，真要和哥哥你一起看，我会有点复杂的心情。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00699",ch="hiy"}}}},block_00047={{"fg",file="hiy_z1a0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1a",face="a0010",x=280,id=12,x2=0,ex05="hiy_z1a0000"},{"fg",file="asu_z1b0210",ch="あすみ",resize=1,lv=2.2999999999999998,mode=3,size="z1",mx=-40,path=":fg/asu/z1/",head="asu_z1b",face="b0003",x=-280,id=9,ex05="asu_z1b0010"},{"text"},linkback="block_00046",delay={vl01={{"fg",file="asu_z1a0210",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1a",face="a0055",x=-280,id=9,ex05="asu_z1a0010"},{"fgact",lv=2.2999999999999998,ch="あすみ",act="うんうん",id=9}}},line=357,linknext="block_00048",text={ja={{"「那个位置应该让妃爱坐。毕竟从今天开始她就是会长了，需要有这个自觉。」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00116",ch="asu"}}}},block_00080={{"fg",file="shi_z1a0500",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1a",face="a0027",x=-430,id=7,ex05="shi_z1a0000"},{"text"},linkback="block_00079",delay={vl01={{"fg",file="shi_z1a0500",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1a",face="a0032",x=-430,id=7,ex05="shi_z1a0000"}},vl02={{"fg",file="shi_z1a0500",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1a",face="a0053",x=-430,id=7,ex05="shi_z1a0000"}}},line=580,linknext="block_00081",text={ja={{"「虽然作为以写文章为业的人，对自己文章力度不足感到沮丧，但如果是华乃写的文章，我也会投降。我也很想阅读。」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00223",ch="shi"}}}},block_00098={{"text"},line=716,linkback="block_00097",linknext="block_00099",text={ja={{"在这个学生会室里，我已经不再是主角。我将这个位置让给了一个我完全没有任何不满的人。那就是我可爱的妹妹，我的恋人。",{"rt2"}}}}},block_00037={{"text"},line=291,linkback="block_00036",linknext="block_00038",text={ja={{"学生大会结束后的放学时间，我听说新的学生会成员在学生会室集合，于是我也过去看了看。结果受到了热烈的欢迎。",{"rt2"}}}}},block_00062={{"fg",file="hiy_nob0200",ch="妃愛",resize=1,lv=2.2000000000000002,mode=1,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0033",id=20,ex05="hiy_nob0000"},{"text"},linkback="block_00061",delay={vl01={{"fg",file="hiy_nob0200",ch="妃愛",lv=2.2000000000000002,mode=1,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0043",id=20,ex05="hiy_nob0000"}}},line=472,linknext="block_00063",text={ja={{"「是「茶包」哦！如果说是「T裤」，那就变成了买了极其危险的东西了呢！」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00684",ch="hiy"}}}},block_00139={{"text"},line=971,linkback="block_00138",linknext="block_00140",text={ja={{"尽管大家都在兴奋地闹腾，但被嫉妒之情燃烧的我妹妹心胸之狭窄，就像意面那么细。人与人之间的联系呢。",{"rt2"}}}}},block_00138={{"fg",file="hiy_z1b0210",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0007",id=12,ex05="hiy_z1b0010"},{"fgact",lv=2.2999999999999998,ch="妃愛",act="ジャンプ",id=12},{"text"},linkback="block_00137",delay={vl01={{"fg",hoho="h",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0017h",id=12,ex05="hiy_z1b0000",file="hiy_z1b0200"}}},line=969,linknext="block_00139",text={ja={{"「闭嘴！哥哥的拥抱是作为妹妹和会长的我唯一的特权！」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00698",ch="hiy"}}}},block_00137={{"fg",file="kan_noa0200",ch="華乃",resize=1,lv=2.2000000000000002,mode=1,size="no",path=":fg/kan/no/",head="kan_noa",face="a0037",id=20,ex05="kan_noa0000"},{"text"},linkback="block_00136",delay={vl01={{"fg",file="kan_noa0200",ch="華乃",lv=2.2000000000000002,mode=1,size="no",path=":fg/kan/no/",head="kan_noa",face="a0049",id=20,ex05="kan_noa0000"}}},line=962,linknext="block_00138",text={ja={{"「呜啊啊啊啊啊，大家都在吃的烤饼，我最喜欢的南瓜馅居然没有剩下！」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00123",ch="kan"}}}},block_00097={{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=3,size="no",path=":fg/ina/no/",head="ina_noa",face="a0032",x=-480,id=5,ex05="ina_noa0000"},{"fg",file="asu_nob0210",ch="あすみ",lv=2.2000000000000002,mode=3,size="no",path=":fg/asu/no/",head="asu_nob",face="b0032",x=-160,id=11,ex05="asu_nob0010"},{"fg",file="hiy_noa0200",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_noa",face="a0032",x=160,id=12,ex05="hiy_noa0000"},{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=3,size="no",path=":fg/kna/no/",head="kna_noa",face="a0032",x=480,id=6,ex05="kna_noa0000"},{"fgact",lv=2.2000000000000002,ch="飯田",act="おじぎ",id=5},{"fgact",lv=2.2000000000000002,ch="あすみ",act="おじぎ",id=11},{"fgact",lv=2.2000000000000002,ch="神田",act="おじぎ",id=6},{"text"},line=714,linkback="block_00096",linknext="block_00098",text={ja={{"看着妹妹深深鞠躬的头顶，我明白了自己作为学生会长的故事，从抽签开始，现在终于结束了。",{"rt2"}}}}},block_00113={{"fg",file="hiy_z1a0210",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1a",face="a0022",id=12,ex05="hiy_z1a0010"},{"fgact",lv=2.2999999999999998,ch="妃愛",act="おじぎ",id=12},{"text"},linkback="block_00112",delay={vl01={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0031",id=12,ex05="hiy_z1b0000"}}},line=840,linknext="block_00114",text={ja={{"「哥哥，你辛苦了。我知道你是个能干的人，但你超出了我的想象，出色地完成了学生会长的工作。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00692",ch="hiy"}}}},block_00125={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="zbg011a",bx=-640,by=-360,ax=1280,zoom=120,ay=720,sync=0,path=":bg/"},{"fg",file="hiy_z2b0200",ch="妃愛",resize=1,lv=2.3999999999999999,mode=3,size="z2",path=":fg/hiy/z2/",head="hiy_z2b",face="b0010",id=12,sync=0,ex05="hiy_z2b0000"},{"extrans",time=500},{"se",file="se抱きつく",id=1},{"text"},line=889,linkback="block_00124",linknext="block_00126",text={ja={{"「请、请你在接下来的一年里加油。抱一下」",{"rt2"},name={"智宏"}}}}},block_00159={{"bg",file="ev_hiy_35_03",set="ev_hiy_35",path=":ev/ev_hiy_35/",time=500},{"text"},line=1054,linkback="block_00158",linknext="block_00160",text={ja={{"「能和喜欢的人在一起真是太幸福了。嘿嘿～」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00705",ch="hiy"}}}},block_00129={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="zbg011a",by=-360,ax=1280,bx=-640,ay=720,sync=0,path=":bg/"},{"fg",hoho="h",ch="妃愛",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0035h",id=12,sync=0,ex05="hiy_z1b0000",file="hiy_z1b0200"},{"extrans",time=500},{"fgact",lv=2.2999999999999998,ch="妃愛",act="いいえ",id=12},{"text"},line=917,linkback="block_00128",linknext="block_00130",text={ja={{"「嘿、嘿～～～～～～，被哥哥抱住了……」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00697",ch="hiy"}}}},block_00068={{"text"},line=510,linkback="block_00067",linknext="block_00069",text={ja={{"「也许并不站在任何人这边……但其实我总是站在后辈们这边的。」",{"rt2"},name={"智宏"}}}}},block_00094={{"fg",file="hiy_nob0200",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0053",x=160,id=12,ex05="hiy_nob0000"},{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=3,size="no",path=":fg/ina/no/",head="ina_noa",face="a0053",x=-480,id=5,ex05="ina_noa0000"},{"fg",file="asu_nob0210",ch="あすみ",lv=2.2000000000000002,mode=3,size="no",path=":fg/asu/no/",head="asu_nob",face="b0023",x=-160,id=11,ex05="asu_nob0010"},{"text"},linkback="block_00093",delay={vl01={{"fg",file="hiy_nob0210",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_nob",face="b0055",x=160,id=12,ex05="hiy_nob0010"}}},line=678,linknext="block_00095",text={ja={{"「因为与前辈们共度的一年很愉快，所以我们想要自己重新建立学生会。我相信这里的四个人都有同样的想法。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00689",ch="hiy"}}}},block_00110={{"fg",file="mir_noa0200",ch="里",lv=2.2000000000000002,mode=1,size="no",path=":fg/mir/no/",head="mir_noa",face="a0035",id=20,ex05="mir_noa0000"},{"text"},linkback="block_00109",delay={vl01={{"fg",file="mir_noa0210",ch="里",lv=2.2000000000000002,mode=1,size="no",path=":fg/mir/no/",head="mir_noa",face="a0023",id=20,ex05="mir_noa0010"}},vl02={{"fg",file="mir_noa0200",ch="里",lv=2.2000000000000002,mode=1,size="no",path=":fg/mir/no/",head="mir_noa",face="a0005",id=20,ex05="mir_noa0000"}}},line=822,linknext="block_00111",text={ja={{"「哎呀，我毕竟是个大人了，不是？我应该在旁边守护你们，对吧？我一直很向往这样的场景。」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00075",ch="mir"}}}},block_00081={{"fg",file="shi_z1a0500",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1a",face="a0023",x=-430,id=7,ex05="shi_z1a0000"},{"text"},linkback="block_00080",delay={vl01={{"fg",file="shi_z1a0510",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1a",face="a0054",x=-430,id=7,ex05="shi_z1a0010"}}},line=586,linknext="block_00082",text={ja={{"「借此机会，向那些对此事内容甚至没有兴趣的其他学生们展示了一下，什么才是学生会活动。那样我、智宏和历任的学生会长也会因此得到安慰。」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00224",ch="shi"}}}},block_00123={{"text"},line=875,linkback="block_00122",linknext="block_00124",text={ja={{"然而除了常磐以外，周围所有人都用期待的眼神看着我。只有常磐一直在哭。啊，啊啊啊？难道这种事情可以作为笑话来接受？",{"rt2"}}}}},block_00115={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0010",id=12,ex05="hiy_z1b0000"},{"text"},line=848,linkback="block_00114",linknext="block_00116",text={ja={{"「哦？那是什么呢？」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00693",ch="hiy"}}}},block_00119={{"text"},line=863,linkback="block_00118",linknext="block_00120",text={ja={{"「我没有别的可以交给你的东西了，就把这个当作会长交接的仪式吧。请你在接下来的一年里加油。」",{"rt2"},name={"智宏"}}}}},block_00117={{"text"},line=853,linkback="block_00116",linknext="block_00118",text={ja={{"「这是，学生会室的钥匙。我要交给新的会长了。」",{"rt2"},name={"智宏"}}}}},block_00114={{"text"},line=843,linkback="block_00113",linknext="block_00115",text={ja={{"「其实，还有一项作为会长的任务没有完成。这都多亏了妃爱让我想起来。」",{"rt2"},name={"智宏"}}}}},block_00135={{"fg",file="ame_noa0210",ch="天梨",resize=1,lv=2.2000000000000002,mode=1,size="no",path=":fg/ame/no/",head="ame_noa",face="a0008",id=20,ex03="a0097",ex05="ame_noa0010"},{"text"},linkback="block_00134",delay={vl01={{"fg",file="ame_noa0210",ch="天梨",lv=2.2000000000000002,mode=1,size="no",path=":fg/ame/no/",head="ame_noa",face="a0055",id=20,ex03="a0097",ex05="ame_noa0010"}},vl02={{"fg",file="ame_noa0210",ch="天梨",lv=2.2000000000000002,mode=1,size="no",path=":fg/ame/no/",head="ame_noa",face="a0035",id=20,ex03="a0097",ex05="ame_noa0010"}}},line=949,linknext="block_00136",text={ja={{"「啊，这太不公平了！那我也要！我不想被冷落！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00046",ch="ame"}}}},block_00112={{"text"},line=833,linkback="block_00111",linknext="block_00113",text={ja={{"我和这个妹妹已经紧密地联系在一起，没有更多的地方可以连接了……",{"rt2"}}}}},block_00111={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="zbg011a",by=-360,ax=1280,bx=-640,ay=720,sync=0,path=":bg/"},{"fg",file="hiy_z1a0200",ch="妃愛",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1a",face="a0004",id=12,sync=0,ex05="hiy_z1a0000"},{"extrans",vague=40,rule="rule_001"},{"text"},line=831,linkback="block_00110",linknext="block_00112",text={ja={{"在学生会的每个成员都在建立联系的场景中，妃爱站在了我面前。",{"rt2"}}}}},block_00128={{"text"},line=903,linkback="block_00127",linknext="block_00129",text={ja={{"在众人面前这么做真的很紧张……话说回来，这是妃爱用玩笑般的口吻向我提出的要求，我就不应该害羞地保持严肃，而应该好好地满足她。",{"rt2"}}}}},block_00079={{"text"},line=571,linkback="block_00078",linknext="block_00080",text={ja={{"这个我也从里姐那里听说过。当然，目标肯定是副会长……妃爱写的文章。他们一定想读那些关于朗读剧评论的部分。",{"rt2"}}}}},block_00014={{"text"},line=115,linkback="block_00013",linknext="block_00015",text={ja={{"一年前，当妃爱看着站在同一个位置上发言的哥哥时，她是否也有着和我现在一样的感觉呢？",{"rt2"}}}}},block_00034={{"fg",file="asu_noa0200",ch="あすみ",lv=2.2000000000000002,mode=3,size="no",path=":fg/asu/no/",head="asu_noa",face="a0052",x=-160,id=11,ex05="asu_noa0000"},{"fgact",lv=2.2000000000000002,ch="あすみ",act="びっくり",id=11},{"text"},linkback="block_00033",delay={vl01={{"fg",file="asu_noa0200",ch="あすみ",lv=2.2000000000000002,mode=3,size="no",path=":fg/asu/no/",head="asu_noa",face="a0034",x=-160,id=11,ex05="asu_noa0000"}},vl02={{"fg",file="asu_nob0200",ch="あすみ",lv=2.2000000000000002,mode=3,size="no",path=":fg/asu/no/",head="asu_nob",face="b0054",x=-160,id=11,ex05="asu_nob0000"}}},line=272,linknext="block_00035",text={ja={{"「前辈！真好，我以为你退任后就不会再来学生会室了……以，以后也请随时过来看看我们吧！」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00114",ch="asu"}}}},block_00066={{"text"},line=501,linkback="block_00065",linknext="block_00067",text={ja={{"「明明超级紧张的，还自己承认了，简直找不到不吐槽的地方了。」",{"rt2"},name={"智宏"}}}}},block_00102={{"fg",file="asu_z1b0200",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1b",face="b0032",x=-280,id=9,ex05="asu_z1b0000"},{"text"},linkback="block_00101",delay={vl01={{"fg",file="asu_z1b0200",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1b",face="b0054",x=-280,id=9,ex05="asu_z1b0000"}}},line=745,linknext="block_00103",text={ja={{"「你是一个非常值得依赖的前辈。希望我们以后也能保持好关系。」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00122",ch="asu"}}}},block_00101={{"fg",file="asu_z1b0210",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1b",face="b0034",x=-280,id=9,ex05="asu_z1b0010"},{"text"},linkback="block_00100",delay={vl01={{"fg",file="asu_z1b0210",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1b",face="b0002",x=-280,id=9,ex05="asu_z1b0010"}},vl02={{"fg",file="asu_z1b0210",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1b",face="b0019",x=-280,id=9,ex05="asu_z1b0010"}}},line=739,linknext="block_00102",text={ja={{"「研讨会真的很累人。之后我们两个人一起努力上学，有时候又一起逃课，然后又被前辈们说服回来……」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00121",ch="asu"}}}},block_00054={{"fg",file="asu_z1a0210",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1a",face="a0046",x=-280,id=9,ex05="asu_z1a0010"},{"fgact",lv=2.2999999999999998,ch="あすみ",act="びっくり",id=9},{"text"},linkback="block_00053",delay={vl01={{"fg",file="asu_z1b0200",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1b",face="b0018",x=-280,id=9,ex05="asu_z1b0000"}},vl02={{"fg",file="asu_z1b0200",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1b",face="b0013",x=-280,id=9,ex05="asu_z1b0000"}}},line=402,linknext="block_00055",text={ja={{"「那、那是因为在前辈面前不能露出难看的表情和姿势……不、不过只要前辈在，这个房间就会变得很有趣！」",{"rt2"},name={"あすみ & 小西"}}},vo={{"vo",file="fem_asu_00117",ch="asu"}}}},block_00149={{"text"},line=1020,linkback="block_00148",linknext="block_00150",text={ja={{"当然，我也觉得有点微妙。妃爱在动画或游戏中扮演妹妹角色我已经习惯了，但是真人版就……",{"rt2"}}}}},block_00141={{"msgoff"},{"bg",file="zev_hiy_35_01",y2=150,y=150,x2=-640,sync=0,set="ev_hiy_35",speed=12000,by=-360,x=640,bx=-640,ay=720,ax=1280,path=":ev/ev_hiy_35/"},{"extrans"},{"bgm",file="bgm31"},{"text"},line=994,linkback="block_00140",linknext="block_00142",text={ja={{"这样，自诗樱前辈的书出版已经过去一年了，在大学一年级开始适应新环境的初夏，我和妃爱又被邀请参加了完整试映会。",{"rt2"}}}}},block_00012={{"se",stop=1,id=1,time=500},{"text"},line=111,linkback="block_00011",linknext="block_00013",text={ja={{"不愧是妃爱，她的演讲毫无迟疑。本来就有着响亮的嗓音。那可爱的声音，流畅的说话方式都让整个场面被她牵引。",{"rt2"}}}}},block_00092={{"text"},line=659,linkback="block_00091",linknext="block_00093",text={ja={{"我猜大家都在疑惑为什么会出现这种情况。在所有人的视线中，我的妹妹低下了头。",{"rt2"}}}}},block_00089={{"fg",file="kan_z1a0210",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1a",face="a0010",x=-430,id=7,ex05="kan_z1a0010"},{"fg",file="ame_z1a0210",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1a",face="a0010",x=0,id=13,ex03="a0097",ex05="ame_z1a0010"},{"fg",file="asu_z1a0200",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1a",face="a0010",x=430,id=8,ex05="asu_z1a0000"},{"text"},line=637,linkback="block_00088",linknext="block_00090",text={ja={{"妃爱突然站起来，向神田和饭田挥了挥手，于是我们停下了刚开始热闹的闲聊，关注起新会长的动作来。",{"rt2"}}}}},block_00057={{"se",file="se扉開く(スライド)教室",id=1},{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="zbg011a",ax=1280,by=-360,x=600,bx=-640,ay=720,sync=0,path=":bg/"},{"fg",file="kan_z1a0200",ch="華乃",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1a",face="a0052",x=-280,id=9,sync=0,ex05="kan_z1a0000"},{"fg",file="ame_z1a0200",ch="天梨",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1a",face="a0054",x=280,id=10,sync=0,ex05="ame_z1a0000"},{"extrans",vague=40,rule="rule_001"},{"text"},linkback="block_00056",delay={vl01={{"fg",file="kan_z1a0200",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1a",face="a0034",x=-280,id=9,ex05="kan_z1a0000"}}},line=433,linknext="block_00058",text={ja={{"「打扰啦。大家选举成功恭喜……话说，选举成功是不是因为没有其他候选人呢？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00115",ch="kan"}}}},block_00016={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="zbg013a",by=-360,ax=1280,bx=-640,ay=720,sync=0,path=":bg/"},{"fg",file="hiy_z1b0200",ch="妃愛",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0032",id=13,sync=0,ex05="hiy_z1b0000"},{"cgdel",time=400,lv=1,rule="cinema",sync=0,id=5},{"extrans",time=500},{"text"},linkback="block_00015",delay={vl03={{"fgact",lv=2.2999999999999998,ch="妃愛",act="おじぎ",id=13}},vl01={{"fg",file="hiy_z1b0200",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0019",id=13,ex05="hiy_z1b0000"}},vl02={{"fg",file="hiy_z1b0210",ch="妃愛",lv=2.2999999999999998,mode=3,size="z1",path=":fg/hiy/z1/",head="hiy_z1b",face="b0006",id=13,ex05="hiy_z1b0010"}}},line=132,linknext="block_00017",text={ja={{"「上一届的学生会长是我哥哥。虽然这么说有些不好意思，但我觉得即使从妹妹的视角看，他也是一个做得非常出色的、我期望能向其看齐的兄长，所以我想把他作为我的个人目标。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00679",ch="hiy"}}}},block_00086={{"fg",file="asu_z1a0200",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1a",face="a0054",x=430,id=8,ex05="asu_z1a0000"},{"text"},linkback="block_00085",delay={vl01={{"fg",file="asu_z1a0200",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1a",face="a0053",x=430,id=8,ex05="asu_z1a0000"}}},line=619,linknext="block_00087",text={ja={{"「虽然冬天做豚骨汤很冷，但也很有趣。今年我还想再做呢。」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00119",ch="asu"}}}},block_00085={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="zbg011a",by=-360,ax=1280,bx=-640,ay=720,sync=0,path=":bg/"},{"fg",file="kan_z1a0210",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1a",face="a0019",x=-430,id=7,sync=0,ex05="kan_z1a0010"},{"fg",file="ame_z1a0210",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1a",face="a0054",x=0,id=13,sync=0,ex03="a0097",ex05="ame_z1a0010"},{"fg",file="asu_z1a0200",ch="あすみ",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1a",face="a0053",x=430,id=8,sync=0,ex05="asu_z1a0000"},{"extrans",vague=40,rule="rule_001"},{"text"},linkback="block_00084",delay={vl01={{"fg",file="kan_z1a0210",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1a",face="a0034",x=-430,id=7,ex05="kan_z1a0010"}}},line=613,linknext="block_00086",text={ja={{"「像是足球队的应援活动，真的出了好多汗。在炎炎夏日的阳光下一直站着……」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00119",ch="kan"}}}},block_00084={{"fg",file="kan_z1b0210",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0001",x=0,id=9,ex05="kan_z1b0010"},{"fg",file="shi_z1a0500",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1a",face="a0032",x=-430,id=7,ex05="shi_z1a0000"},{"fg",file="ame_z1a0200",ch="天梨",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ame/z1/",head="ame_z1a",face="a0053",x=430,id=10,ex05="ame_z1a0000"},{"text"},line=599,linkback="block_00083",linknext="block_00085",text={ja={{"「华乃一直都在更新微博，锦每个月都写新闻资讯，我、和天梨一样和当然新的学生会役员们都很努力呀。」",{"rt2"},name={"智宏"}}}}},block_00126={{"se",stop=1,id=1,time=500},{"fg",hoho="h",ch="妃愛",lv=2.3999999999999999,mode=3,size="z2",path=":fg/hiy/z2/",head="hiy_z2b",face="b0008h",id=12,ex05="hiy_z2b0000",file="hiy_z2b0200"},{"fgact",lv=2.3999999999999999,ch="妃愛",act="びっくり",id=12},{"quake",mode="mw"},{"text"},line=899,linkback="block_00125",linknext="block_00127",text={ja={{"「呼哈哈！！！」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00696",ch="hiy"}}}},block_00078={{"fg",file="shi_z1a0510",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1a",face="a0039",x=-430,id=7,ex05="shi_z1a0010"},{"text"},linkback="block_00077",delay={vl01={{"fg",file="shi_z1a0510",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1a",face="a0053",x=-430,id=7,ex05="shi_z1a0010"}}},line=569,linknext="block_00079",text={ja={{"「新闻通讯等一般都被扔在书包底部或抽屉里，大部分都被扔进垃圾桶。但我听说去年文化祭后，有学生到教师办公室来复印这些材料。」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00222",ch="shi"}}}},block_00032={{"bgm",stop=1},{"cgdel",id=-1},{"bg",file="black",time=1000,vague=40,rule="rule_002",path=":bg/",wait=500},{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="bg011a",sync=0,path=":bg/"},{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=3,size="no",path=":fg/ina/no/",head="ina_noa",face="a0055",x=-480,id=5,sync=0,ex05="ina_noa0000"},{"fg",file="asu_nob0210",ch="あすみ",lv=2.2000000000000002,mode=3,size="no",path=":fg/asu/no/",head="asu_nob",face="b0053",x=-160,id=11,sync=0,ex05="asu_nob0010"},{"fg",file="hiy_noa0200",ch="妃愛",lv=2.2000000000000002,mode=3,size="no",path=":fg/hiy/no/",head="hiy_noa",face="a0022",x=160,id=12,sync=0,ex05="hiy_noa0000"},{"fg",file="kna_noa0200",ch="神田",lv=2.2000000000000002,mode=3,size="no",path=":fg/kna/no/",head="kna_noa",face="a0055",x=480,id=6,sync=0,ex05="kna_noa0000"},{"cgdel",time=400,lv=1,rule="cinema",sync=0,id=5},{"extrans",vague=40,rule="rule_002r"},{"bgm",file="bgm07"},{"text"},line=254,linkback="block_00031",linknext="block_00033",text={ja={{"「大家辛苦了，就职演说做得很好。我有点被感动到了。」",{"rt2"},name={"智宏"}}}}},block_00164={{"text"},line=1071,linkback="block_00163",linknext="block_00165",text={ja={{"「安全！」",{"rt2"},name={"智宏"}}}}},block_00023={{"fg",file="ina_z1a0200",ch="飯田",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ina/z1/",head="ina_z1a",face="a0013",x=-430,id=7,ex05="ina_z1a0000"},{"fgact",lv=2.2999999999999998,ch="飯田",act="おじぎ",id=7},{"text"},linkback="block_00022",delay={vl01={{"fg",file="ina_z1a0200",ch="飯田",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ina/z1/",head="ina_z1a",face="a0054",x=-430,id=7,ex05="ina_z1a0000"}},vl02={{"fgact",lv=2.2999999999999998,ch="飯田",act="おじぎ",id=7}}},line=190,linknext="block_00024",text={ja={{"「我是参选书记的饭田。我将接过之前担任书记的锦的重任，努力使学生会的工作能够顺利进行。」",{"rt2"},name={"飯田 & 伊々奈"}}},vo={{"vo",file="fem_ina_00094",ch="ina"}}}},block_00160={{"text"},line=1056,linkback="block_00159",linknext="block_00161",text={ja={{"如果问为什么这种幸福会让人感到复杂，那可能是因为在电影中我们只是家人，但在现实中我们的关系却超越了那个范畴。",{"rt2"}}}}},block_00107={{"fg",file="ina_z1a0200",ch="飯田",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ina/z1/",head="ina_z1a",face="a0002",x=-430,id=7,ex05="ina_z1a0000"},{"text"},linkback="block_00106",delay={vl01={{"fg",file="ina_z1a0200",ch="飯田",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ina/z1/",head="ina_z1a",face="a0020",x=-430,id=7,ex05="ina_z1a0000"}},vl02={{"fg",file="ina_z1a0200",ch="飯田",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ina/z1/",head="ina_z1a",face="a0019",x=-430,id=7,ex05="ina_z1a0000"}}},line=799,linknext="block_00108",text={ja={{"「我还想向你学习更多的东西……虽然我很平淡，但你以后还会和我做朋友吗？」",{"rt2"},name={"飯田 & 伊々奈"}}},vo={{"vo",file="fem_ina_00099",ch="ina"}}}},block_00167={{"text"},line=1077,linkback="block_00166",linknext="block_00168",text={ja={{"等我去了那个世界，见到我们的父母时，我还是会把和妃爱的爱情故事藏在心底，「我们两个人一直到最后都过得很幸福」我会这样告诉彼此。",{"rt2"}}}}},block_00024={{"fg",file="ina_z1a0200",ch="飯田",lv=2.2999999999999998,mode=3,size="z1",path=":fg/ina/z1/",head="ina_z1a",face="a0004",x=-430,id=7,ex05="ina_z1a0000"},{"text"},line=195,linkback="block_00023",linknext="block_00025",text={ja={{"「我也是因为仰慕上任会长才加入学生会的。」",{"rt2"},name={"飯田 & 伊々奈"}}},vo={{"vo",file="fem_ina_00095",ch="ina"}}}},block_00100={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",file="zbg011a",ax=1280,by=-360,x=400,bx=-640,ay=720,sync=0,path=":bg/"},{"fg",hoho="h",ch="あすみ",resize=1,lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1b",face="b0054h",x=-280,id=9,sync=0,ex05="asu_z1b0000",file="asu_z1b0200"},{"fg",hoho="h",ch="華乃",lv=2.2999999999999998,mode=3,size="z1",path=":fg/kan/z1/",head="kan_z1b",face="b0030h",x=280,id=10,sync=0,ex05="kan_z1b0000",file="kan_z1b0200"},{"extrans",vague=40,rule="rule_001"},{"text"},linkback="block_00099",delay={vl01={{"fgact",lv=2.2999999999999998,ch="あすみ",act="おじぎ",id=9}},vl02={{"fg",file="asu_z1b0200",ch="あすみ",lv=2.2999999999999998,mode=3,size="z1",path=":fg/asu/z1/",head="asu_z1b",face="b0023",x=-280,id=9,ex05="asu_z1b0000"}}},line=732,linknext="block_00101",text={ja={{"「常磐前辈，谢谢你。我很高兴在被邀请加入学生会的那一天，有你在场。」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00120",ch="asu"}}}},block_00050={{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=1,size="no",path=":fg/ina/no/",head="ina_noa",face="a0040",id=20,ex05="ina_noa0000"},{"text"},line=377,linkback="block_00049",linknext="block_00051",text={ja={{"「妃爱最初总是称呼亚澄为「锦，锦」，但不知从何时开始，变成了「小锦」。」",{"rt2"},name={"飯田 & 伊々奈"}}},vo={{"vo",file="fem_ina_00097",ch="ina"}}}},block_00036={{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=3,size="no",path=":fg/ina/no/",head="ina_noa",face="a0022",x=-480,id=5,ex05="ina_noa0000"},{"text"},linkback="block_00035",delay={vl01={{"fg",file="ina_noa0200",ch="飯田",lv=2.2000000000000002,mode=3,size="no",path=":fg/ina/no/",head="ina_noa",face="a0054",x=-480,id=5,ex05="ina_noa0000"}}},line=289,linknext="block_00037",text={ja={{"「前辈，我现在去给您泡茶。座位都是空的，您随便找个地方坐吧！」",{"rt2"},name={"飯田 & 伊々奈"}}},vo={{"vo",file="fem_ina_00096",ch="ina"}}}},block_00077={{"fg",file="shi_z1b0510",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1b",face="b0053",x=-430,id=7,ex05="shi_z1b0010"},{"text"},linkback="block_00076",delay={vl01={{"fg",file="shi_z1b0500",ch="詩桜",lv=2.2999999999999998,mode=3,size="z1",path=":fg/shi/z1/",head="shi_z1b",face="b0032",x=-430,id=7,ex05="shi_z1b0000"}}},line=563,linknext="block_00078",text={ja={{"「你不是说要加强宣传活动吗？妃爱你说出来就有说服力。」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00221",ch="shi"}}}}}