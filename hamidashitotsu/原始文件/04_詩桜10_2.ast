astver = 2.0
ast = {block_00037={{"fg",ch="華乃",mode=3,face="a0034",path=":fg/kan/no/",size="no",head="kan_noa",ex05="kan_noa0000",id=9,x=-230,lv=2.2000000000000002,file="kan_noa0200"},{"fgact",ch="華乃",act="うなづく",id=9,lv=2.2000000000000002},{"text"},text={vo={{"vo",ch="kan",file="fem_kan_00201"}},ja={{"「哈哈哈，我只是在怀疑自己的能力，所以没关系。虽然我跟诗樱学姐说过我是专业的，但每次展示作品时都会紧张得心跳加速」",{"rt2"},name={"華乃"}}}},line=190,linknext="block_00038",linkback="block_00036",delay={vl01={{"fg",ch="華乃",mode=3,face="a0019",path=":fg/kan/no/",size="no",head="kan_noa",ex05="kan_noa0000",id=9,x=-230,lv=2.2000000000000002,file="kan_noa0200"}}}},block_00091={{"fg",ch="華乃",mode=1,face="b0034",path=":fg/kan/no/",size="no",head="kan_nob",ex05="kan_nob0000",id=20,lv=2.2000000000000002,file="kan_nob0200",ex02="b0098"},{"text"},linkback="block_00090",linknext="block_00092",text={vo={{"vo",ch="kan",file="fem_kan_00203"}},ja={{"「妃爱的和泉技能偶尔真厉害。能挺得住她得意的技能，还不觉得痛苦，真是厉害」",{"rt2"},name={"華乃"}}}},line=501},block_00076={{"fg",ch="妃愛",mode=3,face="a0003",path=":fg/hiy/z1/",size="z1",head="hiy_z1a",ex05="hiy_z1a0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1a0200"},{"text"},linkback="block_00075",linknext="block_00077",text={vo={{"vo",ch="hiy",file="fem_hiy_01366"}},ja={{"「表演需要表演者、观众和地点这三者才能完整。你们应该好好考虑一下，这个表演是为了谁。」",{"rt2"},name={"妃愛"}}}},line=419},block_00065={{"msgoff"},{"bg",path=":bg/",sync=0,file="bg011a"},{"fg",ch="妃愛",mode=3,face="b0019",path=":fg/hiy/no/",size="no",head="hiy_nob",sync=0,ex05="hiy_nob0000",id=7,lv=2.2000000000000002,x=-380,file="hiy_nob0200",resize=1},{"fg",ch="あすみ",mode=3,face="a0010",path=":fg/asu/no/",size="no",head="asu_noa",ex05="asu_noa0000",sync=0,id=13,x=0,lv=2.2000000000000002,file="asu_noa0200"},{"fg",ch="天梨",mode=3,face="a0002",path=":fg/ame/no/",size="no",head="ame_noa",ex05="ame_noa0000",sync=0,id=8,x=380,lv=2.2000000000000002,file="ame_noa0200"},{"extrans",time=500},{"text"},linkback="block_00064",linknext="block_00066",text={vo={{"vo",ch="ame",file="fem_ame_00068"}},ja={{"「不过，诗樱前辈之前说过，记住去年的戏剧并期待今年我们再次献艳的观众，如果发现主角换人了，可能会感到失望吧？」",{"rt2"},name={"天梨"}}}},line=353},block_00071={{"fg",ch="妃愛",mode=3,face="b0027",path=":fg/hiy/no/",size="no",head="hiy_nob",ex05="hiy_nob0000",id=7,x=-380,lv=2.2000000000000002,file="hiy_nob0200"},{"text"},linkback="block_00070",linknext="block_00072",text={vo={{"vo",ch="hiy",file="fem_hiy_01363"}},ja={{"「但是，即使我尽百分之一百的努力去表演，如果最想打动的人的头脑里混入了百分之一的噪音，那么这个表演就无法达到最高的评价。」",{"rt2"},name={"妃愛"}}}},line=389},block_00005={{"fg",ch="詩桜",mode=3,face="b0052",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=13,lv=2.2999999999999998,file="shi_z1b0210"},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00890"}},ja={{"「对，就是你。如果是我，听到他们的话大概五分钟就会让他们回去了。现在的我可是你的副会长呢」",{"rt2"},name={"詩桜"}}}},line=41,linknext="block_00006",linkback="block_00004",delay={vl02={{"fg",ch="詩桜",mode=3,face="b0032",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=13,lv=2.2999999999999998,file="shi_z1b0210"}},vl01={{"fg",ch="詩桜",mode=3,face="b0039",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=13,lv=2.2999999999999998,file="shi_z1b0210"}}}},block_00039={{"fg",ch="詩桜",mode=3,face="a0053",path=":fg/shi/no/",size="no",head="shi_noa",ex05="shi_noa0010",id=10,x=230,lv=2.2000000000000002,file="shi_noa0210"},{"fgact",ch="詩桜",act="うなづく",id=10,lv=2.2000000000000002},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00903"}},ja={{"「对，那种新鲜感就是最重要的。我们接下来需要打造生徒会项目的实绩。我们将去各种各样的设施进行参观，并向老人和孩子们展示这个纸片剧」",{"rt2"},name={"詩桜"}}}},line=203,linknext="block_00040",linkback="block_00038",delay={vl01={{"fg",ch="詩桜",mode=3,face="a0003",path=":fg/shi/no/",size="no",head="shi_noa",ex05="shi_noa0010",id=10,x=230,lv=2.2000000000000002,file="shi_noa0210"}}}},block_00033={{"text"},linkback="block_00032",linknext="block_00034",text={ja={{"可能是因为故事很传统，常磐画出的画面应该与大家脑海中想象的很接近。这样的成果让我有这样的感觉。",{"rt2"}}}},line=169},block_00004={{"text"},linkback="block_00003",linknext="block_00005",text={ja={{"「我吗？」",{"rt2"},name={"智宏"}}}},line=34},label={top={block="block_00000",label=1}},block_00011={{"fg",ch="詩桜",mode=3,face="a0028",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0000",id=13,lv=2.2999999999999998,file="shi_z1a0200"},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00892"}},ja={{"「感动什么呢。我还是提醒你一下，就像我今早说过的，人的本性不会那么容易改变。我现在仍然是自私的。」",{"rt2"},name={"詩桜"}}}},line=62,linknext="block_00012",linkback="block_00010",delay={vl01={{"fg",ch="詩桜",mode=3,face="a0001",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0000",id=13,lv=2.2999999999999998,file="shi_z1a0200"}}}},block_00085={{"fg",ch="妃愛",mode=1,face="b0010",path=":fg/hiy/z1/",size="z1",head="hiy_z1b",ex05="hiy_z1b0000",id=20,lv=2.2999999999999998,file="hiy_z1b0200"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_01369"}},ja={{"「你这么突然说出来，虽然我的话还没说完，但你就开始讲自己完全无关的事情了。你真是个大人物啊。」",{"rt2"},name={"妃愛"}}}},line=464,linknext="block_00086",linkback="block_00084",delay={vl01={{"fg",ch="妃愛",mode=1,face="b0043",path=":fg/hiy/z1/",size="z1",head="hiy_z1b",ex05="hiy_z1b0000",id=20,lv=2.2999999999999998,file="hiy_z1b0200"}}}},block_00026={{"fg",ch="詩桜",mode=3,face="a0019",path=":fg/shi/z2/",size="z2",head="shi_z2a",ex05="shi_z2a0000",id=13,lv=2.3999999999999999,file="shi_z2a0200"},{"text"},linkback="block_00025",linknext="block_00027",text={vo={{"vo",ch="shi",file="fem_shi_00901"}},ja={{"「只要我没有错当你的恋人就好了」",{"rt2"},name={"詩桜"}}}},line=133},block_00013={{"fg",ch="詩桜",mode=3,face="a0055",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0000",id=13,lv=2.2999999999999998,file="shi_z1a0200"},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00894"}},ja={{"「但即便如此，我还会去做。因为我想待在你身边。这不仅仅是作为学生会成员的态度，也是为了与伴侣共度一生所必需的理解。」",{"rt2"},name={"詩桜"}}}},line=74,linknext="block_00014",linkback="block_00012",delay={vl01={{"fg",ch="詩桜",mode=3,face="a0052",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0000",id=13,lv=2.2999999999999998,file="shi_z1a0200"}}}},block_00000={{"bgm",file="bgm15"},{"bg",path=":bg/",vague=40,rule="rule_002r",file="bg011a"},{"text"},linknext="block_00001",text={ja={{"那个后辈好像已经多次道谢然后先行离开了。她应该会经常和母亲或医院进行深入的交谈。希望能有好的结果。",{"rt2"}}}},line=13},block_00025={{"fg",ch="詩桜",mode=3,face="a0038",path=":fg/shi/z2/",size="z2",head="shi_z2a",ex05="shi_z2a0000",id=13,lv=2.3999999999999999,file="shi_z2a0200"},{"text"},linkback="block_00024",linknext="block_00026",text={vo={{"vo",ch="shi",file="fem_shi_00900"}},ja={{"「当然，她因为「家庭照顾」导致出勤天数不足，你想帮助她，我也同情她。所以，也不能说没有善意在里面」",{"rt2"},name={"詩桜"}}}},line=128},block_00054={{"fg",ch="妃愛",mode=3,face="a0008",path=":fg/hiy/no/",size="no",head="hiy_noa",ex05="hiy_noa0000",id=13,lv=2.2000000000000002,file="hiy_noa0200"},{"fgact",ch="妃愛",act="いいえ",id=13,lv=2.2000000000000002},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_01354"}},ja={{"「啊哦不是，其实。呃，他们只是跟我说让我看看成品，并没有明确给出许可或者拒绝的回应。」",{"rt2"},name={"妃愛"}}}},line=286,linknext="block_00055",linkback="block_00053",delay={vl01={{"fg",ch="妃愛",mode=3,face="a0020",path=":fg/hiy/no/",size="no",head="hiy_noa",ex05="hiy_noa0000",id=13,lv=2.2000000000000002,file="hiy_noa0200"}}}},block_00112={{"fg",ch="詩桜",mode=3,face="b0054",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=9,lv=2.2999999999999998,file="shi_z1b0210"},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00917"}},ja={{"「我刚刚被ひよりん虐待了，这只是小小的报复。请允许我竞选正式表演的角色」",{"rt2"},name={"詩桜"}}}},line=600,linknext="block_00113",linkback="block_00111",delay={vl01={{"fg",ch="詩桜",mode=3,face="b0006",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=9,lv=2.2999999999999998,file="shi_z1b0210"}}}},block_00082={{"fg",ch="妃愛",mode=3,face="b0002",path=":fg/hiy/z1/",size="z1",head="hiy_z1b",ex05="hiy_z1b0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1b0200"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_01368"}},ja={{"「今年，我并不是学生会干部。但是，有谁为了在文化节上帮助我的哥哥，甚至决定多留在学校一年并加入学生会呢！」",{"rt2"},name={"妃愛"}}}},line=443,linknext="block_00083",linkback="block_00081",delay={vl02={{"fg",ch="妃愛",mode=3,face="b0022",path=":fg/hiy/z1/",size="z1",head="hiy_z1b",ex05="hiy_z1b0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1b0200"}},vl01={{"fg",ch="妃愛",mode=3,face="b0053",path=":fg/hiy/z1/",size="z1",head="hiy_z1b",ex05="hiy_z1b0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1b0200"}}}},block_00117={{"text"},linkback="block_00116",linknext="block_00118",text={ja={{"最后一张翻过后，大家一致决定把文化祭表演全权交给学长。使出全力的我的恋人，果然特别地帅气。",{"rt2"}}}},line=625},block_00100={{"fg",ch="詩桜",mode=3,face="a0038",path=":fg/shi/no/",size="no",head="shi_noa",ex05="shi_noa0000",id=9,lv=2.2000000000000002,file="shi_noa0200"},{"text"},linkback="block_00099",linknext="block_00101",text={vo={{"vo",ch="shi",file="fem_shi_00913"}},ja={{"「连你也要逼我到绝路上吗……」",{"rt2"},name={"詩桜"}}}},line=542},block_00018={{"msgoff"},{"bg",zoom=120,by=-360,sync=0,path=":bg/",bx=-640,ay=720,ax=1280,file="zbg011a"},{"fg",ch="詩桜",mode=3,face="b0053",path=":fg/shi/z2/",size="z2",head="shi_z2b",ex05="shi_z2b0000",sync=0,id=13,lv=2.3999999999999999,file="shi_z2b0200",resize=1},{"extrans",time=500},{"text"},linkback="block_00017",linknext="block_00019",text={ja={{"「诗樱前辈接受这次咨询的态度，我觉得很棒。如果问我的方针是否错误，我想回答七兆分。」",{"rt2"},name={"智宏"}}}},line=94},block_00056={{"msgoff"},{"bg",by=-360,sync=0,path=":bg/",bx=-640,ay=720,ax=1280,file="zbg011a"},{"fg",ch="詩桜",mode=3,face="b0002",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",sync=0,id=9,x=-280,lv=2.2999999999999998,file="shi_z1b0210"},{"fg",ch="妃愛",mode=3,face="a0001",path=":fg/hiy/z1/",size="z1",head="hiy_z1a",sync=0,ex05="hiy_z1a0000",id=10,lv=2.2999999999999998,x=280,file="hiy_z1a0200",resize=1},{"extrans",time=500},{"bgm",file="bgm34"},{"text"},linkback="block_00055",linknext="block_00057",text={vo={{"vo",ch="hiy",file="fem_hiy_01355"}},ja={{"「这个戏剧，诗樱前辈会在文化节前后进行朗读吧？」",{"rt2"},name={"妃愛"}}}},line=300},block_00115={{"fg",ch="詩桜",mode=3,face="a0003",path=":fg/shi/no/",size="no",head="shi_noa",ex05="shi_noa0000",id=9,lv=2.2000000000000002,file="shi_noa0200"},{"text"},linkback="block_00114",linknext="block_00116",text={vo={{"vo",ch="shi",file="fem_shi_00919"}},ja={{"「我希望能自己解决这次的问题。请欣赏我的纸板戏剧吧」",{"rt2"},name={"詩桜"}}}},line=619},block_00007={{"text"},linkback="block_00006",linknext="block_00008",text={ja={{"不喜欢麻烦、不愿向人低头的学姐之所以改变自己，完全是为了我。",{"rt2"}}}},line=49},block_00023={{"text"},linkback="block_00022",linknext="block_00024",text={ja={{"嗯……是这样呢。虽然不能出钱，但如果妃爱的事情是事实，就必须做出牺牲自己的决定了。",{"rt2"}}}},line=117},block_00102={{"text"},linkback="block_00101",linknext="block_00103",text={ja={{"本来真的真的没有那个打算，但在看到里姐开心地笑的样子后，我意识到了。对啊，我现在的话语，也是对诗樱前辈的反击吗。",{"rt2"}}}},line=547},block_00040={{"msgoff"},{"bg",bx=-640,by=-360,sync=0,path=":bg/",ay=720,x=200,ax=1280,file="zbg011a"},{"fg",ch="詩桜",mode=3,face="a0039",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0010",sync=0,id=10,lv=2.2999999999999998,file="shi_z1a0210",resize=1},{"extrans",time=500},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00904"}},ja={{"「在接下来的三周里，我们必须预约访问并获得演出许可……这可能会很困难。」",{"rt2"},name={"詩桜"}}}},line=215,linknext="block_00041",linkback="block_00039",delay={vl01={{"fg",ch="詩桜",mode=3,face="a0028",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0010",id=10,lv=2.2999999999999998,file="shi_z1a0210"},{"fgact",ch="詩桜",act="おじぎ",id=10,lv=2.2999999999999998}}}},block_00035={{"text"},linkback="block_00034",linknext="block_00036",text={ja={{"不仅接近大家脑海中的想象，甚至超越了一些人的想象。锦像是第一次去水族馆的小女孩一样眼睛闪闪发光。超级可爱。",{"rt2"}}}},line=176},block_00073={{"fg",ch="妃愛",mode=3,face="a0003",path=":fg/hiy/z1/",size="z1",head="hiy_z1a",ex05="hiy_z1a0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1a0200"},{"text"},linkback="block_00072",linknext="block_00074",text={vo={{"vo",ch="hiy",file="fem_hiy_01364"}},ja={{"「姐姐，听我说」",{"rt2"},name={"妃愛"}}}},line=405},block_00038={{"fg",ch="華乃",mode=3,face="a0055",path=":fg/kan/no/",size="no",head="kan_noa",ex05="kan_noa0000",id=9,x=-230,lv=2.2000000000000002,file="kan_noa0200"},{"text"},text={vo={{"vo",ch="kan",file="fem_kan_00202"}},ja={{"「但说实话，感觉像是打开了自己新的一扇门，真的很有趣！考虑到这是给孩子们看的，所以与平时有些不同，也有一点新鲜感吧」",{"rt2"},name={"華乃"}}}},line=196,linknext="block_00039",linkback="block_00037",delay={vl01={{"fg",ch="華乃",mode=3,face="a0052",path=":fg/kan/no/",size="no",head="kan_noa",ex05="kan_noa0000",id=9,x=-230,lv=2.2000000000000002,file="kan_noa0200"}}}},block_00003={{"fg",ch="詩桜",mode=3,face="a0008",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0000",id=13,lv=2.2999999999999998,file="shi_z1a0200"},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00889"}},ja={{"「是吗？ 嗯，如果你这么觉得，那应该就没错。我只是想着如果是你会怎么做，然后采取了你可能会选择的方式罢了」",{"rt2"},name={"詩桜"}}}},line=31,linknext="block_00004",linkback="block_00002",delay={vl02={{"fg",ch="詩桜",mode=3,face="a0022",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0000",id=13,lv=2.2999999999999998,file="shi_z1a0200"}},vl01={{"fg",ch="詩桜",mode=3,face="a0055",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0000",id=13,lv=2.2999999999999998,file="shi_z1a0200"}}}},block_00108={{"fg",ch="詩桜",mode=3,face="a0032",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0010",id=9,lv=2.2999999999999998,file="shi_z1a0210"},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00916"}},ja={{"「如果结果能成功挽回之后，心情将会多么晴朗啊。应该知道前辈们表现得多美好。就像去年的文化祭一样」",{"rt2"},name={"詩桜"}}}},line=579,linknext="block_00109",linkback="block_00107",delay={vl01={{"fg",ch="詩桜",mode=3,face="a0022",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0010",id=9,lv=2.2999999999999998,file="shi_z1a0210"}}}},block_00111={{"fg",ch="妃愛",mode=1,face="a0047",path=":fg/hiy/z1/",size="z1",head="hiy_z1a",ex05="hiy_z1a0000",id=20,lv=2.2999999999999998,file="hiy_z1a0200"},{"text"},linkback="block_00110",linknext="block_00112",text={vo={{"vo",ch="hiy",file="fem_hiy_01372"}},ja={{"「诗樱学姐……」",{"rt2"},name={"妃愛"}}}},line=594},block_00095={{"fg",ch="華乃",mode=1,face="a0019",path=":fg/kan/no/",size="no",head="kan_noa",ex05="kan_noa0000",id=20,lv=2.2000000000000002,file="kan_noa0200"},{"text"},linkback="block_00094",linknext="block_00096",text={vo={{"vo",ch="kan",file="fem_kan_00204"}},ja={{"「嗯，呢，其实在我们这些人中，对演戏最了解的就是妃爱，她都这么说了？」",{"rt2"},name={"華乃"}}}},line=523},block_00008={{"text"},linkback="block_00007",linknext="block_00009",text={ja={{"曾经说过「对可能接触的人无需考虑」 的诗樱学姐，却削减了自己的时间，伸出了充满同理心的援手。",{"rt2"}}}},line=51},block_00092={{"text"},linkback="block_00091",linknext="block_00093",text={ja={{"仅仅是模仿爸爸读给我听的纸芝居读出来，即便是这种只会骗过孩子的假戏，也被记住了呢。",{"rt2"}}}},line=503},block_00057={{"fg",ch="詩桜",mode=3,face="b0027",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=9,x=-280,lv=2.2999999999999998,file="shi_z1b0210"},{"fgact",ch="詩桜",act="うなづく",id=9,lv=2.2999999999999998},{"text"},linkback="block_00056",linknext="block_00058",text={vo={{"vo",ch="shi",file="fem_shi_00910"}},ja={{"「我是打算这样做的。作为策划人，我会负责到11月。」",{"rt2"},name={"詩桜"}}}},line=306},block_00063={{"fg",ch="妃愛",mode=3,face="b0038",path=":fg/hiy/z1/",size="z1",head="hiy_z1b",ex05="hiy_z1b0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1b0200"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_01359"}},ja={{"「并不仅仅是因为故事是由诗樱前辈写的，她最理解内容……虽然我作为一个演员也打算把这个剧本变成自己的东西。」",{"rt2"},name={"妃愛"}}}},line=337,linknext="block_00064",linkback="block_00062",delay={vl01={{"fg",ch="妃愛",mode=3,face="b0003",path=":fg/hiy/z1/",size="z1",head="hiy_z1b",ex05="hiy_z1b0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1b0200"}}}},block_00107={{"fg",ch="詩桜",mode=3,face="a0003",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0010",id=9,lv=2.2999999999999998,file="shi_z1a0210"},{"text"},linkback="block_00106",linknext="block_00108",text={vo={{"vo",ch="shi",file="fem_shi_00915"}},ja={{"「仔细想来，这可能是我用副会长的身份挽回在学生会所犯错失的难得机会啊」",{"rt2"},name={"詩桜"}}}},line=569},block_00080={{"text"},linkback="block_00079",linknext="block_00081",text={ja={{"即使有很多人来看「妃爱的舞台」，能把「学生会的舞台」当作主要观赏目标的人，我想可能只有一个。",{"rt2"}}}},line=430},block_00106={{"fg",ch="詩桜",mode=3,face="b0038",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=9,lv=2.2999999999999998,file="shi_z1b0210"},{"text"},linkback="block_00105",linknext="block_00107",text={vo={{"vo",ch="shi",file="fem_shi_00914"}},ja={{"「首先，我想要给所有人讲一个纸片剧，然后在此基础上请他们做判断……」",{"rt2"},name={"詩桜"}}}},line=564},block_00055={{"text"},linkback="block_00054",linknext="block_00056",text={ja={{"之前还显得兴高采烈的诗樱前辈脸上现出了不安的阴影。本来不愿摆出虚弱姿态给他人看的她竟然露出了这样的神色。",{"rt2"}}}},line=288},block_00105={{"text"},linkback="block_00104",linknext="block_00106",text={ja={{"但她脸上的表情并非是被逼无奈而只得放弃，而是由自己的决心做出了觉悟。",{"rt2"}}}},line=559},block_00104={{"msgoff"},{"bg",bx=-640,by=-360,sync=0,path=":bg/",ay=720,x=200,ax=1280,file="zbg011a"},{"fg",ch="詩桜",mode=3,face="b0029",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",sync=0,id=9,lv=2.2999999999999998,file="shi_z1b0210",resize=1},{"extrans",time=500},{"text"},linkback="block_00103",linknext="block_00105",text={ja={{"去年说出的话语一个接一个地降临在自我身上，诗樱前辈像是认命一般低下了头。",{"rt2"}}}},line=557},block_00020={{"fg",ch="詩桜",mode=3,face="b0003",path=":fg/shi/z2/",size="z2",head="shi_z2b",ex05="shi_z2b0010",id=13,lv=2.3999999999999999,file="shi_z2b0210"},{"fgact",ch="詩桜",act="いいえ",id=13,lv=2.3999999999999999},{"text"},linkback="block_00019",linknext="block_00021",text={vo={{"vo",ch="shi",file="fem_shi_00896"}},ja={{"「不，即使有些困难，你也会和我做同样的事情。因为你也和和泉老师一起计划了补足妃爱的出勤天数。」",{"rt2"},name={"詩桜"}}}},line=103},block_00101={{"text"},linkback="block_00100",linknext="block_00102",text={ja={{"「哎呀！？不、不是的，我是想站在诗樱学姐这边……！」",{"rt2"},name={"智宏"}}}},line=545},block_00099={{"text"},linkback="block_00098",linknext="block_00100",text={ja={{"「我觉得，除非诗樱学姐自己说「做」，否则我们不应该强迫她」",{"rt2"},name={"智宏"}}}},line=537},block_00022={{"fg",ch="詩桜",mode=3,face="b0001",path=":fg/shi/z2/",size="z2",head="shi_z2b",ex05="shi_z2b0010",id=13,lv=2.3999999999999999,file="shi_z2b0210"},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00898"}},ja={{"「即使你想用和我一样的手段，从现在这个学期开始让那个学生成为会长，学校方面也不会承认他的实绩。而且你也不可能像我一样抛弃学生会。」",{"rt2"},name={"詩桜"}}}},line=115,linknext="block_00023",linkback="block_00021",delay={vl01={{"fg",ch="詩桜",mode=3,face="b0028",path=":fg/shi/z2/",size="z2",head="shi_z2b",ex05="shi_z2b0010",id=13,lv=2.3999999999999999,file="shi_z2b0210"}}}},block_00098={{"text"},linkback="block_00097",linknext="block_00099",text={ja={{"「如果借用妃爱的话，只关心听众和场地气氛，忽视表演者本人也不好」",{"rt2"},name={"智宏"}}}},line=534},block_00113={{"text"},linkback="block_00112",linknext="block_00114",text={ja={{"诗樱学姐开始认真起来了。既然如此，这个有才华又努力的人肯定会很强的。",{"rt2"}}}},line=602},block_00049={{"fg",ch="詩桜",mode=3,face="b0032",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=10,lv=2.2999999999999998,file="shi_z1b0210"},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00908"}},ja={{"「在这一点上，非常感谢常磐同学因为「针对孩子们」这个理由而感到有干劲。得向他学习关心受众的心情。」",{"rt2"},name={"詩桜"}}}},line=256,linknext="block_00050",linkback="block_00048",delay={vl01={{"fg",ch="詩桜",mode=3,face="b0023",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=10,lv=2.2999999999999998,file="shi_z1b0210"}}}},block_00034={{"fg",ch="あすみ",mode=1,face="b0051",path=":fg/asu/no/",size="no",head="asu_nob",ex05="asu_nob0000",id=20,lv=2.2000000000000002,file="asu_nob0200"},{"text"},linkback="block_00033",linknext="block_00035",text={vo={{"vo",ch="asu",file="fem_asu_00142"}},ja={{"「哈……常磐学姐，你能画出这么温馨的画……而且还是用电子方式……」",{"rt2"},name={"あすみ"}}}},line=174},block_00044={{"text"},linkback="block_00043",linknext="block_00045",text={ja={{"「哦，我也会打电话给千玉市中心的设施！我在技术方面完全没有帮助到你们，所以我至少要努力做到这一点！」",{"rt2"},name={"智宏"}}}},line=235},block_00084={{"msgoff"},{"bg",bx=-640,by=-360,sync=0,path=":bg/",ay=720,x=-400,ax=1280,file="zbg011a"},{"fg",ch="あすみ",mode=3,face="a0010",path=":fg/asu/z1/",size="z1",head="asu_z1a",ex05="asu_z1a0010",sync=0,id=13,lv=2.2999999999999998,file="asu_z1a0210",resize=1},{"extrans",time=500},{"text"},text={vo={{"vo",ch="asu",file="fem_asu_00147"}},ja={{"「啊，那我也……虽然和希鸟的话题完全无关，但是我有关于文化节上如何展示纸板剧的建议……」",{"rt2"},name={"あすみ"}}}},line=458,linknext="block_00085",linkback="block_00083",delay={vl01={{"fg",ch="あすみ",mode=3,face="a0052",path=":fg/asu/z1/",size="z1",head="asu_z1a",ex05="asu_z1a0010",id=13,lv=2.2999999999999998,file="asu_z1a0210"}}}},block_00094={{"msgoff"},{"bg",path=":bg/",sync=0,file="bg011a"},{"fg",ch="詩桜",mode=3,face="a0002",path=":fg/shi/no/",size="no",head="shi_noa",ex05="shi_noa0000",sync=0,id=9,lv=2.2000000000000002,file="shi_noa0200",resize=1},{"extrans",time=500},{"text"},linkback="block_00093",linknext="block_00095",text={vo={{"vo",ch="shi",file="fem_shi_00912"}},ja={{"「已经决定由我来负责正式演出了吗……」",{"rt2"},name={"詩桜"}}}},line=518},block_00093={{"fg",ch="あすみ",mode=3,face="a0053",path=":fg/asu/z1/",size="z1",head="asu_z1a",ex05="asu_z1a0010",id=13,lv=2.2999999999999998,file="asu_z1a0210"},{"text"},text={vo={{"vo",ch="asu",file="fem_asu_00152"}},ja={{"「预计我将负责在屏幕上演绎纸芝居。但在舞台上，让诗樱学姐亲自翻动纸片如何呢？」",{"rt2"},name={"あすみ"}}}},line=509,linknext="block_00094",linkback="block_00092",delay={vl01={{"fg",ch="あすみ",mode=3,face="a0022",path=":fg/asu/z1/",size="z1",head="asu_z1a",ex05="asu_z1a0010",id=13,lv=2.2999999999999998,file="asu_z1a0210"}}}},block_00052={{"fg",ch="妃愛",mode=3,face="b0038",path=":fg/hiy/no/",size="no",head="hiy_nob",ex05="hiy_nob0000",id=13,lv=2.2000000000000002,file="hiy_nob0200"},{"text"},linkback="block_00051",linknext="block_00053",text={vo={{"vo",ch="hiy",file="fem_hiy_01353"}},ja={{"「这个故事，在正式表演时是由我来念的……」",{"rt2"},name={"妃愛"}}}},line=274},block_00110={{"text"},linkback="block_00109",linknext="block_00111",text={ja={{"诗樱的语言不知为何引发了妃爱的反应。我觉得，这并不是令妃爱恐惧的语境啊……。",{"rt2"}}}},line=587},block_00090={{"fg",ch="妃愛",mode=1,face="a0007",path=":fg/hiy/z1/",size="z1",head="hiy_z1a",ex05="hiy_z1a0010",id=20,lv=2.2999999999999998,file="hiy_z1a0210"},{"text"},linkback="block_00089",linknext="block_00091",text={vo={{"vo",ch="hiy",file="fem_hiy_01370"}},ja={{"「是！是！我小时候，哥哥给我读过纸芝居！」",{"rt2"},name={"妃愛"}}}},line=494},block_00015={{"text"},linkback="block_00014",linknext="block_00016",text={ja={{"可能诗樱学姐变得稍微弱了一些。但正因为如此，她变得更加温柔了。都是为了她的恋人——也就是我。",{"rt2"}}}},line=81},block_00051={{"text"},linkback="block_00050",linknext="block_00052",text={ja={{"作品完成后，看起来特别高兴的诗樱先辈在谈论关于文化节的见解，我那过分认真的妹妹突然举手了。",{"rt2"}}}},line=269},block_00103={{"text"},linkback="block_00102",linknext="block_00104",text={ja={{"协助新的学生会成立时，诗樱前辈提出了条件——要我自己说出「做学生会长」的话。",{"rt2"}}}},line=549},block_00061={{"fg",ch="詩桜",mode=3,face="b0008",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=9,x=-280,lv=2.2999999999999998,file="shi_z1b0210"},{"fg",ch="妃愛",mode=3,face="a0008",path=":fg/hiy/z1/",size="z1",head="hiy_z1a",ex05="hiy_z1a0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1a0200"},{"fgact",ch="妃愛",act="いいえ",id=10,lv=2.2999999999999998},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_01357"}},ja={{"「啊，不是说我不想做或者没有自信……我其实非常想参与进来。」",{"rt2"},name={"妃愛"}}}},line=325,linknext="block_00062",linkback="block_00060",delay={vl01={{"fg",ch="妃愛",mode=3,face="a0002",path=":fg/hiy/z1/",size="z1",head="hiy_z1a",ex05="hiy_z1a0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1a0200"}}}},block_00088={{"fg",ch="あすみ",mode=3,face="b0054",path=":fg/asu/z1/",size="z1",head="asu_z1b",ex05="asu_z1b0000",id=13,lv=2.2999999999999998,file="asu_z1b0200"},{"text"},linkback="block_00087",linknext="block_00089",text={vo={{"vo",ch="asu",file="fem_asu_00150"}},ja={{"「如果给老人或者孩子们看，我觉得纸板剧更好。我之所以支持这个项目，也是因为我还记得小时候看的纸板剧……」",{"rt2"},name={"あすみ"}}}},line=481},block_00046={{"fg",ch="詩桜",mode=3,face="b0052",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=10,lv=2.2999999999999998,file="shi_z1b0210"},{"text"},linkback="block_00045",linknext="block_00047",text={vo={{"vo",ch="shi",file="fem_shi_00907"}},ja={{"「最重要的是，我们不能只把\"不仅仅是为了文化节\"当作口号，而应该带着让观众高兴的心态去面对这个活动。」",{"rt2"},name={"詩桜"}}}},line=246},block_00030={{"fg",ch="華乃",mode=3,face="a0031",path=":fg/kan/no/",size="no",head="kan_noa",ex05="kan_noa0010",id=9,x=-230,lv=2.2000000000000002,file="kan_noa0210"},{"fgact",ch="華乃",act="うなづく",id=9,lv=2.2000000000000002},{"text"},linkback="block_00029",linknext="block_00031",text={vo={{"vo",ch="kan",file="fem_kan_00200"}},ja={{"「毕竟我是专业的吧？」",{"rt2"},name={"華乃"}}}},line=161},block_00086={{"fg",ch="あすみ",mode=3,face="a0038",path=":fg/asu/z1/",size="z1",head="asu_z1a",ex05="asu_z1a0000",id=13,lv=2.2999999999999998,file="asu_z1a0200"},{"text"},text={vo={{"vo",ch="asu",file="fem_asu_00148"}},ja={{"「我并不是要抛弃自己的工作，但我觉得这个故事可能更适合用传统方式展示而不是数字化……」",{"rt2"},name={"あすみ"}}}},line=470,linknext="block_00087",linkback="block_00085",delay={vl01={{"fg",ch="あすみ",mode=3,face="a0001",path=":fg/asu/z1/",size="z1",head="asu_z1a",ex05="asu_z1a0000",id=13,lv=2.2999999999999998,file="asu_z1a0200"}}}},block_00060={{"fg",ch="妃愛",mode=3,face="a0018",path=":fg/hiy/z1/",size="z1",head="hiy_z1a",ex05="hiy_z1a0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1a0200"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_01356"}},ja={{"「那么，呃……如果让诗樱前辈在正式演出时也来朗读怎么样呢？」",{"rt2"},name={"妃愛"}}}},line=316,linknext="block_00061",linkback="block_00059",delay={vl01={{"fg",ch="妃愛",mode=3,face="a0007",path=":fg/hiy/z1/",size="z1",head="hiy_z1a",ex05="hiy_z1a0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1a0200"}}}},block_00048={{"text"},linkback="block_00047",linknext="block_00049",text={ja={{"不过，前辈似乎更倾向于那种「只要我觉得有趣，读者就会喜欢我的作品」，而非「迎合读者」的思路，所以他好像认为对作品的自信是给予接续者的方式。",{"rt2"}}}},line=250},block_00014={{"fg",ch="詩桜",mode=3,face="b0023",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0000",id=13,lv=2.2999999999999998,file="shi_z1b0200"},{"text"},linkback="block_00013",linknext="block_00015",text={vo={{"vo",ch="shi",file="fem_shi_00895"}},ja={{"「从现在开始，我希望能够在考虑你会如何行动的同时生活下去，而不只是考虑我自己。」",{"rt2"},name={"詩桜"}}}},line=79},block_00047={{"text"},linkback="block_00046",linknext="block_00048",text={ja={{"虽然这种态度对她来说并不常见，但诗樱学姐作为一位商业上成功的作家，可能一直都把读者（虽然这次是观众）放在心中。",{"rt2"}}}},line=248},block_00083={{"fg",ch="詩桜",mode=3,face="b0020",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0000",id=9,x=-280,lv=2.2999999999999998,file="shi_z1b0200"},{"text"},linkback="block_00082",linknext="block_00084",text={ja={{"即使妃爱自己没有那个意愿，也不得不承认这是一种讽刺。去年那个因为「不是学生会的人」而四处逃避的人，今年却被人用同样的话语对待。",{"rt2"}}}},line=447},block_00081={{"fg",ch="妃愛",mode=3,face="b0022",path=":fg/hiy/z1/",size="z1",head="hiy_z1b",ex05="hiy_z1b0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1b0200"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_01367"}},ja={{"「去年，我能够通过哥哥写的剧本，为哥哥演出一场戏剧。我对此感到非常满意。所以，我非常理解你的感受。」",{"rt2"},name={"妃愛"}}}},line=436,linknext="block_00082",linkback="block_00080",delay={vl01={{"fg",ch="妃愛",mode=3,face="b0054",path=":fg/hiy/z1/",size="z1",head="hiy_z1b",ex05="hiy_z1b0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1b0200"}}}},block_00019={{"text"},linkback="block_00018",linknext="block_00020",text={ja={{"「但是，我对自己能否做出这样善意的回应并不自信。我也很佩服前辈能做到这一步。」",{"rt2"},name={"智宏"}}}},line=97},block_00028={{"text"},linkback="block_00027",linknext="block_00029",text={ja={{"她已经能理解我的心情得如同我亲妹妹一样。妃爱也承认她是家人。我投降了，请结婚吧。",{"rt2"}}}},line=138},block_00021={{"fg",ch="詩桜",mode=3,face="b0027",path=":fg/shi/z2/",size="z2",head="shi_z2b",ex05="shi_z2b0010",id=13,lv=2.3999999999999999,file="shi_z2b0210"},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00897"}},ja={{"「你不是那种只考虑自己方便，对别人的事情装作不知道的性格吧。我可能不会在意，但你应该会感到内疚。」",{"rt2"},name={"詩桜"}}}},line=109,linknext="block_00022",linkback="block_00020",delay={vl01={{"fg",ch="詩桜",mode=3,face="b0039",path=":fg/shi/z2/",size="z2",head="shi_z2b",ex05="shi_z2b0010",id=13,lv=2.3999999999999999,file="shi_z2b0210"}}}},block_00079={{"text"},linkback="block_00078",linknext="block_00080",text={ja={{"作为志愿者访问设施进行的纸面剧，应该是为了让观看的人开心。那么，这回的「观看的人」是谁呢？",{"rt2"}}}},line=428},block_00016={{"text"},linkback="block_00015",linknext="block_00017",text={ja={{"我再次被这样的前辈所吸引，重新坠入爱河。本来就是个美人，还能变得更美吗？",{"rt2"}}}},line=83},block_00006={{"fg",ch="詩桜",mode=3,face="b0053",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=13,lv=2.2999999999999998,file="shi_z1b0210"},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00891"}},ja={{"「我一直都在尽力理解你的心情，并且按照你的方针来做事。如果有什么做错了，请告诉我」",{"rt2"},name={"詩桜"}}}},line=47,linknext="block_00007",linkback="block_00005",delay={vl01={{"fg",ch="詩桜",mode=3,face="b0019",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=13,lv=2.2999999999999998,file="shi_z1b0210"}}}},block_00078={{"fg",ch="詩桜",mode=3,face="b0018",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0000",id=9,x=-280,lv=2.2999999999999998,file="shi_z1b0200"},{"text"},linkback="block_00077",linknext="block_00079",text={ja={{"但是，我们到底为了什么要在学生会里策划活动呢？是为了像戏剧部和吹奏乐部那样，让参观文化节舞台的观众们感到愉快吗？",{"rt2"}}}},line=426},block_00077={{"text"},linkback="block_00076",linknext="block_00078",text={ja={{"「那么……诗樱前辈所说的是，这是为了所有观众准备的学生会舞台……」",{"rt2"},name={"智宏"}}}},line=422},block_00017={{"text"},linkback="block_00016",linknext="block_00018",text={ja={{"想到有这么多人作为副会长在我手下工作，我感到震惊。她就像绝对不会背叛的吕布。",{"rt2"}}}},line=85},block_00002={{"msgoff"},{"bg",by=-360,sync=0,path=":bg/",bx=-640,ay=720,ax=1280,file="zbg011a"},{"fg",ch="詩桜",mode=3,face="a0001",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0000",sync=0,id=13,lv=2.2999999999999998,file="shi_z1a0200",resize=1},{"extrans",time=500},{"text"},linkback="block_00001",linknext="block_00003",text={ja={{"「学姐，这次你真的非常亲切。我都有些感动了」",{"rt2"},name={"智宏"}}}},line=24},block_00116={{"bg",path=":bg/",file="bg099a"},{"text"},linkback="block_00115",linknext="block_00117",text={ja={{"常磐印刷了她画的最后一张画，诗樱学姐向我们大家讲解了这个纸板戏剧。",{"rt2"}}}},line=623},block_00074={{"fg",ch="詩桜",mode=3,face="b0010",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0000",id=9,x=-280,lv=2.2999999999999998,file="shi_z1b0200"},{"text"},linkback="block_00073",linknext="block_00075",text={ja={{"诗樱前辈善于插话的脊椎挺得笔直。在和泉家里，小叔母似乎是强者。",{"rt2"}}}},line=409},block_00069={{"fg",ch="妃愛",mode=3,face="b0054",path=":fg/hiy/no/",size="no",head="hiy_nob",ex05="hiy_nob0010",id=7,x=-380,lv=2.2000000000000002,file="hiy_nob0210"},{"fgact",ch="妃愛",act="いいえ",id=7,lv=2.2000000000000002},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_01361"}},ja={{"「没关系，反倒是你帮我说出了我很难开口的部分，真是帮了大忙。重点就是我的存在是否会成为噪声。亚澄真厉害，真是我的好女人」",{"rt2"},name={"妃愛"}}}},line=377,linknext="block_00070",linkback="block_00068",delay={vl02={{"fg",ch="妃愛",mode=3,face="b0041",path=":fg/hiy/no/",size="no",head="hiy_nob",ex05="hiy_nob0010",id=7,x=-380,lv=2.2000000000000002,file="hiy_nob0210"}},vl01={{"fg",ch="妃愛",mode=3,face="b0053",path=":fg/hiy/no/",size="no",head="hiy_nob",ex05="hiy_nob0010",id=7,x=-380,lv=2.2000000000000002,file="hiy_nob0210"}}}},block_00097={{"text"},linkback="block_00096",linknext="block_00098",text={ja={{"「离正式演出还有时间，平常的志愿者活动我、先辈、天梨和锦……我也包括在内一起来做。等到文化节临近的时候再决定吧。」",{"rt2"},name={"智宏"}}}},line=531},block_00012={{"fg",ch="詩桜",mode=3,face="a0039",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0000",id=13,lv=2.2999999999999998,file="shi_z1a0200"},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00893"}},ja={{"「我更想优先考虑文化节，也在思考是否需要费心去关注他人的学习。如果因此她留级了，那一切都将白费。如果我是会长，我首先会选择不协助。」",{"rt2"},name={"詩桜"}}}},line=68,linknext="block_00013",linkback="block_00011",delay={vl01={{"fg",ch="詩桜",mode=3,face="a0027",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0000",id=13,lv=2.2999999999999998,file="shi_z1a0200"}}}},block_00072={{"msgoff"},{"bg",by=-360,sync=0,path=":bg/",bx=-640,ay=720,ax=1280,file="zbg011a"},{"fg",ch="詩桜",mode=3,face="b0038",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0000",sync=0,id=9,x=-280,lv=2.2999999999999998,file="shi_z1b0200"},{"fg",ch="妃愛",mode=3,face="b0027",path=":fg/hiy/z1/",size="z1",head="hiy_z1b",sync=0,ex05="hiy_z1b0000",id=10,lv=2.2999999999999998,x=280,file="hiy_z1b0200",resize=1},{"extrans",time=500},{"text"},linkback="block_00071",linknext="block_00073",text={vo={{"vo",ch="shi",file="fem_shi_00911"}},ja={{"「但是呢，妃爱……」",{"rt2"},name={"詩桜"}}}},line=399},block_00068={{"fg",ch="あすみ",mode=3,face="a0008",path=":fg/asu/no/",size="no",head="asu_noa",ex05="asu_noa0010",id=13,x=0,lv=2.2000000000000002,file="asu_noa0210"},{"fgact",ch="あすみ",act="びっくり",id=13,lv=2.2000000000000002},{"text"},linkback="block_00067",linknext="block_00069",text={vo={{"vo",ch="asu",file="fem_asu_00146"}},ja={{"「啊，我并不是否定了妃爱……对不起！」",{"rt2"},name={"あすみ"}}}},line=369},block_00059={{"text"},linkback="block_00058",linknext="block_00060",text={ja={{"在养老院也是由诗樱前辈来朗读的。本来应该是和妃爱一起练习的，但是……",{"rt2"}}}},line=310},block_00032={{"text"},linkback="block_00031",linknext="block_00033",text={ja={{"这个故事在去年11月时，大家都已经过目了。每个人的脑海中或许都模糊地有了以纸片剧形式出现的这个故事的完成图。",{"rt2"}}}},line=167},block_00036={{"fg",ch="あすみ",mode=1,face="a0009",path=":fg/asu/no/",size="no",head="asu_noa",ex05="asu_noa0010",id=20,lv=2.2000000000000002,file="asu_noa0210",ex02="a0098"},{"text"},linkback="block_00035",linknext="block_00037",text={vo={{"vo",ch="asu",file="fem_asu_00143"}},ja={{"「啊，我并不是在怀疑常磐学姐的能力……！」",{"rt2"},name={"あすみ"}}}},line=181},block_00070={{"fg",ch="あすみ",mode=3,face="a0002",path=":fg/asu/no/",size="no",head="asu_noa",ex05="asu_noa0010",id=13,x=0,lv=2.2000000000000002,file="asu_noa0210"},{"fg",ch="妃愛",mode=3,face="b0022",path=":fg/hiy/no/",size="no",head="hiy_nob",ex05="hiy_nob0000",id=7,x=-380,lv=2.2000000000000002,file="hiy_nob0200"},{"text"},linkback="block_00069",linknext="block_00071",text={vo={{"vo",ch="hiy",file="fem_hiy_01362"}},ja={{"「尽管这么说，我是小泉妃爱，无论会场再喧闹，或是这次的主题是我不擅长的纸藤剧，我都有自信能让大家听得入神」",{"rt2"},name={"妃愛"}}}},line=384},block_00067={{"fg",ch="あすみ",mode=3,face="a0028",path=":fg/asu/no/",size="no",head="asu_noa",ex05="asu_noa0010",id=13,x=0,lv=2.2000000000000002,file="asu_noa0210"},{"text"},linkback="block_00066",linknext="block_00068",text={vo={{"vo",ch="asu",file="fem_asu_00145"}},ja={{"「我其实很喜欢这个故事……但如果会场变得吵闹起来，我不确定我的声音能否被听清楚……」",{"rt2"},name={"あすみ"}}}},line=363},block_00027={{"text"},linkback="block_00026",linknext="block_00028",text={ja={{"「不，没有错。虽然我给先辈打分有些冒昧，但如果真的可以打分的话，作为恋人我给70兆分」",{"rt2"},name={"智宏"}}}},line=136},block_00066={{"fg",ch="あすみ",mode=3,face="a0002",path=":fg/asu/no/",size="no",head="asu_noa",ex05="asu_noa0000",id=13,x=0,lv=2.2000000000000002,file="asu_noa0200"},{"text"},linkback="block_00065",linknext="block_00067",text={vo={{"vo",ch="asu",file="fem_asu_00144"}},ja={{"「啊，但是……我也有点担心。这个问题和诗樱前辈担心的正好相反……」",{"rt2"},name={"あすみ"}}}},line=358},block_00064={{"fg",ch="妃愛",mode=3,face="b0019",path=":fg/hiy/z1/",size="z1",head="hiy_z1b",ex05="hiy_z1b0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1b0200"},{"text"},linkback="block_00063",linknext="block_00065",text={vo={{"vo",ch="hiy",file="fem_hiy_01360"}},ja={{"「只是在和诗樱前辈练习阅读故事书的时候，我总觉得由前辈亲自读出来的话，最可能深深击中那些我想让他们真正被打动的听众……」",{"rt2"},name={"妃愛"}}}},line=342},block_00109={{"fg",ch="妃愛",mode=1,face="a0046",path=":fg/hiy/z1/",size="z1",head="hiy_z1a",ex05="hiy_z1a0000",id=20,lv=2.2999999999999998,file="hiy_z1a0200"},{"quake",loop=4,mode="mw"},{"text"},linkback="block_00108",linknext="block_00110",text={vo={{"vo",ch="hiy",file="fem_hiy_01371"}},ja={{"「咿！」",{"rt2"},name={"妃愛"}}}},line=585},block_00009={{"text"},linkback="block_00008",linknext="block_00010",text={ja={{"她是多么美好的恋人啊。我的眼角微微有些热涌。",{"rt2"}}}},line=53},block_00029={{"bgm",time=3000,stop=1},{"se",id=-1,time=3000,stop=1},{"vostop",time=3000,stop=1},{"ntclear"},{"cgdel",id=-1},{"bg",path=":bg/",time=3000,file="black"},{"cacheclear"},{"ex",time=1000,func="wait"},{"msgoff"},{"bg",path=":bg/",sync=0,file="bg011a"},{"fg",ch="華乃",mode=3,face="a0001",path=":fg/kan/no/",size="no",head="kan_noa",ex05="kan_noa0000",sync=0,id=9,x=-230,lv=2.2000000000000002,file="kan_noa0200"},{"fg",ch="詩桜",mode=3,face="a0055",path=":fg/shi/no/",size="no",head="shi_noa",sync=0,ex05="shi_noa0000",id=10,lv=2.2000000000000002,x=230,file="shi_noa0200",resize=1},{"extrans",time=2000},{"text"},linkback="block_00028",linknext="block_00030",text={vo={{"vo",ch="shi",file="fem_shi_00902"}},ja={{"「嗯，很好啊。作为最后一张画，无可挑剔」",{"rt2"},name={"詩桜"}}}},line=155},block_00058={{"text"},linkback="block_00057",linknext="block_00059",text={ja={{"因为在11月份的时候妃爱能否参加还不确定，所以作为备用的朗读者，我们决定由诗樱前辈来担任。这是在策划初期就决定的。",{"rt2"}}}},line=308},block_00041={{"fg",ch="詩桜",mode=3,face="a0018",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0010",id=10,lv=2.2999999999999998,file="shi_z1a0210"},{"text"},linkback="block_00040",linknext="block_00042",text={vo={{"vo",ch="shi",file="fem_shi_00905"}},ja={{"「我不知道会有多少设施愿意接受我们突然的联系……我们还需要学校的许可。」",{"rt2"},name={"詩桜"}}}},line=220},block_00075={{"fg",ch="妃愛",mode=3,face="a0027",path=":fg/hiy/z1/",size="z1",head="hiy_z1a",ex05="hiy_z1a0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1a0200"},{"text"},linkback="block_00074",linknext="block_00076",text={vo={{"vo",ch="hiy",file="fem_hiy_01365"}},ja={{"「比如动画。即使我在关键的对话中倾注了全部的灵魂，如果同时从楼下传来「饭做好了」这样的声音，观众们的评价可能就会变得模糊。」",{"rt2"},name={"妃愛"}}}},line=414},block_00118={{"bgm",time=3000,stop=1},{"se",id=-1,time=3000,stop=1},{"vostop",time=3000,stop=1},{"ntclear"},{"cgdel",id=-1},{"bg",path=":bg/",time=3000,file="black"},{"cacheclear"},{"ex",time=1000,func="wait"},{"bg",time=500,path=":cg/",lv=9,id=9,file="catch04"},{"bgm",loop=0,file="bgm94"},{"ex",time=6000,func="wait"},{"bgm",time=3000,stop=1},{"se",id=-1,time=3000,stop=1},{"vostop",time=3000,stop=1},{"ntclear"},{"cgdel",id=-1},{"bg",path=":bg/",time=3000,file="black"},{"cacheclear"},{"ex",time=1000,func="wait"},{"excall",file="04_詩桜11_ed1"},{"text"},linkback="block_00117",line=633},block_00114={{"msgoff"},{"bg",path=":bg/",sync=0,file="bg011a"},{"fg",ch="詩桜",mode=3,face="a0002",path=":fg/shi/no/",size="no",head="shi_noa",ex05="shi_noa0000",sync=0,id=9,lv=2.2000000000000002,file="shi_noa0200",resize=1},{"extrans",time=500},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00918"}},ja={{"「我向大家道歉……天梨是主动参加的，所以即使道歉也许会让她困扰，但我要向大家道歉。对不起我放弃了学生会。对扰乱了你们感到抱歉」",{"rt2"},name={"詩桜"}}}},line=614,linknext="block_00115",linkback="block_00113",delay={vl02={{"fg",ch="詩桜",mode=3,face="a0028",path=":fg/shi/no/",size="no",head="shi_noa",ex05="shi_noa0000",id=9,lv=2.2000000000000002,file="shi_noa0200"}},vl01={{"fg",ch="詩桜",mode=3,face="a0038",path=":fg/shi/no/",size="no",head="shi_noa",ex05="shi_noa0000",id=9,lv=2.2000000000000002,file="shi_noa0200"}}}},block_00089={{"fg",ch="あすみ",mode=3,face="b0023",path=":fg/asu/z1/",size="z1",head="asu_z1b",ex05="asu_z1b0000",id=13,lv=2.2999999999999998,file="asu_z1b0200"},{"text"},text={vo={{"vo",ch="asu",file="fem_asu_00151"}},ja={{"「可能有点夸张，但我觉得存在一种日本人从小就栽种进去的原风景……就像是集体无意识似的。就像是保育员阅读的纸芝居。」",{"rt2"},name={"あすみ"}}}},line=489,linknext="block_00090",linkback="block_00088",delay={vl01={{"fg",ch="あすみ",mode=3,face="b0055",path=":fg/asu/z1/",size="z1",head="asu_z1b",ex05="asu_z1b0000",id=13,lv=2.2999999999999998,file="asu_z1b0200"}}}},block_00087={{"fg",ch="あすみ",mode=3,face="b0022",path=":fg/asu/z1/",size="z1",head="asu_z1b",ex05="asu_z1b0000",id=13,lv=2.2999999999999998,file="asu_z1b0200"},{"text"},text={vo={{"vo",ch="asu",file="fem_asu_00149"}},ja={{"「我始终认为，纸板剧的魅力就在于纸张本身的温度。这并不是说要否定数字化的纸板剧，只是觉得应该根据观众来决定……」",{"rt2"},name={"あすみ"}}}},line=476,linknext="block_00088",linkback="block_00086",delay={vl01={{"fg",ch="あすみ",mode=3,face="b0002",path=":fg/asu/z1/",size="z1",head="asu_z1b",ex05="asu_z1b0000",id=13,lv=2.2999999999999998,file="asu_z1b0200"}}}},block_00050={{"msgoff"},{"bg",path=":bg/",sync=0,file="bg011a"},{"fg",ch="妃愛",mode=3,face="b0002",path=":fg/hiy/no/",size="no",head="hiy_nob",ex05="hiy_nob0000",sync=0,id=13,lv=2.2000000000000002,file="hiy_nob0200"},{"extrans",time=500},{"bgm",stop=1},{"text"},linkback="block_00049",linknext="block_00051",text={vo={{"vo",ch="hiy",file="fem_hiy_01352"}},ja={{"「那个，关于诗樱前辈刚才所说的事情，事实上我也有些想法……」",{"rt2"},name={"妃愛"}}}},line=267},block_00001={{"text"},linkback="block_00000",linknext="block_00002",text={ja={{"诗樱学姐回来后，其他的干部还没到，所以我们有了短暂的二人时光。",{"rt2"}}}},line=15},block_00096={{"fg",ch="天梨",mode=1,face="b0029",path=":fg/ame/no/",size="no",head="ame_nob",ex05="ame_nob0000",id=20,lv=2.2000000000000002,file="ame_nob0200"},{"text"},linkback="block_00095",linknext="block_00097",text={vo={{"vo",ch="ame",file="fem_ame_00069"}},ja={{"「嗯，嗯——但是，肯定也有希优琳目标的人，那样的话也能成为话题……」",{"rt2"},name={"天梨"}}}},line=528},block_00043={{"fg",ch="里",mode=1,face="a0010",path=":fg/mir/no/",size="no",head="mir_noa",ex05="mir_noa0000",id=20,lv=2.2000000000000002,file="mir_noa0200"},{"text"},text={vo={{"vo",ch="mir",file="fem_mir_00174"}},ja={{"「另外，理事长也经营着一些设施，我会问问他是否可以在那里进行演出。如果能在理事长的地盘上演出，那么学校方面的许可也应该没有问题了。」",{"rt2"},name={"里"}}}},line=232,linknext="block_00044",linkback="block_00042",delay={vl01={{"fg",ch="里",mode=1,face="a0006",path=":fg/mir/no/",size="no",head="mir_noa",ex05="mir_noa0000",id=20,lv=2.2000000000000002,file="mir_noa0200"}}}},block_00042={{"fg",ch="里",mode=1,face="a0054",path=":fg/mir/no/",size="no",head="mir_noa",ex05="mir_noa0000",id=20,lv=2.2000000000000002,file="mir_noa0200"},{"text"},text={vo={{"vo",ch="mir",file="fem_mir_00173"}},ja={{"「没问题，没问题。志愿者活动的自由度在一定程度上是由我们决定的。你们先去，我稍后会获取许可。如果有可以去的地方，明天就可以去了。」",{"rt2"},name={"里"}}}},line=226,linknext="block_00043",linkback="block_00041",delay={vl01={{"fg",ch="里",mode=1,face="a0052",path=":fg/mir/no/",size="no",head="mir_noa",ex05="mir_noa0000",id=20,lv=2.2000000000000002,file="mir_noa0200"}}}},block_00053={{"fg",ch="詩桜",mode=1,face="b0002",path=":fg/shi/z1/",size="z1",head="shi_z1b",ex05="shi_z1b0010",id=20,lv=2.2999999999999998,file="shi_z1b0210"},{"text"},linkback="block_00052",linknext="block_00054",text={vo={{"vo",ch="shi",file="fem_shi_00909"}},ja={{"「嗯，难道说是没有得到经纪公司的许可吗？」",{"rt2"},name={"詩桜"}}}},line=279},block_00024={{"fg",ch="詩桜",mode=3,face="a0039",path=":fg/shi/z2/",size="z2",head="shi_z2a",ex05="shi_z2a0000",id=13,lv=2.3999999999999999,file="shi_z2a0200"},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00899"}},ja={{"「所以，把我这次的行为称作善意其实有些不准确。我只是在清算你的愧疚和你对我责难的任性罢了」",{"rt2"},name={"詩桜"}}}},line=123,linknext="block_00025",linkback="block_00023",delay={vl01={{"fg",ch="詩桜",mode=3,face="a0001",path=":fg/shi/z2/",size="z2",head="shi_z2a",ex05="shi_z2a0000",id=13,lv=2.3999999999999999,file="shi_z2a0200"}}}},block_00062={{"fg",ch="妃愛",mode=3,face="b0001",path=":fg/hiy/z1/",size="z1",head="hiy_z1b",ex05="hiy_z1b0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1b0200"},{"text"},text={vo={{"vo",ch="hiy",file="fem_hiy_01358"}},ja={{"「只不过这是我作为演员的直觉，我觉得如果让诗樱前辈朗读的话，对我们日后的美好回忆更有帮助。」",{"rt2"},name={"妃愛"}}}},line=331,linknext="block_00063",linkback="block_00061",delay={vl01={{"fg",ch="妃愛",mode=3,face="b0022",path=":fg/hiy/z1/",size="z1",head="hiy_z1b",ex05="hiy_z1b0000",id=10,x=280,lv=2.2999999999999998,file="hiy_z1b0200"}}}},block_00010={{"text"},linkback="block_00009",linknext="block_00011",text={ja={{"「学姐。我现在有点感动。」",{"rt2"},name={"智宏"}}}},line=56},block_00031={{"bgm",file="bgm11"},{"text"},linkback="block_00030",linknext="block_00032",text={ja={{"？常磐完成的最后一张画似乎符合诗樱学姐的审美，于是今年文化节上学生会的项目「纸片剧」就此完成。",{"rt2"}}}},line=165},block_00045={{"fg",ch="詩桜",mode=3,face="a0027",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0000",id=10,lv=2.2999999999999998,file="shi_z1a0200"},{"text"},text={vo={{"vo",ch="shi",file="fem_shi_00906"}},ja={{"「另外，为了避免被认为只是为了文化节，我们必须坚持到11月任期结束。这是我打算继续做下去的……我想我之前提过这个问题。」",{"rt2"},name={"詩桜"}}}},line=241,linknext="block_00046",linkback="block_00044",delay={vl01={{"fg",ch="詩桜",mode=3,face="a0001",path=":fg/shi/z1/",size="z1",head="shi_z1a",ex05="shi_z1a0000",id=10,lv=2.2999999999999998,file="shi_z1a0200"}}}}}