astver = 2.0
ast = {block_00048={{"fg",head="hiy_z2a",mode=3,lv=2.3999999999999999,ex05="hiy_z2a0010",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2a0710",face="a0055",ch="妃愛"},{"text"},linknext="block_00049",text={vo={{"vo",file="fem_hiy_01248",ch="hiy"}},ja={{"「在温暖的阳光下，我们一起慵懒地度过这20天吧。我可不会放你跑哦」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",head="hiy_z2a",mode=3,lv=2.3999999999999999,ex05="hiy_z2a0000",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2a0700",face="a0026",ch="妃愛"}}},line=238,linkback="block_00047"},block_00006={{"text"},linknext="block_00007",line=35,text={ja={{"「妃爱，我也快要射了……！」",{"rt2"},name={"智宏"}}}},linkback="block_00005"},block_00002={{"text"},linknext="block_00003",line=23,text={vo={{"vo",file="fem_hiy_01209",ch="hiy"}},ja={{"「哥哥！啊，啊啊！嗯呜，要射了！就这样，啊，哥哥……啊，射进来！今天没问题的……！」",{"rt2"},name={"妃愛"}}}},linkback="block_00001"},block_00000={{"savetitle",text="井の中の蛙大海を知らず"},{"bg",file="ev_hiy_h38_08",set="ev_hiy_h38",path=":ev/ev_hiy_h38/",time=500},{"bgm",file="bgm19"},{"se",file="seエロ_ピストン：強",id=1,loop=1},{"lvo",file="fem_hiy_70534",loop=1,ch="hiy"},{"text"},linknext="block_00001",text={ja={{"果然还是想在妃爱里面射出来……！就这样一直要射到最后……！",{"rt2"}}}},line=18},block_00038={{"fg",head="hiy_z2b",mode=3,lv=2.3999999999999999,ex05="hiy_z2b0000",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2b0700",face="b0013",ch="妃愛"},{"text"},linknext="block_00039",line=190,text={vo={{"vo",file="fem_hiy_01243",ch="hiy"}},ja={{"「毕竟已经毕业典礼结束了，从明天开始直到大学开学，哥哥都会一直在家里吧」",{"rt2"},name={"妃愛"}}}},linkback="block_00037"},label={top={label=1,block="block_00000"}},block_00018={{"bg",bx=-640,by=-360,file="zev_hiy_h38_02",y=250,x=-640,ay=720,path=":ev/ev_hiy_h38/",set="ev_hiy_h38",ax=1280,time=500},{"text"},linknext="block_00019",line=89,text={vo={{"vo",file="fem_hiy_01221",ch="hiy"}},ja={{"「啊啊，今天我知道了新的快感……被挠痒痒后再插入，居然这么舒服……」",{"rt2"},name={"妃愛"}}}},linkback="block_00017"},block_00004={{"text"},linknext="block_00005",line=29,text={vo={{"vo",file="fem_hiy_01211",ch="hiy"}},ja={{"「啊啊，哥哥的……用哥哥的小弟弟让我高潮，让我射出来！啊啊，就这样……」",{"rt2"},name={"妃愛"}}}},linkback="block_00003"},block_00023={{"text"},linknext="block_00024",line=104,text={ja={{"真的，我好想一直抱着她。比世界上任何小动物都要有趣，抱着我的妹妹。我对此深信不疑。",{"rt2"}}}},linkback="block_00022"},block_00015={{"text"},linknext="block_00016",line=75,text={vo={{"vo",file="fem_hiy_01219",ch="hiy"}},ja={{"「呼，满足了……让哥哥在里面射出来，我会感到非常幸福……」",{"rt2"},name={"妃愛"}}}},linkback="block_00014"},block_00017={{"text"},linknext="block_00018",line=84,text={ja={{"妃爱疲惫不堪地依偎过来，将整个身体重量都倚靠在我身上。如果我松手的话，我们两个都会摔倒，所以必须要稳稳地支撑住她。",{"rt2"}}}},linkback="block_00016"},block_00040={{"text"},linknext="block_00041",line=195,text={ja={{"「但是当用嘴巴的时候……啊，没什么！」",{"rt2"},name={"智宏"}}}},linkback="block_00039"},block_00054={{"scene",file="scene04"},{"bgm",time=3000,stop=1},{"se",id=-1,time=3000,stop=1},{"vostop",time=3000,stop=1},{"ntclear"},{"cgdel",id=-1},{"bg",file="white",path=":bg/",time=3000},{"cacheclear"},{"ex",func="wait",time=0},{"タイトル"},{"text"},line=263,linkback="block_00053"},block_00026={{"text"},linknext="block_00027",line=125,text={ja={{"虽然我说的大部分都是开玩笑，但妃爱在我的手中沉思了一会儿。",{"rt2"}}}},linkback="block_00025"},block_00053={{"text"},linknext="block_00054",line=255,text={ja={{"但是驾照培训营坚决反对我去，看来只能在暑假的时候去了。两周分开对她来说是无法忍受的。其实我也是一样。",{"rt2"}}}},linkback="block_00052"},block_00052={{"text"},linknext="block_00053",line=253,text={ja={{"总之，我约定考到驾照后和妃爱一起去看羊驼。她非常期待这个约定，经常浏览牧场的网页。",{"rt2"}}}},linkback="block_00051"},block_00005={{"text"},linknext="block_00006",line=32,text={vo={{"vo",file="fem_hiy_01212",ch="hiy"}},ja={{"「要射了！快要射了！啊啊，哥哥，喜欢你，啊，要射了……啊，不行了！」",{"rt2"},name={"妃愛"}}}},linkback="block_00004"},block_00051={{"text"},linknext="block_00052",line=251,text={ja={{"虽然是人生的春假，但作为一个不想懈怠的人来说，和妃爱的斗争让我紧张得心跳加速……！绝对不能输……！",{"rt2"}}}},linkback="block_00050"},block_00050={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",speed=60000,sync=0,bx=-640,by=-360,file="zbg099a",y=360,x=0,ay=720,path=":bg/",y2=-360,ax=1280,x2=0},{"extrans"},{"text"},linknext="block_00051",line=249,text={ja={{"然而，我的抵抗毫无用处，在房间里被她弄了两次，在浴室里被她弄了一次。多亏了她，我的名牌都消失了。",{"rt2"}}}},linkback="block_00049"},block_00019={{"text"},linknext="block_00020",line=92,text={vo={{"vo",file="fem_hiy_01222",ch="hiy"}},ja={{"「虽然被欺负得无法抵抗，但是如果能得到这么舒服的待遇，把自己交给哥哥也不错呢」",{"rt2"},name={"妃愛"}}}},linkback="block_00018"},block_00049={{"text"},linknext="block_00050",line=241,text={ja={{"「对了，为了保持精力，我考虑去参加驾照培训营……啊，不行！等会再洗就好了！」",{"rt2"},name={"智宏"}}}},linkback="block_00048"},block_00003={{"text"},linknext="block_00004",line=26,text={vo={{"vo",file="fem_hiy_01210",ch="hiy"}},ja={{"「好厉害……顶到最深处了……那里，感觉好舒服……！来吧，戳那里！用哥哥的小弟弟磨擦着……！」",{"rt2"},name={"妃愛"}}}},linkback="block_00002"},block_00047={{"fg",head="hiy_z2b",mode=3,lv=2.3999999999999999,ex05="hiy_z2b0000",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2b0700",face="b0042",ch="妃愛"},{"fgact",id=13,act="いいえ",lv=2.3999999999999999,ch="妃愛"},{"text"},linknext="block_00048",text={vo={{"vo",file="fem_hiy_01247",ch="hiy"}},ja={{"「去上大学的话，被女孩子们围住可不好，所以就让自己变得邋遢一点吧。只要能保证不留级就好了」",{"rt2"},name={"妃愛"}}}},delay={vl02={{"fg",head="hiy_z2a",mode=3,lv=2.3999999999999999,ex05="hiy_z2a0000",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2a0700",face="a0026",ch="妃愛"}},vl01={{"fg",head="hiy_z2b",mode=3,lv=2.3999999999999999,ex05="hiy_z2b0000",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2b0700",face="b0033",ch="妃愛"}}},line=232,linkback="block_00046"},block_00020={{"text"},linknext="block_00021",line=95,text={ja={{"「啊啊，真是的……因为今天想让妃爱感到舒服」",{"rt2"},name={"智宏"}}}},linkback="block_00019"},block_00010={{"text"},linknext="block_00011",line=49,text={ja={{"「我也要射了！一起射吧……！」",{"rt2"},name={"智宏"}}}},linkback="block_00009"},block_00028={{"text"},linknext="block_00029",line=131,text={ja={{"「也是啊」",{"rt2"},name={"智宏"}}}},linkback="block_00027"},block_00016={{"bg",sync=0,path=":ev/ev_hiy_h38/",file="ev_hiy_h38_13",set="ev_hiy_h38",face="1",time=400},{"text"},linknext="block_00017",line=82,text={vo={{"vo",file="fem_hiy_01220",ch="hiy"}},ja={{"「今天也被你爱得好多，谢谢……现在只有这一刻，我是世界上最幸福的妹妹了哦～」",{"rt2"},name={"妃愛"}}}},linkback="block_00015"},block_00021={{"bg",sync=0,bx=-640,by=-360,file="zev_hiy_h38_04",y=250,x=-640,ay=720,ax=1280,path=":ev/ev_hiy_h38/",set="ev_hiy_h38",face="1",time=400},{"text"},linknext="block_00022",line=100,text={vo={{"vo",file="fem_hiy_01223",ch="hiy"}},ja={{"「嗯，但是一被你这么说，我就会涌起想要全心全意侍奉哥哥的心情呢。嘿嘿嘿，喜欢侍奉你，哥哥」",{"rt2"},name={"妃愛"}}}},linkback="block_00020"},block_00033={{"msgoff"},{"bg",sync=0,bx=-640,by=-360,file="zbg001a",x=-600,zoom=120,path=":bg/",ax=1280,ay=720},{"fg",sync=0,head="hiy_z2b",mode=3,lv=2.3999999999999999,resize=1,id=13,size="z2",ex05="hiy_z2b0000",path=":fg/hiy/z2/",file="hiy_z2b0700",face="b0032",ch="妃愛"},{"extrans",rule="008b"},{"fgact",id=13,act="ゆらゆら",lv=2.3999999999999999,ch="妃愛"},{"bgm",file="bgm15"},{"text"},linknext="block_00034",line=171,text={vo={{"vo",file="fem_hiy_01241",ch="hiy"}},ja={{"「妃、爱、专、用……」",{"rt2"},name={"妃愛"}}}},linkback="block_00032"},block_00044={{"fg",head="hiy_z2a",mode=3,lv=2.3999999999999999,ex05="hiy_z2a0000",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2a0700",face="a0025",ch="妃愛"},{"text"},linknext="block_00045",line=214,text={vo={{"vo",file="fem_hiy_01245",ch="hiy"}},ja={{"「直到大学开学之前，在家里做任何事情都是一起的，出门的时候也是一起的哦」",{"rt2"},name={"妃愛"}}}},linkback="block_00043"},block_00012={{"se",id=1,time=500,stop=1},{"bg",sync=1,path=":bg/",id=9,lv=9,file="white",time=100},{"bg",sync=1,path=":bg/",file="white",id=9,lv=9,alpha=0,time=300},{"bg",sync=1,path=":bg/",file="white",id=9,lv=9,alpha=255,time=500},{"cgdel",id=9,lv=9},{"bg",sync=0,path=":ev/ev_hiy_h38/",file="ev_hiy_h38_12",set="ev_hiy_h38",vague=100,time=1000},{"se",vol=100,file="seエロ_射精中出し5",pan=0,id=9,time=0},{"ex",func="wait",time=100},{"lvo",stop=1,ch="hiy"},{"text"},linknext="block_00013",line=65,text={vo={{"vo",file="fem_hiy_01217",ch="hiy"}},ja={{"「啊啊啊啊啊啊！啊啊，射出来了，啊啊，好舒服！好舒服！」",{"rt2"},name={"妃愛"}}}},linkback="block_00011"},block_00043={{"text"},linknext="block_00044",line=209,text={ja={{"「不、不要……！妃爱刚刚已经洗过澡了……！」",{"rt2"},name={"智宏"}}}},linkback="block_00042"},block_00014={{"text"},linknext="block_00015",line=72,text={vo={{"vo",file="fem_hiy_01218",ch="hiy"}},ja={{"「啊，哥哥的东西，出来了……啊，好温暖，进入了我体内……哥哥的快感，充满了我……」",{"rt2"},name={"妃愛"}}}},linkback="block_00013"},block_00029={{"cgdel",id=-1},{"bg",rule="rule_001",wait=250,file="black",path=":bg/",vague=40,time=1000},{"bg",file="bg001a",rule="rule_001",vague=40,path=":bg/"},{"text"},linknext="block_00030",line=138,text={ja={{"用纸巾擦拭了一下身体后，妃爱穿上内衣向一楼走去。",{"rt2"}}}},linkback="block_00028"},block_00042={{"fg",head="hiy_z2a",mode=3,lv=2.3999999999999999,ex05="hiy_z2a0010",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2a0710",face="a0054",ch="妃愛"},{"fgact",id=13,act="びっくり",lv=2.3999999999999999,ch="妃愛"},{"text"},linknext="block_00043",text={vo={{"vo",file="fem_hiy_01244",ch="hiy"}},ja={{"「那么现在开始给你洗个干净吧！一起去洗澡吧！」",{"rt2"},name={"妃愛"}}}},delay={vl01={{"fg",head="hiy_z2a",mode=3,lv=2.3999999999999999,ex05="hiy_z2a0010",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2a0710",face="a0041",ch="妃愛"}}},line=206,linkback="block_00041"},block_00022={{"text"},linknext="block_00023",line=102,text={ja={{"在这种情况下，妃爱只能自由地动动脖子以上的部分，亲了我的脸好多次。",{"rt2"}}}},linkback="block_00021"},block_00007={{"text"},linknext="block_00008",line=40,text={vo={{"vo",file="fem_hiy_01213",ch="hiy"}},ja={{"「想要射了，和哥哥一起……啊，啊啊！我也要射了！啊啊，不行了，啊啊，和哥哥一起，啊，要射了，要射出来！」",{"rt2"},name={"妃愛"}}}},linkback="block_00006"},block_00011={{"text"},linknext="block_00012",line=52,text={vo={{"vo",file="fem_hiy_01216",ch="hiy"}},ja={{"「呀啊啊啊！要射了，一起来吧！和哥哥一起……啊啊，要射了！要一起……啊啊，射出来！好舒服！」",{"rt2"},name={"妃愛"}}}},linkback="block_00010"},block_00041={{"fg",head="hiy_z2b",mode=3,lv=2.3999999999999999,ex05="hiy_z2b0000",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2b0700",face="b0026",ch="妃愛"},{"text"},linknext="block_00042",line=199,text={ja={{"听到我的失言后，妃爱得意地笑了笑，亲吻了一下顶端，然后我的东西又勃起了。",{"rt2"}}}},linkback="block_00040"},block_00030={{"text"},linknext="block_00031",line=140,text={ja={{"以为失去心爱的人会感到寂寞，但突然感到疲惫袭来，连寂寞都来不及感受就躺在床上了。",{"rt2"}}}},linkback="block_00029"},block_00039={{"text"},linknext="block_00040",line=192,text={ja={{"说起来，是这样啊。",{"rt2"}}}},linkback="block_00038"},block_00037={{"text"},linknext="block_00038",line=185,text={ja={{"「话虽如此，但我们不知道哪里会有危险啊」",{"rt2"},name={"智宏"}}}},linkback="block_00036"},block_00031={{"bg",file="black",rule="008a",path=":bg/"},{"se",file="se倒れる(ベッド)",id=1},{"text"},linknext="block_00032",line=146,text={ja={{"唔唔，果然还是洗个淋浴比较好……但是好困啊……",{"rt2"}}}},linkback="block_00030"},block_00001={{"text"},linknext="block_00002",line=20,text={ja={{"加快腰的冲击，妃爱皱起了眉头，下定决心忍耐到最后一刻。",{"rt2"}}}},linkback="block_00000"},block_00035={{"text"},linknext="block_00036",line=176,text={ja={{"「要是被发现的话……我们的关系就会结束了……」",{"rt2"},name={"智宏"}}}},linkback="block_00034"},block_00024={{"savetitle",text="牧場にはひよこもいた"},{"bg",file="ev_hiy_h38_13",set="ev_hiy_h38",path=":ev/ev_hiy_h38/",time=500},{"text"},linknext="block_00025",line=120,text={ja={{"「要不要我带你去洗澡呢？」",{"rt2"},name={"智宏"}}}},linkback="block_00023"},block_00036={{"fg",head="hiy_z2b",mode=3,lv=2.3999999999999999,ex05="hiy_z2b0010",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2b0710",face="b0041",ch="妃愛"},{"fgact",id=13,act="うんうん",lv=2.3999999999999999,ch="妃愛"},{"text"},linknext="block_00037",line=182,text={vo={{"vo",file="fem_hiy_01242",ch="hiy"}},ja={{"「水性、水性」",{"rt2"},name={"妃愛"}}}},linkback="block_00035"},block_00034={{"text"},linknext="block_00035",line=173,text={ja={{"然后下次醒来时，妹妹用毛笔在我的物品上写下了她的名字。你在干什么……。",{"rt2"}}}},linkback="block_00033"},block_00025={{"text"},linknext="block_00026",line=123,text={vo={{"vo",file="fem_hiy_01239",ch="hiy"}},ja={{"「那也挺好的呢……」",{"rt2"},name={"妃愛"}}}},linkback="block_00024"},block_00008={{"text"},linknext="block_00009",line=43,text={vo={{"vo",file="fem_hiy_01214",ch="hiy"}},ja={{"「哥哥……要射了……啊啊，不行了，我已经受不了了……！啊啊，要射了，对不起哥哥，我忍不住了……啊啊，要射了！」",{"rt2"},name={"妃愛"}}}},linkback="block_00007"},block_00045={{"fg",head="hiy_z2b",mode=3,lv=2.3999999999999999,ex05="hiy_z2b0000",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2b0700",face="b0051",ch="妃愛"},{"text"},linknext="block_00046",text={vo={{"vo",file="fem_hiy_01246",ch="hiy"}},ja={{"「每天都要做那个哦。哥哥只需要睡觉就好了。我会宠你很多很多的」",{"rt2"},name={"妃愛"}}}},delay={vl02={{"fg",head="hiy_z2b",mode=3,lv=2.3999999999999999,ex05="hiy_z2b0000",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2b0700",face="b0041",ch="妃愛"}},vl01={{"fg",head="hiy_z2b",mode=3,lv=2.3999999999999999,ex05="hiy_z2b0000",id=13,size="z2",path=":fg/hiy/z2/",file="hiy_z2b0700",face="b0055",ch="妃愛"}}},line=221,linkback="block_00044"},block_00009={{"text"},linknext="block_00010",line=46,text={vo={{"vo",file="fem_hiy_01215",ch="hiy"}},ja={{"「哥哥！我要射了，我快要射了！对不起，我忍不住了……哥哥！」",{"rt2"},name={"妃愛"}}}},linkback="block_00008"},block_00046={{"text"},linknext="block_00047",line=224,text={ja={{"「虽然刚刚毕业后我决定要有坚定的意志去生活，但如果这样下去，半个月就会堕落下去了……！」",{"rt2"},name={"智宏"}}}},linkback="block_00045"},block_00027={{"text"},linknext="block_00028",line=128,text={vo={{"vo",file="fem_hiy_01240",ch="hiy"}},ja={{"「不过如果被资正看到肯定会吓一跳，而且在楼梯上摔倒可能会受伤，还是算了吧」",{"rt2"},name={"妃愛"}}}},linkback="block_00026"},block_00013={{"se",id=1,time=500,stop=1},{"text"},linknext="block_00014",line=69,text={ja={{"当我的小弟弟顶到她最深处并射进去时，妃爱用力抓住我仿佛要把我勒死。",{"rt2"}}}},linkback="block_00012"},block_00032={{"se",id=1,time=500,stop=1},{"bgm",stop=1},{"text"},linknext="block_00033",line=154,text={ja={{"就这样像泥一样熟睡了。妃爱要回来了，我没有锁门，也没有穿内衣。",{"rt2"}}}},linkback="block_00031"}}