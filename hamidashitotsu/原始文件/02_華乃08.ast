astver = 2.0
ast = {block_00068={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1a",size="z1",face="a0052",file="kan_z1a1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10520",ch="kan"}},ja={{"「我记得那次谈话。那是在我们还在犹豫是否要交往的时候吧。说起来，那时你也说过要做饭给我吃。我当时真的感动了」",{"rt2"},name={"華乃"}}}},linkback="block_00067",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1a",size="z1",face="a0055",file="kan_z1a1100",lv=2.2999999999999998}}},line=331,linknext="block_00069"},block_00150={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0031",file="kan_z1b1100",lv=2.2999999999999998},{"text"},line=730,text={vo={{"vo",file="fem_kan_10561",ch="kan"}},ja={{"「你不是很擅长文科吗？我现古汉全部都很懒得学。」",{"rt2"},name={"華乃"}}}},linkback="block_00149",linknext="block_00151"},block_00000={{"savetitle",text="どうせいっちゅうねん"},{"bg",by=-360,time=2000,path=":bg/",speed=120000,bx=-640,ay=720,ax=1280,y2=-100,y=-100,x=-640,file="zbg002c",x2=640},{"bgm",file="bgm08"},{"text"},text={ja={{"「资正，对不起让你感到寂寞。我会每天都回来的。」",{"rt2"},name={"智宏"}}}},line=15,linknext="block_00001"},block_00117={{"msgoff"},{"bg",sync=0,file="bg036c",path=":bg/"},{"fg",ch="華乃",resize=1,ex05="kan_nob0000",path=":fg/kan/no/",mode=3,id=13,head="kan_nob",sync=0,size="no",face="b0054",file="kan_nob1100",lv=2.2000000000000002},{"extrans",time=500},{"text"},line=585,text={ja={{"「嗯嗯，洗碗就由我来负责好了！？以后我们要分担家务，今天华乃做了饭，所以洗碗就我来！」",{"rt2"},name={"智宏"}}}},linkback="block_00116",linknext="block_00118"},block_00038={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,id=13,head="kan_z2a",size="z2",face="a0034",file="kan_z2a1100",lv=2.3999999999999999},{"text"},line=199,text={vo={{"vo",file="fem_kan_10505",ch="kan"}},ja={{"「我都快不明白「喜欢」这词的意思了」",{"rt2"},name={"華乃"}}}},linkback="block_00037",linknext="block_00039"},block_00144={{"text"},line=697,text={ja={{"「如果能一个个解决问题，可能能比上次给出更积极的回答吧。如果我在场有帮助的话，我可以同席的。」",{"rt2"},name={"智宏"}}}},linkback="block_00143",linknext="block_00145"},block_00108={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0010",file="kan_z1a1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10541",ch="kan"}},ja={{"「那个我有点想看看。如果你想起来你打算写什么，有时间告诉我哦」",{"rt2"},name={"華乃"}}}},linkback="block_00107",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0055",file="kan_z1a1100",lv=2.2999999999999998}}},line=543,linknext="block_00109"},block_00176={{"bgm",stop=1,time=3000},{"se",id=-1,stop=1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",time=3000,file="black",path=":bg/"},{"cacheclear"},{"ex",func="wait",time=1000},{"bg",by=-360,time=2000,path=":bg/",speed=120000,bx=-640,ay=720,ax=1280,y2=100,y=100,x=640,file="zbg036a",x2=-640},{"bgm",file="bgm23"},{"text"},line=846,text={ja={{"第二天，周六的午饭和晚饭我们都是在外面吃的，不过周日为了以后做准备，我煮了米饭，热了即食咖喱。华乃对男孩子做的料理充满了兴趣，眼睛都亮了起来。",{"rt2"}}}},linkback="block_00175",linknext="block_00177"},block_00139={{"text"},line=679,text={ja={{"即使这么说，但考试没有什么确切的对策，除了「努力学习」之外的解决方案也是……。",{"rt2"}}}},linkback="block_00138",linknext="block_00140"},block_00154={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0053",file="kan_z1b1110",lv=2.2999999999999998},{"text"},line=746,text={vo={{"vo",file="fem_kan_10563",ch="kan"}},ja={{"「下次遇到不懂的地方，可以请你教我吗？」",{"rt2"},name={"華乃"}}}},linkback="block_00153",linknext="block_00155"},block_00011={{"text"},line=66,text={ja={{"「华乃你为了这个而一个接一个地完成眼前的工作，真的很积极……就像活力的化身一样。」",{"rt2"},name={"智宏"}}}},linkback="block_00010",linknext="block_00012"},block_00080={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0019h",file="kan_z2a1100",lv=2.3999999999999999},{"text"},line=403,text={vo={{"vo",file="fem_kan_10526",ch="kan"}},ja={{"「了解到你过去的心情，现在我有点高兴……再让我沉浸一会儿吧」",{"rt2"},name={"華乃"}}}},linkback="block_00079",linknext="block_00081"},block_00017={{"text"},line=92,text={ja={{"在未来，可能会有更多这样的发现。至少今天，吃汉堡时，用筷子而不是叉子就够了。",{"rt2"}}}},linkback="block_00016",linknext="block_00018"},block_00033={{"text"},line=174,text={ja={{"「华乃很可爱」",{"rt2"},name={"智宏"}}}},linkback="block_00032",linknext="block_00034"},block_00104={{"text"},line=529,text={ja={{"「不、不是，完全没有那种感觉……只是，那之后我一晚上都在纠结而已」",{"rt2"},name={"智宏"}}}},linkback="block_00103",linknext="block_00105"},block_00182={{"fg",ch="資正",path=":fg/suk/no/",mode=1,id=20,head="suk_noa",size="no",face="a0004",file="suk_noa0000",lv=2.2000000000000002},{"text"},line=881,text={vo={{"vo",file="etc_suk_10002",ch="suk"}},ja={{"「呜～汪，汪！」",{"rt2"},name={"資正"}}}},linkback="block_00181",linknext="block_00183"},block_00046={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,id=13,head="kan_z2a",size="z2",face="a0023",file="kan_z2a1100",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10508",ch="kan"}},ja={{"「嗯？嗯～哈哈哈，我很幸福。当然会很幸福啦，毕竟和喜欢的人一起吃饭呢」",{"rt2"},name={"華乃"}}}},linkback="block_00045",delay={vl01={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,id=13,head="kan_z2a",size="z2",face="a0055",file="kan_z2a1100",lv=2.3999999999999999},{"fgact",ch="華乃",act="おじぎ",id=13,lv=2.3999999999999999}},vl02={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,id=13,head="kan_z2a",size="z2",face="a0022",file="kan_z2a1100",lv=2.3999999999999999}}},line=231,linknext="block_00047"},block_00018={{"text"},line=95,text={ja={{"「不是我不擅长文科……刚才那个其实更像是英语范围内的问题。话说回来，现在想起来，选择文科进修课程真的好吗……」",{"rt2"},name={"智宏"}}}},linkback="block_00017",linknext="block_00019"},block_00122={{"fg",ch="華乃",ex05="kan_noa0000",path=":fg/kan/no/",mode=3,id=13,head="kan_noa",size="no",face="a0019",file="kan_noa1100",lv=2.2000000000000002},{"text"},line=610,text={vo={{"vo",file="fem_kan_10547",ch="kan"}},ja={{"「只是因为工作不能贴在一起有点遗憾。好想早点和你一起上床呢。」",{"rt2"},name={"華乃"}}}},linkback="block_00121",linknext="block_00123"},block_00095={{"text"},line=482,text={ja={{"「啊，但是，让我稍微说一下。因为我想和华乃两个人谈话，所以被你们留下一人回家的时候我有点伤心」",{"rt2"},name={"智宏"}}}},linkback="block_00094",linknext="block_00096"},block_00184={{"fg",ch="資正",path=":fg/suk/no/",mode=1,id=20,head="suk_noa",size="no",face="a0002",file="suk_noa0000",lv=2.2000000000000002},{"text"},line=893,text={vo={{"vo",file="etc_suk_10003",ch="suk"}},ja={{"「汪」",{"rt2"},name={"資正"}}}},linkback="block_00183",linknext="block_00185"},block_00142={{"text"},line=689,text={ja={{"「考虑到她的性格，好像只要有机会依赖华乃，她就愿意等待……但我不确定学姐能否独自做这个决定。」",{"rt2"},name={"智宏"}}}},linkback="block_00141",linknext="block_00143"},block_00024={{"text"},line=125,text={ja={{"「嗯，是我错了。对不起」",{"rt2"},name={"智宏"}}}},linkback="block_00023",linknext="block_00025"},block_00138={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0038",file="kan_z1b1100",lv=2.2999999999999998},{"text"},line=677,text={vo={{"vo",file="fem_kan_10556",ch="kan"}},ja={{"「如果能够做好考试的准备，不管何时来自诗樱学姐的请求，我都能做出积极的回应……」",{"rt2"},name={"華乃"}}}},linkback="block_00137",linknext="block_00139"},block_00112={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0051",file="kan_z1a1100",lv=2.2999999999999998},{"fgact",ch="華乃",act="びっくり",id=13,lv=2.2999999999999998},{"se",file="seコミ_察する",id=1},{"text"},line=560,text={vo={{"vo",file="fem_kan_10542",ch="kan"}},ja={{"「啊！说起来，那个时候你不是说过如果我们在一起你会帮我出柩的cosplay吗？」",{"rt2"},name={"華乃"}}}},linkback="block_00111",linknext="block_00113"},block_00116={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0008",file="kan_z1b1100",lv=2.2999999999999998},{"fgact",ch="華乃",act="うなづく",id=13,lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10543",ch="kan"}},ja={{"「啊对，我们确实谈过这个！那作为我的男朋友的你，就应该帮出柩的cosplay吧？就是这样吧？」",{"rt2"},name={"華乃"}}}},linkback="block_00115",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0006",file="kan_z1b1100",lv=2.2999999999999998}},vl02={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0054",file="kan_z1b1100",lv=2.2999999999999998}}},line=576,linknext="block_00117"},block_00066={{"bg",file="bg099c",path=":bg/"},{"text"},line=322,text={ja={{"「我记得有一次，我想是在我们还没开始交往的时候……我们说过，如果要一起度过时间，最好找一个最让人舒适的伴侣」",{"rt2"},name={"智宏"}}}},linkback="block_00065",linknext="block_00067"},block_00148={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0010",file="kan_z1b1110",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10560",ch="kan"}},ja={{"「嗯？ 如果你愿意帮助我，那么还有其他可以做的事情吧？」",{"rt2"},name={"華乃"}}}},linkback="block_00147",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0015",file="kan_z1b1110",lv=2.2999999999999998}}},line=723,linknext="block_00149"},block_00031={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0019h",file="kan_z2a1100",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10500",ch="kan"}},ja={{"「啊，谢谢。其实，每次看见你对我做的饭兴致勃勃的样子，我就已经很开心了」",{"rt2"},name={"華乃"}}}},linkback="block_00030",delay={vl01={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0052h",file="kan_z2a1100",lv=2.3999999999999999}}},line=165,linknext="block_00032"},block_00026={{"text"},line=135,text={ja={{"「诶？今天是第一天？我还以为只是预演呢」",{"rt2"},name={"智宏"}}}},linkback="block_00025",linknext="block_00027"},block_00165={{"text"},line=796,text={ja={{"「虽然想，但直到新书制作完成我会自我克制的。去年的Comicado也是华乃说绝对不想落下新书，我才到了连夜帮忙修图的地步」",{"rt2"},name={"智宏"}}}},linkback="block_00164",linknext="block_00166"},block_00188={{"fg",ch="妃愛",ex05="hiy_noa0000",path=":fg/hiy/no/",mode=3,id=13,head="hiy_noa",size="no",face="a0022",file="hiy_noa1000",lv=2.2000000000000002},{"text"},text={vo={{"vo",file="fem_hiy_10025",ch="hiy"}},ja={{"「我希望我不会经常离家好几天……啊，但是下个月——」",{"rt2"},name={"妃愛"}}}},linkback="block_00187",delay={vl01={{"fg",ch="妃愛",ex05="hiy_noa0000",path=":fg/hiy/no/",mode=3,id=13,head="hiy_noa",size="no",face="a0046",file="hiy_noa1000",lv=2.2000000000000002},{"fgact",ch="妃愛",act="びっくり",id=13,lv=2.2000000000000002}}},line=917,linknext="block_00189"},block_00061={{"text"},line=297,text={ja={{"她真是能说一些令人害羞的话。她的每一句话都是全力以赴的直言，即使我们平时总是在一起，我还是感到害羞。",{"rt2"}}}},linkback="block_00060",linknext="block_00062"},block_00130={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0001",file="kan_z1b1110",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10551",ch="kan"}},ja={{"「诗樱的工作主要看她自己的活动安排，不过最近半年新画稿的需求并不多，而且这是我最想优先处理的工作，所以不在考虑之列。」",{"rt2"},name={"華乃"}}}},linkback="block_00129",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0055",file="kan_z1b1110",lv=2.2999999999999998}}},line=642,linknext="block_00131"},block_00015={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0028",file="kan_z1b1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10492",ch="kan"}},ja={{"「我的词汇量真的很少……果然我还是更适合文科一些，尤其是国语科目有点弱。」",{"rt2"},name={"華乃"}}}},linkback="block_00014",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0002",file="kan_z1b1100",lv=2.2999999999999998}}},line=87,linknext="block_00016"},block_00034={{"fg",ch="華乃",ex05="kan_z2b0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2b",size="z2",face="b0019h",file="kan_z2b1100",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10502",ch="kan"}},ja={{"「呃，你突然说什么啊。我很开心，但……啊」",{"rt2"},name={"華乃"}}}},linkback="block_00033",delay={vl01={{"fg",ch="華乃",ex05="kan_z2b0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2b",size="z2",face="b0008h",file="kan_z2b1100",lv=2.3999999999999999}}},line=180,linknext="block_00035"},block_00037={{"text"},line=194,text={ja={{"「那如果明显失败了，我就会先说「我喜欢你」，然后再指出问题」",{"rt2"},name={"智宏"}}}},linkback="block_00036",linknext="block_00038"},block_00115={{"text"},line=568,text={ja={{"「那只是华乃提出的交往男生的条件之一，就是要他们扮演柩的cosplay……」",{"rt2"},name={"智宏"}}}},linkback="block_00114",linknext="block_00116"},block_00022={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0016",file="kan_z1a1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10495",ch="kan"}},ja={{"「课程内容不论，如果我和你不在同一个班，单是上学就够累的了。所以我根本没有选择其他课程的余地。」",{"rt2"},name={"華乃"}}}},linkback="block_00021",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0007",file="kan_z1a1100",lv=2.2999999999999998}}},line=116,linknext="block_00023"},block_00060={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1a",size="z1",face="a0055",file="kan_z1a1100",lv=2.2999999999999998},{"text"},line=295,text={vo={{"vo",file="fem_kan_10517",ch="kan"}},ja={{"「我期待明天早上。我一直向往的朝霞下的早晨咖啡。唯一让我担心的就是我今晚可能会因为兴奋而睡不着。」",{"rt2"},name={"華乃"}}}},linkback="block_00059",linknext="block_00061"},block_00003={{"text"},line=24,text={ja={{"换了佛龛里的花水，没有忘记合十祈祷。爸爸妈妈，我也有了女朋友。看起来我们会长久相处，我会好好珍惜她的。",{"rt2"}}}},linkback="block_00002",linknext="block_00004"},block_00036={{"fg",ch="華乃",ex05="kan_z2a0010",path=":fg/kan/z2/",mode=3,id=13,head="kan_z2a",size="z2",face="a0003",file="kan_z2a1110",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10504",ch="kan"}},ja={{"「但如果我做的东西明显失败了，你就不用硬说好吃了。我不会生气的，那时候你笑笑给我指出来就好」",{"rt2"},name={"華乃"}}}},linkback="block_00035",delay={vl01={{"fg",ch="華乃",ex05="kan_z2a0010",path=":fg/kan/z2/",mode=3,id=13,head="kan_z2a",size="z2",face="a0022",file="kan_z2a1110",lv=2.3999999999999999}}},line=191,linknext="block_00037"},block_00007={{"text"},line=45,text={ja={{"虽然厨房是可以用的，但是还没选好餐厅用的桌子和椅子，所以今天的晚餐就在华乃的房间里吃了。餐桌是个小矮桌。",{"rt2"}}}},linkback="block_00006",linknext="block_00008"},block_00010={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0038",file="kan_z1a1100",lv=2.2999999999999998},{"text"},line=63,text={vo={{"vo",file="fem_kan_10489",ch="kan"}},ja={{"「之所以要提前完成，是为了优先处理其他的工作，虽然对接收方的公司有些抱歉。」",{"rt2"},name={"華乃"}}}},linkback="block_00009",linknext="block_00011"},block_00032={{"fg",ch="華乃",ex05="kan_z2b0010",path=":fg/kan/z2/",mode=3,id=13,head="kan_z2b",size="z2",face="b0053",file="kan_z2b1110",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10501",ch="kan"}},ja={{"「之后我会为你做一大堆的料理……不会刚好和料理一样多，但你要多多夸夸我哦」",{"rt2"},name={"華乃"}}}},linkback="block_00031",delay={vl01={{"fg",ch="華乃",ex05="kan_z2b0010",path=":fg/kan/z2/",mode=3,id=13,head="kan_z2b",size="z2",face="b0055",file="kan_z2b1110",lv=2.3999999999999999}}},line=171,linknext="block_00033"},block_00019={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0010",file="kan_z1b1110",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10493",ch="kan"}},ja={{"「诶，等等？我们本来在谈论插图的话题，怎么变成了关于考试的讨论？哦，但是我似乎确实引导了这个话题呢。」",{"rt2"},name={"華乃"}}}},linkback="block_00018",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0039",file="kan_z1b1110",lv=2.2999999999999998}}},line=101,linknext="block_00020"},block_00040={{"text"},line=204,text={ja={{"「周末是不是让华乃教我做做菜吧。华乃忙的时候我做饭给她吃」",{"rt2"},name={"智宏"}}}},linkback="block_00039",linknext="block_00041"},block_00107={{"text"},line=537,text={ja={{"「而且我也睡着了，没能及时发送重要的信息」",{"rt2"},name={"智宏"}}}},linkback="block_00106",linknext="block_00108"},block_00071={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1b",size="z1",face="b0010h",file="kan_z1b1110",lv=2.2999999999999998},{"text"},line=350,text={ja={{"她可能意识到自己误解了并且陷入了幻想，还无意间透露了当时不必说出的感情。华乃的脸色立刻变得羞红。",{"rt2"}}}},linkback="block_00070",linknext="block_00072"},block_00047={{"bg",ax=1280,by=-360,path=":bg/",speed=80000,bx=-640,ay=720,zoom=120,y2=-360,y=360,x=400,file="zbg036c",x2=400},{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=1,id=20,head="kan_z2a",size="z2",face="a0031",file="kan_z2a1100",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10509",ch="kan"}},ja={{"「接着想到等下不需要说再见，心里就充满了安心感。这可能是我以前从未有过的感情」",{"rt2"},name={"華乃"}}}},linkback="block_00046",delay={vl01={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=1,id=20,head="kan_z2a",size="z2",face="a0023",file="kan_z2a1100",lv=2.3999999999999999}}},line=239,linknext="block_00048"},block_00049={{"fg",ch="華乃",ex05="kan_z2b0000",path=":fg/kan/z2/",mode=1,id=20,head="kan_z2b",size="z2",face="b0034",file="kan_z2b1100",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10511",ch="kan"}},ja={{"「我这么喜欢你，但是没法把所有的理由都告诉你。但是我还是想在忘记之前，尽可能多地传达出我对你的「喜欢」……」",{"rt2"},name={"華乃"}}}},linkback="block_00048",delay={vl01={{"fg",ch="華乃",ex05="kan_z2b0000",path=":fg/kan/z2/",mode=1,id=20,head="kan_z2b",size="z2",face="b0023",file="kan_z2b1100",lv=2.3999999999999999}}},line=251,linknext="block_00050"},block_00146={{"text"},line=706,text={ja={{"想起在学生会的对话，让人无法笑开来。虽然我们并没有不和，也相互尊重，但是我们两个人每天都在开对方的玩笑。",{"rt2"}}}},linkback="block_00145",linknext="block_00147"},block_00053={{"fg",ch="華乃",resize=1,ex05="kan_z2b0000",path=":fg/kan/z2/",mode=1,id=20,head="kan_z2b",size="z2",face="b0019",file="kan_z2b1100",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10513",ch="kan"}},ja={{"「如果每天见面都要告诉你，那真的没完没了……但也没办法。新的喜欢就是无止境地增加啊」",{"rt2"},name={"華乃"}}}},linkback="block_00052",delay={vl01={{"fg",ch="華乃",resize=1,ex05="kan_z1b0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0053",file="kan_z1b1100",lv=2.2999999999999998}}},line=267,linknext="block_00054"},block_00186={{"text"},line=902,text={ja={{"「嗯，其实我被资正治愈了……每次回来他都会陪我玩。华乃，她现在很忙……一直在工作。」",{"rt2"},name={"智宏"}}}},linkback="block_00185",linknext="block_00187"},block_00118={{"fg",ch="華乃",ex05="kan_nob0010",path=":fg/kan/no/",mode=3,id=13,head="kan_nob",size="no",face="b0019",file="kan_nob1110",lv=2.2000000000000002},{"text"},text={vo={{"vo",file="fem_kan_10544",ch="kan"}},ja={{"「因为这个房间里没有服装，所以我现在不会再说什么，但你可别想逃跑哦？」",{"rt2"},name={"華乃"}}}},linkback="block_00117",delay={vl01={{"fg",ch="華乃",ex05="kan_nob0010",path=":fg/kan/no/",mode=3,id=13,head="kan_nob",size="no",face="b0015",file="kan_nob1110",lv=2.2000000000000002}}},line=591,linknext="block_00119"},block_00171={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1a",size="z1",face="a0034h",file="kan_z1a1100",lv=2.2999999999999998},{"fgact",ch="華乃",act="おじぎ",id=13,lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10569",ch="kan"}},ja={{"「谢谢……我现在真的很高兴……我会听你的话，明天和后天都会优先处理原画和同人的稿子。」",{"rt2"},name={"華乃"}}}},linkback="block_00170",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1a",size="z1",face="a0053h",file="kan_z1a1100",lv=2.2999999999999998}}},line=822,linknext="block_00172"},block_00059={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1a",size="z1",face="a0054",file="kan_z1a1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10516",ch="kan"}},ja={{"「不用担心时间真是太好了。和你在一起的时候，我总是希望「时间能不能停下来」，分开后我又在想「时间能不能快点过去」。」",{"rt2"},name={"華乃"}}}},linkback="block_00058",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1a",size="z1",face="a0019",file="kan_z1a1100",lv=2.2999999999999998}}},line=290,linknext="block_00060"},block_00058={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0055",file="kan_z1b1110",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10515",ch="kan"}},ja={{"「不过今天我们不用说再见。我们可以一直待到明天早上。所以我不用焦急地现在就告诉你，只要我想起来就可以告诉你。」",{"rt2"},name={"華乃"}}}},linkback="block_00057",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0022",file="kan_z1b1110",lv=2.2999999999999998}}},line=284,linknext="block_00059"},block_00111={{"text"},line=551,text={ja={{"「对了华乃……那次在学生会室谈恋爱的时候，你问我能不能Cosplay，如果那时我回答「可以」，你会有什么反应——」",{"rt2"},name={"智宏"}}}},linkback="block_00110",linknext="block_00112"},block_00039={{"text"},line=201,text={ja={{"虽然华乃嘴上这么说，但她显然是很开心，唰唰地拍打着我的手。她似乎想要挨得更近些，瞬间有些起身，但因为正在吃饭，所以她忍住了。",{"rt2"}}}},linkback="block_00038",linknext="block_00040"},block_00124={{"text"},line=614,text={ja={{"我也是怀着和华乃过上恩爱生活的期待来到这里的。考虑到华乃的性格，我也预想到会有很多甜言蜜语。",{"rt2"}}}},linkback="block_00123",linknext="block_00125"},block_00141={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0002",file="kan_z1b1100",lv=2.2999999999999998},{"text"},line=686,text={vo={{"vo",file="fem_kan_10557",ch="kan"}},ja={{"「如果能在考试结束后接受请求就最好了，但下一步就要考虑诗樱学姐的出版计划是否可以等那么久了。」",{"rt2"},name={"華乃"}}}},linkback="block_00140",linknext="block_00142"},block_00175={{"text"},line=837,text={ja={{"但男人一旦发表了言论就不能轻易撤回，因此我今天在华乃的房间里进行涂色练习，当华乃口渴时我会泡茶，如果她累了就会任由她撒娇。",{"rt2"}}}},linkback="block_00174",linknext="block_00176"},block_00132={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0001",file="kan_z1b1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10552",ch="kan"}},ja={{"「那么，接下来想要解决的问题就是……考试学习了吧。」",{"rt2"},name={"華乃"}}}},linkback="block_00131",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0018",file="kan_z1b1100",lv=2.2999999999999998}}},line=650,linknext="block_00133"},block_00074={{"text"},line=365,text={ja={{"「对不起，我错了。但是在我房间里，只是有喜欢的人在身边就让我无法控制地紧张，所以能知道华乃的感受我很开心。」",{"rt2"},name={"智宏"}}}},linkback="block_00073",linknext="block_00075"},block_00085={{"text"},line=431,text={ja={{"「等、等等，稍等一下。虽然我确实在会场里一直在关心华乃，但华乃也在看我吗？」",{"rt2"},name={"智宏"}}}},linkback="block_00084",linknext="block_00086"},block_00193={{"bgm",stop=1,time=3000},{"se",id=-1,stop=1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",time=3000,file="black",path=":bg/"},{"cacheclear"},{"ex",func="wait",time=1000},{"bg",id=9,path=":cg/",time=500,file="catch02",lv=9},{"bgm",file="bgm92",loop=0},{"ex",func="wait",time=6000},{"bgm",stop=1,time=3000},{"se",id=-1,stop=1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",time=3000,file="black",path=":bg/"},{"cacheclear"},{"ex",func="wait",time=1000},{"excall",file="02_華乃09_1"},{"text"},linkback="block_00192",line=933},block_00048={{"fg",ch="華乃",ex05="kan_z2b0000",path=":fg/kan/z2/",mode=1,id=20,head="kan_z2b",size="z2",face="b0022",file="kan_z2b1100",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10510",ch="kan"}},ja={{"「平时我都会急着在和你在一起的时候，把当天的所有喜欢的感情都传达给你……想告诉你这个，也想告诉你那个」",{"rt2"},name={"華乃"}}}},linkback="block_00047",delay={vl01={{"fg",ch="華乃",ex05="kan_z2b0000",path=":fg/kan/z2/",mode=1,id=20,head="kan_z2b",size="z2",face="b0032",file="kan_z2b1100",lv=2.3999999999999999}}},line=245,linknext="block_00049"},block_00079={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0008h",file="kan_z2a1100",lv=2.3999999999999999},{"fgact",ch="華乃",act="びっくり",id=13,lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10525",ch="kan"}},ja={{"「等、等一下，我们还没吃完饭呢！虽、虽然已经吃完了……但还要洗碗……」",{"rt2"},name={"華乃"}}}},linkback="block_00078",delay={vl01={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0002h",file="kan_z2a1100",lv=2.3999999999999999}},vl02={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0020h",file="kan_z2a1100",lv=2.3999999999999999}}},line=398,linknext="block_00080"},block_00157={{"text"},line=756,text={ja={{"嗯，确实如此。恋人之间开学习会，我以为那只是漫画中才会出现的事。我也想让华乃教我一些理科科目呢……她教我图形学的时候总是很温柔。",{"rt2"}}}},linkback="block_00156",linknext="block_00158"},block_00128={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1a",size="z1",face="a0027",file="kan_z1a1100",lv=2.2999999999999998},{"text"},line=631,text={vo={{"vo",file="fem_kan_10549",ch="kan"}},ja={{"「我正在一口气处理掉色情游戏的工作，下一部新作开始之前应该会有一段空闲时间，所以这个问题基本上算是解决了。」",{"rt2"},name={"華乃"}}}},linkback="block_00127",linknext="block_00129"},block_00183={{"fg",ch="妃愛",ex05="hiy_noa0000",path=":fg/hiy/no/",mode=3,id=13,head="hiy_noa",size="no",face="a0035",file="hiy_noa1000",lv=2.2000000000000002},{"text"},text={vo={{"vo",file="fem_hiy_10022",ch="hiy"}},ja={{"「哦，资正，对不起让你感到寂寞。离开两天真是对不起呢。你和哥哥相处得好吗？」",{"rt2"},name={"妃愛"}}}},linkback="block_00182",delay={vl01={{"fg",ch="妃愛",ex05="hiy_noa0000",path=":fg/hiy/no/",mode=3,id=13,head="hiy_noa",size="no",face="a0019",file="hiy_noa1000",lv=2.2000000000000002}},vl02={{"fg",ch="妃愛",ex05="hiy_noa0000",path=":fg/hiy/no/",mode=3,id=13,head="hiy_noa",size="no",face="a0053",file="hiy_noa1000",lv=2.2000000000000002}}},line=888,linknext="block_00184"},block_00192={{"text"},line=927,text={ja={{"华乃是为了生活而工作，我也要说出「我也要为了我们两人的未来好好备考！」这样的真心话，成为那样的男人。今天用华乃的R18画来慰籍自己吧……。",{"rt2"}}}},linkback="block_00191",linknext="block_00193"},block_00161={{"text"},line=781,text={ja={{"难道我刚才的表情很失望吗。那个华乃她一脸紧张地，急忙改口前面的话。",{"rt2"}}}},linkback="block_00160",linknext="block_00162"},block_00190={{"text"},line=923,text={ja={{"华乃毕竟是个社会人，和我这样的学生想法不同……不，问题是她说了即使不做爱也没关系，那我还纠结什么呢。",{"rt2"}}}},linkback="block_00189",linknext="block_00191"},block_00102={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0038",file="kan_z1a1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10539",ch="kan"}},ja={{"「不仅是为了社团，我也是用社团的事情作为借口逃避的。如果我做好了准备，可能我会有心理准备，但是我很胆小」",{"rt2"},name={"華乃"}}}},linkback="block_00101",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0028",file="kan_z1a1100",lv=2.2999999999999998}},vl02={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0034",file="kan_z1a1100",lv=2.2999999999999998}}},line=521,linknext="block_00103"},block_00044={{"text"},line=220,text={ja={{"华乃并不知道我做菜的手艺。虽然我看起来料理手艺还不错…至少炒饭我是做得很好的。但是，看着眼前的幸福表情，所有的思绪都被夺走了。",{"rt2"}}}},linkback="block_00043",linknext="block_00045"},block_00189={{"bg",file="bg099c",path=":bg/"},{"text"},line=921,text={ja={{"本以为和华乃一起生活，每天都会过得欲火焚身、几乎无暇自顾，然而理所当然和现实是，今天我明白了，忙的时候工作是优先考虑的。",{"rt2"}}}},linkback="block_00188",linknext="block_00190"},block_00187={{"fg",ch="妃愛",ex05="hiy_nob0000",path=":fg/hiy/no/",mode=3,id=13,head="hiy_nob",size="no",face="b0008",file="hiy_nob1000",lv=2.2000000000000002},{"text"},text={vo={{"vo",file="fem_hiy_10024",ch="hiy"}},ja={{"「哎呀，那真是时机不好。嗯——如果是资正的照顾问题，我会和里姐商量一下，然后再重新考虑的。」",{"rt2"},name={"妃愛"}}}},linkback="block_00186",delay={vl01={{"fg",ch="妃愛",ex05="hiy_noa0000",path=":fg/hiy/no/",mode=3,id=13,head="hiy_noa",size="no",face="a0018",file="hiy_noa1000",lv=2.2000000000000002},{"fgact",ch="妃愛",act="おじぎ",id=13,lv=2.2000000000000002}},vl02={{"fg",ch="妃愛",ex05="hiy_noa0000",path=":fg/hiy/no/",mode=3,id=13,head="hiy_noa",size="no",face="a0053",file="hiy_noa1000",lv=2.2000000000000002}}},line=910,linknext="block_00188"},block_00027={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0010",file="kan_z1a1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10498",ch="kan"}},ja={{"「嗯—，确实把今天作为同居纪念日有点不合适。那就算做预演的第一天吧」",{"rt2"},name={"華乃"}}}},linkback="block_00026",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0054",file="kan_z1a1100",lv=2.2999999999999998}}},line=141,linknext="block_00028"},block_00185={{"fg",ch="妃愛",ex05="hiy_nob0000",path=":fg/hiy/no/",mode=3,id=13,head="hiy_nob",size="no",face="b0023",file="hiy_nob1000",lv=2.2000000000000002},{"text"},text={vo={{"vo",file="fem_hiy_10023",ch="hiy"}},ja={{"「哥哥，谢谢你照顾资正。毕竟两天还是很长的，下次如果有2days之类的演唱会，我想我会请宠物保姆帮忙。」",{"rt2"},name={"妃愛"}}}},linkback="block_00184",delay={vl01={{"fg",ch="妃愛",ex05="hiy_nob0000",path=":fg/hiy/no/",mode=3,id=13,head="hiy_nob",size="no",face="b0001",file="hiy_nob1000",lv=2.2000000000000002}}},line=899,linknext="block_00186"},block_00113={{"text"},line=562,text={ja={{"华乃的眼睛开始闪耀得像个孩子一样。那并不是一个恋人的表情，而更像是猎人的。",{"rt2"}}}},linkback="block_00112",linknext="block_00114"},block_00073={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1b",size="z1",face="b0045h",file="kan_z1b1100",lv=2.2999999999999998},{"fgact",ch="華乃",act="びっくり",id=13,lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10522",ch="kan"}},ja={{"「你这个……！如果那样的话，就明确说明会话的时间……要说清楚啊！我，心动了或者紧张了这些，都是在回味着说出来的……你这个！」",{"rt2"},name={"華乃"}}}},linkback="block_00072",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1b",size="z1",face="b0014h",file="kan_z1b1100",lv=2.2999999999999998}},vl02={{"fg",ch="華乃",ex01="b0099",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1b",size="z1",face="b0042h",file="kan_z1b1110",lv=2.2999999999999998}}},line=362,linknext="block_00074"},block_00077={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1a",size="z1",face="a0018h",file="kan_z1a1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10524",ch="kan"}},ja={{"「我当时就是倔强，虽然觉得肯定不行……但如果被你强行压倒……我竟然会想这种事，明明还没确定关系就在你身边感到兴奋。」",{"rt2"},name={"華乃"}}}},linkback="block_00076",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1a",size="z1",face="a0002h",file="kan_z1a1100",lv=2.2999999999999998}},vl02={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1a",size="z1",face="a0012h",file="kan_z1a1100",lv=2.2999999999999998}}},line=381,linknext="block_00078"},block_00109={{"text"},line=545,text={ja={{"回忆起我们刚开始的那段时间，我和华乃都有些害羞地低下了头。不过，现在还会因为这样的事情感到害羞，我们还算是新手……吧？",{"rt2"}}}},linkback="block_00108",linknext="block_00110"},block_00174={{"text"},line=835,text={ja={{"被这么一说，我今晚竟然情难自禁……我们是共享同一张床的嘛……",{"rt2"}}}},linkback="block_00173",linknext="block_00175"},block_00089={{"bg",file="white",path=":bg/"},{"msgoff"},{"bg",by=-360,sync=0,zoom=120,bx=-640,path=":bg/",ax=1280,file="zbg036c",ay=720},{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",sync=0,size="z2",face="a0052h",file="kan_z2a1100",lv=2.3999999999999999},{"extrans"},{"text"},text={vo={{"vo",file="fem_kan_10532",ch="kan"}},ja={{"「说到底，你也是我第一次觉得现实中的男孩子很帅的人。那次研讨会和……你保护我免受学姐们欺负的时候」",{"rt2"},name={"華乃"}}}},linkback="block_00088",delay={vl01={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0023h",file="kan_z2a1100",lv=2.3999999999999999}}},line=461,linknext="block_00090"},block_00078={{"msgoff"},{"bg",by=-360,sync=0,zoom=120,bx=-640,path=":bg/",ax=1280,file="zbg036c",ay=720},{"fg",ch="華乃",resize=1,ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",sync=0,size="z2",face="a0012h",file="kan_z2a1100",lv=2.3999999999999999},{"extrans",time=500},{"text"},line=390,text={ja={{"「华、华乃！」",{"rt2"},name={"智宏"}}}},linkback="block_00077",linknext="block_00079"},block_00021={{"text"},line=110,text={ja={{"「说到考试科目，因为是国文课程，所以课程内容偏向文科。」",{"rt2"},name={"智宏"}}}},linkback="block_00020",linknext="block_00022"},block_00101={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0002",file="kan_z1b1100",lv=2.2999999999999998},{"text"},line=514,text={vo={{"vo",file="fem_kan_10538",ch="kan"}},ja={{"「但是我没有当场解释的原因之一就是，我想再确认一下自己的感情」",{"rt2"},name={"華乃"}}}},linkback="block_00100",linknext="block_00102"},block_00055={{"text"},line=271,text={ja={{"阳光男孩应该不会这么充满情感地说出爱的话吧。不，我不知道，他们会说吗？如果是的话，我觉得我们可以成为好朋友。偶尔认真地聊聊也挺好的。",{"rt2"}}}},linkback="block_00054",linknext="block_00056"},block_00062={{"msgoff"},{"bg",sync=0,by=-360,bx=-640,path=":bg/",ax=1280,file="zbg036c",ay=720},{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",sync=0,size="z1",face="a0055",file="kan_z1a1100",lv=2.2999999999999998},{"extrans",time=500},{"text"},line=306,text={vo={{"vo",file="fem_kan_10518",ch="kan"}},ja={{"「我喜欢你」",{"rt2"},name={"華乃"}}}},linkback="block_00061",linknext="block_00063"},block_00008={{"msgoff"},{"bg",sync=0,by=-360,bx=-640,path=":bg/",ax=1280,file="zbg036c",ay=720},{"fg",ch="華乃",resize=1,ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",sync=0,size="z1",face="a0023",file="kan_z1a1100",lv=2.2999999999999998},{"extrans",time=500},{"text"},text={vo={{"vo",file="fem_kan_10488",ch="kan"}},ja={{"「今天我完成了两幅线稿。这个速度的话，似乎可以在截止日期前一个月就交货了。他们都惊讶地说「没有人能这么快手啊」」",{"rt2"},name={"華乃"}}}},linkback="block_00007",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0055",file="kan_z1a1100",lv=2.2999999999999998}}},line=56,linknext="block_00009"},block_00005={{"text"},line=41,text={ja={{"这个女朋友是因为「期待我回家」而高兴得无法自已吗？她一边咬着后牙，一边按着忍不住要笑出来的脸颊，来迎接我。",{"rt2"}}}},linkback="block_00004",linknext="block_00006"},block_00025={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0009",file="kan_z1a1100",lv=2.2999999999999998},{"fgact",ch="華乃",act="いいえ",id=13,lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10497",ch="kan"}},ja={{"「啊，我也对不起。如果我说的话显得有点刻薄，请原谅我。从同居的第一天就成这样……」",{"rt2"},name={"華乃"}}}},linkback="block_00024",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0020",file="kan_z1a1100",lv=2.2999999999999998}}},line=132,linknext="block_00026"},block_00178={{"text"},line=850,text={ja={{"结果，周日我们也没有发生过关系。因为华乃太累了。尽管我抱着她亲吻，全程都很温柔地亲昵着，但只有这些，然后一天就这样结束了。",{"rt2"}}}},linkback="block_00177",linknext="block_00179"},block_00001={{"fg",ch="資正",path=":fg/suk/no/",mode=1,id=20,head="suk_noa",size="no",face="a0001",file="suk_noa0000",lv=2.2000000000000002},{"text"},line=20,text={vo={{"vo",file="etc_suk_10001",ch="suk"}},ja={{"「呜～」",{"rt2"},name={"資正"}}}},linkback="block_00000",linknext="block_00002"},block_00004={{"cgdel",id=-1},{"bg",wait=250,vague=40,rule="rule_001",time=1000,file="black",path=":bg/"},{"msgoff"},{"bg",sync=0,file="bg036c",path=":bg/"},{"fg",ch="華乃",ex05="kan_noa0000",path=":fg/kan/no/",mode=3,hoho="h",id=13,head="kan_noa",sync=0,size="no",face="a0035h",file="kan_noa1100",lv=2.2000000000000002},{"extrans",vague=40,rule="rule_001"},{"text"},line=39,text={vo={{"vo",file="fem_kan_10487",ch="kan"}},ja={{"「欢、欢迎回来」",{"rt2"},name={"華乃"}}}},linkback="block_00003",linknext="block_00005"},block_00137={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1a",size="z1",face="a0028",file="kan_z1a1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10555",ch="kan"}},ja={{"「其实我现在就想去学习了，但是考虑到工作，那也是做不到的……如果说有什么烦恼，那这就是最大的烦恼。」",{"rt2"},name={"華乃"}}}},linkback="block_00136",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1a",size="z1",face="a0018",file="kan_z1a1100",lv=2.2999999999999998}}},line=672,linknext="block_00138"},block_00050={{"text"},line=253,text={ja={{"在这一点上，我是幸运的。因为即使和华乃分开了，野野香还是会偶尔更新推特。我能看到喜欢的人的呢喃和我最爱的涂鸦。",{"rt2"}}}},linkback="block_00049",linknext="block_00051"},block_00094={{"text"},line=479,text={ja={{"「我，无论是刚见到华乃还是再次见到她，都觉得她很可爱……我意识到自己的恋情是在漫画市集那晚」",{"rt2"},name={"智宏"}}}},linkback="block_00093",linknext="block_00095"},block_00177={{"text"},line=848,text={ja={{"华乃上午画原画，下午开始画新书的稿子，看起来真的很忙。因此，我只在华乃休息的时候和她亲热。",{"rt2"}}}},linkback="block_00176",linknext="block_00178"},block_00054={{"text"},line=269,text={ja={{"作为宅男，总是在各种恋爱作品中看到尴尬的男女对话，然后把它们带入现实，这是个坏习惯。",{"rt2"}}}},linkback="block_00053",linknext="block_00055"},block_00006={{"text"},line=43,text={ja={{"她一边说「其实也很向往两个人一起选餐厅」的话，一边穿上围裙为我准备了自己做的晚餐。她平时似乎不怎么做饭。",{"rt2"}}}},linkback="block_00005",linknext="block_00007"},block_00023={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0028",file="kan_z1a1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10496",ch="kan"}},ja={{"「但话又说回来，我不能让你的升学计划受我的影响……你的考试是为了长远的未来，而我的考试只是为了这四年能过得愉快。」",{"rt2"},name={"華乃"}}}},linkback="block_00022",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0018",file="kan_z1a1100",lv=2.2999999999999998}}},line=122,linknext="block_00024"},block_00100={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0009",file="kan_z1b1110",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10537",ch="kan"}},ja={{"「嗯，那个，我知道如果和你单独谈话的话，肯定会谈到重要的事情……不管结果好还是坏，我都会感到心神不宁」",{"rt2"},name={"華乃"}}}},linkback="block_00099",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0020",file="kan_z1b1100",lv=2.2999999999999998}}},line=509,linknext="block_00101"},block_00133={{"text"},line=652,text={ja={{"说起来，吃饭时她也提到了关于学习的事情。或许，这才是今天的主题。",{"rt2"}}}},linkback="block_00132",linknext="block_00134"},block_00172={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1b",size="z1",face="b0055h",file="kan_z1b1100",lv=2.2999999999999998},{"text"},line=827,text={vo={{"vo",file="fem_kan_10570",ch="kan"}},ja={{"「我真的很开心……等全部结束后，我会满足你的所有要求。」",{"rt2"},name={"華乃"}}}},linkback="block_00171",linknext="block_00173"},block_00170={{"msgoff"},{"bg",sync=0,by=-360,bx=-640,path=":bg/",ax=1280,file="zbg036c",ay=720},{"fg",ch="華乃",resize=1,ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1a",sync=0,size="z1",face="a0012h",file="kan_z1a1100",lv=2.2999999999999998},{"extrans",time=500},{"text"},line=815,text={ja={{"「话说回来，『在同一个房间里却互不干扰』是华乃理想男性的条件。所以你就专心你的原稿吧，我去搜集活动的新书。」",{"rt2"},name={"智宏"}}}},linkback="block_00169",linknext="block_00171"},block_00169={{"text"},line=806,text={ja={{"甚至她轻轻地磨着脚。华乃的眼睛好像更期待……。",{"rt2"}}}},linkback="block_00168",linknext="block_00170"},block_00035={{"fg",ch="華乃",ex05="kan_z2b0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2b",size="z2",face="b0055h",file="kan_z2b1100",lv=2.3999999999999999},{"text"},line=185,text={vo={{"vo",file="fem_kan_10503",ch="kan"}},ja={{"「对了，从我们开始交往那天起，你每天都没忘记告诉我我很可爱。令我心安」",{"rt2"},name={"華乃"}}}},linkback="block_00034",linknext="block_00036"},block_00168={{"text"},line=804,text={ja={{"其实华乃正在将眉头皱成了八字吧，眼睛微微湿润，凝视着我。我在稿子提交前表示理解，这行为能打动一个创作者的内心……！",{"rt2"}}}},linkback="block_00167",linknext="block_00169"},block_00166={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0012h",file="kan_z2a1100",lv=2.3999999999999999},{"text"},line=800,text={ja={{"虽然说是这么说，那时候还是做了。嘛，那时候修改已经结束了，算安全！",{"rt2"}}}},linkback="block_00165",linknext="block_00167"},block_00167={{"text"},line=802,text={ja={{"其实我这个没有出过同人志的人对于，距离活动还有两周（未投稿）的那种修罗场到底怎样还不清楚。虽然非常想要，只是我在华乃面前装酷而已。",{"rt2"}}}},linkback="block_00166",linknext="block_00168"},block_00088={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=1,hoho="h",id=20,head="kan_z2a",size="z2",face="a0038h",file="kan_z2a1100",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10531",ch="kan"}},ja={{"「我现在可以列举出很多喜欢你的理由……但是，我觉得你最「帅」的可能就是那一次了」",{"rt2"},name={"華乃"}}}},linkback="block_00087",delay={vl01={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=1,hoho="h",id=20,head="kan_z2a",size="z2",face="a0023h",file="kan_z2a1100",lv=2.3999999999999999}}},line=448,linknext="block_00089"},block_00063={{"text"},line=309,text={ja={{"「噢，我也喜欢你」",{"rt2"},name={"智宏"}}}},linkback="block_00062",linknext="block_00064"},block_00125={{"text"},line=616,text={ja={{"然而，她比我想象的更加撒娇了。如果说今天的预演练习有什么问题的话，就如华乃所说的「太过幸福了」。",{"rt2"}}}},linkback="block_00124",linknext="block_00126"},block_00056={{"text"},line=273,text={ja={{"华乃平常总是那么倔强，一旦变得坦率起来，就会用稍微夸张的语言告诉我她的恋爱之情。我非常爱这样的恋人。",{"rt2"}}}},linkback="block_00055",linknext="block_00057"},block_00163={{"text"},line=787,text={ja={{"「仔细想想，离要在GW发布新书还有两周……那边也得加把劲啊。明天后天尽可能多面对电脑画画」",{"rt2"},name={"智宏"}}}},linkback="block_00162",linknext="block_00164"},block_00064={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0022",file="kan_z1a1100",lv=2.2999999999999998},{"text"},line=314,text={vo={{"vo",file="fem_kan_10519",ch="kan"}},ja={{"「期待明年」",{"rt2"},name={"華乃"}}}},linkback="block_00063",linknext="block_00065"},block_00162={{"text"},line=784,text={ja={{"「如果可以忍耐的话，是不是应该改到星期日？ 如果隔天是平常日的话……去上课可能更能休息一下」",{"rt2"},name={"智宏"}}}},linkback="block_00161",linknext="block_00163"},block_00134={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1a",size="z1",face="a0028",file="kan_z1a1100",lv=2.2999999999999998},{"text"},line=657,text={vo={{"vo",file="fem_kan_10553",ch="kan"}},ja={{"「虽然模拟考试我得到了B评价，但离有余裕这个词还很远，所以我也必须认真学习。」",{"rt2"},name={"華乃"}}}},linkback="block_00133",linknext="block_00135"},block_00191={{"text"},line=925,text={ja={{"因为我真的很想做爱啊啊啊啊。即使再忙，我也希望至少能做一次，这种想法是不是说明我还太幼稚了。",{"rt2"}}}},linkback="block_00190",linknext="block_00192"},block_00180={{"text"},line=854,text={ja={{"相较之下，我这个看似成熟的大人扮演者，反而因为不能满足欲望而感到悲伤。归根结底我只是一个正常的男孩而已。",{"rt2"}}}},linkback="block_00179",linknext="block_00181"},block_00042={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,id=13,head="kan_z2a",size="z2",face="a0054",file="kan_z2a1100",lv=2.3999999999999999},{"text"},line=215,text={vo={{"vo",file="fem_kan_10507",ch="kan"}},ja={{"「吃前你每次都说「我喜欢你」呢」",{"rt2"},name={"華乃"}}}},linkback="block_00041",linknext="block_00043"},block_00092={{"text"},line=474,text={ja={{"不知道以后还会听到她多少次表达这样的感情。每次听到都会让人心跳加速。恋情溢出来，让胸口疼痛。",{"rt2"}}}},linkback="block_00091",linknext="block_00093"},block_00158={{"msgoff"},{"bg",by=-360,sync=0,zoom=120,bx=-640,path=":bg/",ax=1280,file="zbg036c",ay=720},{"fg",ch="華乃",resize=1,ex05="kan_z2b0000",path=":fg/kan/z2/",mode=3,id=13,head="kan_z2b",sync=0,size="z2",face="b0018",file="kan_z2b1100",lv=2.3999999999999999},{"extrans",time=500},{"text"},line=765,text={vo={{"vo",file="fem_kan_10565",ch="kan"}},ja={{"「也不是说要你帮忙……不过明天后天我打算继续努力画线稿……」",{"rt2"},name={"華乃"}}}},linkback="block_00157",linknext="block_00159"},block_00119={{"text"},line=593,text={ja={{"华乃伸出双臂搭在桌子上，看着我咯咯地笑。这种调皮的笑容让我想起了我们刚相遇的那段日子……真好。",{"rt2"}}}},linkback="block_00118",linknext="block_00120"},block_00030={{"text"},line=159,text={ja={{"「我原本打算把所有的食物都吃完后再大力夸奖你……但显然我应该在吃第一口的时候就说，华乃做的饭，很好吃」",{"rt2"},name={"智宏"}}}},linkback="block_00029",linknext="block_00031"},block_00155={{"text"},line=749,text={ja={{"「明白了……虽然我也并不特别擅长古文和汉文」",{"rt2"},name={"智宏"}}}},linkback="block_00154",linknext="block_00156"},block_00014={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0008",file="kan_z1b1100",lv=2.2999999999999998},{"fgact",ch="華乃",act="びっくり",id=13,lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10491",ch="kan"}},ja={{"「啊，嗯，对不起对不起。我知道你是在夸我。我只是纯粹不知道它的原始含义。这个词在日语里没有对应的吧。」",{"rt2"},name={"華乃"}}}},linkback="block_00013",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0022",file="kan_z1b1100",lv=2.2999999999999998}}},line=81,linknext="block_00015"},block_00135={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1a",size="z1",face="a0038",file="kan_z1a1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10554",ch="kan"}},ja={{"「夏季的同人活动，既然已经报名了，就要好好出新刊，但在后半段我打算去补习班接受夏季课程。你也会去吗？」",{"rt2"},name={"華乃"}}}},linkback="block_00134",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1a",size="z1",face="a0001",file="kan_z1a1100",lv=2.2999999999999998}}},line=663,linknext="block_00136"},block_00051={{"text"},line=255,text={ja={{"虽然我的账号也应该被她注意到了，但我没什么值得说的。我只是发些无聊的推特而已。",{"rt2"}}}},linkback="block_00050",linknext="block_00052"},block_00029={{"text"},line=156,text={ja={{"不过在同居之前，她已经让我试过好几次了。虽然有些无趣，但我还是吃下了华乃伸手递来的汉堡。",{"rt2"}}}},linkback="block_00028",linknext="block_00030"},block_00143={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0001",file="kan_z1b1100",lv=2.2999999999999998},{"text"},line=694,text={vo={{"vo",file="fem_kan_10558",ch="kan"}},ja={{"「虽然前一次我给了模棱两可的回应，但是我或许应该再和诗樱学姐好好谈一谈？」",{"rt2"},name={"華乃"}}}},linkback="block_00142",linknext="block_00144"},block_00152={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0016",file="kan_z1b1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10562",ch="kan"}},ja={{"「你一个要去教育系的人这是什么话。你不是和我一样，不只是表面上，而是真的对老师有所憧憬吗？像米莉老师那样的。」",{"rt2"},name={"華乃"}}}},linkback="block_00151",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0003",file="kan_z1b1100",lv=2.2999999999999998}}},line=739,linknext="block_00153"},block_00087={{"fg",ch="華乃",ex05="kan_z2b0000",path=":fg/kan/z2/",mode=1,hoho="h",id=20,head="kan_z2b",size="z2",face="b0002h",file="kan_z2b1100",lv=2.3999999999999999},{"text"},line=442,text={vo={{"vo",file="fem_kan_10530",ch="kan"}},ja={{"「在里老师说『让他休息』之后，我几乎要陷入沮丧无法回应时，你立即回答说『我会去会场』。你明白那时我的心情吗？」",{"rt2"},name={"華乃"}}}},linkback="block_00086",linknext="block_00088"},block_00151={{"text"},line=733,text={ja={{"「你是要我教你吗？我对教别人学习真的不太行。」",{"rt2"},name={"智宏"}}}},linkback="block_00150",linknext="block_00152"},block_00149={{"text"},line=725,text={ja={{"她直来直去让人喜欢，但这次她的期待目光不仅毫无顾忌，甚至可以说大胆无畏。她……到底在期待什么呢。",{"rt2"}}}},linkback="block_00148",linknext="block_00150"},block_00147={{"msgoff"},{"bg",sync=0,by=-360,bx=-640,path=":bg/",ax=1280,file="zbg036c",ay=720},{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",sync=0,size="z1",face="b0023",file="kan_z1b1100",lv=2.2999999999999998},{"extrans"},{"text"},line=715,text={ja={{"「因为我一直都在受你的照顾，如果能为了华乃想做的事出一份力，说真的我会有点开心。还有，作为野野香的粉丝，我也会很开心的。」",{"rt2"},name={"智宏"}}}},linkback="block_00146",linknext="block_00148"},block_00145={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0038",file="kan_z1b1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10559",ch="kan"}},ja={{"「可能会请你的帮忙。我觉得我和诗樱学姐独自在一起的话似乎不太合适，所以我觉得更希望你能来调和一下气氛。」",{"rt2"},name={"華乃"}}}},linkback="block_00144",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0023",file="kan_z1b1100",lv=2.2999999999999998}}},line=704,linknext="block_00146"},block_00045={{"text"},line=223,text={ja={{"「华乃，你现在看起来非常幸福」",{"rt2"},name={"智宏"}}}},linkback="block_00044",linknext="block_00046"},block_00082={{"bg",file="white",path=":bg/"},{"msgoff"},{"bg",ay=720,by=-360,path=":bg/",speed=120000,bx=-640,sync=0,ax=1280,y2=100,y=100,x=-640,file="zbg020a",x2=640},{"bg",id=9,sync=0,path=":sys/",file="reco",lv=9},{"extrans"},{"text"},line=417,text={ja={{"「虽然中途就结束了，但是之后我一整天都在华乃的附近度过，感觉随时会在会场内抱住你，压制自己真是太辛苦了……」",{"rt2"},name={"智宏"}}}},linkback="block_00081",linknext="block_00083"},block_00153={{"text"},line=741,text={ja={{"不仅新川，华乃也知道了。她是个很了解我内心的恋人，原来如此。",{"rt2"}}}},linkback="block_00152",linknext="block_00154"},block_00140={{"text"},line=681,text={ja={{"然而，华乃正在忙于消化大量的工作，对于此刻的恋人，我显然无法提供任何实质性的建议。",{"rt2"}}}},linkback="block_00139",linknext="block_00141"},block_00097={{"text"},line=489,text={ja={{"我稍微抱怨了一下，结果遭到了无法反驳的反击，直接击中了我的鼻子。我真的很抱歉，低头道歉。",{"rt2"}}}},linkback="block_00096",linknext="block_00098"},block_00136={{"text"},line=666,text={ja={{"「啊，也罢……虽然我说去，但我和华乃想要强化的科目完全相反，看起来将要上的辅导课完全不一样。」",{"rt2"},name={"智宏"}}}},linkback="block_00135",linknext="block_00137"},block_00131={{"text"},line=644,text={ja={{"听到这些话，锦应该会很高兴吧。说实话，作为一个粉丝，我也希望诗樱能有更多新的画稿和新装备。",{"rt2"}}}},linkback="block_00130",linknext="block_00132"},block_00129={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0039",file="kan_z1b1110",lv=2.2999999999999998},{"text"},line=636,text={vo={{"vo",file="fem_kan_10550",ch="kan"}},ja={{"「社交游戏的工作量我可以自己控制，而且还有不少尚未使用的画稿，所以这个也不成问题。」",{"rt2"},name={"華乃"}}}},linkback="block_00128",linknext="block_00130"},block_00127={{"bg",by=-360,path=":bg/",speed=120000,bx=-640,ay=720,ax=1280,y2=100,y=100,x=640,file="zbg036c",x2=-640},{"fg",ch="華乃",resize=1,ex05="kan_z1a0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1a",size="z1",face="a0023",file="kan_z1a1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10548",ch="kan"}},ja={{"「如果这样下去，明年应该和你的新生活也不会有问题……首先要把工作处理好，对吧？」",{"rt2"},name={"華乃"}}}},linkback="block_00126",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1a",size="z1",face="a0001",file="kan_z1a1100",lv=2.2999999999999998}}},line=626,linknext="block_00128"},block_00126={{"text"},line=618,text={ja={{"在尝过这份幸福之后，比如华乃要去海外的活动几天不在，那我可能会很难受……我想做一个大度的男朋友，但我似乎会很寂寞。",{"rt2"}}}},linkback="block_00125",linknext="block_00127"},block_00164={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0022h",file="kan_z2a1100",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10568",ch="kan"}},ja={{"「谢谢……但是，你不会觉得无聊吗？　不会想做那个吗？」",{"rt2"},name={"華乃"}}}},linkback="block_00163",delay={vl01={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0002h",file="kan_z2a1100",lv=2.3999999999999999}}},line=793,linknext="block_00165"},block_00110={{"text"},line=548,text={ja={{"「能这样听到还没开始交往时华乃的心情真不错……关于我们重逢的记忆也一个接一个地涌现出来……啊」",{"rt2"},name={"智宏"}}}},linkback="block_00109",linknext="block_00111"},block_00043={{"text"},line=218,text={ja={{"「这不是只有做失败的时候才说的吗？」",{"rt2"},name={"智宏"}}}},linkback="block_00042",linknext="block_00044"},block_00076={{"fg",ch="華乃",ex05="kan_z1a0010",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1a",size="z1",face="a0036h",file="kan_z1a1110",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10523",ch="kan"}},ja={{"「哼，不好意思，那时候我们其实已经是相思了。我也是有这个心理准备才去的，结果在男生的房间里，只有我们两个人，我紧张得要死。」",{"rt2"},name={"華乃"}}}},linkback="block_00075",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0010",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1a",size="z1",face="a0003h",file="kan_z1a1110",lv=2.2999999999999998}}},line=374,linknext="block_00077"},block_00012={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0010",file="kan_z1b1110",lv=2.2999999999999998},{"text"},line=71,text={vo={{"vo",file="fem_kan_10490",ch="kan"}},ja={{"「活力这个词我也经常用，大概理解为意愿或者行动力之类的含义，这样对吗？还是说它有坚韧或者体力之类的意思？」",{"rt2"},name={"華乃"}}}},linkback="block_00011",linknext="block_00013"},block_00083={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=1,hoho="h",id=20,head="kan_z2a",size="z2",face="a0002h",file="kan_z2a1100",lv=2.3999999999999999},{"text"},line=422,text={vo={{"vo",file="fem_kan_10527",ch="kan"}},ja={{"「我，我也是，如果没有作为社团代表的责任感、对来帮忙的天梨和来参观的客人的歉意，我也会一直想着你」",{"rt2"},name={"華乃"}}}},linkback="block_00082",linknext="block_00084"},block_00123={{"text"},line=612,text={ja={{"虽然只是预演，但毕竟是正处于恋爱高潮的恋人们共同生活，尽管有一些无奈，但这比预期的甜蜜过头了吧。",{"rt2"}}}},linkback="block_00122",linknext="block_00124"},block_00121={{"fg",ch="華乃",ex05="kan_noa0000",path=":fg/kan/no/",mode=3,id=13,head="kan_noa",size="no",face="a0001",file="kan_noa1100",lv=2.2000000000000002},{"text"},text={vo={{"vo",file="fem_kan_10546",ch="kan"}},ja={{"「虽然才刚开始，实际生活起来可能会有各种问题……但现在只有幸福感。预演练习，可能大成功了呢。」",{"rt2"},name={"華乃"}}}},linkback="block_00120",delay={vl01={{"fg",ch="華乃",ex05="kan_noa0000",path=":fg/kan/no/",mode=3,id=13,head="kan_noa",size="no",face="a0055",file="kan_noa1100",lv=2.2000000000000002}}},line=605,linknext="block_00122"},block_00120={{"fg",ch="華乃",ex05="kan_noa0000",path=":fg/kan/no/",mode=3,id=13,head="kan_noa",size="no",face="a0054",file="kan_noa1100",lv=2.2000000000000002},{"text"},text={vo={{"vo",file="fem_kan_10545",ch="kan"}},ja={{"「和你一起吃饭真开心。我并没太担心，看来我们24小时在一起也没问题。」",{"rt2"},name={"華乃"}}}},linkback="block_00119",delay={vl01={{"fg",ch="華乃",ex05="kan_noa0000",path=":fg/kan/no/",mode=3,id=13,head="kan_noa",size="no",face="a0022",file="kan_noa1100",lv=2.2000000000000002}}},line=599,linknext="block_00121"},block_00041={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,id=13,head="kan_z2a",size="z2",face="a0051",file="kan_z2a1100",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10506",ch="kan"}},ja={{"「男生做菜真的很让人向往。如果亲眼看到了，我会很开心，好像什么都可以原谅。我不想让菜凉，所以我会先吃」",{"rt2"},name={"華乃"}}}},linkback="block_00040",delay={vl01={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,id=13,head="kan_z2a",size="z2",face="a0019",file="kan_z2a1100",lv=2.3999999999999999}}},line=210,linknext="block_00042"},block_00114={{"text"},line=565,text={ja={{"「啊啊，那时候怎么可能会说『如果在一起』这种话呢！还没有到恋爱的阶段，如果我说出那样的话，华乃肯定会以为我对她有意思了！」",{"rt2"},name={"智宏"}}}},linkback="block_00113",linknext="block_00115"},block_00069={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0025",file="kan_z1b1110",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10521",ch="kan"}},ja={{"「\"根本没想到会交往\"这句话是假的吧？你当时肯定很在意我。我都看出你紧张了。虽然我也很紧张……」",{"rt2"},name={"華乃"}}}},linkback="block_00068",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0034",file="kan_z1b1110",lv=2.2999999999999998}}},line=337,linknext="block_00070"},block_00181={{"bgm",stop=1},{"cgdel",id=-1},{"bg",wait=500,vague=40,rule="rule_002",time=1000,file="black",path=":bg/"},{"msgoff"},{"bg",sync=0,file="bg002c",path=":bg/"},{"fg",ch="妃愛",ex05="hiy_nob0000",path=":fg/hiy/no/",mode=3,id=13,head="hiy_nob",sync=0,size="no",face="b0054",file="hiy_nob1000",lv=2.2000000000000002},{"extrans",vague=40,rule="rule_002r"},{"bgm",file="bgm01"},{"text"},text={vo={{"vo",file="fem_hiy_10021",ch="hiy"}},ja={{"「我回来了。这是从九州带回来的纪念品。有小鸡形状的巧克力哦！是草莓味的！」",{"rt2"},name={"妃愛"}}}},linkback="block_00180",delay={vl01={{"fg",ch="妃愛",ex05="hiy_nob0000",path=":fg/hiy/no/",mode=3,id=13,head="hiy_nob",size="no",face="b0052",file="hiy_nob1000",lv=2.2000000000000002}},vl02={{"fg",ch="妃愛",ex05="hiy_nob0010",path=":fg/hiy/no/",mode=3,id=13,head="hiy_nob",size="no",face="b0041",file="hiy_nob1010",lv=2.2000000000000002}}},line=876,linknext="block_00182"},block_00160={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0009h",file="kan_z2a1100",lv=2.3999999999999999},{"fgact",ch="華乃",act="びっくり",id=13,lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10567",ch="kan"}},ja={{"「啊，不过今天是啊！　考虑到是同居的第一天，嗯……我也想要」",{"rt2"},name={"華乃"}}}},linkback="block_00159",delay={vl01={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0034h",file="kan_z2a1100",lv=2.3999999999999999},{"fgact",ch="華乃",act="うなづく",id=13,lv=2.3999999999999999}}},line=779,linknext="block_00161"},block_00002={{"text"},line=22,text={ja={{"结束了资正的散步，喂完食和水后，清理了厕所和笼子。妃爱说家里的清洁工作不需要我做，所以就没动。",{"rt2"}}}},linkback="block_00001",linknext="block_00003"},block_00106={{"text"},line=534,text={ja={{"我们彼此都很忙碌，但都尽力关心着对方度过了充实的一天。只要明白这一点就足够了。",{"rt2"}}}},linkback="block_00105",linknext="block_00107"},block_00105={{"text"},line=532,text={ja={{"「再说，我也没打算责怪一个因为初恋而惊慌的女孩子是胆小鬼」",{"rt2"},name={"智宏"}}}},linkback="block_00104",linknext="block_00106"},block_00067={{"text"},line=325,text={ja={{"「在那个时候，我根本没想到会和华乃交往。如果让当时的我听到现在和华乃的对话，我可能会吓得腿软」",{"rt2"},name={"智宏"}}}},linkback="block_00066",linknext="block_00068"},block_00099={{"text"},line=501,text={ja={{"「啊！是、是这样……对不起。现在想想，这是理所当然的，毕竟有很多客人在等待呢」",{"rt2"},name={"智宏"}}}},linkback="block_00098",linknext="block_00100"},block_00075={{"text"},line=368,text={ja={{"「就像稍微碰到肩膀华乃就会僵硬地缩回去……我觉得这么可爱的生物真是太棒了。那时候我正深深地暗恋着她。」",{"rt2"},name={"智宏"}}}},linkback="block_00074",linknext="block_00076"},block_00098={{"msgoff"},{"bg",sync=0,by=-360,bx=-640,path=":bg/",ax=1280,file="zbg036c",ay=720},{"fg",ch="華乃",resize=1,ex05="kan_z1b0010",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",sync=0,size="z1",face="b0018",file="kan_z1b1110",lv=2.2999999999999998},{"extrans",time=500},{"text"},line=498,text={vo={{"vo",file="fem_kan_10536",ch="kan"}},ja={{"「对不起。我那时候必须去做社团的工作，所以完全没空……」",{"rt2"},name={"華乃"}}}},linkback="block_00097",linknext="block_00099"},block_00096={{"fg",ch="華乃",ex05="kan_z2b0000",path=":fg/kan/z2/",mode=3,id=13,head="kan_z2b",size="z2",face="b0017",file="kan_z2b1100",lv=2.3999999999999999},{"text"},line=487,text={vo={{"vo",file="fem_kan_10535",ch="kan"}},ja={{"「毕竟如果我们两个人单独谈话的话，你肯定会问我「什么时候开始喜欢我的？」对吧」",{"rt2"},name={"華乃"}}}},linkback="block_00095",linknext="block_00097"},block_00052={{"fg",ch="華乃",resize=1,ex05="kan_z1b0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0052",file="kan_z1b1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10512",ch="kan"}},ja={{"「真的呢，只要和你在一起，无意中说出的话，偷偷看你的侧脸，不经意间触碰到的手……那些只在那一天才有的喜欢实在太多了」",{"rt2"},name={"華乃"}}}},linkback="block_00051",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0034",file="kan_z1b1100",lv=2.2999999999999998}}},line=261,linknext="block_00053"},block_00028={{"msgoff"},{"bg",by=-360,sync=0,zoom=120,bx=-640,path=":bg/",ax=1280,file="zbg036c",ay=720},{"fg",ch="華乃",resize=1,ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",sync=0,size="z2",face="a0038h",file="kan_z2a1100",lv=2.3999999999999999},{"extrans",time=500},{"text"},text={vo={{"vo",file="fem_kan_10499",ch="kan"}},ja={{"「这也是预演的一部分……一个想在同居之后尝试的事。这、这个，啊～」",{"rt2"},name={"華乃"}}}},linkback="block_00027",delay={vl01={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0019h",file="kan_z2a1100",lv=2.3999999999999999}},vl02={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0035h",file="kan_z2a1100",lv=2.3999999999999999},{"fgact",ch="華乃",act="うなづく",id=13,lv=2.3999999999999999}}},line=154,linknext="block_00029"},block_00093={{"text"},line=476,text={ja={{"原来，不用被时间追赶也是一件好事。可以慢慢回想起当时的心情，讲述恋爱的初期。",{"rt2"}}}},linkback="block_00092",linknext="block_00094"},block_00159={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0020h",file="kan_z2a1100",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10566",ch="kan"}},ja={{"「画一张是一回事，要画两三张就是持久战了……不要太激进，那个……」",{"rt2"},name={"華乃"}}}},linkback="block_00158",delay={vl01={{"fg",ch="華乃",ex05="kan_z2a0000",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2a",size="z2",face="a0002h",file="kan_z2a1100",lv=2.3999999999999999}}},line=771,linknext="block_00160"},block_00103={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0002",file="kan_z1a1100",lv=2.2999999999999998},{"text"},line=526,text={vo={{"vo",file="fem_kan_10540",ch="kan"}},ja={{"「我并没有打算忽视你的感情……但如果让你有这样的感觉，那么我很抱歉」",{"rt2"},name={"華乃"}}}},linkback="block_00102",linknext="block_00104"},block_00013={{"text"},line=74,text={ja={{"「啊，对不起，其实我也不太清楚。感觉是一种很积极的……这样解释的话意思又变了。我是想夸你的。」",{"rt2"},name={"智宏"}}}},linkback="block_00012",linknext="block_00014"},block_00179={{"text"},line=852,text={ja={{"我曾戏称自己的恋人为欲望怪兽，但华乃也是可以为了工作平常忍耐不做爱的女性。精神力与专业人士不相上下……不，其实这是正常的。",{"rt2"}}}},linkback="block_00178",linknext="block_00180"},block_00072={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1b",size="z1",face="b0046h",file="kan_z1b1100",lv=2.2999999999999998},{"text"},line=354,text={ja={{"华乃毫不害羞地表达自己的恋心也很好，但毕竟害羞的华乃是最可爱的。我忍不住笑出来，华乃的脸更红了。",{"rt2"}}}},linkback="block_00071",linknext="block_00073"},block_00086={{"fg",ch="華乃",ex05="kan_z2b0000",path=":fg/kan/z2/",mode=1,hoho="h",id=20,head="kan_z2b",size="z2",face="b0008h",file="kan_z2b1100",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10529",ch="kan"}},ja={{"「嗯，嗯，因为我会看你啊，这是当然的。我们第一次做了……虽然没做到最后，但那是紧接着的事情……你跟我一起来到会场，我觉得很依赖你……」",{"rt2"},name={"華乃"}}}},linkback="block_00085",delay={vl01={{"fg",ch="華乃",ex05="kan_z2b0000",path=":fg/kan/z2/",mode=1,hoho="h",id=20,head="kan_z2b",size="z2",face="b0020h",file="kan_z2b1100",lv=2.3999999999999999}}},line=437,linknext="block_00087"},label={top={block="block_00000",label=1}},block_00084={{"fg",ch="華乃",ex05="kan_z2b0010",path=":fg/kan/z2/",mode=1,hoho="h",id=20,head="kan_z2b",size="z2",face="b0009h",file="kan_z2b1110",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10528",ch="kan"}},ja={{"「我很担心你的身体状况，稍微看了一下你……发现你正在看我的时候真是吓坏了。『我现在，在意这个人』这样……」",{"rt2"},name={"華乃"}}}},linkback="block_00083",delay={vl01={{"fg",ch="華乃",ex05="kan_z2b0010",path=":fg/kan/z2/",mode=1,hoho="h",id=20,head="kan_z2b",size="z2",face="b0019h",file="kan_z2b1110",lv=2.3999999999999999}}},line=428,linknext="block_00085"},block_00009={{"text"},line=58,text={ja={{"虽然我还没完全弄清楚这有多了不起，但是在许多画师都在抱怨截止日期的同时，我的恋人不仅能按时完成，还能提前交货，我觉得她真的很棒。",{"rt2"}}}},linkback="block_00008",linknext="block_00010"},block_00081={{"text"},line=406,text={ja={{"「那，那我就说说我单恋时痛苦的事吧，连漫画市场那天都很危险」",{"rt2"},name={"智宏"}}}},linkback="block_00080",linknext="block_00082"},block_00070={{"msgoff"},{"bg",sync=0,by=-360,bx=-640,path=":bg/",ax=1280,file="zbg036c",ay=720},{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",sync=0,size="z1",face="b0034",file="kan_z1b1110",lv=2.2999999999999998},{"extrans"},{"text"},line=346,text={ja={{"「啊？那次在我房间的对话？对不起，我说的不是那次，而是我们在学生会室里独处的那次对话……我想是在期末考试前吧」",{"rt2"},name={"智宏"}}}},linkback="block_00069",linknext="block_00071"},block_00091={{"fg",ch="華乃",ex05="kan_z2b0010",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2b",size="z2",face="b0055h",file="kan_z2b1110",lv=2.3999999999999999},{"text"},line=472,text={vo={{"vo",file="fem_kan_10534",ch="kan"}},ja={{"「而现在，我已经喜欢你到可以献出我的全部了」",{"rt2"},name={"華乃"}}}},linkback="block_00090",linknext="block_00092"},block_00065={{"text"},line=317,text={ja={{"「我也期待和华乃一起度过的每一天。回到家就可以两个人一起懒洋洋地躺着，永远地悠闲度日。如果我们两个都无所事事了，就去游戏中心玩吧」",{"rt2"},name={"智宏"}}}},linkback="block_00064",linknext="block_00066"},block_00057={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=1,id=20,head="kan_z1b",size="z1",face="b0002",file="kan_z1b1110",lv=2.2999999999999998},{"text"},line=278,text={vo={{"vo",file="fem_kan_10514",ch="kan"}},ja={{"「我想每天都把我喜欢你的部分告诉你，但是我们能在一起的时间是有限的，一旦那个时间过去，保持我们面对面时的热情就很困难了……」",{"rt2"},name={"華乃"}}}},linkback="block_00056",linknext="block_00058"},block_00173={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1b",size="z1",face="b0053h",file="kan_z1b1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10571",ch="kan"}},ja={{"「即使你做什么我都不会讨厌，被告知的事情我会全部做到。你要对我做什么，现在就开始考虑吧。」",{"rt2"},name={"華乃"}}}},linkback="block_00172",delay={vl01={{"fg",ch="華乃",ex05="kan_z1b0000",path=":fg/kan/z1/",mode=3,hoho="h",id=13,head="kan_z1b",size="z1",face="b0022h",file="kan_z1b1100",lv=2.2999999999999998}}},line=833,linknext="block_00174"},block_00156={{"fg",ch="華乃",ex05="kan_z1b0010",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1b",size="z1",face="b0054",file="kan_z1b1110",lv=2.2999999999999998},{"text"},line=754,text={vo={{"vo",file="fem_kan_10564",ch="kan"}},ja={{"「我也曾憧憬过你教我学习的场景呢」",{"rt2"},name={"華乃"}}}},linkback="block_00155",linknext="block_00157"},block_00020={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0001",file="kan_z1a1100",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10494",ch="kan"}},ja={{"「实际上，因为可以选择考试科目，所以文理之间的差别并不大……只是需要讨论如何设立备选方案而已。」",{"rt2"},name={"華乃"}}}},linkback="block_00019",delay={vl01={{"fg",ch="華乃",ex05="kan_z1a0000",path=":fg/kan/z1/",mode=3,id=13,head="kan_z1a",size="z1",face="a0038",file="kan_z1a1100",lv=2.2999999999999998}}},line=107,linknext="block_00021"},block_00016={{"text"},line=90,text={ja={{"华乃先用筷子切了汉堡，然后才淋上酱。我和妃爱都是先淋酱再切的。生活中的差异就体现在这些小地方。",{"rt2"}}}},linkback="block_00015",linknext="block_00017"},block_00090={{"fg",ch="華乃",ex05="kan_z2b0010",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2b",size="z2",face="b0022h",file="kan_z2b1110",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10533",ch="kan"}},ja={{"「你帮我办活动的时候，我一个人在会场里你赶来陪我的时候，我都很开心。也许在我恋爱之前，就有点喜欢……你了吧」",{"rt2"},name={"華乃"}}}},linkback="block_00089",delay={vl01={{"fg",ch="華乃",ex05="kan_z2b0010",path=":fg/kan/z2/",mode=3,hoho="h",id=13,head="kan_z2b",size="z2",face="b0010h",file="kan_z2b1110",lv=2.3999999999999999}}},line=467,linknext="block_00091"}}