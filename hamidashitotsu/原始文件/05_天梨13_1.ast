ast = {block_00104={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",sync=0,ax=1280,ay=720,path=":bg/",by=-360,bx=-640,file="zbg011a"},{"fg",ex05="kan_z1a0010",size="z1",ch="華乃",id=7,lv=2.2999999999999998,resize=1,face="a0031",sync=0,head="kan_z1a",path=":fg/kan/z1/",mode=3,file="kan_z1a0210"},{"extrans",time=500},{"text"},line=712,text={ja={{"「没办法啊，那我就陪你――」",{"rt2"},name={"華乃"}}},vo={{"vo",ch="kan",file="fem_kan_00462"}}},linkback="block_00103",linknext="block_00105"},block_00086={{"fg",ex05="ame_z1a0010",size="z1",ch="天梨",id=9,lv=2.2999999999999998,face="a0010",head="ame_z1a",mode=3,path=":fg/ame/z1/",x=-280,ex03="a0097",file="ame_z1a0210"},{"text"},linkback="block_00085",delay={vl01={{"fg",ex05="ame_z1a0000",size="z1",ch="天梨",id=9,lv=2.2999999999999998,face="a0052",head="ame_z1a",path=":fg/ame/z1/",x=-280,mode=3,file="ame_z1a0200"},{"fgact",ch="天梨",id=9,lv=2.2999999999999998,act="びっくり"}}},line=589,text={ja={{"「花的装饰……啊，难道是要请花道部帮忙？」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_00798"}}},linknext="block_00087"},block_00070={{"fg",ex05="shi_noa0010",size="no",ch="詩桜",id=20,lv=2.2000000000000002,face="a0001",head="shi_noa",path=":fg/shi/no/",mode=1,file="shi_noa0210"},{"text"},line=486,text={ja={{"「他那句充满活力的「加油！」让人觉得更痛苦。那明亮的笑容完全没有效果呢」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01773"}}},linkback="block_00069",linknext="block_00071"},block_00025={{"fg",ex05="shi_nob0010",size="no",ch="詩桜",id=10,lv=2.2000000000000002,face="b0032",head="shi_nob",path=":fg/shi/no/",x=230,mode=3,file="shi_nob0210"},{"text"},linkback="block_00024",delay={vl02={{"fg",ex05="shi_noa0010",size="no",ch="詩桜",id=10,lv=2.2000000000000002,face="a0054",head="shi_noa",path=":fg/shi/no/",x=230,mode=3,file="shi_noa0210"}},vl01={{"fg",ex05="shi_noa0010",size="no",ch="詩桜",id=10,lv=2.2000000000000002,face="a0052",head="shi_noa",path=":fg/shi/no/",x=230,mode=3,file="shi_noa0210"}}},line=184,text={ja={{"「昨天晚上希优铃的现场歌声真是太棒了……下次我一定要在现场享受呢，呵呵」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01770"}}},linknext="block_00026"},block_00006={{"fg",ex05="shi_z1a0000",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0043",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0200"},{"text"},linkback="block_00005",delay={vl02={{"fg",ex05="shi_z1b0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="b0039",head="shi_z1b",path=":fg/shi/z1/",mode=3,file="shi_z1b0210"}},vl03={{"fg",ex05="shi_z1b0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="b0042",head="shi_z1b",path=":fg/shi/z1/",mode=3,file="shi_z1b0210"}},vl01={{"fg",ex05="shi_z1a0000",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0028",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0200"},{"fgact",ch="詩桜",id=13,lv=2.2999999999999998,act="おじぎ"}}},line=51,text={ja={{"「你真是个废物。连自己可爱的妹妹的表演都不去支持。原来是我看错了你。算了，你就去和鳄鱼战斗，流血流汗吧。」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01762"}}},linknext="block_00007"},block_00087={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",by=-360,sync=0,ax=1280,ay=720,path=":bg/",x=-600,bx=-640,file="zbg011a"},{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=8,lv=2.2999999999999998,resize=1,face="a0001",sync=0,head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0210"},{"extrans",time=500},{"fgact",ch="詩桜",id=8,lv=2.2999999999999998,act="おじぎ"},{"text"},line=601,text={ja={{"「原来如此……如果让她们负责装饰可以说是文化祭的门面，她们肯定会很开心的。」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01776"}}},linkback="block_00086",linknext="block_00088"},block_00050={{"fg",ex05="ame_nob0010",size="no",ch="天梨",id=13,lv=2.2000000000000002,face="b0038",head="ame_nob",path=":fg/ame/no/",mode=3,file="ame_nob0210"},{"fgact",ch="天梨",id=13,lv=2.2000000000000002,act="おじぎ"},{"text"},linkback="block_00049",delay={vl01={{"fg",ex05="ame_nob0000",size="no",ch="天梨",id=13,lv=2.2000000000000002,face="b0055",head="ame_nob",path=":fg/ame/no/",mode=3,file="ame_nob0200"}}},line=343,text={ja={{"「嗯，嗯～，那个嘛～。我是学生嘛，我想好好参加文化节？」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_00792"}}},linknext="block_00051"},block_00074={{"text"},line=503,text={ja={{"虽然文化节的拱门是个难题，但妃爱说她想试试看，所以我就全部交给她了。",{"rt2"}}}},linkback="block_00073",linknext="block_00075"},block_00059={{"fg",ex05="hiy_nob0000",size="no",ch="妃愛",id=10,lv=2.2000000000000002,face="b0052",head="hiy_nob",path=":fg/hiy/no/",x=0,mode=3,file="hiy_nob0200"},{"text"},linkback="block_00058",delay={vl01={{"fg",ex05="hiy_nob0000",size="no",ch="妃愛",id=10,lv=2.2000000000000002,face="b0048h",head="hiy_nob",hoho="h",path=":fg/hiy/no/",x=0,mode=3,file="hiy_nob0200"}}},line=419,text={ja={{"「我还没开始捉弄她，她的脸就已经红通通的了。好可爱啊」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01482"}}},linknext="block_00060"},block_00100={{"fg",ex05="hiy_nob0010",ch="妃愛",id=7,lv=2.2000000000000002,face="b0031",head="hiy_nob",x=-160,path=":fg/hiy/no/",x2=-380,mode=3,file="hiy_nob0210"},{"fg",ex05="asu_noa0010",ch="あすみ",id=13,lv=2.2000000000000002,face="a0054",head="asu_noa",x=160,path=":fg/asu/no/",x2=0,mode=3,file="asu_noa0210"},{"fg",ex05="shi_noa0000",ch="詩桜",id=8,lv=2.2000000000000002,face="a0001",head="shi_noa",x=480,path=":fg/shi/no/",x2=380,mode=3,file="shi_noa0200"},{"fg",ex05="mir_noa0000",size="no",ch="里",id=5,lv=2.2000000000000002,face="a0054h",mode=3,head="mir_noa",hoho="h",path=":fg/mir/no/",x=-480,mx=-40,file="mir_noa0200"},{"text"},line=686,text={ja={{"「诗樱酱，渡月她现在有空哦。看起来马上就能见面了」",{"rt2"},name={"里"}}},vo={{"vo",ch="mir",file="fem_mir_00212"}}},linkback="block_00099",linknext="block_00101"},block_00022={{"fg",ex05="hiy_nob0000",size="no",ch="妃愛",id=9,lv=2.2000000000000002,face="b0052",head="hiy_nob",path=":fg/hiy/no/",x=-230,mode=3,file="hiy_nob0200"},{"fgact",ch="妃愛",id=9,lv=2.2000000000000002,act="うなづく"},{"text"},linkback="block_00021",delay={vl01={{"fg",ex05="hiy_nob0000",size="no",ch="妃愛",id=9,lv=2.2000000000000002,face="b0032",head="hiy_nob",path=":fg/hiy/no/",x=-230,mode=3,file="hiy_nob0200"}}},line=166,text={ja={{"「好的，如果原价可以的话我会转让给你。虽然我很想直接赠送给你，但华乃已经购买过了」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01473"}}},linknext="block_00023"},block_00082={{"fg",ex05="shi_noa0010",size="no",ch="詩桜",id=8,lv=2.2000000000000002,face="a0031",head="shi_noa",path=":fg/shi/no/",x=380,mode=3,file="shi_noa0210"},{"text"},linkback="block_00081",delay={vl01={{"fg",ex05="shi_noa0010",size="no",ch="詩桜",id=8,lv=2.2000000000000002,face="a0006",head="shi_noa",path=":fg/shi/no/",x=380,mode=3,file="shi_noa0210"}}},line=560,text={ja={{"「莉莉子喜欢园林游览，似乎已经去过日本各地的许多名园。她也喜欢看复古的洋房和府邸，都内的著名建筑她基本上都参观过了。」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01774"}}},linknext="block_00083"},block_00023={{"fg",ex05="shi_noa0000",size="no",ch="詩桜",id=10,lv=2.2000000000000002,face="a0051",head="shi_noa",path=":fg/shi/no/",x=230,mode=3,file="shi_noa0200"},{"fgact",ch="詩桜",id=10,lv=2.2000000000000002,act="おじぎ"},{"text"},linkback="block_00022",delay={vl01={{"fg",ex05="shi_noa0000",size="no",ch="詩桜",id=10,lv=2.2000000000000002,face="a0035",head="shi_noa",path=":fg/shi/no/",x=230,mode=3,file="shi_noa0200"}}},line=173,text={ja={{"「谢谢你……谢谢你，妃爱……我永远不会忘记这份恩情……！」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01769"}}},linknext="block_00024"},block_00000={{"savetitle",text="クリムゾン"},{"bg",by=-360,time=2000,bx=-640,x=0,y=360,speed=75000,ax=1280,ay=720,path=":bg/",x2=0,y2=-360,file="zbg099a"},{"text"},line=13,text={ja={{"距离文化祭还剩15天。",{"rt2"}}}},linknext="block_00001"},block_00019={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",path=":bg/",sync=0,file="bg011a"},{"fg",ex05="hiy_nob0000",size="no",ch="妃愛",id=9,lv=2.2000000000000002,face="b0052",sync=0,head="hiy_nob",path=":fg/hiy/no/",x=-230,mode=3,file="hiy_nob0200"},{"fg",ex05="shi_nob0010",size="no",ch="詩桜",id=10,lv=2.2000000000000002,resize=1,face="b0028",sync=0,head="shi_nob",path=":fg/shi/no/",x=230,mode=3,file="shi_nob0210"},{"extrans",time=500},{"text"},linkback="block_00018",delay={vl01={{"fg",ex05="hiy_nob0010",size="no",ch="妃愛",id=9,lv=2.2000000000000002,face="b0055",head="hiy_nob",path=":fg/hiy/no/",x=-230,mode=3,file="hiy_nob0210"}}},line=141,text={ja={{"「对于一直以来支持我的大家，我感到的感谢是共享回忆的『喜悦』，而对于现在支持我的大家，我感到的感谢同样是『爱』哟」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01472"}}},linknext="block_00020"},block_00077={{"fg",ex05="hiy_z1b0010",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="b0006",head="hiy_z1b",path=":fg/hiy/z1/",mode=3,file="hiy_z1b0210"},{"text"},linkback="block_00076",delay={vl02={{"fg",ex05="hiy_z1a0010",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="a0006",head="hiy_z1a",path=":fg/hiy/z1/",mode=3,file="hiy_z1a0210"}},vl01={{"fg",ex05="hiy_z1b0010",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="b0015",head="hiy_z1b",path=":fg/hiy/z1/",mode=3,file="hiy_z1b0210"}}},line=518,text={ja={{"「这次我们有一个特别麻烦的贵宾要来，我打算专门为她设计！让我们做一个高雅的学院大小姐会喜欢的拱门吧！」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01486"}}},linknext="block_00078"},block_00010={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0045",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0210"},{"fg",ex05="shi_z1a0010",ch="詩桜",id=13,lv=2.2999999999999998,face="a0045",head="shi_z1a",x=280,path=":fg/shi/z1/",x2=0,mode=3,file="shi_z1a0210"},{"fg",ex05="kan_z1a0010",size="z1",ch="華乃",id=9,lv=2.2999999999999998,resize=1,face="a0032",head="kan_z1a",mode=3,path=":fg/kan/z1/",x=-280,mx=-40,file="kan_z1a0210"},{"text"},line=75,text={ja={{"在我和诗樱学姐的交涉结束时，华乃从包里拿出了一支『风铃！』黄色的应援棒，悄然展示出来。她为什么会带着这个。",{"rt2"}}}},linkback="block_00009",linknext="block_00011"},block_00075={{"text"},line=505,text={ja={{"我一直在意妃爱无法参与实际工作，所以也想让她负责一些需要动脑筋的工作，而且不管怎么说，妃爱的审美观非常好。",{"rt2"}}}},linkback="block_00074",linknext="block_00076"},block_00090={{"fg",ex05="hiy_noa0000",size="no",ch="妃愛",id=20,lv=2.2000000000000002,resize=1,face="a0054",head="hiy_noa",path=":fg/hiy/no/",mode=1,file="hiy_noa0200"},{"text"},line=618,text={ja={{"「其实我有点期待你会这么说。这也是我送你那个荧光棒的一部分原因。」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01489"}}},linkback="block_00089",linknext="block_00091"},block_00065={{"text"},line=459,text={ja={{"「我一直以我这个最有空闲时间的人为标准来推进事情，对不起。妃爱下午也要工作，今天的工作就到上午为止吧」",{"rt2"},name={"智宏"}}}},linkback="block_00064",linknext="block_00066"},block_00041={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",by=-360,sync=0,ax=1280,ay=720,path=":bg/",x=600,bx=-640,file="zbg011a"},{"fg",ex05="mir_z1a0010",size="z1",ch="里",id=13,lv=2.2999999999999998,resize=1,face="a0019",sync=0,head="mir_z1a",path=":fg/mir/z1/",mode=3,file="mir_z1a0210"},{"extrans",time=500},{"text"},linkback="block_00040",delay={vl01={{"fg",ex05="mir_z1a0000",size="z1",ch="里",id=13,lv=2.2999999999999998,face="a0002",head="mir_z1a",path=":fg/mir/z1/",mode=3,file="mir_z1a0200"}}},line=285,text={ja={{"「嘛，但是呢。虽然妃爱你这么说，但反过来说不也是让大家得到了解压的机会吗？从暑假开始制作到现在，大家可是一天都没有休息过哦？」",{"rt2"},name={"里"}}},vo={{"vo",ch="mir",file="fem_mir_00206"}}},linknext="block_00042"},block_00054={{"fg",ex05="ame_noa0010",ch="天梨",id=13,lv=2.2000000000000002,face="a0002",x=-380,head="ame_noa",mode=3,path=":fg/ame/no/",x2=-230,ex03="a0097",file="ame_noa0210"},{"fg",ex05="hiy_noa0000",ch="妃愛",id=10,lv=2.2000000000000002,face="a0028",head="hiy_noa",x=0,path=":fg/hiy/no/",x2=230,mode=3,file="hiy_noa0200"},{"fg",ex05="asu_noa0010",size="no",ch="あすみ",id=8,lv=2.2000000000000002,face="a0007",head="asu_noa",mode=3,path=":fg/asu/no/",x=380,mx=40,file="asu_noa0210"},{"text"},linkback="block_00053",delay={vl01={{"fg",ex05="asu_noa0010",size="no",ch="あすみ",id=8,lv=2.2000000000000002,face="a0003",head="asu_noa",path=":fg/asu/no/",x=380,mode=3,file="asu_noa0210"},{"fgact",ch="あすみ",id=8,lv=2.2000000000000002,act="うんうん"}}},line=381,text={ja={{"「是，是的！不能牺牲梦想！如果你能站在像Gal Collection这样的大舞台上，我会支持你的！」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_00199"}}},linknext="block_00055"},block_00058={{"fg",ex05="asu_nob0000",size="no",ch="あすみ",id=8,lv=2.2000000000000002,face="b0046",head="asu_nob",path=":fg/asu/no/",x=380,mode=3,file="asu_nob0200"},{"fgact",ch="あすみ",id=8,lv=2.2000000000000002,act="縦揺れ"},{"text"},linkback="block_00057",delay={vl01={{"fg",ex05="asu_noa0000",size="no",ch="あすみ",id=8,lv=2.2000000000000002,face="a0046",head="asu_noa",path=":fg/asu/no/",x=380,mode=3,file="asu_noa0200"},{"fgact",ch="あすみ",id=8,lv=2.2000000000000002,act="縦揺れ"}}},line=411,text={ja={{"「那、那个。虽然我不能说请不要看，但是这个话题在这里很尴尬。或者说你们可能不知道的游戏实况观看，那个」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_00201"}}},linknext="block_00059"},block_00047={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",path=":bg/",sync=0,file="bg011a"},{"fg",ex05="ame_noa0010",size="no",ch="天梨",id=13,lv=2.2000000000000002,face="a0008",sync=0,head="ame_noa",path=":fg/ame/no/",mode=3,ex03="a0097",file="ame_noa0210"},{"extrans",time=500},{"fgact",ch="天梨",id=13,lv=2.2000000000000002,act="びっくり"},{"text"},linkback="block_00046",delay={vl02={{"fg",ex05="ame_noa0010",size="no",ch="天梨",id=13,lv=2.2000000000000002,face="a0034",head="ame_noa",path=":fg/ame/no/",mode=3,ex03="a0097",file="ame_noa0210"}},vl04={{"fg",ex05="ame_noa0000",size="no",ch="天梨",id=13,lv=2.2000000000000002,face="a0019",head="ame_noa",path=":fg/ame/no/",mode=3,file="ame_noa0200"}},vl03={{"fgact",ch="天梨",id=13,lv=2.2000000000000002,act="いいえ"}},vl01={{"fg",ex05="ame_noa0010",size="no",ch="天梨",id=13,lv=2.2000000000000002,face="a0020",head="ame_noa",path=":fg/ame/no/",mode=3,ex03="a0097",file="ame_noa0210"}}},line=326,text={ja={{"「哎呀。啊，那个，我并没有勉强自己哦，完全没有勉强自己。如果有排练或者其他的事情的话，当然会优先考虑那些」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_00791"}}},linknext="block_00048"},block_00018={{"fg",ex05="shi_z1b0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="b0028",head="shi_z1b",path=":fg/shi/z1/",x=280,mode=3,file="shi_z1b0210"},{"fgact",ch="詩桜",id=13,lv=2.2999999999999998,act="いいえ"},{"text"},line=128,text={ja={{"「我能感觉到……！」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01766"}}},linkback="block_00017",linknext="block_00019"},block_00098={{"fg",ex05="asu_noa0010",size="no",ch="あすみ",id=13,lv=2.2000000000000002,face="a0053",head="asu_noa",path=":fg/asu/no/",x=0,mode=3,file="asu_noa0210"},{"text"},linkback="block_00097",delay={vl01={{"fg",ex05="asu_noa0010",size="no",ch="あすみ",id=13,lv=2.2000000000000002,face="a0054",head="asu_noa",path=":fg/asu/no/",x=0,mode=3,file="asu_noa0210"}}},line=674,text={ja={{"「我也正好休息了一天，打算从下午开始作曲……我想做些编曲」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_00203"}}},linknext="block_00099"},block_00044={{"text"},line=299,text={ja={{"啊……应该由我这个会长来考虑这些事情的。大家虽然都很乐意参与制作，但并不意味着他们就不会感到疲劳……",{"rt2"}}}},linkback="block_00043",linknext="block_00045"},block_00008={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0054",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0210"},{"text"},linkback="block_00007",delay={vl02={{"fgact",ch="詩桜",id=13,lv=2.2999999999999998,act="いいえ"}},vl01={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0003",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0210"}},vl04={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0031",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0210"}},vl03={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0007",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0210"}},vl05={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0026",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0210"}}},line=64,text={ja={{"「我早就看好你是个有前途的男人了。我出两万……不，三万。我觉得这个价格不错……你怎么看？」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01763"}}},linknext="block_00009"},block_00043={{"fg",ex05="mir_z1a0000",size="z1",ch="里",id=13,lv=2.2999999999999998,face="a0031",head="mir_z1a",path=":fg/mir/z1/",mode=3,file="mir_z1a0200"},{"text"},linkback="block_00042",delay={vl01={{"fg",ex05="mir_z1a0000",size="z1",ch="里",id=13,lv=2.2999999999999998,face="a0054",head="mir_z1a",path=":fg/mir/z1/",mode=3,file="mir_z1a0200"}}},line=297,text={ja={{"「\"大家应该找个时间放松一下\"，我一直这么想着。所以昨天看到大家玩得很开心，我觉得很好」",{"rt2"},name={"里"}}},vo={{"vo",ch="mir",file="fem_mir_00208"}}},linknext="block_00044"},block_00014={{"fg",ex05="hiy_noa0010",size="no",ch="妃愛",id=20,lv=2.2000000000000002,face="a0008",head="hiy_noa",path=":fg/hiy/no/",mode=1,file="hiy_noa0210"},{"text"},linkback="block_00013",delay={vl01={{"fg",ex05="hiy_noa0010",size="no",ch="妃愛",id=20,lv=2.2000000000000002,face="a0006",head="hiy_noa",path=":fg/hiy/no/",mode=1,file="hiy_noa0210"}}},line=101,text={ja={{"「哇哦，那你可以说是我的最早期粉丝了。一直以来谢谢你的支持！」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01471"}}},linknext="block_00015"},block_00064={{"fg",mode=-2},{"bg",by=-360,bx=-640,x=640,y=100,speed=150000,ax=1280,ay=720,path=":bg/",x2=-640,y2=100,file="zbg011a"},{"text"},line=456,text={ja={{"「话虽如此，正如里姐所说，我们现在还处于长距离赛的中段，就像天梨说的，锦每天都在努力做广播，其他人也都有自己的工作……」",{"rt2"},name={"智宏"}}}},linkback="block_00063",linknext="block_00065"},block_00049={{"fg",ex05="mir_noa0000",size="no",ch="里",id=20,lv=2.2000000000000002,resize=1,face="a0007",head="mir_noa",path=":fg/mir/no/",mode=1,file="mir_noa0200"},{"text"},line=336,text={ja={{"「参加Gal Collection的人在这个时候会没有拍摄工作吗？说实话，你是不是拒绝了一些工作？」",{"rt2"},name={"里"}}},vo={{"vo",ch="mir",file="fem_mir_00210"}}},linkback="block_00048",linknext="block_00050"},block_00055={{"fg",ex05="asu_noa0000",size="no",ch="あすみ",id=8,lv=2.2000000000000002,face="a0055",head="asu_noa",path=":fg/asu/no/",x=380,mode=3,file="asu_noa0200"},{"fg",ex05="hiy_nob0000",size="no",ch="妃愛",id=10,lv=2.2000000000000002,face="b0053",head="hiy_nob",path=":fg/hiy/no/",x=0,mode=3,file="hiy_nob0200"},{"text"},linkback="block_00054",delay={vl01={{"fg",ex05="asu_noa0000",size="no",ch="あすみ",id=8,lv=2.2000000000000002,face="a0052",head="asu_noa",path=":fg/asu/no/",x=380,mode=3,file="asu_noa0200"}}},line=388,text={ja={{"「如果你正在准备重要的舞台，我可以替你做一些工作。随时向我求助哦」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_00200"}}},linknext="block_00056"},block_00096={{"text"},line=655,text={ja={{"「话说回来，我一个人去有点不放心……有，有谁能」",{"rt2"},name={"智宏"}}}},linkback="block_00095",linknext="block_00097"},label={top={label=1,block="block_00000"}},block_00093={{"text"},line=633,text={ja={{"哇哦！多亏了妃爱，拱门的问题似乎马上就能解决了！如果是装饰了法式花艺的拱门的话，莉莉子会长肯定会喜欢的。",{"rt2"}}}},linkback="block_00092",linknext="block_00094"},block_00112={{"bgm",stop=1},{"cgdel",id=-1},{"bg",rule="rule_001",vague=40,path=":bg/",time=1000,wait=250,file="black"},{"excall",file="05_天梨13_2"},{"text"},linkback="block_00111",line=770},block_00111={{"cgdel",id=-1},{"bg",rule="rule_001",vague=40,path=":bg/",time=1000,wait=250,file="black"},{"msgoff"},{"bg",path=":bg/",sync=0,file="bg030a"},{"extrans",rule="rule_001",vague=40},{"text"},line=763,text={ja={{"既然已经决定了要做什么，我们就转移到家政室，在上午专注于制作衣服。一旦开始工作，能够集中精力真的让人感到庆幸。",{"rt2"}}}},linkback="block_00110",linknext="block_00112"},block_00091={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=8,lv=2.2999999999999998,face="a0031",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0210"},{"fgact",ch="詩桜",id=8,lv=2.2999999999999998,act="うんうん"},{"text"},linkback="block_00090",delay={vl01={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=8,lv=2.2999999999999998,face="a0039",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0210"}}},line=625,text={ja={{"「哈哈，被妃爱你这么宠着感觉还挺好的。对了……我本来就打算花超过荧光棒定价的钱来买的。」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01779"}}},linknext="block_00092"},block_00109={{"fg",ex05="ame_z1a0000",size="z1",ch="天梨",id=9,lv=2.2999999999999998,face="a0008",head="ame_z1a",path=":fg/ame/z1/",mode=3,file="ame_z1a0200"},{"fgact",ch="天梨",id=9,lv=2.2999999999999998,act="びっくり"},{"text"},linkback="block_00108",delay={vl02={{"fg",ex05="ame_z1a0010",size="z1",ch="天梨",id=9,lv=2.2999999999999998,face="a0054",head="ame_z1a",path=":fg/ame/z1/",mode=3,ex03="a0097",file="ame_z1a0210"}},vl01={{"fg",ex05="ame_z1a0010",size="z1",ch="天梨",id=9,lv=2.2999999999999998,face="a0052",head="ame_z1a",path=":fg/ame/z1/",mode=3,ex03="a0097",file="ame_z1a0210"}}},line=752,text={ja={{"「哎！啊，是的我有空！我、如果我可以的话，我会陪你去购物！」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_00799"}}},linknext="block_00110"},block_00097={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",path=":bg/",sync=0,file="bg011a"},{"fg",ex05="hiy_nob0010",size="no",ch="妃愛",id=7,lv=2.2000000000000002,face="b0031",sync=0,head="hiy_nob",path=":fg/hiy/no/",x=-380,mode=3,file="hiy_nob0210"},{"fg",ex05="asu_noa0000",size="no",ch="あすみ",id=13,lv=2.2000000000000002,face="a0010",sync=0,head="asu_noa",path=":fg/asu/no/",x=0,mode=3,file="asu_noa0200"},{"fg",ex05="shi_noa0000",size="no",ch="詩桜",id=8,lv=2.2000000000000002,resize=1,face="a0001",sync=0,head="shi_noa",path=":fg/shi/no/",x=380,mode=3,file="shi_noa0200"},{"extrans",time=500},{"text"},line=668,text={ja={{"「我要工作哦」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01491"}}},linkback="block_00096",linknext="block_00098"},block_00002={{"text"},line=17,text={ja={{"但是，不能说他们只是为了准备文化祭才来上学的。",{"rt2"}}}},linkback="block_00001",linknext="block_00003"},block_00005={{"text"},line=40,text={ja={{"「嗯……其实我也没有特意去参加妃爱的演唱会或活动。因为我不喜欢去那种贵宾席，还要被人打招呼、寒暄什么的。」",{"rt2"},name={"智宏"}}}},linkback="block_00004",linknext="block_00006"},block_00103={{"text"},line=702,text={ja={{"「啊啊ー……即使你说我碍事，我还是要请你帮忙啊……」",{"rt2"},name={"智宏"}}}},linkback="block_00102",linknext="block_00104"},block_00004={{"fg",ex05="shi_z1a0000",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0018",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0200"},{"text"},linkback="block_00003",delay={vl01={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0007",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0210"}}},line=37,text={ja={{"「羞愧地说，直到去年我对于参加现场演唱会或活动都持消极态度。我非常想得到那些我当时没能购买的周边商品。我愿意出一万日元购买。」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01761"}}},linknext="block_00005"},block_00073={{"text"},line=501,text={ja={{"「啊，拱门……你想出了什么好主意吗？」",{"rt2"},name={"智宏"}}}},linkback="block_00072",linknext="block_00074"},block_00107={{"msgoff"},{"fgf",frame="frame_lt",sync=0,disp="消",mode=-1},{"fg",mode=-2,sync=0},{"bg",by=-360,sync=0,ax=1280,ay=720,path=":bg/",x=600,bx=-640,file="zbg011a"},{"fg",ex05="ame_z1a0000",size="z1",ch="天梨",id=9,lv=2.2999999999999998,face="a0020h",sync=0,head="ame_z1a",hoho="h",path=":fg/ame/z1/",mode=3,file="ame_z1a0200"},{"extrans",time=500},{"text"},line=739,text={ja={{"这份关心真的让人感动和高兴。但是，如果这么直白的话，还是会有点害羞，能不能稍微收敛一点。",{"rt2"}}}},linkback="block_00106",linknext="block_00108"},block_00056={{"fg",ex05="hiy_nob0010",size="no",ch="妃愛",id=10,lv=2.2000000000000002,face="b0054",head="hiy_nob",path=":fg/hiy/no/",x=0,mode=3,file="hiy_nob0210"},{"text"},linkback="block_00055",delay={vl02={{"fgact",ch="妃愛",id=10,lv=2.2000000000000002,act="うんうん"}},vl01={{"fg",ex05="hiy_nob0010",size="no",ch="妃愛",id=10,lv=2.2000000000000002,face="b0048",head="hiy_nob",path=":fg/hiy/no/",x=0,mode=3,file="hiy_nob0210"}}},line=396,text={ja={{"「这孩子还是一如既往的乖巧。不禁让人想要捉弄她一番，呵呵呵」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01481"}}},linknext="block_00057"},block_00038={{"text"},line=250,text={ja={{"当我送给妃爱她挑选的蝴蝶结发卡时，她给了这个无能的哥哥一个无比开心、让人感觉过于奢侈的笑容。真是个可爱至极的妹妹。",{"rt2"}}}},linkback="block_00037",linknext="block_00039"},block_00012={{"fg",ex05="kan_z1a0010",size="z1",ch="華乃",id=9,lv=2.2999999999999998,face="a0031",head="kan_z1a",path=":fg/kan/z1/",x=-280,mode=3,file="kan_z1a0210"},{"text"},linkback="block_00011",delay={vl01={{"fg",ex05="kan_z1a0010",size="z1",ch="華乃",id=9,lv=2.2999999999999998,face="a0025",head="kan_z1a",path=":fg/kan/z1/",x=-280,mode=3,file="kan_z1a0210"}}},line=91,text={ja={{"「嗯，我是从日和声优首次亮相的作品『青菜花』开始就关注她了……所以我不觉得自己是新粉丝哦？」",{"rt2"},name={"華乃"}}},vo={{"vo",ch="kan",file="fem_kan_00455"}}},linknext="block_00013"},block_00046={{"fg",ex05="kan_noa0000",size="no",ch="華乃",id=20,lv=2.2000000000000002,face="a0007",head="kan_noa",path=":fg/kan/no/",mode=1,file="kan_noa0200"},{"text"},line=310,text={ja={{"「啊。难道天梨你减少工作量，优先参加学生会活动了吗？」",{"rt2"},name={"華乃"}}},vo={{"vo",ch="kan",file="fem_kan_00459"}}},linkback="block_00045",linknext="block_00047"},block_00033={{"fg",ex05="shi_noa0000",size="no",ch="詩桜",id=20,lv=2.2000000000000002,face="a0043",head="shi_noa",path=":fg/shi/no/",mode=1,file="shi_noa0200"},{"text"},line=229,text={ja={{"「就连在妃爱的生日那天，你还让她做晚餐，你这个人啊」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01771"}}},linkback="block_00032",linknext="block_00034"},block_00027={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",by=-360,sync=0,ax=1280,ay=720,path=":bg/",x=200,bx=-640,file="zbg011a"},{"fg",ex05="hiy_z1b0010",size="z1",ch="妃愛",id=9,lv=2.2999999999999998,resize=1,face="b0054",sync=0,head="hiy_z1b",path=":fg/hiy/z1/",mode=3,file="hiy_z1b0210"},{"extrans",time=500},{"text"},linkback="block_00026",delay={vl01={{"fg",ex05="hiy_z1a0010",size="z1",ch="妃愛",id=9,lv=2.2999999999999998,face="a0006",head="hiy_z1a",path=":fg/hiy/z1/",mode=3,file="hiy_z1a0210"}}},line=202,text={ja={{"「不，不，我才应该感谢你们，昨天你们为我举办了如此美好的生日会。如果我的歌能有所回馈，那就太好了」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01474"}}},linknext="block_00028"},block_00066={{"text"},line=461,text={ja={{"这样妃爱也能感到一些责任减轻。我是一个非常疼爱妹妹的哥哥。",{"rt2"}}}},linkback="block_00065",linknext="block_00067"},block_00102={{"fg",ex05="shi_noa0010",size="no",ch="詩桜",id=8,lv=2.2000000000000002,face="a0039",head="shi_noa",path=":fg/shi/no/",x=480,mode=3,file="shi_noa0210"},{"text"},linkback="block_00101",delay={vl02={{"fg",ex05="shi_noa0010",size="no",ch="詩桜",id=8,lv=2.2000000000000002,face="a0001",head="shi_noa",path=":fg/shi/no/",x=480,mode=3,file="shi_noa0210"}},vl01={{"fg",ex05="shi_noa0010",size="no",ch="詩桜",id=8,lv=2.2000000000000002,face="a0032",head="shi_noa",path=":fg/shi/no/",x=480,mode=3,file="shi_noa0210"}}},line=699,text={ja={{"「首先要得到她的同意才能帮忙。而且我还想和她聊聊闲话。你会碍事的」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01781"}}},linknext="block_00103"},block_00101={{"fg",ex05="hiy_nob0010",size="no",ch="妃愛",id=7,lv=2.2000000000000002,face="b0010",head="hiy_nob",path=":fg/hiy/no/",x=-160,mode=3,file="hiy_nob0210"},{"fg",ex05="asu_noa0010",size="no",ch="あすみ",id=13,lv=2.2000000000000002,face="a0010",head="asu_noa",path=":fg/asu/no/",x=160,mode=3,file="asu_noa0210"},{"text"},line=692,text={ja={{"「动作，好快。那么渡月学姐也请她过来吧？」",{"rt2"},name={"智宏"}}}},linkback="block_00100",linknext="block_00102"},block_00099={{"text"},line=677,text={ja={{"「话说回来，如果是诗樱学姐负责的话，让学姐陪同去不是最好的吗？」",{"rt2"},name={"智宏"}}}},linkback="block_00098",linknext="block_00100"},block_00061={{"fg",ex05="ame_z1b0010",size="z1",ch="天梨",id=13,lv=2.2999999999999998,face="b0006",head="ame_z1b",path=":fg/ame/z1/",mode=3,file="ame_z1b0210"},{"text"},linkback="block_00060",delay={vl02={{"fgact",ch="天梨",id=13,lv=2.2999999999999998,act="うなづく"}},vl01={{"fg",ex05="ame_z1b0000",size="z1",ch="天梨",id=13,lv=2.2999999999999998,face="b0054",head="ame_z1b",path=":fg/ame/z1/",mode=3,file="ame_z1b0200"}}},line=439,text={ja={{"「只有三次机会的大马学院文化节也非常重要！今年似乎能留下特别美好的回忆，既然已经决定要「做」，就不会犹豫！对！」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_00796"}}},linknext="block_00062"},block_00094={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",path=":bg/",sync=0,file="bg011a"},{"extrans",time=500},{"bg",time=400,my=40,path=":cg/",id=1,lv=5,file="cutin_021"},{"fg",ex05="hiy_nob0010",size="no",ch="妃愛",id=20,lv=2.2000000000000002,face="b0052",head="hiy_nob",path=":fg/hiy/no/",mode=1,file="hiy_nob0210"},{"text"},line=649,text={ja={{"「所以说，我希望哥哥你能去买拱门的主体部分。诗樱学姐看起来马上就会开始行动，所以最好是今天就……」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01490"}}},linkback="block_00093",linknext="block_00095"},block_00034={{"text"},line=232,text={ja={{"「因为在妃爱的生日那天，我想让她吃到好吃的东西啊。我所知道的料理中，妃爱做的晚餐是最好吃的」",{"rt2"},name={"智宏"}}}},linkback="block_00033",linknext="block_00035"},block_00092={{"fg",ex05="shi_z1b0010",size="z1",ch="詩桜",id=8,lv=2.2999999999999998,face="b0006",head="shi_z1b",path=":fg/shi/z1/",mode=3,file="shi_z1b0210"},{"text"},linkback="block_00091",delay={vl01={{"fg",ex05="shi_z1b0000",size="z1",ch="詩桜",id=8,lv=2.2999999999999998,face="b0031",head="shi_z1b",path=":fg/shi/z1/",mode=3,file="shi_z1b0200"}}},line=631,text={ja={{"「我会和渡月她们商量，如果制作满意的花朵需要的费用不够，我可以稍微补一些。但是不会超过荧光棒的市场价。」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01780"}}},linknext="block_00093"},block_00110={{"text"},line=754,text={ja={{"天梨的回答也有些生硬。听起来不像是害羞，而更像是在尽力避免让学生会工作带出恋爱色彩。",{"rt2"}}}},linkback="block_00109",linknext="block_00111"},block_00011={{"fg",ex05="shi_z1a0000",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0046",head="shi_z1a",path=":fg/shi/z1/",x=280,mode=3,file="shi_z1a0200"},{"fg",ex05="kan_z1a0010",size="z1",ch="華乃",id=9,lv=2.2999999999999998,face="a0006",head="kan_z1a",path=":fg/kan/z1/",x=-280,mode=3,file="kan_z1a0210"},{"fgact",ch="詩桜",id=13,lv=2.2999999999999998,act="びっくり"},{"se",id=1,file="seコミ_察する"},{"text"},linkback="block_00010",delay={vl02={{"fg",ex05="shi_z1b0000",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="b0043",head="shi_z1b",path=":fg/shi/z1/",x=280,mode=3,file="shi_z1b0200"}},vl01={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0031",head="shi_z1a",path=":fg/shi/z1/",x=280,mode=3,file="shi_z1a0210"}}},line=85,text={ja={{"「等、等等，那正是，日和色的应援棒……！那么华乃，难道你当天真的在现场……！？」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01764"}}},linknext="block_00012"},block_00089={{"fg",ex05="shi_z1b0010",size="z1",ch="詩桜",id=8,lv=2.2999999999999998,face="b0001",head="shi_z1b",path=":fg/shi/z1/",mode=3,file="shi_z1b0210"},{"text"},linkback="block_00088",delay={vl01={{"fg",ex05="shi_z1b0010",size="z1",ch="詩桜",id=8,lv=2.2999999999999998,face="b0053",head="shi_z1b",path=":fg/shi/z1/",mode=3,file="shi_z1b0210"}}},line=613,text={ja={{"「在那件事上，我也给你们带来了麻烦。如果能回报你们，我愿意帮忙制作拱门。」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01778"}}},linknext="block_00090"},block_00088={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=8,lv=2.2999999999999998,face="a0053",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0210"},{"text"},linkback="block_00087",delay={vl01={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=8,lv=2.2999999999999998,face="a0031",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0210"}}},line=607,text={ja={{"「关于花展的事情，我还一直在受到他们的感谢。我可以去和部长渡月她们谈谈。她们自己可能也有活动，但我们先去问问看吧。」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01777"}}},linknext="block_00089"},block_00085={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",by=-360,sync=0,ax=1280,ay=720,path=":bg/",x=400,bx=-640,file="zbg011a"},{"fg",ex05="ame_z1a0000",size="z1",ch="天梨",id=9,lv=2.2999999999999998,face="a0001",sync=0,head="ame_z1a",path=":fg/ame/z1/",x=-280,mode=3,file="ame_z1a0200"},{"fg",ex05="hiy_z1a0000",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="a0034",sync=0,head="hiy_z1a",path=":fg/hiy/z1/",x=280,mode=3,file="hiy_z1a0200"},{"extrans",time=500},{"text"},linkback="block_00084",delay={vl01={{"fg",ex05="hiy_z1a0000",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="a0006",head="hiy_z1a",path=":fg/hiy/z1/",x=280,mode=3,file="hiy_z1a0200"}}},line=582,text={ja={{"「虽然买花可能要稍微超出预算一点，但是在装饰方面，我们不是有个可靠的人脉吗？」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01488"}}},linknext="block_00086"},block_00084={{"text"},line=569,text={ja={{"「她什么都能做得到啊。」",{"rt2"},name={"智宏"}}}},linkback="block_00083",linknext="block_00085"},block_00083={{"fg",ex05="shi_nob0010",size="no",ch="詩桜",id=8,lv=2.2000000000000002,face="b0031",head="shi_nob",path=":fg/shi/no/",x=380,mode=3,file="shi_nob0210"},{"text"},linkback="block_00082",delay={vl01={{"fg",ex05="shi_nob0010",size="no",ch="詩桜",id=8,lv=2.2000000000000002,face="b0025",head="shi_nob",path=":fg/shi/no/",x=380,mode=3,file="shi_nob0210"}}},line=566,text={ja={{"「她也喜欢园艺，甚至参加了前不久从荷兰来日的世界级园林设计师主持的种植活动。她的眼光肯定是很高的。」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01775"}}},linknext="block_00084"},block_00081={{"fg",ex05="asu_noa0000",size="no",ch="あすみ",id=13,lv=2.2000000000000002,face="a0002",head="asu_noa",path=":fg/asu/no/",x=0,mode=3,file="asu_noa0200"},{"text"},linkback="block_00080",delay={vl01={{"fg",ex05="asu_nob0000",size="no",ch="あすみ",id=13,lv=2.2000000000000002,face="b0002",head="asu_nob",path=":fg/asu/no/",x=0,mode=3,file="asu_nob0200"}}},line=554,text={ja={{"「即使是假花，花的部分似乎也会很贵……而且装饰这个工作谁来做？如果只是随便装饰一下，不但无法讨好莉莉子会长，反而可能会惹她生气……」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_00202"}}},linknext="block_00082"},block_00031={{"fg",ex05="asu_noa0000",size="no",ch="あすみ",id=20,lv=2.2000000000000002,face="a0001",head="asu_noa",path=":fg/asu/no/",mode=1,file="asu_noa0200"},{"text"},line=218,text={ja={{"「工作现场没有人为你庆祝吗？」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_00197"}}},linkback="block_00030",linknext="block_00032"},block_00080={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",path=":bg/",sync=0,file="bg011a"},{"fg",ex05="asu_nob0010",size="no",ch="あすみ",id=13,lv=2.2000000000000002,face="b0001",sync=0,head="asu_nob",path=":fg/asu/no/",x=0,mode=3,file="asu_nob0210"},{"fg",ex05="kan_noa0000",size="no",ch="華乃",id=7,lv=2.2000000000000002,face="a0054",sync=0,head="kan_noa",path=":fg/kan/no/",x=-380,mode=3,file="kan_noa0200"},{"fg",ex05="shi_noa0010",size="no",ch="詩桜",id=8,lv=2.2000000000000002,face="a0001",sync=0,head="shi_noa",path=":fg/shi/no/",x=380,mode=3,file="shi_noa0210"},{"extrans",time=500},{"fgact",ch="華乃",id=7,lv=2.2000000000000002,act="うんうん"},{"text"},linkback="block_00079",delay={vl02={{"fg",ex05="kan_noa0000",size="no",ch="華乃",id=7,lv=2.2000000000000002,face="a0010",head="kan_noa",path=":fg/kan/no/",x=-380,mode=3,file="kan_noa0200"}},vl01={{"fg",ex05="kan_noa0000",size="no",ch="華乃",id=7,lv=2.2000000000000002,face="a0038",head="kan_noa",path=":fg/kan/no/",x=-380,mode=3,file="kan_noa0200"}}},line=548,text={ja={{"「哈哈哈，确实有法国的味道。但是这个不会很贵吗……啊，2.5米的价格是1万日元。这个还能接受。学生会的预算应该能应付得来。」",{"rt2"},name={"華乃"}}},vo={{"vo",ch="kan",file="fem_kan_00461"}}},linknext="block_00081"},block_00045={{"fg",ex05="mir_z1a0010",size="z1",ch="里",id=13,lv=2.2999999999999998,face="a0001",head="mir_z1a",path=":fg/mir/z1/",mode=3,file="mir_z1a0210"},{"text"},linkback="block_00044",delay={vl01={{"fg",ex05="mir_z1a0010",size="z1",ch="里",id=13,lv=2.2999999999999998,face="a0009",head="mir_z1a",path=":fg/mir/z1/",mode=3,file="mir_z1a0210"}}},line=305,text={ja={{"「天梨你也是啊，下个月就要参加Gal Collection了吧？你已经开始排练了吗？每天参加学生会活动真的没问题吗？」",{"rt2"},name={"里"}}},vo={{"vo",ch="mir",file="fem_mir_00209"}}},linknext="block_00046"},block_00079={{"text"},line=530,text={ja={{"妃爱手机屏幕上显示的是很多半圆形骨架上缠绕着蔷薇的拱门。",{"rt2"}}}},linkback="block_00078",linknext="block_00080"},block_00071={{"fg",ex05="kan_nob0000",size="no",ch="華乃",id=20,lv=2.2000000000000002,face="b0019",head="kan_nob",path=":fg/kan/no/",mode=1,file="kan_nob0200"},{"text"},line=491,text={ja={{"「看到妃爱那满脸幸福的表情更让人。这对兄妹真是关系好啊」",{"rt2"},name={"華乃"}}},vo={{"vo",ch="kan",file="fem_kan_00460"}}},linkback="block_00070",linknext="block_00072"},block_00076={{"fg",ex05="hiy_z1b0000",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="b0002",head="hiy_z1b",path=":fg/hiy/z1/",mode=3,file="hiy_z1b0200"},{"text"},linkback="block_00075",delay={vl01={{"fg",ex05="hiy_z1b0000",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="b0053",head="hiy_z1b",path=":fg/hiy/z1/",mode=3,file="hiy_z1b0200"}}},line=511,text={ja={{"「现在用纸箱做门显然不现实，所以我想买现成的产品进行改造，关键是如何把它做得像模像样。」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01485"}}},linknext="block_00077"},block_00108={{"fg",ex05="mir_noa0000",size="no",ch="里",id=20,lv=2.2000000000000002,face="a0052",head="mir_noa",path=":fg/mir/no/",mode=1,file="mir_noa0200"},{"text"},line=744,text={ja={{"「天梨呢？你接下来有空吗？」",{"rt2"},name={"里"}}},vo={{"vo",ch="mir",file="fem_mir_00213"}}},linkback="block_00107",linknext="block_00109"},block_00036={{"text"},line=246,text={ja={{"直到去年为止，妃爱自己都会说「能得到哥哥的祝福是最开心的」。按照妃爱的愿望，在我们家里两个人一起庆祝过去了。",{"rt2"}}}},linkback="block_00035",linknext="block_00037"},block_00035={{"fg",ex05="hiy_z1b0010",size="z1",ch="妃愛",id=9,lv=2.2999999999999998,face="b0048",head="hiy_z1b",path=":fg/hiy/z1/",mode=3,file="hiy_z1b0210"},{"fgact",ch="妃愛",id=9,lv=2.2999999999999998,act="いいえ"},{"se",file="seコミ_目を輝かせる",id=1},{"text"},line=239,text={ja={{"「我的心跳加速了！」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01477"}}},linkback="block_00034",linknext="block_00036"},block_00007={{"text"},line=54,text={ja={{"「如果只是周边商品的话，我们家空着的房间里堆满了这些东西，因为妃爱都会把它们带回家。」",{"rt2"},name={"智宏"}}}},linkback="block_00006",linknext="block_00008"},block_00040={{"fg",ex05="hiy_nob0000",size="no",ch="妃愛",id=9,lv=2.2000000000000002,face="b0002",head="hiy_nob",path=":fg/hiy/no/",x=-230,mode=3,file="hiy_nob0200"},{"fgact",ch="妃愛",id=9,lv=2.2000000000000002,act="いいえ"},{"text"},linkback="block_00039",delay={vl02={{"fg",ex05="hiy_noa0000",size="no",ch="妃愛",id=9,lv=2.2000000000000002,face="a0029",head="hiy_noa",path=":fg/hiy/no/",x=-230,mode=3,file="hiy_noa0200"}},vl01={{"fg",ex05="hiy_noa0000",size="no",ch="妃愛",id=9,lv=2.2000000000000002,face="a0038",head="hiy_noa",path=":fg/hiy/no/",x=-230,mode=3,file="hiy_noa0200"}}},line=273,text={ja={{"「我才是，对不起，我根本没怎么帮上生徒会的忙。尽管如此，我却一直在享受美食。真是太不像话了」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01478"}}},linknext="block_00041"},block_00078={{"fg",ex05="hiy_z1a0010",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="a0052",head="hiy_z1a",path=":fg/hiy/z1/",mode=3,file="hiy_z1a0210"},{"text"},linkback="block_00077",delay={vl02={{"fg",ex05="hiy_z1b0010",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="b0052",head="hiy_z1b",path=":fg/hiy/z1/",mode=3,file="hiy_z1b0210"}},vl01={{"fg",ex05="hiy_z1a0010",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="a0027",head="hiy_z1a",path=":fg/hiy/z1/",mode=3,file="hiy_z1a0210"}}},line=527,text={ja={{"「我们学校的环境非常好，那个，我们可以准备一座看起来像是在海外花园里，蔷薇缠绕的拱门，你觉得怎么样呢？」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01487"}}},linknext="block_00079"},block_00026={{"fg",ex05="kan_noa0000",size="no",ch="華乃",id=20,lv=2.2000000000000002,resize=1,face="a0008",head="kan_noa",path=":fg/kan/no/",mode=1,file="kan_noa0200"},{"text"},line=189,text={ja={{"「话说回来，虽然是在卡拉OK，但希优铃能为我们献上自己的歌曲，冷静想想真是太不可思议了……」",{"rt2"},name={"華乃"}}},vo={{"vo",ch="kan",file="fem_kan_00458"}}},linkback="block_00025",linknext="block_00027"},block_00069={{"se",file="seコミ_転ぶ",id=1},{"text"},line=481,text={ja={{"「刚刚由会长亲自宣布下午休息，接着这位副会长就……！」",{"rt2"},name={"智宏"}}}},linkback="block_00068",linknext="block_00070"},block_00068={{"msgoff"},{"bg",by=-360,sync=0,ax=1280,ay=720,path=":bg/",x=200,bx=-640,file="zbg011a"},{"fg",ex05="hiy_z1a0000",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,resize=1,face="a0008",sync=0,head="hiy_z1a",path=":fg/hiy/z1/",mode=3,file="hiy_z1a0200"},{"extrans"},{"text"},linkback="block_00067",delay={vl02={{"fg",ex05="hiy_z1b0010",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="b0054",head="hiy_z1b",path=":fg/hiy/z1/",mode=3,file="hiy_z1b0210"}},vl01={{"fg",ex05="hiy_z1a0000",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="a0053",head="hiy_z1a",path=":fg/hiy/z1/",mode=3,file="hiy_z1a0200"}}},line=476,text={ja={{"「啊，对了哥哥。我接下来要去工作，可以麻烦你下午去买点东西吗？当然是和学生会有关的」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01483"}}},linknext="block_00069"},block_00003={{"msgoff"},{"bg",by=-360,sync=0,ax=1280,ay=720,path=":bg/",x=-600,bx=-640,file="zbg011a"},{"fg",ex05="shi_z1b0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,resize=1,face="b0003",sync=0,head="shi_z1b",path=":fg/shi/z1/",mode=3,file="shi_z1b0210"},{"extrans"},{"bgm",file="bgm28"},{"text"},linkback="block_00002",delay={vl02={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0006",head="shi_z1a",path=":fg/shi/z1/",mode=3,file="shi_z1a0210"}},vl01={{"fg",ex05="shi_z1b0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="b0031",head="shi_z1b",path=":fg/shi/z1/",mode=3,file="shi_z1b0210"}}},line=31,text={ja={{"「智宏君，我有个重要的事情要商量。我想要去年的『ウィンドル！』全国巡演的应援棒。当然，我希望的颜色是黄色。你应该有吧？」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01760"}}},linknext="block_00004"},block_00060={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",by=-360,sync=0,ax=1280,ay=720,path=":bg/",x=600,bx=-640,file="zbg011a"},{"fg",ex05="ame_z1a0000",size="z1",ch="天梨",id=13,lv=2.2999999999999998,resize=1,face="a0053",sync=0,head="ame_z1a",path=":fg/ame/z1/",mode=3,file="ame_z1a0200"},{"extrans",time=500},{"text"},linkback="block_00059",delay={vl02={{"fg",ex05="ame_z1a0000",size="z1",ch="天梨",id=13,lv=2.2999999999999998,face="a0019",head="ame_z1a",path=":fg/ame/z1/",mode=3,file="ame_z1a0200"}},vl01={{"fg",ex05="ame_z1a0000",size="z1",ch="天梨",id=13,lv=2.2999999999999998,face="a0002",head="ame_z1a",path=":fg/ame/z1/",mode=3,file="ame_z1a0200"}}},line=432,text={ja={{"「而且，我今年决定要努力参加文化节。我已经和公司商量好了日程安排，现在不能说「其实我……」了」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_00795"}}},linknext="block_00061"},block_00013={{"text"},line=93,text={ja={{"华乃之所以了不起，是因为她对自己口中称赞的东西有着足够多的知识，即使面对狂热的粉丝也能轻松应对，并且真心实意地支持。",{"rt2"}}}},linkback="block_00012",linknext="block_00014"},block_00029={{"text"},line=206,text={ja={{"新川昨天和今天都在打工，只留下完成的模板就没参加活动。虽然感觉我们关系变得更好了一些，但他的打工内容至今还没告诉我。",{"rt2"}}}},linkback="block_00028",linknext="block_00030"},block_00062={{"fg",ex05="ame_z1a0010",size="z1",ch="天梨",id=13,lv=2.2999999999999998,face="a0052",head="ame_z1a",path=":fg/ame/z1/",mode=3,file="ame_z1a0210"},{"text"},line=444,text={ja={{"「啊，但是但是，下个月的Gal Collection我会邀请大家来看的，如果有时间的话，请来看看哦！」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_00797"}}},linkback="block_00061",linknext="block_00063"},block_00067={{"text"},line=464,text={ja={{"「今天我们不需要带走正在处理的部件，学生会活动暂时休息。为了顺利进行工作，从明天开始再加油吧！」",{"rt2"},name={"智宏"}}}},linkback="block_00066",linknext="block_00068"},block_00105={{"fgf",mode=3,bg="zbg011a",by=-250,bx=-250,frame="frame_lt",ch="天梨",disp="出右",bgpx=":bg/"},{"fg",ex05="ame_z1a0000",size="z1",ch="天梨",id=21,lv=2.2999999999999998,face="a0001",head="ame_z1a",path=":fg/ame/z1/",x=-130,mode=3,file="ame_z1a0200"},{"fg",ex05="kan_z1b0000",size="z1",ch="華乃",id=7,lv=2.2999999999999998,face="b0001",head="kan_z1b",path=":fg/kan/z1/",mode=3,file="kan_z1b0200"},{"text"},line=718,text={ja={{"说到这里，常磐突然注意到了什么，向右看去，和坐在她旁边的座位上的锦，以及锦旁边的人对上了眼。",{"rt2"}}}},linkback="block_00104",linknext="block_00106"},block_00063={{"fg",ex05="mir_noa0000",size="no",ch="里",id=20,lv=2.2000000000000002,face="a0054",head="mir_noa",path=":fg/mir/no/",mode=1,file="mir_noa0200"},{"text"},line=449,text={ja={{"「嘛，既然你已经有了明确的决心，我们外人就不必多说什么了。无论是学校还是工作都要加油哦！」",{"rt2"},name={"里"}}},vo={{"vo",ch="mir",file="fem_mir_00211"}}},linkback="block_00062",linknext="block_00064"},block_00095={{"text"},line=652,text={ja={{"「啊？这种东西在哪里能买到？去号泣汉兹就能找到吗？」",{"rt2"},name={"智宏"}}}},linkback="block_00094",linknext="block_00096"},block_00020={{"fg",ex05="shi_noa0000",size="no",ch="詩桜",id=10,lv=2.2000000000000002,face="a0051",head="shi_noa",path=":fg/shi/no/",x=230,mode=3,file="shi_noa0200"},{"fgact",ch="詩桜",id=10,lv=2.2000000000000002,act="おじぎ"},{"se",id=1,file="seコミ_目を輝かせる"},{"text"},linkback="block_00019",delay={vl02={{"fg",ex05="shi_noa0010",size="no",ch="詩桜",id=10,lv=2.2000000000000002,face="a0007",head="shi_noa",path=":fg/shi/no/",x=230,mode=3,file="shi_noa0210"}},vl01={{"fg",ex05="shi_noa0000",size="no",ch="詩桜",id=10,lv=2.2000000000000002,face="a0031",head="shi_noa",path=":fg/shi/no/",x=230,mode=3,file="shi_noa0200"}}},line=152,text={ja={{"「谢谢你……谢谢你，妃爱……这如同圣母般的爱……你才是真正的『表演者』……！」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01767"}}},linknext="block_00021"},block_00001={{"text"},line=15,text={ja={{"尽管昨天和今天是周六和周日，但全体学生会成员都来上学了。",{"rt2"}}}},linkback="block_00000",linknext="block_00002"},block_00053={{"fg",ex05="ame_noa0010",size="no",ch="天梨",id=13,lv=2.2000000000000002,face="a0002",head="ame_noa",mode=3,path=":fg/ame/no/",x=-230,ex03="a0097",file="ame_noa0210"},{"fg",ex05="hiy_noa0000",size="no",ch="妃愛",id=10,lv=2.2000000000000002,face="a0003",head="hiy_noa",path=":fg/hiy/no/",x=230,mode=3,file="hiy_noa0200"},{"text"},linkback="block_00052",delay={vl01={{"fg",ex05="hiy_noa0000",size="no",ch="妃愛",id=10,lv=2.2000000000000002,face="a0028",head="hiy_noa",path=":fg/hiy/no/",x=230,mode=3,file="hiy_noa0200"}}},line=368,text={ja={{"「如果你开始节省工作，我也会觉得难以优先处理自己的工作」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01480"}}},linknext="block_00054"},block_00028={{"text"},line=204,text={ja={{"昨天我们在学生会室为妃爱举办了生日会。回家的路上，里姐请我们吃饭，并带我们去唱卡拉OK。",{"rt2"}}}},linkback="block_00027",linknext="block_00029"},block_00057={{"fg",ex05="ame_noa0000",size="no",ch="天梨",id=13,lv=2.2000000000000002,face="a0047",head="ame_noa",path=":fg/ame/no/",x=-380,mode=3,file="ame_noa0200"},{"text"},linkback="block_00056",delay={vl02={{"fg",ex05="ame_noa0010",size="no",ch="天梨",id=13,lv=2.2000000000000002,face="a0009",head="ame_noa",mode=3,path=":fg/ame/no/",x=-380,ex03="a0097",file="ame_noa0210"}},vl01={{"fg",ex05="ame_noa0010",size="no",ch="天梨",id=13,lv=2.2000000000000002,face="a0012",head="ame_noa",mode=3,path=":fg/ame/no/",x=-380,ex03="a0097",file="ame_noa0210"}}},line=403,text={ja={{"「亚、亚澄酱！但是亚澄酱你也是，每天都在忙于广播工作，还参与了制作，我不能依赖你啊！就像昨天的广播那样——」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_00794"}}},linknext="block_00058"},block_00052={{"fg",ex05="ame_noa0010",ch="天梨",id=13,lv=2.2000000000000002,face="a0035",x=-230,head="ame_noa",mode=3,path=":fg/ame/no/",x2=0,ex03="a0097",file="ame_noa0210"},{"fg",ex05="hiy_noa0000",size="no",ch="妃愛",id=10,lv=2.2000000000000002,face="a0010",head="hiy_noa",mode=3,path=":fg/hiy/no/",x=230,mx=40,file="hiy_noa0200"},{"text"},linkback="block_00051",delay={vl01={{"fg",ex05="hiy_noa0000",size="no",ch="妃愛",id=10,lv=2.2000000000000002,face="a0002",head="hiy_noa",path=":fg/hiy/no/",x=230,mode=3,file="hiy_noa0200"}}},line=360,text={ja={{"「但是，正值有工作机会的时候却拒绝，这对你的未来真的有好处吗？」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01479"}}},linknext="block_00053"},block_00048={{"fg",ex05="shi_noa0010",size="no",ch="詩桜",id=20,lv=2.2000000000000002,face="a0031",head="shi_noa",path=":fg/shi/no/",mode=1,file="shi_noa0210"},{"text"},line=331,text={ja={{"「哦。听起来你好像在说，除了排练以外的工作，你都把这里优先考虑了呢」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01772"}}},linkback="block_00047",linknext="block_00049"},block_00051={{"fg",ex05="ame_noa0000",size="no",ch="天梨",id=13,lv=2.2000000000000002,face="a0052",head="ame_noa",path=":fg/ame/no/",mode=3,file="ame_noa0200"},{"fgact",ch="天梨",id=13,lv=2.2000000000000002,act="うんうん"},{"text"},linkback="block_00050",delay={vl02={{"fg",ex05="ame_noa0010",size="no",ch="天梨",id=13,lv=2.2000000000000002,face="a0035",head="ame_noa",path=":fg/ame/no/",mode=3,ex03="a0097",file="ame_noa0210"},{"fgact",ch="天梨",id=13,lv=2.2000000000000002,act="おじぎ"}},vl01={{"fg",ex05="ame_noa0010",size="no",ch="天梨",id=13,lv=2.2000000000000002,face="a0041",head="ame_noa",path=":fg/ame/no/",mode=3,ex03="a0097",file="ame_noa0210"}}},line=352,text={ja={{"「而且，我在社交媒体上也说过我正在努力准备文化节，反响也很好！ 把精力投入到文化节也算是工作的一部分，哈哈哈……」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_00793"}}},linknext="block_00052"},block_00016={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0029",head="shi_z1a",path=":fg/shi/z1/",x=280,mode=3,file="shi_z1a0210"},{"text"},linkback="block_00015",delay={vl01={{"fg",ex05="shi_z1a0010",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="a0037",head="shi_z1a",path=":fg/shi/z1/",x=280,mode=3,file="shi_z1a0210"},{"fgact",ch="詩桜",id=13,lv=2.2999999999999998,act="いいえ"}}},line=117,text={ja={{"「呜……无论我怎么深爱她，无论我怎么为她付出，我都无法融入到彼此两人所构筑的历史中……真是让人懊恼……！」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01765"}}},linknext="block_00017"},block_00042={{"fg",ex05="mir_z1a0010",size="z1",ch="里",id=13,lv=2.2999999999999998,face="a0039",head="mir_z1a",path=":fg/mir/z1/",mode=3,file="mir_z1a0210"},{"text"},linkback="block_00041",delay={vl01={{"fg",ex05="mir_z1a0010",size="z1",ch="里",id=13,lv=2.2999999999999998,face="a0003",head="mir_z1a",path=":fg/mir/z1/",mode=3,file="mir_z1a0210"}}},line=291,text={ja={{"「我明白时间紧迫，而且我也喜欢看大家为了一个目标齐心协力的样子，所以我一直在支持你们。但这次不是短距离赛跑，而是马拉松啊？」",{"rt2"},name={"里"}}},vo={{"vo",ch="mir",file="fem_mir_00207"}}},linknext="block_00043"},block_00017={{"fg",ex05="kan_z1a0000",size="z1",ch="華乃",id=9,lv=2.2999999999999998,face="a0025",head="kan_z1a",path=":fg/kan/z1/",x=-280,mode=3,file="kan_z1a0200"},{"text"},line=122,text={ja={{"「但是呢？」",{"rt2"},name={"華乃"}}},vo={{"vo",ch="kan",file="fem_kan_00457"}}},linkback="block_00016",linknext="block_00018"},block_00009={{"text"},line=68,text={ja={{"「就算是大量的，但这是妃爱送给我的……我个人无法做出买卖的决定。请直接向妃爱求购吧」",{"rt2"},name={"智宏"}}}},linkback="block_00008",linknext="block_00010"},block_00039={{"msgoff"},{"bg",path=":bg/",sync=0,file="bg011a"},{"fg",ex05="hiy_nob0010",size="no",ch="妃愛",id=9,lv=2.2000000000000002,resize=1,face="b0048",sync=0,head="hiy_nob",path=":fg/hiy/no/",x=-230,mode=3,file="hiy_nob0210"},{"fg",ex05="asu_noa0000",size="no",ch="あすみ",id=10,lv=2.2000000000000002,face="a0023",sync=0,head="asu_noa",path=":fg/asu/no/",x=230,mode=3,file="asu_noa0200"},{"extrans",time=500},{"text"},linkback="block_00038",delay={vl02={{"fg",ex05="asu_noa0000",size="no",ch="あすみ",id=10,lv=2.2000000000000002,face="a0052",head="asu_noa",path=":fg/asu/no/",x=230,mode=3,file="asu_noa0200"}},vl01={{"fg",ex05="asu_noa0000",size="no",ch="あすみ",id=10,lv=2.2000000000000002,face="a0055",head="asu_noa",path=":fg/asu/no/",x=230,mode=3,file="asu_noa0200"}}},line=265,text={ja={{"「谢谢你总是抽出时间为我们大家。尽管今天上午你还在工作。签名会，辛苦你了」",{"rt2"},name={"あすみ"}}},vo={{"vo",ch="asu",file="fem_asu_00198"}}},linknext="block_00040"},block_00106={{"fg",ex05="kan_z1b0010",size="z1",ch="華乃",id=7,lv=2.2999999999999998,face="b0035",head="kan_z1b",path=":fg/kan/z1/",mode=3,file="kan_z1b0210"},{"fgact",ch="華乃",id=7,lv=2.2999999999999998,act="びっくり"},{"se",file="seコミ_察する",id=1},{"text"},linkback="block_00105",delay={vl01={{"fg",ex05="kan_z1b0010",size="z1",ch="華乃",id=7,lv=2.2999999999999998,face="b0031",head="kan_z1b",path=":fg/kan/z1/",mode=3,file="kan_z1b0210"}}},line=727,text={ja={{"「――对了，我想起来要写立绘衣装差异的订单。既然有空闲时间，我也要工作！」",{"rt2"},name={"華乃"}}},vo={{"vo",ch="kan",file="fem_kan_00463"}}},linknext="block_00107"},block_00030={{"fg",ex05="hiy_z1a0000",size="z1",ch="妃愛",id=9,lv=2.2999999999999998,face="a0021",head="hiy_z1a",path=":fg/hiy/z1/",mode=3,file="hiy_z1a0200"},{"text"},linkback="block_00029",delay={vl02={{"fgact",ch="妃愛",id=9,lv=2.2999999999999998,act="いいえ"}},vl01={{"fg",ex05="hiy_z1a0000",size="z1",ch="妃愛",id=9,lv=2.2999999999999998,face="a0049",head="hiy_z1a",path=":fg/hiy/z1/",mode=3,file="hiy_z1a0200"}}},line=213,text={ja={{"「和朋友们一起过生日已经有好多年没经历过了……感动得我眼泪都快流出来了……唔」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01475"}}},linknext="block_00031"},block_00015={{"fg",ex05="kan_z1b0000",size="z1",ch="華乃",id=9,lv=2.2999999999999998,face="b0046h",head="kan_z1b",hoho="h",path=":fg/kan/z1/",x=-280,mode=3,file="kan_z1b0200"},{"fg",ex05="shi_z1b0000",size="z1",ch="詩桜",id=13,lv=2.2999999999999998,face="b0045",head="shi_z1b",path=":fg/shi/z1/",x=280,mode=3,file="shi_z1b0200"},{"fgact",ch="華乃",id=9,lv=2.2999999999999998,act="びっくり"},{"text"},linkback="block_00014",delay={vl01={{"fg",ex05="kan_z1b0000",size="z1",ch="華乃",id=9,lv=2.2999999999999998,face="b0049h",head="kan_z1b",hoho="h",path=":fg/kan/z1/",x=-280,mode=3,file="kan_z1b0200"},{"fgact",ch="華乃",id=9,lv=2.2999999999999998,act="おじぎ"}}},line=110,text={ja={{"「等、等等，这不能接受啊，能被自己偶像亲自感谢，前世积累了多少德才能获得这样的幸福啊？等一下我快要哭了」",{"rt2"},name={"華乃"}}},vo={{"vo",ch="kan",file="fem_kan_00456"}}},linknext="block_00016"},block_00072={{"fg",ex05="hiy_z1a0010",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="a0034",head="hiy_z1a",path=":fg/hiy/z1/",mode=3,file="hiy_z1a0210"},{"text"},linkback="block_00071",delay={vl02={{"fg",ex05="hiy_z1b0000",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="b0055",head="hiy_z1b",path=":fg/hiy/z1/",mode=3,file="hiy_z1b0200"}},vl01={{"fg",ex05="hiy_z1b0000",size="z1",ch="妃愛",id=10,lv=2.2999999999999998,face="b0053",head="hiy_z1b",path=":fg/hiy/z1/",mode=3,file="hiy_z1b0200"}}},line=498,text={ja={{"「嗯，平时在学校里和哥哥嬉闹的机会可不多。但是，购物这件事我还是希望你能认真对待。我想买的是文化节拱门的材料」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01484"}}},linknext="block_00073"},block_00021={{"fg",ex05="shi_nob0010",size="no",ch="詩桜",id=10,lv=2.2000000000000002,face="b0001",head="shi_nob",path=":fg/shi/no/",x=230,mode=3,file="shi_nob0210"},{"text"},linkback="block_00020",delay={vl02={{"fg",ex05="shi_nob0010",size="no",ch="詩桜",id=10,lv=2.2000000000000002,face="b0038h",head="shi_nob",hoho="h",path=":fg/shi/no/",x=230,mode=3,file="shi_nob0210"}},vl01={{"fg",ex05="shi_nob0010",size="no",ch="詩桜",id=10,lv=2.2000000000000002,face="b0034",head="shi_nob",path=":fg/shi/no/",x=230,mode=3,file="shi_nob0210"}}},line=159,text={ja={{"「话说回来，关于去年演唱会销售的黄色手电筒……我非常想要得到它，如果可能的话……」",{"rt2"},name={"詩桜"}}},vo={{"vo",ch="shi",file="fem_shi_01768"}}},linknext="block_00022"},block_00032={{"fg",ex05="hiy_z1b0000",size="z1",ch="妃愛",id=9,lv=2.2999999999999998,face="b0023",head="hiy_z1b",path=":fg/hiy/z1/",mode=3,file="hiy_z1b0200"},{"text"},linkback="block_00031",delay={vl01={{"fg",ex05="hiy_z1b0000",size="z1",ch="妃愛",id=9,lv=2.2999999999999998,face="b0055",head="hiy_z1b",path=":fg/hiy/z1/",mode=3,file="hiy_z1b0200"}}},line=224,text={ja={{"「工作现场是有收到蛋糕之类的，但晚上我都是直接回家的。因为要做给哥哥的晚餐」",{"rt2"},name={"妃愛"}}},vo={{"vo",ch="hiy",file="fem_hiy_01476"}}},linknext="block_00033"},block_00037={{"text"},line=248,text={ja={{"如果说在去年和今年妃爱的生日中有什么最大的不同，那就是我第一次用自己挣来的钱给妃爱买了礼物。",{"rt2"}}}},linkback="block_00036",linknext="block_00038"},block_00024={{"fg",ex05="hiy_nob0000",size="no",ch="妃愛",id=9,lv=2.2000000000000002,face="b0055",head="hiy_nob",path=":fg/hiy/no/",x=-230,mode=3,file="hiy_nob0200"},{"text"},line=177,text={ja={{"看来谈判成功了，诗樱前辈向妃爱表达了感谢。至于她为什么突然想要手电筒的原因。",{"rt2"}}}},linkback="block_00023",linknext="block_00025"}}