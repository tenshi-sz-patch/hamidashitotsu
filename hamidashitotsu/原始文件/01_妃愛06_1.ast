ast = {block_00094={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=0,file="hiy_z1a1000",lv=2.2999999999999998,face="a0053",ex05="hiy_z1a0000",id=13,mode=3},{"text"},linkback="block_00093",text={ja={{"「如果让大家久等了就不好了，你快去拿包包吧？ 好吗」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00233",ch="hiy"}}},linknext="block_00095",line=624},block_00013={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",file="hiy_z1a1000",lv=2.2999999999999998,face="a0032",ex05="hiy_z1a0000",id=13,mode=3},{"text"},text={ja={{"「对我来说，我已经得到了比我曾经期望的未来还要美好的东西，感觉人生已经半退休了」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00210",ch="hiy"}}},line=86,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",file="hiy_z1a1010",lv=2.2999999999999998,face="a0041",ex05="hiy_z1a0010",id=13,mode=3}}},linknext="block_00014",linkback="block_00012"},block_00067={{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1110",lv=2.2999999999999998,face="b0054",ex05="asu_z1b0010",id=9,mode=3},{"text"},linkback="block_00066",text={ja={{"「鳗鱼寿司很好吃。话说，你看了我的直播，谢谢」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00047",ch="asu"}}},linknext="block_00068",line=444},block_00052={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,file="hiy_z1a1000",lv=2.2999999999999998,face="a0019",ex05="hiy_z1a0000",id=10,mode=3},{"fgact",act="いいえ",id=10,lv=2.2999999999999998,ch="妃愛"},{"text"},text={ja={{"「啊，不是，图片是昨天拍的，但年菜是我自己做的哦～」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00220",ch="hiy"}}},line=343,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,file="hiy_z1a1000",lv=2.2999999999999998,face="a0054",ex05="hiy_z1a0000",id=10,mode=3}}},linknext="block_00053",linkback="block_00051"},block_00031={{"se",time=1000,stop=1,id=1},{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,ax=1280,by=-360,bx=-640,path=":bg/",file="zbg004a",ay=720},{"fg",resize=1,ch="里",path=":fg/mir/z1/",head="mir_z1a",size="z1",sync=0,file="mir_z1a0300",lv=2.2999999999999998,face="a0053",ex05="mir_z1a0000",id=13,mode=3},{"extrans",time=500},{"text"},linkback="block_00030",text={ja={{"「我本来是要去智宏家的，路上碰到了华乃和天梨，所以就一起来了」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00026",ch="mir"}}},linknext="block_00032",line=201},block_00008={{"se",time=500,stop=1,id=1},{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,ax=1280,by=-360,bx=-640,path=":bg/",file="zbg002a",ay=720},{"fg",resize=1,ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1b",size="z1",sync=0,file="hiy_z1b1000",lv=2.2999999999999998,face="b0028",ex05="hiy_z1b0000",id=13,mode=3},{"extrans",time=500},{"text"},linkback="block_00007",text={ja={{"「去年真是波澜壮阔的一年啊……」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00207",ch="hiy"}}},linknext="block_00009",line=63},block_00019={{"text"},linkback="block_00018",text={ja={{"「我想用我的工资买结婚戒指，所以可能要再等一段时间才能举行婚礼」",{"rt2"},name={"智宏"}}}},linknext="block_00020",line=117},block_00040={{"fg",ch="華乃",path=":fg/kan/no/",head="kan_noa",size="no",x=-480,file="kan_noa1100",lv=2.2000000000000002,face="a0052",ex05="kan_noa0000",id=5,mode=3},{"fg",ch="天梨",path=":fg/ame/no/",head="ame_noa",size="no",x=-275,file="ame_noa1000",lv=2.2000000000000002,face="a0052",ex05="ame_noa0000",id=9,mode=3},{"fg",ch="詩桜",path=":fg/shi/no/",head="shi_nob",size="no",x=160,file="shi_nob1000",lv=2.2000000000000002,face="b0052",ex05="shi_nob0000",id=12,mode=3},{"fg",ch="あすみ",path=":fg/asu/no/",head="asu_nob",size="no",x=0,file="asu_nob1010",lv=2.2000000000000002,face="b0052",ex05="asu_nob0010",id=13,mode=3},{"fgact",act="ジャンプ",id=5,lv=2.2000000000000002,ch="華乃"},{"text"},text={ja={{"「哇——，诗樱新年快乐！等等，不行，稍微休息一下的诗樱也太可爱了吧！？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00030",ch="kan"}}},line=261,delay={vl01={{"fg",ch="華乃",path=":fg/kan/no/",head="kan_noa",size="no",x=-480,file="kan_noa1100",lv=2.2000000000000002,face="a0048",ex05="kan_noa0000",id=5,mode=3},{"fgact",act="いいえ",id=5,lv=2.2000000000000002,ch="華乃"}}},linknext="block_00041",linkback="block_00039"},block_00043={{"fg",ch="天梨",mode=3,path=":fg/ame/no/",head="ame_noa",size="no",x=-275,lv=2.2000000000000002,ex05="ame_noa0010",face="a0052",file="ame_noa1010",id=9,ex03="a0097"},{"fg",ch="妃愛",path=":fg/hiy/no/",head="hiy_noa",size="no",x=480,file="hiy_noa1000",lv=2.2000000000000002,face="a0053",ex05="hiy_noa0000",id=6,mode=3},{"fg",ch="あすみ",path=":fg/asu/no/",head="asu_nob",size="no",x=0,file="asu_nob1010",lv=2.2000000000000002,face="b0055",ex05="asu_nob0010",id=13,mode=3},{"fgact",act="びっくり",id=9,lv=2.2000000000000002,ch="天梨"},{"fg",ch="華乃",id=5,path=":fg/kan/no/",head="kan_noa",size="no",x=-480,lv=2.2000000000000002,ex05="kan_noa0000",face="a0012h",file="kan_noa1100",hoho="h",mode=3},{"text"},text={ja={{"「啊——！会长，你养了一只大型犬啊！哇超级可爱！啊——我想和它一起拍照然后发到Ins上！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00013",ch="ame"}}},line=283,delay={vl01={{"fg",ch="天梨",mode=3,path=":fg/ame/no/",head="ame_noa",size="no",x=-275,lv=2.2000000000000002,ex05="ame_noa0010",face="a0035",file="ame_noa1010",id=9,ex03="a0097"}}},linknext="block_00044",linkback="block_00042"},block_00060={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,file="hiy_z1a1000",lv=2.2999999999999998,face="a0043",ex05="hiy_z1a0000",id=10,mode=3},{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1100",lv=2.2999999999999998,face="b0010",ex05="asu_z1b0000",id=9,mode=3},{"fgact",act="うんうん",id=10,lv=2.2999999999999998,ch="妃愛"},{"text"},text={ja={{"「你肯定比我瘦得多，我真是受不了！ 抓住肚子揉一揉吧」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00223",ch="hiy"}}},line=400,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,file="hiy_z1a1000",lv=2.2999999999999998,face="a0026",ex05="hiy_z1a0000",id=10,mode=3}}},linknext="block_00061",linkback="block_00059"},block_00056={{"text"},linkback="block_00055",text={ja={{"我家因为妃爱想要和爸爸妈妈一起吃饭的愿望，所以在餐厅里放了佛像。能让朋友们见到我的父母，真是太好了。",{"rt2"}}}},linknext="block_00057",line=366},block_00049={{"fg",ch="妃愛",path=":fg/hiy/no/",head="hiy_noa",size="no",x=480,file="hiy_noa1010",lv=2.2000000000000002,face="a0054",ex05="hiy_noa0010",id=6,mode=3},{"text"},text={ja={{"「既然这样，我也要把它发到推特上去。请等待办公室确认后再发布好吗——」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00219",ch="hiy"}}},line=318,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/no/",head="hiy_noa",size="no",x=480,file="hiy_noa1000",lv=2.2000000000000002,face="a0052",ex05="hiy_noa0000",id=6,mode=3}}},linknext="block_00050",linkback="block_00048"},block_00083={{"fg",ch="里",path=":fg/mir/z1/",head="mir_z1a",size="z1",x=-430,file="mir_z1a0210",lv=2.2999999999999998,face="a0010",ex05="mir_z1a0010",id=7,mode=3},{"text"},linkback="block_00082",text={ja={{"「她逃走了呢。」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00032",ch="mir"}}},linknext="block_00084",line=559},block_00064={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,file="hiy_z1a1010",lv=2.2999999999999998,face="a0007",ex05="hiy_z1a0010",id=10,mode=3},{"fgact",act="ジャンプ",id=10,lv=2.2999999999999998,ch="妃愛"},{"text"},text={ja={{"「吃点更实在的东西吧。有虾哦，虾。鲷鱼也有。话说你没吃早饭就过来了吗」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00224",ch="hiy"}}},line=425,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,file="hiy_z1a1010",lv=2.2999999999999998,face="a0045",ex05="hiy_z1a0010",id=10,mode=3},{"fgact",act="いいえ",ch="妃愛",time=80,lv=2.2999999999999998,id=10}}},linknext="block_00065",linkback="block_00063"},block_00061={{"fgact",act="いいえ",id=10,lv=2.2999999999999998,ch="妃愛"},{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1110",lv=2.2999999999999998,face="b0019",ex05="asu_z1b0010",id=9,mode=3},{"fgact",act="うんうん",id=9,lv=2.2999999999999998,ch="あすみ"},{"se",id=1,file="seコミ_ぽよん",count=2},{"text"},text={ja={{"「即使是有着职业运动员体质的你们这样说我……话说回来，我只是个不健康的人罢了」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00044",ch="asu"}}},line=410,delay={vl01={{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1110",lv=2.2999999999999998,face="b0053",ex05="asu_z1b0010",id=9,mode=3}}},linknext="block_00062",linkback="block_00060"},block_00042={{"fg",ch="資正",path=":fg/suk/no/",head="suk_noa",size="no",file="suk_noa0000",face="a0004",lv=2.2000000000000002,id=20,mode=1},{"text"},linkback="block_00041",text={ja={{"「呜～嗷、嗷嗷！」",{"rt2"},name={"資正"}}},vo={{"vo",file="etc_suk_00005",ch="suk"}}},linknext="block_00043",line=273},block_00026={{"text"},linkback="block_00025",text={ja={{"妃爱靠在我的胸口，脸上洋溢着幸福的笑容，我只需抚摸她的头就感到无比愉快，感觉这样下去一整天都会过去。",{"rt2"}}}},linknext="block_00027",line=150},block_00090={{"fg",ch="妃愛",id=13,path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=0,lv=2.2999999999999998,ex05="hiy_z1a0000",face="a0035h",file="hiy_z1a1000",hoho="h",mode=3},{"text"},text={ja={{"「但是，还是很开心。谢谢」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00232",ch="hiy"}}},line=611,delay={vl01={{"fg",ch="妃愛",id=13,path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=0,lv=2.2999999999999998,ex05="hiy_z1a0000",face="a0013h",file="hiy_z1a1000",hoho="h",mode=3}}},linknext="block_00091",linkback="block_00089"},block_00003={{"se",file="seししおどし",id=1},{"bgm",file="bgm01"},{"fg",ch="妃愛",path=":fg/hiy/no/",head="hiy_nob",size="no",file="hiy_nob1010",lv=2.2000000000000002,face="b0032",ex05="hiy_nob0010",id=13,mode=3},{"fgact",act="おじぎ",id=13,lv=2.2000000000000002,ch="妃愛"},{"text"},linkback="block_00002",text={ja={{"新的一年开始了。我和妃爱在餐厅对坐，我们彼此鞠躬。资正也同时低下了头。",{"rt2"}}}},linknext="block_00004",line=41},block_00024={{"text"},linkback="block_00023",text={ja={{"顺便说一下，汤里放的是蛤蜊。据说这寓意着夫妻和睦。",{"rt2"}}}},linknext="block_00025",line=138},block_00025={{"cgdel",id=-1},{"bg",vague=40,rule="rule_002",path=":bg/",time=1000,file="black",wait=500},{"bg",path=":bg/",file="bg002a",vague=40,rule="rule_002r"},{"text"},linkback="block_00024",text={ja={{"吃过年菜后，我把妃爱放在膝盖上，在沙发上闲适地度过了一个小时。",{"rt2"}}}},linknext="block_00026",line=148},block_00048={{"fg",ch="天梨",path=":fg/ame/no/",head="ame_noa",size="no",x=-275,file="ame_noa1000",lv=2.2000000000000002,face="a0035",ex05="ame_noa0000",id=9,mode=3},{"fgact",act="うんうん",id=9,lv=2.2000000000000002,ch="天梨"},{"text"},linkback="block_00047",text={ja={{"「啊——！那我可以和妃爱拍照吗！？啊，我想拍照，我想发出来，等等等等，啊——让我也一起拍照！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00015",ch="ame"}}},linknext="block_00049",line=312},block_00081={{"fg",ch="妃愛",id=8,path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=430,lv=2.2999999999999998,ex05="hiy_z1a0010",face="a0008h",file="hiy_z1a1010",hoho="h",mode=3},{"fgact",act="いいえ",id=8,lv=2.2999999999999998,ch="妃愛"},{"text"},text={ja={{"「啊，差不多该准备初诣了吧～？哥哥，我们去房间吧！」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00228",ch="hiy"}}},line=546,delay={vl01={{"fg",ch="妃愛",id=8,path=":fg/hiy/z1/",head="hiy_z1b",size="z1",x=430,lv=2.2999999999999998,ex05="hiy_z1b0000",face="b0007h",file="hiy_z1b1000",hoho="h",mode=3}}},linknext="block_00082",linkback="block_00080"},block_00054={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,file="hiy_z1a1000",lv=2.2999999999999998,face="a0006",ex05="hiy_z1a0000",id=10,mode=3},{"text"},text={ja={{"「要去看真品吗？如果要去初诣的话，在我和哥哥准备出门之前，请先上来等一下。请进～」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00221",ch="hiy"}}},line=355,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1b",size="z1",x=280,file="hiy_z1b1010",lv=2.2999999999999998,face="b0054",ex05="hiy_z1b0010",id=10,mode=3}}},linknext="block_00055",linkback="block_00053"},block_00084={{"se",time=500,stop=1,id=1},{"cgdel",id=-1},{"bg",vague=40,rule="rule_001",path=":bg/",time=1000,file="black",wait=250},{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,ax=1280,by=-360,bx=-640,path=":bg/",file="zbg004a",ay=720},{"fg",sync=0,ch="妃愛",id=13,path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=0,lv=2.2999999999999998,ex05="hiy_z1a0000",face="a0002h",file="hiy_z1a1000",hoho="h",mode=3},{"extrans",vague=40,rule="rule_001"},{"text"},linkback="block_00083",text={ja={{"即使关上门，还是能听到背后传来常磐的尖叫声，妃爱拉着我的手从餐厅逃了出去。",{"rt2"}}}},linknext="block_00085",line=576},block_00045={{"fg",ch="天梨",mode=3,path=":fg/ame/no/",head="ame_noa",size="no",x=-275,lv=2.2000000000000002,ex05="ame_noa0010",face="a0008",file="ame_noa1010",id=9,ex03="a0097"},{"text"},text={ja={{"「诶，真的可以吗？我和妃爱的粉丝群似乎不同……如果我们都发了同一只小狗的照片，别人会不会发现我们在同一所学校？我并没有隐藏我在大马戏团学院的事情哦？」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00014",ch="ame"}}},line=295,delay={vl01={{"fg",ch="天梨",mode=3,path=":fg/ame/no/",head="ame_noa",size="no",x=-275,lv=2.2000000000000002,ex05="ame_noa0010",face="a0010",file="ame_noa1010",id=9,ex03="a0097"}},vl02={{"fg",ch="天梨",path=":fg/ame/no/",head="ame_noa",size="no",x=-275,file="ame_noa1000",lv=2.2000000000000002,face="a0002",ex05="ame_noa0000",id=9,mode=3}}},linknext="block_00046",linkback="block_00044"},block_00038={{"text"},linkback="block_00037",text={ja={{"通常是和家人一起去参拜吧。诗樱前辈和锦虽然一个人住，但是不会去见父母吗？",{"rt2"}}}},linknext="block_00039",line=233},block_00051={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,x=-600,by=-360,bx=-640,path=":bg/",ax=1280,file="zbg004a",ay=720},{"fg",resize=1,ch="あすみ",sync=0,path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1010",lv=2.2999999999999998,face="b0010",ex05="asu_z1b0010",id=9,mode=3},{"fg",resize=1,ch="妃愛",sync=0,path=":fg/hiy/z1/",head="hiy_z1b",size="z1",x=280,file="hiy_z1b1010",lv=2.2999999999999998,face="b0010",ex05="hiy_z1b0010",id=10,mode=3},{"extrans",time=500},{"text"},linkback="block_00050",text={ja={{"「你上传的那张和狗狗一起的年菜图片，是为了拍摄而准备的吗？」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00041",ch="asu"}}},linknext="block_00052",line=336},block_00020={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",file="hiy_z1a1000",lv=2.2999999999999998,face="a0008",ex05="hiy_z1a0000",id=13,mode=3},{"fgact",act="びっくり",id=13,lv=2.2999999999999998,ch="妃愛"},{"text"},linkback="block_00019",text={ja={{"「啊！但是我已经收到了很棒的戒指……」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00214",ch="hiy"}}},linknext="block_00021",line=123},block_00029={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,file="bg004a",path=":bg/"},{"fg",sync=0,ch="天梨",path=":fg/ame/no/",head="ame_noa",size="no",x=230,file="ame_noa1000",lv=2.2000000000000002,face="a0052",ex05="ame_noa0000",id=10,mode=3},{"fg",sync=0,ch="あすみ",path=":fg/asu/no/",head="asu_nob",size="no",x=-230,file="asu_nob1010",lv=2.2000000000000002,face="b0052",ex05="asu_nob0010",id=9,mode=3},{"fg",sync=0,ch="里",path=":fg/mir/no/",head="mir_noa",size="no",x=0,file="mir_noa0300",lv=2.2000000000000002,face="a0054",ex05="mir_noa0000",id=13,mode=3},{"fg",sync=0,ch="華乃",path=":fg/kan/no/",head="kan_noa",size="no",x=-520,file="kan_noa1100",lv=2.2000000000000002,face="a0006",ex05="kan_noa0000",id=3,mode=3},{"fg",sync=0,ch="詩桜",path=":fg/shi/no/",head="shi_nob",size="no",x=520,file="shi_nob1010",lv=2.2000000000000002,face="b0053",ex05="shi_nob0010",id=4,mode=3},{"extrans",time=500},{"bgm",file="bgm11"},{"text"},linkback="block_00028",text={ja={{"「新年快乐～！今年也请多关照～！」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00025",ch="mir"}}},linknext="block_00030",line=184},block_00027={{"se",file="seインターフォン",id=1},{"bgm",stop=1},{"text"},linkback="block_00026",text={ja={{"但我们有个照顾周到又善良的表亲，所以新年似乎还会更加热闹。他们肯定是来给我们拜年的。",{"rt2"}}}},linknext="block_00028",line=157},block_00036={{"text"},linkback="block_00035",text={ja={{"「神田和饭田呢？」",{"rt2"},name={"智宏"}}}},linknext="block_00037",line=224},block_00097={{"se",time=500,stop=1,id=1},{"text"},linkback="block_00096",text={ja={{"感觉有点热，查了下气温居然有１３度。对于一月来说挺罕见的，据说最高气温还会升至１５度。这样的话可能不需要穿大衣吧……。",{"rt2"}}}},linknext="block_00098",line=642},block_00077={{"fg",ch="詩桜",path=":fg/shi/no/",head="shi_noa",size="no",x=380,file="shi_noa1100",lv=2.2000000000000002,face="a0029",ex05="shi_noa0000",id=8,mode=3},{"fgact",act="おじぎ",id=8,lv=2.2000000000000002,ch="詩桜"},{"text"},linkback="block_00076",text={ja={{"「对不起……」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00140",ch="shi"}}},linknext="block_00078",line=513},block_00074={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,file="bg002a",path=":bg/"},{"fg",resize=1,ch="天梨",sync=0,path=":fg/ame/no/",head="ame_noa",size="no",x=-380,file="ame_noa1100",lv=2.2000000000000002,face="a0054",ex05="ame_noa0000",id=7,mode=3},{"fg",sync=0,ch="華乃",path=":fg/kan/no/",head="kan_nob",size="no",x=0,file="kan_nob1100",lv=2.2000000000000002,face="b0051",ex05="kan_nob0000",id=13,mode=3},{"fg",sync=0,ch="詩桜",path=":fg/shi/no/",head="shi_noa",size="no",x=380,file="shi_noa1110",lv=2.2000000000000002,face="a0032",ex05="shi_noa0010",id=8,mode=3},{"extrans",time=500},{"fgact",act="うんうん",id=13,lv=2.2000000000000002,ch="華乃"},{"text"},text={ja={{"「真的是，直播中的妃爱太可爱了！啊，一想起『Ｒｅａｌ　ｉｎｔｅｎｔｉｏｎ』的时候他向我伸出手，简直不敢回想，那是在对我做粉丝服务吗？」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00031",ch="kan"}}},line=495,delay={vl01={{"fg",ch="華乃",path=":fg/kan/no/",head="kan_nob",size="no",x=0,file="kan_nob1100",lv=2.2000000000000002,face="b0048",ex05="kan_nob0000",id=13,mode=3}}},linknext="block_00075",linkback="block_00073"},label={top={label=1,block="block_00000"}},block_00087={{"fg",ch="妃愛",id=13,path=":fg/hiy/z1/",head="hiy_z1b",size="z1",x=0,lv=2.2999999999999998,ex05="hiy_z1b0010",face="b0007h",file="hiy_z1b1010",hoho="h",mode=3},{"text"},text={ja={{"「就算是我，如果疏忽大意被集中攻击，也会像蜂窝一样防不胜防。因为没有做好心理准备，啊——真是太羞人了」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00230",ch="hiy"}}},line=596,delay={vl01={{"fg",ch="妃愛",id=13,path=":fg/hiy/z1/",head="hiy_z1b",size="z1",x=0,lv=2.2999999999999998,ex05="hiy_z1b0010",face="b0029h",file="hiy_z1b1010",hoho="h",mode=3},{"fgact",act="おじぎ",id=13,lv=2.2999999999999998,ch="妃愛"}},vl02={{"fg",ch="妃愛",id=13,path=":fg/hiy/z1/",head="hiy_z1b",size="z1",x=0,lv=2.2999999999999998,ex05="hiy_z1b0010",face="b0014h",file="hiy_z1b1010",hoho="h",mode=3}}},linknext="block_00088",linkback="block_00086"},block_00098={{"cgdel",id=-1},{"bg",vague=40,rule="rule_001",path=":bg/",time=1000,file="black",wait=250},{"bg",path=":bg/",file="bg004a",vague=40,rule="rule_001"},{"text"},linkback="block_00097",text={ja={{"当我做好出门的准备回到餐厅时，常磐还在兴奋之中。但是已经冷静下来的妃爱没有给她任何机会，以专业的表情做出了完美的应对。",{"rt2"}}}},linknext="block_00099",line=651},block_00004={{"text"},linkback="block_00003",text={ja={{"和泉家非常重视新年的活动。家门口挂着松树装饰，餐桌上摆满了妃爱从三天前就开始准备的传统日式新年料理。",{"rt2"}}}},linknext="block_00005",line=43},block_00050={{"fg",ch="天梨",id=9,mx=40,lv=2.2000000000000002,mode=-1},{"fg",ch="妃愛",id=6,mx=40,lv=2.2000000000000002,mode=-1},{"fg",ch="華乃",id=5,path=":fg/kan/no/",head="kan_noa",size="no",x=-480,lv=2.2000000000000002,ex05="kan_noa0010",face="a0029h",file="kan_noa1110",hoho="h",mode=3},{"fg",ch="詩桜",path=":fg/shi/no/",head="shi_nob",size="no",x=160,file="shi_nob1000",lv=2.2000000000000002,face="b0055",ex05="shi_nob0000",id=12,mode=3},{"text"},linkback="block_00049",text={ja={{"不仅天梨处理得迅速，尽管是元旦，妃爱的经纪人的回复也很快，妃爱和天梨同时上传了照片。瞬间，轰动万人。",{"rt2"}}}},linknext="block_00051",line=325},block_00096={{"fg",resize=1,ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1b",size="z1",x=0,file="hiy_z1b1010",lv=2.2999999999999998,face="b0047",ex05="hiy_z1b0010",id=13,mode=3},{"text"},linkback="block_00095",text={ja={{"最近她老是来我这个房间，看起来她的包包似乎一直放在这里。很危险呀，要小心啊。（只有里姐才可能看到吧）",{"rt2"}}}},linknext="block_00097",line=638},block_00095={{"se",file="seドア(室内開き)",id=1},{"cgdel",id=-1},{"bg",vague=40,rule="rule_001",path=":bg/",time=1000,file="black",wait=250},{"bg",path=":bg/",file="bg001a",vague=40,rule="rule_001"},{"fg",resize=1,ch="妃愛",mx=40,path=":fg/hiy/no/",head="hiy_noa",size="no",x=0,file="hiy_noa1000",lv=2.2000000000000002,face="a0009",ex05="hiy_noa0000",id=13,mode=3},{"text"},linkback="block_00094",text={ja={{"这么说着进了自己的房间的妃爱立刻就出来了， 然后小小地慌乱地跑到我的房间里来。",{"rt2"}}}},linknext="block_00096",line=634},block_00089={{"fg",ch="妃愛",id=13,path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=0,lv=2.2999999999999998,ex05="hiy_z1a0000",face="a0043h",file="hiy_z1a1000",hoho="h",mode=3},{"text"},text={ja={{"「哥哥也真是唠叨。我在演唱会后已经得到了哥哥的大量赞美，所以我已经非常满足了。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00231",ch="hiy"}}},line=605,delay={vl01={{"fg",ch="妃愛",id=13,path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=0,lv=2.2999999999999998,ex05="hiy_z1a0000",face="a0003h",file="hiy_z1a1000",hoho="h",mode=3}}},linknext="block_00090",linkback="block_00088"},block_00093={{"text"},linkback="block_00092",text={ja={{"虽然内心有些愧疚，但妃爱那天实在太累了，所以没什么特殊庆祝，希望她能谅解。今年的圣诞节，是作为家人与妹妹一起度过的。",{"rt2"}}}},linknext="block_00094",line=617},block_00032={{"text"},linkback="block_00031",text={ja={{"「你们不是开车过来的吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00033",line=204},block_00062={{"text"},linkback="block_00061",text={ja={{"我猜只要我现在发一言，无论说什么都会失误吧。敢在这个空间的我，甚至连轻率地离开餐厅也不被允许，真是个地位微弱的人。",{"rt2"}}}},linknext="block_00063",line=412},block_00017={{"text"},linkback="block_00016",text={ja={{"妃爱似乎真的打算和我秘密举行婚礼，在一个无人、不会被任何人发现的教堂中，只有我们两个人的婚礼是她目前的目标。",{"rt2"}}}},linknext="block_00018",line=107},block_00034={{"fg",ch="里",path=":fg/mir/z1/",head="mir_z1a",size="z1",file="mir_z1a0310",lv=2.2999999999999998,face="a0022",ex05="mir_z1a0010",id=13,mode=3},{"text"},linkback="block_00033",text={ja={{"「她们两个说要去初诣，那我们就一起带上智宏和妃爱一起去吧，就这么决定了」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00028",ch="mir"}}},linknext="block_00035",line=214},block_00091={{"text"},linkback="block_00090",text={ja={{"本来，圣诞节应该在社交网络上公开自己的行踪，但是多亏了现场直播，妃爱无需报告任何事情，安全得到了保证。",{"rt2"}}}},linknext="block_00092",line=613},block_00088={{"text"},linkback="block_00087",text={ja={{"「圣诞音乐会上的妃爱真的太可爱了。我都想狠狠地夸奖你一番」",{"rt2"},name={"智宏"}}}},linknext="block_00089",line=599},block_00085={{"text"},linkback="block_00084",text={ja={{"「诶，妃爱，你在害羞吗？这种事情你不是很习惯吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00086",line=579},block_00099={{"bgm",stop=1},{"cgdel",id=-1},{"bg",vague=40,rule="rule_002",path=":bg/",time=1000,file="black",wait=500},{"excall",file="01_妃愛06_2"},{"text"},line=656,linkback="block_00098"},block_00086={{"fg",ch="妃愛",id=13,path=":fg/hiy/z1/",head="hiy_z1b",size="z1",x=0,lv=2.2999999999999998,ex05="hiy_z1b0000",face="b0020h",file="hiy_z1b1000",hoho="h",mode=3},{"fgact",act="いいえ",id=13,lv=2.2999999999999998,ch="妃愛"},{"text"},text={ja={{"「哎呀……我一直在家庭模式下。我居然忘记了我邀请你来看演唱会。更准确地说，那事已经过去了」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00229",ch="hiy"}}},line=587,delay={vl01={{"fg",ch="妃愛",id=13,path=":fg/hiy/z1/",head="hiy_z1b",size="z1",x=0,lv=2.2999999999999998,ex05="hiy_z1b0000",face="b0028h",file="hiy_z1b1000",hoho="h",mode=3}},vl02={{"fg",ch="妃愛",id=13,path=":fg/hiy/z1/",head="hiy_z1b",size="z1",x=0,lv=2.2999999999999998,ex05="hiy_z1b0000",face="b0047h",file="hiy_z1b1000",hoho="h",mode=3}}},linknext="block_00087",linkback="block_00085"},block_00082={{"fg",ch="妃愛",id=8,mx=40,lv=2.2999999999999998,mode=-1},{"se",file="seドア(室内開き)",id=1},{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=0,file="asu_z1b1100",lv=2.2999999999999998,face="b0010",ex05="asu_z1b0000",id=13,mode=3},{"text"},linkback="block_00081",text={ja={{"「逃了。」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00051",ch="asu"}}},linknext="block_00083",line=554},block_00028={{"cgdel",id=-1},{"bg",vague=40,rule="rule_001",path=":bg/",time=1000,file="black",wait=250},{"bg",vague=40,ay=720,ax=1280,zoom=120,rule="rule_001",by=-360,path=":bg/",bx=-640,file="zbg004a"},{"text"},linkback="block_00027",text={ja={{"「好的好的，新年快乐……哎呀」",{"rt2"},name={"智宏"}}}},linknext="block_00029",line=168},block_00005={{"text"},linkback="block_00004",text={ja={{"只是与往年不同的是……比如说去年，我们一起度过了跨年的瞬间，在轻轻地说了句「新的一年开始了呢」之后就各自回到自己的房间，然后第二天早上在这里再次碰面。",{"rt2"}}}},linknext="block_00006",line=45},block_00021={{"text"},linkback="block_00020",text={ja={{"「那个是作为恋人证明，或者说是从兄妹关系进一步发展的意愿的象征。嗯，就像是订婚戒指一样。」",{"rt2"},name={"智宏"}}}},linknext="block_00022",line=126},block_00035={{"fg",ch="里",path=":fg/mir/z1/",head="mir_z1a",size="z1",file="mir_z1a0300",lv=2.2999999999999998,face="a0055",ex05="mir_z1a0000",id=13,mode=3},{"fgact",act="うんうん",id=13,lv=2.2999999999999998,ch="里"},{"text"},text={ja={{"「既然这样，就叫上大家一起来嘛？然后，我等着澄和诗樱，一起过来了！」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00029",ch="mir"}}},line=221,delay={vl01={{"fg",ch="里",path=":fg/mir/z1/",head="mir_z1a",size="z1",file="mir_z1a0300",lv=2.2999999999999998,face="a0006",ex05="mir_z1a0000",id=13,mode=3}}},linknext="block_00036",linkback="block_00034"},block_00041={{"fg",ch="詩桜",path=":fg/shi/no/",head="shi_nob",size="no",x=160,file="shi_nob1000",lv=2.2000000000000002,face="b0055",ex05="shi_nob0000",id=12,mode=3},{"fgact",act="おじぎ",id=12,lv=2.2000000000000002,ch="詩桜"},{"text"},text={ja={{"「新年快乐。啊，那边的小狗就是妃爱在新年祝福推特上发的小狗吧」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00138",ch="shi"}}},line=268,delay={vl01={{"fg",ch="詩桜",path=":fg/shi/no/",head="shi_nob",size="no",x=160,file="shi_nob1000",lv=2.2000000000000002,face="b0053",ex05="shi_nob0000",id=12,mode=3}}},linknext="block_00042",linkback="block_00040"},block_00011={{"text"},linkback="block_00010",text={ja={{"「嗯，确实。这应该是最重大的事件。感觉人生已经定下来了」",{"rt2"},name={"智宏"}}}},linknext="block_00012",line=74},block_00063={{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1a",size="z1",x=-280,file="asu_z1a1100",lv=2.2999999999999998,face="a0054",ex05="asu_z1a0000",id=9,mode=3},{"text"},linkback="block_00062",text={ja={{"「因为什么都没吃，所以新年料理真是太帮忙了。豆子好好吃」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00045",ch="asu"}}},linknext="block_00064",line=417},block_00016={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1b",size="z1",file="hiy_z1b1000",lv=2.2999999999999998,face="b0039",ex05="hiy_z1b0000",id=13,mode=3},{"text"},text={ja={{"「首先我今年要努力工作，然后在空闲时间努力学习西班牙语。虽然南美的深处让我有点担心，但是虽然是葡萄牙，但亚速尔群岛也是可以的」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00212",ch="hiy"}}},line=105,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1b",size="z1",file="hiy_z1b1000",lv=2.2999999999999998,face="b0055",ex05="hiy_z1b0000",id=13,mode=3}}},linknext="block_00017",linkback="block_00015"},block_00010={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",file="hiy_z1a1000",lv=2.2999999999999998,face="a0055",ex05="hiy_z1a0000",id=13,mode=3},{"text"},linkback="block_00009",text={ja={{"「因为和哥哥在一起了呢」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00208",ch="hiy"}}},linknext="block_00011",line=71},block_00022={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",file="hiy_z1a1000",lv=2.2999999999999998,face="a0048",ex05="hiy_z1a0000",id=13,mode=3},{"fgact",act="いいえ",id=13,lv=2.2999999999999998,ch="妃愛"},{"text"},linkback="block_00021",text={ja={{"「太幸福了……新年伊始就感到如此甜蜜……今年肯定会是美好的一年……」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00215",ch="hiy"}}},linknext="block_00023",line=132},block_00079={{"fg",ch="華乃",id=13,path=":fg/kan/no/",head="kan_noa",size="no",x=0,lv=2.2000000000000002,ex05="kan_noa0000",face="a0002h",file="kan_noa1100",hoho="h",mode=3},{"text"},text={ja={{"「妃爱的那个眨眼，是舞蹈动作吗？还是指示？如果妃爱是自然而然地做的话，我也想一辈子跟着妃爱啊啊啊啊谢谢我做不到」",{"rt2"},name={"華乃"}}},vo={{"vo",file="fem_kan_00032",ch="kan"}}},line=527,delay={vl01={{"fg",ch="華乃",id=13,path=":fg/kan/no/",head="kan_noa",size="no",x=0,lv=2.2000000000000002,ex05="kan_noa0000",face="a0048h",file="kan_noa1100",hoho="h",mode=3},{"fgact",act="いいえ",id=13,lv=2.2000000000000002,ch="華乃"}},vl02={{"fg",ch="華乃",id=13,path=":fg/kan/no/",head="kan_noa",size="no",x=0,lv=2.2000000000000002,ex05="kan_noa0000",face="a0049h",file="kan_noa1100",hoho="h",mode=3}}},linknext="block_00080",linkback="block_00078"},block_00002={{"fg",ch="資正",path=":fg/suk/no/",head="suk_noa",size="no",file="suk_noa0000",face="a0004",lv=2.2000000000000002,id=20,mode=1},{"text"},linkback="block_00001",text={ja={{"「汪」",{"rt2"},name={"資正"}}},vo={{"vo",file="etc_suk_00004",ch="suk"}}},linknext="block_00003",line=28},block_00078={{"fg",ch="天梨",path=":fg/ame/no/",head="ame_noa",size="no",x=-380,file="ame_noa1110",lv=2.2000000000000002,face="a0050",ex05="ame_noa0010",id=7,mode=3},{"fgact",act="うなづく",id=7,lv=2.2000000000000002,ch="天梨"},{"text"},linkback="block_00077",text={ja={{"「华乃也是！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00018",ch="ame"}}},linknext="block_00079",line=519},block_00076={{"fg",ch="天梨",mode=3,path=":fg/ame/no/",head="ame_noa",size="no",x=-380,lv=2.2000000000000002,ex05="ame_noa0010",face="a0043",file="ame_noa1110",id=7,ex03="a0097"},{"fgact",act="うんうん",id=7,lv=2.2000000000000002,ch="天梨"},{"text"},linkback="block_00075",text={ja={{"「你刚才稍微撞到我了呢！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00017",ch="ame"}}},linknext="block_00077",line=507},block_00075={{"fg",ch="詩桜",path=":fg/shi/no/",head="shi_nob",size="no",x=380,file="shi_nob1110",lv=2.2000000000000002,face="b0031",ex05="shi_nob0010",id=8,mode=3},{"text"},text={ja={{"「表演和舞台效果真的完美结合了。我觉得跟看过的DVD里的以往的演唱会相比，这次真的达到了最高层次。我都忘我地挥着荧光棒了」",{"rt2"},name={"詩桜"}}},vo={{"vo",file="fem_shi_00139",ch="shi"}}},line=501,delay={vl01={{"fg",ch="詩桜",path=":fg/shi/no/",head="shi_nob",size="no",x=380,file="shi_nob1100",lv=2.2000000000000002,face="b0006",ex05="shi_nob0000",id=8,mode=3}}},linknext="block_00076",linkback="block_00074"},block_00015={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1b",size="z1",file="hiy_z1b1010",lv=2.2999999999999998,face="b0054",ex05="hiy_z1b0010",id=13,mode=3},{"text"},text={ja={{"「呀，我接下来要和哥哥一起度过很长的时间，我想要经历很多有趣和开心的事情，所以还有很多事情要做呢」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00211",ch="hiy"}}},line=99,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1b",size="z1",file="hiy_z1b1010",lv=2.2999999999999998,face="b0006",ex05="hiy_z1b0010",id=13,mode=3},{"fgact",act="うんうん",id=13,lv=2.2999999999999998,ch="妃愛"}}},linknext="block_00016",linkback="block_00014"},block_00073={{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1110",lv=2.2999999999999998,face="b0053",ex05="asu_z1b0010",id=9,mode=3},{"text"},text={ja={{"「除了短信，如果能直接碰面的话，我还想说些感谢的话和点评。打扮成演唱会衣装的你真的好可爱」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00050",ch="asu"}}},line=477,delay={vl01={{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1110",lv=2.2999999999999998,face="b0054",ex05="asu_z1b0010",id=9,mode=3}}},linknext="block_00074",linkback="block_00072"},block_00072={{"fg",ch="妃愛",id=10,path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,lv=2.2999999999999998,ex05="hiy_z1a0000",face="a0009h",file="hiy_z1a1000",hoho="h",mode=3},{"fgact",act="いいえ",ch="妃愛",time=80,lv=2.2999999999999998,id=10},{"text"},text={ja={{"「不是的，就是没想到你现在会说……有点被打了个措手不及。我以为在LIME上的交流就已经结束了」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00227",ch="hiy"}}},line=471,delay={vl01={{"fg",ch="妃愛",id=10,path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,lv=2.2999999999999998,ex05="hiy_z1a0000",face="a0020h",file="hiy_z1a1000",hoho="h",mode=3}}},linknext="block_00073",linkback="block_00071"},block_00066={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,file="hiy_z1a1000",lv=2.2999999999999998,face="a0050",ex05="hiy_z1a0000",id=10,mode=3},{"text"},linkback="block_00065",text={ja={{"「是在昨天的直播时吗？我听到了你大口吸食面条的声音呢」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00225",ch="hiy"}}},linknext="block_00067",line=439},block_00058={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,file="hiy_z1a1010",lv=2.2999999999999998,face="a0054",ex05="hiy_z1a0010",id=10,mode=3},{"fgact",act="うなづく",id=10,lv=2.2999999999999998,ch="妃愛"},{"text"},text={ja={{"「呵，吃吧吃吧。然后变胖。还有饺子汤！」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00222",ch="hiy"}}},line=387,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,file="hiy_z1a1010",lv=2.2999999999999998,face="a0006",ex05="hiy_z1a0010",id=10,mode=3}}},linknext="block_00059",linkback="block_00057"},block_00000={{"savetitle",text="あけました"},{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,file="bg002a",path=":bg/"},{"fg",sync=0,ch="妃愛",path=":fg/hiy/no/",head="hiy_nob",size="no",file="hiy_nob1000",lv=2.2000000000000002,face="b0054",ex05="hiy_nob0000",id=13,mode=3},{"extrans"},{"text"},text={ja={{"「新年快乐～」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00206",ch="hiy"}}},linknext="block_00001",line=20},block_00059={{"fg",resize=1,ch="天梨",path=":fg/ame/z1/",head="ame_z1a",size="z1",file="ame_z1a1100",lv=2.2999999999999998,face="a0008",ex05="ame_z1a0000",id=20,mode=1},{"text"},linkback="block_00058",text={ja={{"「亚澄，你真的好瘦啊。可能比我还要瘦呢」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00016",ch="ame"}}},linknext="block_00060",line=392},block_00057={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,x=-600,by=-360,bx=-640,path=":bg/",ax=1280,file="zbg002a",ay=720},{"fg",sync=0,ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1100",lv=2.2999999999999998,face="b0052",ex05="asu_z1b0000",id=9,mode=3},{"fg",sync=0,ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,file="hiy_z1a1000",lv=2.2999999999999998,face="a0053",ex05="hiy_z1a0000",id=10,mode=3},{"extrans",time=500},{"fgact",act="びっくり",id=9,lv=2.2999999999999998,ch="あすみ"},{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1100",lv=2.2999999999999998,face="b0054",ex05="asu_z1b0000",id=9,mode=3},{"text"},linkback="block_00056",text={ja={{"「哇—，妃爱真的很擅长烹饪哦。能尝一口吗？」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00043",ch="asu"}}},linknext="block_00058",line=380},block_00037={{"fg",ch="里",path=":fg/mir/z1/",head="mir_z1a",size="z1",file="mir_z1a0300",lv=2.2999999999999998,face="a0052",ex05="mir_z1a0000",id=13,mode=3},{"text"},linkback="block_00036",text={ja={{"「彼此两个都是每年都会和家人一起去楚川的神社参拜的呀」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00030",ch="mir"}}},linknext="block_00038",line=231},block_00069={{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1110",lv=2.2999999999999998,face="b0053",ex05="asu_z1b0010",id=9,mode=3},{"text"},text={ja={{"「说到谢谢……因为演唱会后没见面，所以还没直接说过谢谢呢。妃爱的演唱会，无论是歌曲还是MC都超棒的」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00048",ch="asu"}}},line=452,delay={vl01={{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1100",lv=2.2999999999999998,face="b0054",ex05="asu_z1b0000",id=9,mode=3}}},linknext="block_00070",linkback="block_00068"},block_00068={{"text"},linkback="block_00067",text={ja={{"亚澄啊……关于式酱的跨年直播，我和妃爱一起看的。她一边吃着荞麦面，一边轻松地自言自语，最后以一首歌作为结束，那时候的同时在线人数超过了两万。",{"rt2"}}}},linknext="block_00069",line=446},block_00006={{"fg",ch="妃愛",path=":fg/hiy/no/",head="hiy_nob",size="no",file="hiy_nob1010",lv=2.2000000000000002,face="b0053",ex05="hiy_nob0010",id=13,mode=3},{"text"},linkback="block_00005",text={ja={{"但今年，过完年之后我们还是一起睡觉，早上起床也是一起，一起下楼，所以新年的问候也拉得很长，感觉有些温馨。",{"rt2"}}}},linknext="block_00007",line=49},block_00071={{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1100",lv=2.2999999999999998,face="b0010",ex05="asu_z1b0000",id=9,mode=3},{"text"},linkback="block_00070",text={ja={{"「嗯，你为什么会紧张？」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00049",ch="asu"}}},linknext="block_00072",line=464},block_00065={{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1100",lv=2.2999999999999998,face="b0001",ex05="asu_z1b0000",id=9,mode=3},{"fgact",act="おじぎ",id=9,lv=2.2999999999999998,ch="あすみ"},{"se",stop=1,id=1},{"text"},text={ja={{"「嗯—，昨天晚上吃了荞麦面，所以我觉得……应该没问题」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00046",ch="asu"}}},line=434,delay={vl01={{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1100",lv=2.2999999999999998,face="b0019",ex05="asu_z1b0000",id=9,mode=3}}},linknext="block_00066",linkback="block_00064"},block_00080={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,x=-400,by=-360,bx=-640,path=":bg/",ax=1280,file="zbg002a",ay=720},{"fg",sync=0,ch="里",path=":fg/mir/z1/",head="mir_z1a",size="z1",x=-430,file="mir_z1a0200",lv=2.2999999999999998,face="a0050",ex05="mir_z1a0000",id=7,mode=3},{"fg",sync=0,ch="あすみ",path=":fg/asu/z1/",head="asu_z1a",size="z1",x=0,file="asu_z1a1100",lv=2.2999999999999998,face="a0054",ex05="asu_z1a0000",id=13,mode=3},{"fg",sync=0,ch="妃愛",id=8,path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=430,lv=2.2999999999999998,ex05="hiy_z1a0000",face="a0020h",file="hiy_z1a1000",hoho="h",mode=3},{"extrans",time=500},{"text"},linkback="block_00079",text={ja={{"「被这两个人夹在中间两小时的天梨，不会很糟糕吗？」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00031",ch="mir"}}},linknext="block_00081",line=539},block_00092={{"text"},linkback="block_00091",text={ja={{"在活动的那一天，能和上传了回家报告图片的妃爱一起度过，这是家庭的特权。一边吃着蛋糕，一边谈论现场直播的感想，顺便夸赞她做得很好。",{"rt2"}}}},linknext="block_00093",line=615},block_00046={{"fg",ch="妃愛",path=":fg/hiy/no/",head="hiy_noa",size="no",x=480,file="hiy_noa1000",lv=2.2000000000000002,face="a0054",ex05="hiy_noa0000",id=6,mode=3},{"fg",ch="あすみ",path=":fg/asu/no/",head="asu_nob",size="no",x=0,file="asu_nob1010",lv=2.2000000000000002,face="b0053",ex05="asu_nob0010",id=13,mode=3},{"text"},text={ja={{"「在之前的文化节上已经被人发现了，所以没关系的。为了保险起见，我提前问过天梨前辈是否可以告诉他人我们在同一个学生会。她已经给了我许可。」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00218",ch="hiy"}}},line=302,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/no/",head="hiy_noa",size="no",x=480,file="hiy_noa1000",lv=2.2000000000000002,face="a0006",ex05="hiy_noa0000",id=6,mode=3}}},linknext="block_00047",linkback="block_00045"},block_00070={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,file="hiy_z1a1000",lv=2.2999999999999998,face="a0008",ex05="hiy_z1a0000",id=10,mode=3},{"fgact",act="びっくり",id=10,lv=2.2999999999999998,ch="妃愛"},{"text"},text={ja={{"「哈！？啊，啊对，自从圣诞节以后就没见过面了……因为突然所以我吓了一跳。谢、谢谢你」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00226",ch="hiy"}}},line=459,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",x=280,file="hiy_z1a1000",lv=2.2999999999999998,face="a0019",ex05="hiy_z1a0000",id=10,mode=3}}},linknext="block_00071",linkback="block_00069"},block_00055={{"cgdel",id=-1},{"bg",vague=40,rule="rule_001",path=":bg/",time=1000,file="black",wait=250},{"bg",path=":bg/",file="bg002a",vague=40,rule="rule_001"},{"text"},linkback="block_00054",text={ja={{"我家的餐厅瞬间变成了学生会室。这个房间是否曾有过如此热闹的日子呢？",{"rt2"}}}},linknext="block_00056",line=364},block_00007={{"text"},linkback="block_00006",text={ja={{"顺便说一下，新年的初吻已经完成了。是在睡觉前她要求的。醒来时又亲了一次。她说这是「最好的新年」。我也有同样的感觉。",{"rt2"}}}},linknext="block_00008",line=51},block_00047={{"fg",ch="天梨",path=":fg/ame/no/",head="ame_noa",size="no",x=-275,file="ame_noa1000",lv=2.2000000000000002,face="a0052",ex05="ame_noa0000",id=9,mode=3},{"text"},linkback="block_00046",text={ja={{"既然在文化节上参加了学生会的演出，天梨没有隐藏她的制服和参加学生会的事实，那么她和妃爱之间的关系被发现的可能性也已经提前确认过了。真是严谨。",{"rt2"}}}},linknext="block_00048",line=306},block_00044={{"fg",ch="妃愛",path=":fg/hiy/no/",head="hiy_noa",size="no",x=480,file="hiy_noa1000",lv=2.2000000000000002,face="a0052",ex05="hiy_noa0000",id=6,mode=3},{"text"},linkback="block_00043",text={ja={{"「啊，没问题的哦。我们家的资正已经习惯被拍照了，所以会很安静的」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00217",ch="hiy"}}},linknext="block_00045",line=288},block_00053={{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1000",lv=2.2999999999999998,face="b0008",ex05="asu_z1b0000",id=9,mode=3},{"text"},text={ja={{"「做得这么好，我还以为是餐厅的呢……妃爱你自己做的这个年菜真的好厉害！」",{"rt2"},name={"あすみ"}}},vo={{"vo",file="fem_asu_00042",ch="asu"}}},line=349,delay={vl01={{"fg",ch="あすみ",path=":fg/asu/z1/",head="asu_z1b",size="z1",x=-280,file="asu_z1b1000",lv=2.2999999999999998,face="b0054",ex05="asu_z1b0000",id=9,mode=3}}},linknext="block_00054",linkback="block_00052"},block_00039={{"msgoff"},{"fg",sync=0,mode=-2},{"bg",sync=0,file="bg004a",path=":bg/"},{"fg",sync=0,ch="天梨",path=":fg/ame/no/",head="ame_noa",size="no",x=-275,file="ame_noa1000",lv=2.2000000000000002,face="a0054",ex05="ame_noa0000",id=9,mode=3},{"fg",sync=0,ch="華乃",path=":fg/kan/no/",head="kan_noa",size="no",x=-480,file="kan_noa1100",lv=2.2000000000000002,face="a0055",ex05="kan_noa0000",id=5,mode=3},{"fg",sync=0,ch="詩桜",path=":fg/shi/no/",head="shi_nob",size="no",x=160,file="shi_nob1000",lv=2.2000000000000002,face="b0032",ex05="shi_nob0000",id=12,mode=3},{"fg",sync=0,ch="あすみ",path=":fg/asu/no/",head="asu_nob",size="no",x=0,file="asu_nob1010",lv=2.2000000000000002,face="b0023",ex05="asu_nob0010",id=13,mode=3},{"extrans",time=500},{"fg",resize=1,ch="妃愛",mx=40,path=":fg/hiy/no/",head="hiy_noa",size="no",x=480,file="hiy_noa1000",lv=2.2000000000000002,face="a0052",ex05="hiy_noa0000",id=6,mode=3},{"text"},text={ja={{"「哎呀什么什么，大家都在吗？新年快乐哟ー」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00216",ch="hiy"}}},line=250,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/no/",head="hiy_noa",size="no",x=480,file="hiy_noa1000",lv=2.2000000000000002,face="a0055",ex05="hiy_noa0000",id=6,mode=3},{"fgact",act="おじぎ",id=6,lv=2.2000000000000002,ch="妃愛"}}},linknext="block_00040",linkback="block_00038"},block_00030={{"text"},linkback="block_00029",text={ja={{"「我也请多关照，去年真的受到了您的很多照顾……这个，那个？」",{"rt2"},name={"智宏"}}}},linknext="block_00031",line=187},block_00001={{"text"},linkback="block_00000",text={ja={{"「恭喜发财」",{"rt2"},name={"智宏"}}}},linknext="block_00002",line=23},block_00023={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",id=13,lv=2.2999999999999998,ex05="hiy_z1a0000",face="a0055h",file="hiy_z1a1000",hoho="h",mode=3},{"text"},linkback="block_00022",text={ja={{"妃爱满足地吃着栗子糕点。我也很喜欢栗子糕点，希望她不要吃太多。",{"rt2"}}}},linknext="block_00024",line=136},block_00012={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",file="hiy_z1a1000",lv=2.2999999999999998,face="a0002",ex05="hiy_z1a0000",id=13,mode=3},{"text"},text={ja={{"「还首次经历了网络暴动呢。但因为现在的结果，所以并不觉得那是痛苦的回忆」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00209",ch="hiy"}}},line=80,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",file="hiy_z1a1000",lv=2.2999999999999998,face="a0052",ex05="hiy_z1a0000",id=13,mode=3}}},linknext="block_00013",linkback="block_00011"},block_00018={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1b",size="z1",file="hiy_z1b1010",lv=2.2999999999999998,face="b0035",ex05="hiy_z1b0010",id=13,mode=3},{"text"},text={ja={{"「地中海的岛屿也很好啊。听说西班牙附近有几个教堂。但是从日本过去的游客似乎也不少呢」",{"rt2"},name={"妃愛"}}},vo={{"vo",file="fem_hiy_00213",ch="hiy"}}},line=114,delay={vl01={{"fg",ch="妃愛",path=":fg/hiy/z1/",head="hiy_z1a",size="z1",file="hiy_z1a1000",lv=2.2999999999999998,face="a0038",ex05="hiy_z1a0000",id=13,mode=3},{"fgact",act="おじぎ",id=13,lv=2.2999999999999998,ch="妃愛"}}},linknext="block_00019",linkback="block_00017"},block_00014={{"se",id=1,file="seコミ_胸を張る"},{"quake",mode="mw"},{"text"},linkback="block_00013",text={ja={{"「人生半退休就太早了吧！」",{"rt2"},name={"智宏"}}}},linknext="block_00015",line=92},block_00009={{"text"},linkback="block_00008",text={ja={{"「确实如此。在未来回顾人生时，肯定会把这一年作为一个重要的转折点。从学生会开始……」",{"rt2"},name={"智宏"}}}},linknext="block_00010",line=66},block_00033={{"fg",ch="里",path=":fg/mir/z1/",head="mir_z1a",size="z1",file="mir_z1a0300",lv=2.2999999999999998,face="a0019",ex05="mir_z1a0000",id=13,mode=3},{"text"},linkback="block_00032",text={ja={{"「因为今天是新年嘛，肯定要喝点酒对吧？」",{"rt2"},name={"里"}}},vo={{"vo",file="fem_mir_00027",ch="mir"}}},linknext="block_00034",line=209}}