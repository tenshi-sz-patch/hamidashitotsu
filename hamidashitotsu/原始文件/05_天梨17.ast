astver = 2.0
ast = {block_00001={{"fg",file="ame_z1a0210",ex05="ame_z1a0010",id=13,path=":fg/ame/z1/",x=0,ch="天梨",mode=3,size="z1",face="a0009",head="ame_z1a",lv=2.2999999999999998,ex03="a0097"},{"fgact",id=13,ch="天梨",act="いいえ",lv=2.2999999999999998},{"text"},delay={vl01={{"fg",file="ame_z1a0200",id=13,path=":fg/ame/z1/",ex05="ame_z1a0000",ch="天梨",mode=3,size="z1",face="a0052",x=0,lv=2.2999999999999998,head="ame_z1a"}},vl02={{"fg",file="ame_z1a0200",id=13,path=":fg/ame/z1/",ex05="ame_z1a0000",ch="天梨",mode=3,size="z1",face="a0054",x=0,lv=2.2999999999999998,head="ame_z1a"}}},line=29,text={ja={{"「啊——我只是为了送你坐了电车，还要你回头，真是不好意思。今天谢谢你！很开心！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00997",ch="ame"}}},linknext="block_00002",linkback="block_00000"},block_00032={{"fg",file="ame_z1a0200",id=20,path=":fg/ame/z1/",ch="天梨",mode=1,size="z1",face="a0027",ex05="ame_z1a0000",lv=2.2999999999999998,head="ame_z1a"},{"text"},delay={vl01={{"fg",file="ame_noa0200",resize=1,id=20,path=":fg/ame/no/",ch="天梨",mode=1,size="no",face="a0007",ex05="ame_noa0000",lv=2.2000000000000002,head="ame_noa"}},vl02={{"fg",file="ame_noa0200",id=20,path=":fg/ame/no/",ch="天梨",mode=1,size="no",face="a0038",ex05="ame_noa0000",lv=2.2000000000000002,head="ame_noa"}}},line=175,text={ja={{"「嗯，这是我自己提出的计划。正因为如此……所以我，呃……」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01011",ch="ame"}}},linknext="block_00033",linkback="block_00031"},block_00000={{"savetitle",text="ラブ・ストーリーは突然に"},{"msgoff"},{"fg",mode=-2,sync=0},{"bg",by=-360,file="zbg018c",sync=0,ax=1280,ay=720,bx=-640,path=":bg/"},{"fg",file="ame_z1a0200",sync=0,resize=1,id=13,path=":fg/ame/z1/",ex05="ame_z1a0000",ch="天梨",mode=3,size="z1",face="a0004",x=0,lv=2.2999999999999998,head="ame_z1a"},{"extrans",vague=40,rule="rule_002r"},{"bgm",file="bgm08"},{"text"},text={ja={{"「那么，这里……抱歉不能送你到大野」",{"rt2"},name={"智宏"}}}},linknext="block_00001",line=21},block_00034={{"text"},line=179,text={ja={{"但是，天梨还是那个天梨。我以为她会紧皱眉头，但她却满脸自信地给我一个明亮的笑容，直视着站在她面前的我。",{"rt2"}}}},linknext="block_00035",linkback="block_00033"},block_00007={{"text"},line=46,text={ja={{"其实她想说的是「每天都可以」。她关心我的情况，也顾及到妃爱的存在……我遇到了一个好的恋人。",{"rt2"}}}},linknext="block_00008",linkback="block_00006"},block_00023={{"fg",file="ame_z1a0200",id=13,path=":fg/ame/z1/",ex05="ame_z1a0000",ch="天梨",mode=3,size="z1",face="a0034",x=0,lv=2.2999999999999998,head="ame_z1a"},{"text"},line=127,text={ja={{"天梨轻轻地笑了。不能做到的矫情就不要说出口了。",{"rt2"}}}},linknext="block_00024",linkback="block_00022"},block_00012={{"fg",file="ame_z2a0210",ex05="ame_z2a0010",id=13,path=":fg/ame/z2/",x=0,ch="天梨",mode=3,size="z2",face="a0008",head="ame_z2a",lv=2.3999999999999999,ex03="a0097"},{"text"},delay={vl01={{"fg",file="ame_z2a0200",id=13,path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0003",x=0,lv=2.3999999999999999,head="ame_z2a"}}},line=82,text={ja={{"「啊，当然，我会努力保持被智宏称赞为美丽的身材和腿……」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01002",ch="ame"}}},linknext="block_00013",linkback="block_00011"},block_00051={{"fg",file="ame_z1a0200",id=13,path=":fg/ame/z1/",ex05="ame_z1a0000",ch="天梨",mode=3,size="z1",face="a0032",x=0,lv=2.2999999999999998,head="ame_z1a"},{"text"},delay={vl01={{"fg",file="ame_z1a0200",id=13,path=":fg/ame/z1/",ex05="ame_z1a0000",ch="天梨",mode=3,size="z1",face="a0052",x=0,lv=2.2999999999999998,head="ame_z1a"},{"fgact",id=13,ch="天梨",act="いいえ",lv=2.2999999999999998}},vl02={{"fg",file="ame_z1a0200",id=13,path=":fg/ame/z1/",ex05="ame_z1a0000",ch="天梨",mode=3,size="z1",face="a0022",x=0,lv=2.2999999999999998,head="ame_z1a"}}},line=265,text={ja={{"「自从和会长在一起……不，自从我喜欢上会长以来，一切都很美好。谢谢你。」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01018",ch="ame"}}},linknext="block_00052",linkback="block_00050"},block_00017={{"text"},line=98,text={ja={{"但即使如此，我还是想和天梨在一起，不去纠结过去，向前看才是天梨的作风。等到真的需要努力的时候，再考虑该怎么办吧。",{"rt2"}}}},linknext="block_00018",linkback="block_00016"},block_00053={{"text"},line=275,text={ja={{"即便如此，我们仍处在人群之中，所以她并没有大声说话。但是，她的声音比平时稍微大了一些，像是在向我的背影投掷过去。",{"rt2"}}}},linknext="block_00054",linkback="block_00052"},block_00037={{"fg",file="ame_z2a0200",id=13,path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0052",x=0,lv=2.3999999999999999,head="ame_z2a"},{"text"},line=200,text={ja={{"「我会全力以赴地做好模特的工作，你就期待看场下的观众们沸腾吧！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01014",ch="ame"}}},linknext="block_00038",linkback="block_00036"},block_00011={{"fg",file="ame_z2a0210",ex05="ame_z2a0010",id=13,path=":fg/ame/z2/",x=0,ch="天梨",mode=3,size="z2",face="a0054",head="ame_z2a",lv=2.3999999999999999,ex03="a0097"},{"fgact",id=13,ch="天梨",act="びっくり",lv=2.3999999999999999},{"text"},delay={vl01={{"fg",file="ame_z2a0200",id=13,path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0022",x=0,lv=2.3999999999999999,head="ame_z2a"}}},line=76,text={ja={{"「但是今天，我明白了交往后其实不需要特意去努力！只要和喜欢的人在一起，就能越来越深地喜欢上对方」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01001",ch="ame"}}},linknext="block_00012",linkback="block_00010"},block_00043={{"text"},line=221,text={ja={{"「其实我很想在那个场合炫耀她是我的恋人。但是我怕被嫉妒的人杀掉，所以我保持沉默」",{"rt2"},name={"智宏"}}}},linknext="block_00044",linkback="block_00042"},block_00016={{"text"},line=96,text={ja={{"实际上并非如此简单……这样的想法闪过脑海，但很快就被我抛到了一边。",{"rt2"}}}},linknext="block_00017",linkback="block_00015"},block_00058={{"bgm",time=3000,stop=1},{"se",stop=1,id=-1,time=3000},{"vostop",time=3000,stop=1},{"ntclear"},{"cgdel",id=-1},{"bg",path=":bg/",file="black",time=3000},{"cacheclear"},{"ex",func="wait",time=1000},{"bg",file="catch05",time=500,lv=9,id=9,path=":cg/"},{"bgm",file="bgm96",loop=0},{"ex",func="wait",time=6000},{"bgm",time=3000,stop=1},{"se",stop=1,id=-1,time=3000},{"vostop",time=3000,stop=1},{"ntclear"},{"cgdel",id=-1},{"bg",path=":bg/",file="black",time=3000},{"cacheclear"},{"ex",func="wait",time=1000},{"excall",file="05_天梨18_1"},{"text"},line=289,linkback="block_00057"},block_00003={{"text"},line=34,text={ja={{"我们从酒店出来后，立刻换上了制服，没有绕道任何地方，直接来到了千玉站。",{"rt2"}}}},linknext="block_00004",linkback="block_00002"},block_00057={{"text"},line=283,text={ja={{"如果是这样，我甚至觉得可以接受作为天梨的男朋友在INS上被介绍出去了。算了，还是不要了吧。天梨的INS有10万个赞呢。",{"rt2"}}}},linknext="block_00058",linkback="block_00056"},block_00005={{"text"},line=38,text={ja={{"这已经是作为学生无法避免的事情了。如果想要和天梨长时间在一起，就只能自己生活自理，独自生活了。",{"rt2"}}}},linknext="block_00006",linkback="block_00004"},block_00035={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",by=-360,sync=0,file="zbg018c",path=":bg/",ay=720,ax=1280,bx=-640,zoom=120},{"fg",file="ame_z2a0200",sync=0,resize=1,id=13,path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0053",x=0,lv=2.3999999999999999,head="ame_z2a"},{"extrans"},{"text"},line=189,text={ja={{"「如果我们站在同一个舞台上，会长你就不能从正面看到我的走路和摆姿势了」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01012",ch="ame"}}},linknext="block_00036",linkback="block_00034"},label={top={block="block_00000",label=1}},block_00044={{"fg",file="ame_z2a0210",ex05="ame_z2a0010",id=13,path=":fg/ame/z2/",x=0,ch="天梨",mode=3,size="z2",face="a0051",head="ame_z2a",lv=2.3999999999999999,ex03="a0097"},{"fgact",id=13,ch="天梨",act="ジャンプ",lv=2.3999999999999999},{"text"},delay={vl01={{"fg",file="ame_z2a0210",ex05="ame_z2a0010",id=13,path=":fg/ame/z2/",x=0,ch="天梨",mode=3,size="z2",face="a0054",head="ame_z2a",lv=2.3999999999999999,ex03="a0097"}},vl02={{"fg",file="ame_z2a0210",id=13,ex05="ame_z2a0010",hoho="h",path=":fg/ame/z2/",x=0,ch="天梨",mode=3,size="z2",face="a0035h",head="ame_z2a",lv=2.3999999999999999,ex03="a0097"}}},line=229,text={ja={{"「啊——在舞台上说出恋人的事情好危险呢！但是等舞台结束后，我也想在社交网络上炫耀一下，「和喜欢的人一起站在文化节的舞台上感觉太棒了！」」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01016",ch="ame"}}},linknext="block_00045",linkback="block_00043"},block_00052={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",sync=0,file="bg018c",path=":bg/"},{"extrans"},{"text"},line=273,text={ja={{"可能是因为度过了幸福的时间，或者是因为谈到了文化节而情绪高涨，天梨的话语带着一丝感伤，她的声音中充满了深情。",{"rt2"}}}},linknext="block_00053",linkback="block_00051"},block_00050={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",by=-360,file="zbg018c",sync=0,ax=1280,ay=720,bx=-640,path=":bg/"},{"fg",file="ame_z1a0200",sync=0,resize=1,id=13,path=":fg/ame/z1/",ex05="ame_z1a0000",ch="天梨",mode=3,size="z1",face="a0023",x=0,lv=2.2999999999999998,head="ame_z1a"},{"extrans",time=500},{"text"},line=257,text={ja={{"就在我即将背对恋人的那一刹那，我看到天梨的表情发生了变化。她的笑容从明亮变为温柔，那双炫目的眼睛望向了我。",{"rt2"}}}},linknext="block_00051",linkback="block_00049"},block_00049={{"text"},line=248,text={ja={{"本来我想送天梨回家的，没想到自己反而成了被送的一方。时间管理应该是男人的责任啊……我这样想着，匆忙地转身。",{"rt2"}}}},linknext="block_00050",linkback="block_00048"},block_00018={{"fg",file="ame_z2a0200",id=13,hoho="h",path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0023h",x=0,lv=2.3999999999999999,head="ame_z2a"},{"text"},line=102,text={ja={{"我想，如果是在人前也只做这么一点的话应该没问题吧，于是抚摸了天梨的头。她看上去很幸福地微笑了。",{"rt2"}}}},linknext="block_00019",linkback="block_00017"},block_00021={{"fg",file="ame_z1a0200",id=13,path=":fg/ame/z1/",ex05="ame_z1a0000",ch="天梨",mode=3,size="z1",face="a0055",x=0,lv=2.2999999999999998,head="ame_z1a"},{"text"},line=120,text={ja={{"「但是，如果让日和等待的话，你会选择跑回去吧？」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01005",ch="ame"}}},linknext="block_00022",linkback="block_00020"},block_00010={{"fg",file="ame_z2b0200",id=13,path=":fg/ame/z2/",ex05="ame_z2b0000",ch="天梨",mode=3,size="z2",face="b0019",x=0,lv=2.3999999999999999,head="ame_z2b"},{"text"},delay={vl01={{"fg",file="ame_z2b0200",id=13,path=":fg/ame/z2/",ex05="ame_z2b0000",ch="天梨",mode=3,size="z2",face="b0009",x=0,lv=2.3999999999999999,head="ame_z2b"},{"fgact",id=13,ch="天梨",act="おじぎ",lv=2.3999999999999999}}},line=69,text={ja={{"「其实呢，我一直想和智宏交往，也为此一直在努力……但我完全没有考虑过交往之后的事情」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01000",ch="ame"}}},linknext="block_00011",linkback="block_00009"},block_00030={{"fg",file="ame_z1a0200",resize=1,id=20,path=":fg/ame/z1/",ch="天梨",mode=1,size="z1",face="a0002",ex05="ame_z1a0000",lv=2.2999999999999998,head="ame_z1a"},{"text"},line=165,text={ja={{"「但是，在制作服装的方面，我真的是从零开始……我还是觉得我应该做出更多的贡献」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01010",ch="ame"}}},linknext="block_00031",linkback="block_00029"},block_00047={{"text"},line=239,text={ja={{"「那么，我们明天早上再见。走到这一步，如果衣服没做完就太丢人了，所以我们要坚持到最后」",{"rt2"},name={"智宏"}}}},linknext="block_00048",linkback="block_00046"},block_00009={{"fg",file="ame_z2a0200",id=13,hoho="h",path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0052h",x=0,lv=2.3999999999999999,head="ame_z2a"},{"text"},delay={vl01={{"fg",file="ame_z2a0210",id=13,ex05="ame_z2a0010",hoho="h",path=":fg/ame/z2/",x=0,ch="天梨",mode=3,size="z2",face="a0035h",head="ame_z2a",lv=2.3999999999999999,ex03="a0097"}}},line=62,text={ja={{"「我、我也是。我才发现，和喜欢的人在一起的时间，竟然如此的幸福」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00999",ch="ame"}}},linknext="block_00010",linkback="block_00008"},block_00014={{"text"},line=88,text={ja={{"「我今天也更喜欢天梨了。如果仅仅在一起就能这么深爱她，那么正如天梨所说，真的很轻松」",{"rt2"},name={"智宏"}}}},linknext="block_00015",linkback="block_00013"},block_00002={{"text"},line=32,text={ja={{"「只是开心就好吗……不，应该是我说谢谢」",{"rt2"},name={"智宏"}}}},linknext="block_00003",linkback="block_00001"},block_00020={{"text"},line=115,text={ja={{"「啊啊，大概每15分钟就有一班……走路回去的距离也并不遥远」",{"rt2"},name={"智宏"}}}},linknext="block_00021",linkback="block_00019"},block_00019={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",by=-360,file="zbg018c",sync=0,ax=1280,ay=720,bx=-640,path=":bg/"},{"fg",file="ame_z1a0200",sync=0,resize=1,id=13,path=":fg/ame/z1/",ex05="ame_z1a0000",ch="天梨",mode=3,size="z1",face="a0002",x=0,lv=2.2999999999999998,head="ame_z1a"},{"extrans",time=500},{"text"},line=112,text={ja={{"「会长的公交车，差不多该到了吧？」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01004",ch="ame"}}},linknext="block_00020",linkback="block_00018"},block_00033={{"text"},line=177,text={ja={{"她是想说「所以我想对制作做出贡献」吗？这对于不喜欢纠结的天梨来说真是罕见。她怎么了呢。",{"rt2"}}}},linknext="block_00034",linkback="block_00032"},block_00046={{"fg",file="ame_z2a0200",id=13,path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0054",x=0,lv=2.3999999999999999,head="ame_z2a"},{"text"},line=236,text={ja={{"天梨是明白还是不明白，她用明亮的声音笑了。现在应该是撤退的时候了。已经到了必须冲到公交站、跳上公交的时间了。",{"rt2"}}}},linknext="block_00047",linkback="block_00045"},block_00006={{"fg",file="ame_z1b0200",id=13,path=":fg/ame/z1/",ex05="ame_z1b0000",ch="天梨",mode=3,size="z1",face="b0022",x=0,lv=2.2999999999999998,head="ame_z1b"},{"text"},delay={vl01={{"fg",file="ame_z1b0200",id=13,path=":fg/ame/z1/",ex05="ame_z1b0000",ch="天梨",mode=3,size="z1",face="b0055",x=0,lv=2.2999999999999998,head="ame_z1b"}}},line=44,text={ja={{"「希望能像今天这样，在放学后一起过。一个月一次这样——」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_00998",ch="ame"}}},linknext="block_00007",linkback="block_00005"},block_00045={{"text"},line=232,text={ja={{"「那个，那个，我看过图片后再做判断……似乎比在学校里炫耀更容易引起嫉妒……」",{"rt2"},name={"智宏"}}}},linknext="block_00046",linkback="block_00044"},block_00054={{"text"},line=277,text={ja={{"仔细回想一下，自从我和天梨第一次交谈以来……虽然成为学生会长很辛苦，但是现在想想，一切都是美好的。",{"rt2"}}}},linknext="block_00055",linkback="block_00053"},block_00024={{"bg",by=-360,file="zbg018c",bx=-640,path=":bg/",y2=100,x2=640,y=100,x=-640,ay=720,ax=1280,speed=150000},{"fg",file="ame_z1b0210",id=20,path=":fg/ame/z1/",ch="天梨",mode=1,size="z1",face="b0052",ex05="ame_z1b0010",lv=2.2999999999999998,head="ame_z1b"},{"text"},delay={vl01={{"fg",file="ame_z1b0210",id=20,path=":fg/ame/z1/",ch="天梨",mode=1,size="z1",face="b0054",ex05="ame_z1b0010",lv=2.2999999999999998,head="ame_z1b"}}},line=135,text={ja={{"「那么，我就在回去前快速说完我想说的话。嗯，文化节你要穿的服装，看起来已经快完成了，真是太好了。恭喜你」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01006",ch="ame"}}},linknext="block_00025",linkback="block_00023"},block_00042={{"fg",file="ame_z2a0200",id=13,path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0054",x=0,lv=2.3999999999999999,head="ame_z2a"},{"fgact",id=13,ch="天梨",act="うなづく",lv=2.3999999999999999},{"text"},delay={vl01={{"fg",file="ame_z2a0210",ex05="ame_z2a0010",id=13,path=":fg/ame/z2/",x=0,ch="天梨",mode=3,size="z2",face="a0019",head="ame_z2a",lv=2.3999999999999999,ex03="a0097"}}},line=218,text={ja={{"「嗯！但是，一定要注意不要在舞台上笑出来哦」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01015",ch="ame"}}},linknext="block_00043",linkback="block_00041"},block_00041={{"text"},line=211,text={ja={{"「当天，我期待着从舞台上看到观众们的面孔」",{"rt2"},name={"智宏"}}}},linknext="block_00042",linkback="block_00040"},block_00013={{"text"},line=85,text={ja={{"「这不仅仅是为了和我持续交往而努力，更是为了天梨作为模特维持人气而努力，所以这是为了自己而努力，这样就好了」",{"rt2"},name={"智宏"}}}},linknext="block_00014",linkback="block_00012"},block_00055={{"text"},line=279,text={ja={{"不，可能和天梨的对话本身，就是我人生中奇迹般的「美好事物」。那些曾经的篮子搬运工，现在都已经是出人头地的人物了。",{"rt2"}}}},linknext="block_00056",linkback="block_00054"},block_00039={{"text"},line=204,text={ja={{"确实很遗憾。不能从观众席上看到天梨的精彩表演真是可惜。但是下个月还有更大的舞台——Gal Collection。我会期待那一天的。",{"rt2"}}}},linknext="block_00040",linkback="block_00038"},block_00038={{"text"},line=202,text={ja={{"这是一种宣言，即使有恋人在同一舞台上，她也不会看向旁边或后面，只会向前看。",{"rt2"}}}},linknext="block_00039",linkback="block_00037"},block_00040={{"fg",file="ame_z2a0200",id=13,path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0055",x=0,lv=2.3999999999999999,head="ame_z2a"},{"text"},line=208,text={ja={{"对于恋人充满正能量的话语，我只能回答「谢谢」。不行，如果再待在这里，我会忍不住想要拥抱天梨。现在我们正处在人群中。",{"rt2"}}}},linknext="block_00041",linkback="block_00039"},block_00036={{"fg",file="ame_z2a0200",id=13,path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0054",x=0,lv=2.3999999999999999,head="ame_z2a"},{"text"},delay={vl01={{"fg",file="ame_z2a0200",id=13,path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0006",x=0,lv=2.3999999999999999,head="ame_z2a"}}},line=195,text={ja={{"「既然在制作上我没有像其他人那样做出贡献，那么在文化节那天，我就会在我擅长的领域做出贡献！如果是穿校服的话，就交给我吧！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01013",ch="ame"}}},linknext="block_00037",linkback="block_00035"},block_00028={{"text"},line=154,text={ja={{"「哎？为什么？我刚刚还说天梨你帮了我很大的忙……你还负责了窗口工作」",{"rt2"},name={"智宏"}}}},linknext="block_00029",linkback="block_00027"},block_00056={{"text"},line=281,text={ja={{"在文化祭当日，如果能在舞台上完成任务，我就能被周围的人认同，出任学生会长。",{"rt2"}}}},linknext="block_00057",linkback="block_00055"},block_00031={{"text"},line=168,text={ja={{"「天梨你提出了这个计划。正是因为你提出的想法，学生会的表演才有可能形成」",{"rt2"},name={"智宏"}}}},linknext="block_00032",linkback="block_00030"},block_00004={{"text"},line=36,text={ja={{"虽然我想和她一起度过甜蜜的时光，但因为我家的原因感到抱歉。然而每次都说对不起也很烦人，对妃爱也感到抱歉。",{"rt2"}}}},linknext="block_00005",linkback="block_00003"},block_00029={{"fg",file="ame_nob0200",resize=1,id=20,path=":fg/ame/no/",ch="天梨",mode=1,size="no",face="b0006",ex05="ame_nob0000",lv=2.2000000000000002,head="ame_nob"},{"text"},delay={vl01={{"fg",file="ame_nob0210",id=20,path=":fg/ame/no/",ch="天梨",mode=1,size="no",face="b0054",ex05="ame_nob0010",lv=2.2000000000000002,head="ame_nob"}}},line=160,text={ja={{"「嗯，嗯！我在那方面确实努力了！能在学生会的工作中得到会长你说我「帮了大忙」的赞扬，我真的很开心！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01009",ch="ame"}}},linknext="block_00030",linkback="block_00028"},block_00048={{"fg",file="ame_z2a0200",id=13,path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0006",x=0,lv=2.3999999999999999,head="ame_z2a"},{"fgact",id=13,ch="天梨",act="うなづく",lv=2.3999999999999999},{"text"},delay={vl01={{"fg",file="ame_z2a0200",id=13,path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0052",x=0,lv=2.3999999999999999,head="ame_z2a"}}},line=246,text={ja={{"「嗯！我们一定要完成它！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01017",ch="ame"}}},linknext="block_00049",linkback="block_00047"},block_00026={{"fg",file="ame_z1a0210",id=20,path=":fg/ame/z1/",ch="天梨",mode=1,size="z1",face="a0031",ex05="ame_z1a0010",lv=2.2999999999999998,head="ame_z1a"},{"text"},delay={vl01={{"fg",file="ame_z1a0200",id=20,path=":fg/ame/z1/",ch="天梨",mode=1,size="z1",face="a0053",ex05="ame_z1a0000",lv=2.2999999999999998,head="ame_z1a"}},vl02={{"fg",file="ame_z1a0200",id=20,path=":fg/ame/z1/",ch="天梨",mode=1,size="z1",face="a0054",ex05="ame_z1a0000",lv=2.2999999999999998,head="ame_z1a"}}},line=145,text={ja={{"「我很期待能和会长一起在舞台上……去年我只是稍微参加了班级的活动，只是随便转了转，但今年的文化节似乎会成为我一生的回忆」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01007",ch="ame"}}},linknext="block_00027",linkback="block_00025"},block_00025={{"text"},line=138,text={ja={{"「嗯……虽然到最后都不能掉以轻心，但从今天的进度来看，应该可以放心了。天梨你也辛苦了」",{"rt2"},name={"智宏"}}}},linknext="block_00026",linkback="block_00024"},block_00022={{"text"},line=123,text={ja={{"「跑回去啊……嗯，我还是老实坐公交吧」",{"rt2"},name={"智宏"}}}},linknext="block_00023",linkback="block_00021"},block_00027={{"fg",file="ame_z1a0200",id=20,path=":fg/ame/z1/",ch="天梨",mode=1,size="z1",face="a0002",ex05="ame_z1a0000",lv=2.2999999999999998,head="ame_z1a"},{"text"},delay={vl01={{"fg",file="ame_z1a0200",id=20,path=":fg/ame/z1/",ch="天梨",mode=1,size="z1",face="a0029",ex05="ame_z1a0000",lv=2.2999999999999998,head="ame_z1a"}}},line=151,text={ja={{"「只是，嗯……我刚才说我为了和会长在一起而努力，但作为学生会的一员，我感觉我并没有像大家那样做出贡献」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01008",ch="ame"}}},linknext="block_00028",linkback="block_00026"},block_00008={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",by=-360,sync=0,file="zbg018c",path=":bg/",ay=720,ax=1280,bx=-640,zoom=120},{"fg",file="ame_z2b0200",sync=0,resize=1,id=13,path=":fg/ame/z2/",ex05="ame_z2b0000",ch="天梨",mode=3,size="z2",face="b0055",x=0,lv=2.3999999999999999,head="ame_z2b"},{"extrans",time=500},{"text"},line=56,text={ja={{"「我很幸福」",{"rt2"},name={"智宏"}}}},linknext="block_00009",linkback="block_00007"},block_00015={{"fg",file="ame_z2a0200",id=13,path=":fg/ame/z2/",ex05="ame_z2a0000",ch="天梨",mode=3,size="z2",face="a0055",x=0,lv=2.3999999999999999,head="ame_z2a"},{"fgact",id=13,ch="天梨",act="うなづく",lv=2.3999999999999999},{"text"},line=94,text={ja={{"「嗯！」",{"rt2"},name={"天梨"}}},vo={{"vo",file="fem_ame_01003",ch="ame"}}},linknext="block_00016",linkback="block_00014"}}