ast = {block_00053={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0053",file="ame_z1a0200"},{"text"},linkback="block_00052",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0054",file="ame_z1a0200"}}},linknext="block_00054",line=292,text={vo={{"vo",file="fem_ame_00870",ch="ame"}},ja={{"「但是如果班级换了……虽然还想和那些孩子们做朋友，但是已经和智宏说过想一起去同一个大学，想要增加两个人一起学习的时间」",{"rt2"},name={"天梨"}}}}},block_00061={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",x=-200,y=100,sync=0,file="zbg034b",zoom=120,by=-360,bx=-640,ay=720,ax=1280,path=":bg/"},{"fg",ch="天梨",mode=3,resize=1,path=":fg/ame/z2/",sync=0,ex05="ame_z2a0000",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0022",file="ame_z2a0200"},{"extrans",time=500},{"text"},linkback="block_00060",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z2a0000",path=":fg/ame/z2/",file="ame_z2a0200",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0055h",hoho="h"}}},linknext="block_00062",line=341,text={vo={{"vo",file="fem_ame_00876",ch="ame"}},ja={{"「我对学长完全没有任何怀疑。我会一直忠诚的」",{"rt2"},name={"天梨"}}}}},block_00079={{"bgm",stop=1,time=3000},{"se",id=-1,stop=1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",time=3000,file="black",path=":bg/"},{"cacheclear"},{"ex",func="wait",time=1000},{"bg",path=":cg/",lv=9,id=9,file="catch05",time=500},{"bgm",file="bgm96",loop=0},{"ex",func="wait",time=6000},{"bgm",stop=1,time=3000},{"se",id=-1,stop=1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",time=3000,file="black",path=":bg/"},{"cacheclear"},{"ex",func="wait",time=1000},{"excall",file="05_天梨15_1"},{"text"},linkback="block_00078",line=442},block_00032={{"text"},linkback="block_00031",linknext="block_00033",line=171,text={ja={{"「我本来打算等过了11月就主动告白的。但我等不下去了，我变得非常喜欢她。这样的好女孩，我现在就想和她在一起，真是无法自拔」",{"rt2"},name={"智宏"}}}}},block_00072={{"fg",ch="天梨",mode=3,ex05="ame_z2a0010",path=":fg/ame/z2/",ex03="a0097",file="ame_z2a0210",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0008h",hoho="h"},{"fgact",ch="天梨",id=13,act="びっくり",lv=2.3999999999999999},{"text"},linkback="block_00071",delay={vl03={{"fg",ch="天梨",mode=3,ex05="ame_z2a0000",path=":fg/ame/z2/",file="ame_z2a0200",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0047h",hoho="h"},{"fgact",ch="天梨",id=13,act="おじぎ",lv=2.3999999999999999}},vl01={{"fg",ch="天梨",mode=3,ex05="ame_z2a0000",path=":fg/ame/z2/",file="ame_z2a0200",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0012h",hoho="h"}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z2a0000",path=":fg/ame/z2/",file="ame_z2a0200",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0019h",hoho="h"}}},linknext="block_00073",line=404,text={vo={{"vo",file="fem_ame_00882",ch="ame"}},ja={{"「啊，不是的！我没觉得无聊！但是，那个，比起故事，我更专注于学长……所以我可能没怎么记住故事的内容……呜呜，对不起」",{"rt2"},name={"天梨"}}}}},block_00042={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0053",file="ame_z1a0200"},{"text"},linkback="block_00041",linknext="block_00043",line=222,text={ja={{"「我觉得今天我们之间的关系因为强烈的爱情而更加深化了。我告诉她这些是因为我觉得我们彼此都已经准备好了」",{"rt2"},name={"智宏"}}}}},block_00074={{"text"},linkback="block_00073",linknext="block_00075",line=416,text={ja={{"「只要你理解就好。而且，害羞的天梨很可爱」",{"rt2"},name={"智宏"}}}}},block_00012={{"text"},linkback="block_00011",linknext="block_00013",line=56,text={ja={{"「然后，我听到对话的一部分，就无力站立了。在教室外面坐了一会儿，结果就无意中偷听到了天梨她们的对话」",{"rt2"},name={"智宏"}}}}},block_00065={{"text"},linkback="block_00064",linknext="block_00066",line=361,text={ja={{"啊……虽然她总是很乐观，不会过于纠结，但是这样的前提下，她也会感到不安的。我做错了。",{"rt2"}}}}},block_00044={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0002",file="ame_z1a0200"},{"fgact",ch="天梨",id=13,act="うなづく",lv=2.2999999999999998},{"text"},linkback="block_00043",delay={vl03={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0019",file="ame_z1a0200"}},vl06={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",file="ame_z1a0200",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0022h",hoho="h"},{"fgact",ch="天梨",id=13,act="うなづく",lv=2.2999999999999998}},vl04={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0055",file="ame_z1a0200"},{"fgact",ch="天梨",id=13,act="うなづく",lv=2.2999999999999998}},vl05={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",file="ame_z1a0200",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0023h",hoho="h"}},vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0027",file="ame_z1a0200"},{"fgact",ch="天梨",id=13,act="うなづく",lv=2.2999999999999998}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0054",file="ame_z1a0200"},{"fgact",ch="天梨",id=13,act="うなづく",lv=2.2999999999999998}}},linknext="block_00045",line=244,text={vo={{"vo",file="fem_ame_00864",ch="ame"}},ja={{"「嗯。嗯，嗯。虽然我被吓到了，但如果我按照你说的积极接受的话……嗯。如果因为那件事，现在我能感到如此幸福的话。嗯」",{"rt2"},name={"天梨"}}}}},block_00005={{"bg",x=-200,y=100,sync=0,file="zbg034b",by=-360,bx=-640,ay=720,ax=1280,path=":bg/"},{"fg",ch="天梨",mode=3,resize=1,path=":fg/ame/z1/",sync=0,ex05="ame_z1a0000",file="ame_z1a0200",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0047h",hoho="h"},{"extrans"},{"text"},linkback="block_00004",linknext="block_00006",line=32,text={ja={{"「啊，还有大约10分钟……或者15分钟吧。我要说的事情很简短，所以在你回家之前，我想和你认真谈一谈。」",{"rt2"},name={"智宏"}}}}},block_00036={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0023",file="ame_z1a0200"},{"text"},linkback="block_00035",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0055",file="ame_z1a0200"}}},linknext="block_00037",line=193,text={vo={{"vo",file="fem_ame_00861",ch="ame"}},ja={{"「因为我想回应学长的告白……所以能够好好地回应真好」",{"rt2"},name={"天梨"}}}}},block_00051={{"text"},linkback="block_00050",linknext="block_00052",line=278,text={ja={{"「天梨明年也要考虑如何和那些孩子相处了」",{"rt2"},name={"智宏"}}}}},block_00052={{"fg",ch="天梨",mode=3,ex05="ame_z1b0000",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0038",file="ame_z1b0200"},{"fgact",ch="天梨",id=13,act="おじぎ",lv=2.2999999999999998},{"text"},linkback="block_00051",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1b0010",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0002",file="ame_z1b0210"}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z1b0010",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0053",file="ame_z1b0210"}}},linknext="block_00053",line=286,text={vo={{"vo",file="fem_ame_00869",ch="ame"}},ja={{"「嗯，嗯。突然表现得很冷淡也不好。如果要疏远，还是要明确地说出来……在同一个班级的时候，还是像以前那样相处吧」",{"rt2"},name={"天梨"}}}}},block_00034={{"text"},linkback="block_00033",linknext="block_00035",line=180,text={ja={{"「但是如果那天我先说了「我在教室听到你的话」这样的前提，天梨可能会像现在这样混乱和道歉，我觉得那样就没法告白了」",{"rt2"},name={"智宏"}}}}},block_00011={{"text"},linkback="block_00010",linknext="block_00012",line=53,text={ja={{"「我并没有打算偷听。但是声音传到了走廊，我听到了自己的名字，就有点在意了」",{"rt2"},name={"智宏"}}}}},block_00038={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0009",file="ame_z1a0210"},{"fgact",ch="天梨",id=13,act="いいえ",lv=2.2999999999999998},{"text"},linkback="block_00037",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0020",file="ame_z1a0200"}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0028",file="ame_z1a0200"}}},linknext="block_00039",line=204,text={vo={{"vo",file="fem_ame_00862",ch="ame"}},ja={{"「所，所以那个，学长没有错！如果在教室里听到自己的话，关心是理所当然的……而且还被说了那么难听的话……」",{"rt2"},name={"天梨"}}}}},block_00022={{"fg",ch="天梨",mode=3,ex05="ame_z1b0000",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0008",file="ame_z1b0200"},{"fgact",ch="天梨",act="いいえ",lv=2.2999999999999998,time=50,id=13},{"text"},linkback="block_00021",linknext="block_00023",line=113,text={ja={{"「一起度过暑假的时候，我已经喜欢上天梨了，但不仅仅是喜欢，我爱上了她」",{"rt2"},name={"智宏"}}}}},block_00035={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0038",file="ame_z1a0200"},{"text"},linkback="block_00034",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1b0000",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0029",file="ame_z1b0200"}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z1b0000",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0038",file="ame_z1b0200"}}},linknext="block_00036",line=187,text={vo={{"vo",file="fem_ame_00860",ch="ame"}},ja={{"「啊……嗯，可能是那样……而且如果听到那个话题，我就不用回应了……啊……对呀……」",{"rt2"},name={"天梨"}}}}},block_00006={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0001",file="ame_z1a0200"},{"text"},linkback="block_00005",linknext="block_00007",line=37,text={ja={{"「我更愿意在我们两人独处的时候说这些，而不是在人多的地方。如果时间不够，我们可以稍后换个地方继续，但至少我要先说出最重要的部分。」",{"rt2"},name={"智宏"}}}}},block_00063={{"fg",ch="天梨",mode=3,ex05="ame_z2a0010",path=":fg/ame/z2/",ex03="a0097",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0054",file="ame_z2a0210"},{"fgact",ch="天梨",id=13,act="うんうん",lv=2.3999999999999999},{"text"},linkback="block_00062",delay={vl03={{"fg",ch="天梨",mode=3,ex05="ame_z2a0010",path=":fg/ame/z2/",ex03="a0097",file="ame_z2a0210",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0054h",hoho="h"}},vl01={{"fg",ch="天梨",mode=3,ex05="ame_z2a0010",path=":fg/ame/z2/",ex03="a0097",file="ame_z2a0210",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0029h",hoho="h"},{"fgact",ch="天梨",id=13,act="おじぎ",lv=2.3999999999999999}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z2a0010",path=":fg/ame/z2/",ex03="a0097",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0051",file="ame_z2a0210"}}},linknext="block_00064",line=353,text={vo={{"vo",file="fem_ame_00877",ch="ame"}},ja={{"「还有还有，当学长说我是「很酷的女孩子」时，我心里咯噔一下！被学长这么认为，我真的好开心！」",{"rt2"},name={"天梨"}}}}},block_00077={{"fg",ch="天梨",mode=3,ex05="ame_z2a0000",path=":fg/ame/z2/",file="ame_z2a0200",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0011h",hoho="h"},{"text"},linkback="block_00076",linknext="block_00078",line=432,text={ja={{"因为天梨抬头看着我撒娇，所以我亲了她。她开始变得亲昵起来，那是一种甜蜜蜜的吻。",{"rt2"}}}}},block_00008={{"text"},linkback="block_00007",linknext="block_00009",line=42,text={ja={{"「上周，开学典礼那天放学后……我去教室叫天梨的时候」",{"rt2"},name={"智宏"}}}}},block_00003={{"text"},linkback="block_00002",linknext="block_00004",line=20,text={ja={{"然而，天梨似乎已经预先给父母发送了LIME消息，「我要回家时会联系你们」，结果收到了他们的回复。",{"rt2"}}}}},block_00016={{"text"},linkback="block_00015",linknext="block_00017",line=81,text={ja={{"「天梨没必要道歉。天梨是在帮我。虽然说帮我也不太对……嗯，那个，当被说是惩罚游戏的时候我差点儿心灰意冷」",{"rt2"},name={"智宏"}}}}},block_00010={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0008",file="ame_z1a0210"},{"text"},linkback="block_00009",linknext="block_00011",line=50,text={vo={{"vo",file="fem_ame_00851",ch="ame"}},ja={{"「诶」",{"rt2"},name={"天梨"}}}}},block_00050={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0052",file="ame_z1a0210"},{"fgact",ch="天梨",id=13,act="うんうん",lv=2.2999999999999998},{"text"},linkback="block_00049",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0006",file="ame_z1a0210"}}},linknext="block_00051",line=275,text={vo={{"vo",file="fem_ame_00868",ch="ame"}},ja={{"「啊，我好像明白了！胸口不畅快的时候，就会有种憋闷的感觉！」",{"rt2"},name={"天梨"}}}}},block_00066={{"fg",ch="天梨",mode=3,ex05="ame_z2b0000",path=":fg/ame/z2/",head="ame_z2b",size="z2",lv=2.3999999999999999,id=13,face="b0009",file="ame_z2b0200"},{"text"},linkback="block_00065",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z2b0000",path=":fg/ame/z2/",head="ame_z2b",size="z2",lv=2.3999999999999999,id=13,face="b0028",file="ame_z2b0200"}}},linknext="block_00067",line=367,text={vo={{"vo",file="fem_ame_00879",ch="ame"}},ja={{"「啊，但是……我虽然可以说我很幸福，但学长也有不好的感受对吧。对不起」",{"rt2"},name={"天梨"}}}}},block_00026={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",file="ame_z1a0210",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0002h",hoho="h"},{"text"},linkback="block_00025",linknext="block_00027",line=145,text={ja={{"「所以，既然听到了天梨的心意，我觉得我必须马上表达我的感情」",{"rt2"},name={"智宏"}}}}},block_00014={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0046",file="ame_z1a0210"},{"fgact",ch="天梨",id=13,act="びっくり",lv=2.2999999999999998},{"text"},linkback="block_00013",delay={vl03={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0047",file="ame_z1a0210"},{"fgact",ch="天梨",id=13,act="いいえ",lv=2.2999999999999998}},vl01={{"fgact",ch="天梨",id=13,act="びっくり",lv=2.2999999999999998}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0009",file="ame_z1a0210"},{"fgact",ch="天梨",id=13,act="ジャンプ",lv=2.2999999999999998}}},linknext="block_00015",line=70,text={vo={{"vo",file="fem_ame_00852",ch="ame"}},ja={{"「诶诶那次的对话……诶！那难道是，我需要道歉的那个……诶诶诶！」",{"rt2"},name={"天梨"}}}}},block_00029={{"fg",ch="天梨",mode=3,ex05="ame_z1b0010",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0010",file="ame_z1b0210"},{"text"},linkback="block_00028",linknext="block_00030",line=162,text={ja={{"「了解到天梨的真心之后，我认为只有我一个人隐藏喜欢的感情是不诚实的，这也是我的真心话。这很重要」",{"rt2"},name={"智宏"}}}}},block_00056={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0032",file="ame_z1a0200"},{"text"},linkback="block_00055",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0022",file="ame_z1a0200"}}},linknext="block_00057",line=306,text={vo={{"vo",file="fem_ame_00872",ch="ame"}},ja={{"「能够依赖学长，向学长咨询问题，还被学长带进学生会，真的很开心。我找到了喜欢的人，也交到了一生的朋友」",{"rt2"},name={"天梨"}}}}},block_00041={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0008",file="ame_z1a0200"},{"text"},linkback="block_00040",linknext="block_00042",line=217,text={ja={{"「但是，我觉得如果我对天梨隐瞒了我早已知道她的心意以及我听到那天的对话的事实，那就是不诚实，所以我一直在寻找机会来告诉她」",{"rt2"},name={"智宏"}}}}},block_00040={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0002",file="ame_z1a0200"},{"text"},linkback="block_00039",linknext="block_00041",line=212,text={ja={{"「按照天梨妈妈的「积极向前，不要纠结」的原则，我接受了这是我和天梨开始交往所必需的事件」",{"rt2"},name={"智宏"}}}}},block_00002={{"text"},linkback="block_00001",linknext="block_00003",line=18,text={ja={{"之后，我们一边看天梨的毕业纪念册和过去的杂志等，一边时而贴着脸颊，时而亲吻，度过了恋人的时光。",{"rt2"}}}}},block_00046={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0002",file="ame_z1a0200"},{"text"},linkback="block_00045",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0028",file="ame_z1a0200"}}},linknext="block_00047",line=257,text={vo={{"vo",file="fem_ame_00866",ch="ame"}},ja={{"「但是，我想再道歉一次。对不起，我朋友们做了那样的事情」",{"rt2"},name={"天梨"}}}}},block_00019={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0002",file="ame_z1a0200"},{"text"},linkback="block_00018",linknext="block_00020",line=97,text={ja={{"「我惊讶于她能有如此坚定的意愿并且明确地表达出来。我觉得她是个很酷的女孩子」",{"rt2"},name={"智宏"}}}}},block_00000={{"savetitle",text="ざんげちゃん"},{"bg",x=640,y2=100,speed=150000,file="zbg034b",by=-360,bx=-640,y=100,x2=-640,ay=720,ax=1280,path=":bg/"},{"bgm",file="bgm18"},{"text"},linknext="block_00001",line=14,text={ja={{"天梨洗完澡回来后，我们靠着床坐在地板上，肩膀相依，度过了一段时间。",{"rt2"}}}}},block_00058={{"text"},linkback="block_00057",linknext="block_00059",line=316,text={ja={{"果然她说的「一生在一起」是真心的。能在人生中找到这么出色的伴侣，真是我的幸运。继妹妹抽奖之后又一次大赢家，就是我。",{"rt2"}}}}},block_00059={{"fg",ch="天梨",mode=3,ex05="ame_z1b0000",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0027",file="ame_z1b0200"},{"text"},linkback="block_00058",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1b0010",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0052",file="ame_z1b0210"}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z1b0010",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0019",file="ame_z1b0210"}}},linknext="block_00060",line=323,text={vo={{"vo",file="fem_ame_00874",ch="ame"}},ja={{"「但是……即使不考虑负责任这事，我已经更喜欢和我无话不谈的学长了」",{"rt2"},name={"天梨"}}}}},block_00018={{"text"},linkback="block_00017",linknext="block_00019",line=92,text={ja={{"「啊，不……现在天梨说的「不能接受」，对于我心灰意冷的话，听到天梨真的生气的声音……」",{"rt2"},name={"智宏"}}}}},block_00021={{"text"},linkback="block_00020",linknext="block_00022",line=107,text={ja={{"「之后听到她明确地说出我的名字，并告诉我「喜欢我」，我立刻就被打动了」",{"rt2"},name={"智宏"}}}}},block_00078={{"fg",ch="天梨",mode=3,ex05="ame_z2a0000",path=":fg/ame/z2/",file="ame_z2a0200",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0023h",hoho="h"},{"text"},linkback="block_00077",linknext="block_00079",line=436,text={ja={{"和天梨在一起后，每天都如此甜蜜，我都怕会得蛀牙。不，说实话，我感觉我的牙已经开始松动了。",{"rt2"}}}}},block_00015={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0009",file="ame_z1a0200"},{"text"},linkback="block_00014",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0029",file="ame_z1a0200"}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0038",file="ame_z1a0200"},{"fgact",ch="天梨",id=13,act="いいえ",lv=2.2999999999999998}}},linknext="block_00016",line=78,text={vo={{"vo",file="fem_ame_00853",ch="ame"}},ja={{"「诶，哪部分？你听到了哪部分的对话？无法站立，是指你受伤了对吧。真的对不起，诶，我该怎么办呢」",{"rt2"},name={"天梨"}}}}},block_00013={{"text"},linkback="block_00012",linknext="block_00014",line=59,text={ja={{"「所以，我要道歉。对不起。还有，对不起我一直没告诉你这件事」",{"rt2"},name={"智宏"}}}}},block_00069={{"fg",ch="天梨",mode=3,ex05="ame_z2a0010",path=":fg/ame/z2/",ex03="a0097",file="ame_z2a0210",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0045h",hoho="h"},{"fgact",ch="天梨",id=13,act="びっくり",lv=2.3999999999999999},{"text"},linkback="block_00068",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z2a0010",path=":fg/ame/z2/",ex03="a0097",file="ame_z2a0210",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0009h",hoho="h"}}},linknext="block_00070",line=382,text={vo={{"vo",file="fem_ame_00880",ch="ame"}},ja={{"「啊啊啊啊啊啊啊啊！那、那个！我、我好像在大家面前说出了我喜欢学长的哪一点！」",{"rt2"},name={"天梨"}}}}},block_00039={{"text"},linkback="block_00038",linknext="block_00040",line=207,text={ja={{"「对于天梨的朋友们，因为天梨已经明确地表达了愤怒，所以在和解之后，我就停止了生气。同时，我也停止了自责」",{"rt2"},name={"智宏"}}}}},label={top={block="block_00000",label=1}},block_00007={{"text"},linkback="block_00006",linknext="block_00008",line=39,text={ja={{"尽管我做了这么煞有介事的铺垫，但天梨并没有显得讨厌或者焦虑。她肯定是保持着积极的心态。",{"rt2"}}}}},block_00076={{"fg",ch="天梨",mode=3,ex05="ame_z2b0010",path=":fg/ame/z2/",file="ame_z2b0210",head="ame_z2b",size="z2",lv=2.3999999999999999,id=13,face="b0019h",hoho="h"},{"text"},linkback="block_00075",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z2a0000",path=":fg/ame/z2/",file="ame_z2a0200",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0055h",hoho="h"}}},linknext="block_00077",line=428,text={vo={{"vo",file="fem_ame_00885",ch="ame"}},ja={{"「但是被学长疼爱让我很开心。让我的心跳加速」",{"rt2"},name={"天梨"}}}}},block_00075={{"fg",ch="天梨",mode=3,ex05="ame_z2b0010",path=":fg/ame/z2/",file="ame_z2b0210",head="ame_z2b",size="z2",lv=2.3999999999999999,id=13,face="b0029h",hoho="h"},{"text"},linkback="block_00074",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z2b0010",path=":fg/ame/z2/",file="ame_z2b0210",head="ame_z2b",size="z2",lv=2.3999999999999999,id=13,face="b0047h",hoho="h"}}},linknext="block_00076",line=422,text={vo={{"vo",file="fem_ame_00884",ch="ame"}},ja={{"「呜……被学长欺负了……知道我会害羞还故意捉弄我……」",{"rt2"},name={"天梨"}}}}},block_00070={{"fg",ch="天梨",mode=3,ex05="ame_z2b0010",path=":fg/ame/z2/",file="ame_z2b0210",head="ame_z2b",size="z2",lv=2.3999999999999999,id=13,face="b0045h",hoho="h"},{"fgact",ch="天梨",id=13,act="いいえ",lv=2.3999999999999999},{"text"},linkback="block_00069",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z2b0010",path=":fg/ame/z2/",file="ame_z2b0210",head="ame_z2b",size="z2",lv=2.3999999999999999,id=13,face="b0046h",hoho="h"}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z2b0010",path=":fg/ame/z2/",file="ame_z2b0210",head="ame_z2b",size="z2",lv=2.3999999999999999,id=13,face="b0047h",hoho="h"},{"fgact",ch="天梨",id=13,act="うんうん",lv=2.3999999999999999}}},linknext="block_00071",line=391,text={vo={{"vo",file="fem_ame_00881",ch="ame"}},ja={{"「呃，不要啦，好害羞！在大家面前说学长很可爱……呜哇啊啊啊，对不起啊！」",{"rt2"},name={"天梨"}}}}},block_00073={{"fg",ch="天梨",mode=3,ex05="ame_z2a0010",path=":fg/ame/z2/",ex03="a0097",file="ame_z2a0210",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0009h",hoho="h"},{"text"},linkback="block_00072",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z2a0010",path=":fg/ame/z2/",ex03="a0097",file="ame_z2a0210",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0029h",hoho="h"},{"fgact",ch="天梨",id=13,act="いいえ",lv=2.3999999999999999}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z2a0000",path=":fg/ame/z2/",file="ame_z2a0200",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0012h",hoho="h"},{"fgact",ch="天梨",id=13,act="うんうん",lv=2.3999999999999999}}},linknext="block_00074",line=413,text={vo={{"vo",file="fem_ame_00883",ch="ame"}},ja={{"「你说的课堂上的事情，你真的全部都听到了吗！太害羞了，别再说了！我、我不会再因为这个道歉了！」",{"rt2"},name={"天梨"}}}}},block_00031={{"text"},linkback="block_00030",linknext="block_00032",line=168,text={ja={{"「我无法停止喜欢那个直率、酷炫，却又极其可爱，坚韧且一直向我示爱的天梨」",{"rt2"},name={"智宏"}}}}},block_00047={{"text"},linkback="block_00046",linknext="block_00048",line=260,text={ja={{"「嗯。说实话，我偶尔也会想起那个秘密账号的事情。但是，既然天梨正在努力忘记，我也会忘记的」",{"rt2"},name={"智宏"}}}}},block_00001={{"text"},linkback="block_00000",linknext="block_00002",line=16,text={ja={{"我们谈论了关于文化祭的计划，如在舞台上的走位以及演出的展示方式等，但后来意识到「这和做学生会的工作没什么两样」，就不再谈论文化祭的事情了。",{"rt2"}}}}},block_00030={{"text"},linkback="block_00029",linknext="block_00031",line=165,text={ja={{"「但是，我那天表白的原因，是因为我喜欢上了在别人面前大方地说出喜欢我的天梨，我无法抑制自己对她的喜欢」",{"rt2"},name={"智宏"}}}}},block_00071={{"text"},linkback="block_00070",linknext="block_00072",line=394,text={ja={{"「我一直在担心你会不会觉得我讲的历史故事无聊，看到你在城堡玩得开心，我很高兴」",{"rt2"},name={"智宏"}}}}},block_00064={{"fg",ch="天梨",mode=3,ex05="ame_z2a0000",path=":fg/ame/z2/",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0038",file="ame_z2a0200"},{"text"},linkback="block_00063",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z2a0000",path=":fg/ame/z2/",head="ame_z2a",size="z2",lv=2.3999999999999999,id=13,face="a0034",file="ame_z2a0200"}}},linknext="block_00065",line=359,text={vo={{"vo",file="fem_ame_00878",ch="ame"}},ja={{"「当学长告诉我「有正经事要谈」时，我真的有点害怕，所以能听到这么开心的事情真是太好了」",{"rt2"},name={"天梨"}}}}},block_00068={{"fg",ch="天梨",mode=3,ex05="ame_z2b0000",path=":fg/ame/z2/",head="ame_z2b",size="z2",lv=2.3999999999999999,id=13,face="b0010",file="ame_z2b0200"},{"text"},linkback="block_00067",linknext="block_00069",line=375,text={ja={{"「当我和天梨去城堡的时候，你在午餐时盯着我看，我没想到。我很害羞」",{"rt2"},name={"智宏"}}}}},block_00025={{"text"},linkback="block_00024",linknext="block_00026",line=140,text={ja={{"「啊，不，真的！真的！是我不对！我不应该去听！像是偷听天梨的心声，我真的非常抱歉！」",{"rt2"},name={"智宏"}}}}},block_00033={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0002",file="ame_z1a0200"},{"fgact",ch="天梨",id=13,act="うなづく",lv=2.2999999999999998},{"text"},linkback="block_00032",linknext="block_00034",line=177,text={vo={{"vo",file="fem_ame_00859",ch="ame"}},ja={{"「嗯，嗯」",{"rt2"},name={"天梨"}}}}},block_00060={{"fg",ch="天梨",mode=3,ex05="ame_z1b0000",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0054",file="ame_z1b0200"},{"fgact",ch="天梨",id=13,act="うんうん",lv=2.2999999999999998},{"text"},linkback="block_00059",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0006",file="ame_z1a0200"}}},linknext="block_00061",line=329,text={vo={{"vo",file="fem_ame_00875",ch="ame"}},ja={{"「我喜欢诚实的人！我的模特朋友们都说，最好找个不会偷情的男朋友。这点最重要，在选择交往对象时优先考虑」",{"rt2"},name={"天梨"}}}}},block_00062={{"text"},linkback="block_00061",linknext="block_00063",line=343,text={ja={{"她紧紧地抱着我的手臂。能够为在告白前就知道了天梨的心意这件事道歉真是太好了。现在我没有任何隐瞒，也没有任何愧疚感。",{"rt2"}}}}},block_00057={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0052",file="ame_z1a0210"},{"text"},linkback="block_00056",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0054",file="ame_z1a0200"}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0006",file="ame_z1a0200"},{"fgact",ch="天梨",id=13,act="うんうん",lv=2.2999999999999998}}},linknext="block_00058",line=314,text={vo={{"vo",file="fem_ame_00873",ch="ame"}},ja={{"「啊，喜欢的人也是一生……学长，你是打算负责任的对吧。我也有同样的决心」",{"rt2"},name={"天梨"}}}}},block_00055={{"text"},linkback="block_00054",linknext="block_00056",line=300,text={ja={{"真想大力表扬她。虽然还有半年，但是在承受了那些阴险攻击之后，我的女朋友还能如此积极地振作起来，真想把她表扬得满天飞。",{"rt2"}}}}},block_00054={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0052",file="ame_z1a0210"},{"text"},linkback="block_00053",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0055",file="ame_z1a0210"}}},linknext="block_00055",line=298,text={vo={{"vo",file="fem_ame_00871",ch="ame"}},ja={{"「这样就不会背叛那些孩子们，和智宏在一起的时间也会自然地增加……不知道能不能和智宏变得更亲密呢」",{"rt2"},name={"天梨"}}}}},block_00049={{"text"},linkback="block_00048",linknext="block_00050",line=268,text={ja={{"「就是感觉胸口堵得慌，有种憋闷的感觉」",{"rt2"},name={"智宏"}}}}},block_00048={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0010",file="ame_z1a0200"},{"text"},linkback="block_00047",linknext="block_00049",line=265,text={vo={{"vo",file="fem_ame_00867",ch="ame"}},ja={{"「什么是憋闷？」",{"rt2"},name={"天梨"}}}}},block_00045={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0031",file="ame_z1a0200"},{"fgact",ch="天梨",id=13,act="うなづく",lv=2.2999999999999998},{"text"},linkback="block_00044",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0052",file="ame_z1a0200"}}},linknext="block_00046",line=251,text={vo={{"vo",file="fem_ame_00865",ch="ame"}},ja={{"「我明白了！我本来想让那些孩子们道歉，但如果你能原谅他们，我也不会再去想那些事情了！」",{"rt2"},name={"天梨"}}}}},block_00017={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0046",file="ame_z1a0210"},{"text"},linkback="block_00016",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0012",file="ame_z1a0210"}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0029",file="ame_z1a0210"},{"fgact",ch="天梨",id=13,act="うなづく",lv=2.2999999999999998}}},linknext="block_00018",line=89,text={vo={{"vo",file="fem_ame_00854",ch="ame"}},ja={{"「啊——对不起！那个我也最不能接受！啊——对不起，是我的朋友的错！对不起！」",{"rt2"},name={"天梨"}}}}},block_00004={{"text"},linkback="block_00003",linknext="block_00005",line=22,text={ja={{"'我们即将离开珍宿站'。从珍宿到大野乘坐快速急行需要35分钟。从这里到车站大约需要10分钟。考虑到在车站可能遇见他们的风险，实际上并没有多少时间可以浪费。",{"rt2"}}}}},block_00043={{"fg",ch="天梨",mode=3,ex05="ame_z1a0000",path=":fg/ame/z1/",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0055",file="ame_z1a0200"},{"fgact",ch="天梨",id=13,act="うなづく",lv=2.2999999999999998},{"text"},linkback="block_00042",linknext="block_00044",line=228,text={vo={{"vo",file="fem_ame_00863",ch="ame"}},ja={{"「嗯，嗯……我明白了……我明白你没有错，也明白你不想对我隐瞒任何事情……」",{"rt2"},name={"天梨"}}}}},block_00037={{"text"},linkback="block_00036",linknext="block_00038",line=196,text={ja={{"「关于我一直保持沉默的事情，首先对此表示抱歉。其次，对于提前听到天梨的心意也表示抱歉」",{"rt2"},name={"智宏"}}}}},block_00067={{"text"},linkback="block_00066",linknext="block_00068",line=370,text={ja={{"「只能再道歉一次，我已经说过要忘记了，你还在道歉。那我也要提一下，本来我不应该知道的，我偷听到的那段对话了」",{"rt2"},name={"智宏"}}}}},block_00028={{"text"},linkback="block_00027",linknext="block_00029",line=157,text={ja={{"「我要明确一点，以免产生误解！我并不是因为先听到了天梨的心意，所以无奈地承认了我的感情」",{"rt2"},name={"智宏"}}}}},block_00027={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",file="ame_z1a0210",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0008h",hoho="h"},{"text"},linkback="block_00026",delay={vl03={{"fg",ch="天梨",mode=3,ex05="ame_z1b0010",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0012",file="ame_z1b0210"},{"fgact",ch="天梨",id=13,act="おじぎ",lv=2.2999999999999998}},vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1b0000",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0002",file="ame_z1b0200"}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z1b0010",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0008",file="ame_z1b0210"}}},linknext="block_00028",line=154,text={vo={{"vo",file="fem_ame_00858",ch="ame"}},ja={{"「啊……啊！所以那天……我一直在想为什么是今天……啊，原来是这样……啊——……」",{"rt2"},name={"天梨"}}}}},block_00020={{"fg",ch="天梨",mode=3,ex05="ame_z1b0010",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0038",file="ame_z1b0210"},{"fgact",ch="天梨",id=13,act="おじぎ",lv=2.2999999999999998},{"text"},linkback="block_00019",delay={vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1b0000",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0002",file="ame_z1b0200"}}},linknext="block_00021",line=104,text={vo={{"vo",file="fem_ame_00855",ch="ame"}},ja={{"「嗯，嗯。但是呃……只靠我生气也解决不了问题吧……」",{"rt2"},name={"天梨"}}}}},block_00024={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",file="ame_z1a0210",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0014h",hoho="h"},{"fgact",ch="天梨",id=13,act="いいえ",lv=2.2999999999999998},{"text"},linkback="block_00023",delay={vl03={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",file="ame_z1a0210",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0045h",hoho="h"}},vl04={{"fgact",ch="天梨",id=13,act="いいえ",lv=2.2999999999999998},{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",file="ame_z1a0210",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0047h",hoho="h"}},vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1b0000",path=":fg/ame/z1/",file="ame_z1b0200",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0037h",hoho="h"},{"fgact",ch="天梨",id=13,act="縦揺れ",lv=2.2999999999999998}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z1b0010",path=":fg/ame/z1/",file="ame_z1b0210",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0012h",hoho="h"},{"fgact",ch="天梨",id=13,act="びっくり",lv=2.2999999999999998}}},linknext="block_00025",line=137,text={vo={{"vo",file="fem_ame_00857",ch="ame"}},ja={{"「啊——怎么办，那个是被学长听到了吗！啊——好烦！我，啊，我……啊啊啊啊啊——！对不起啊——！」",{"rt2"},name={"天梨"}}}}},block_00023={{"fg",ch="天梨",mode=3,ex05="ame_z1b0010",path=":fg/ame/z1/",head="ame_z1b",size="z1",lv=2.2999999999999998,id=13,face="b0046",file="ame_z1b0210"},{"fgact",ch="天梨",id=13,act="quake",lv=2.2999999999999998},{"text"},linkback="block_00022",delay={vl03={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",file="ame_z1a0210",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0037h",hoho="h"},{"fgact",ch="天梨",id=13,act="おじぎ",lv=2.2999999999999998}},vl01={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",file="ame_z1a0210",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0014h",hoho="h"}},vl02={{"fg",ch="天梨",mode=3,ex05="ame_z1a0010",path=":fg/ame/z1/",ex03="a0097",file="ame_z1a0210",head="ame_z1a",size="z1",lv=2.2999999999999998,id=13,face="a0046h",hoho="h"},{"fgact",ch="天梨",id=13,act="いいえ",lv=2.2999999999999998}}},linknext="block_00024",line=124,text={vo={{"vo",file="fem_ame_00856",ch="ame"}},ja={{"「啊啊啊啊——！那、那个，那个，说起来我那时候说喜欢智宏……啊、啊啊、啊啊啊啊啊啊——！」",{"rt2"},name={"天梨"}}}}},block_00009={{"text"},linkback="block_00008",linknext="block_00010",line=45,text={ja={{"「争吵……也不能算是争吵。口角……也不算是争执。我无意间听到了天梨她们的对话」",{"rt2"},name={"智宏"}}}}}}