astver = 2.0
ast = {block_00022={{"fg",face="b0023h",mode=3,file="kan_z1b1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"},{"text"},text={vo={{"vo",file="fem_kan_10006",ch="kan"}},ja={{"「所以我就只做了准备，但觉得既然是我的生日就没关系了……想被你做很多色色的事情」",{"rt2"},name={"華乃"}}}},linkback="block_00021",delay={vl01={{"fg",face="b0032h",mode=3,file="kan_z1b1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"}}},linknext="block_00023",line=130},block_00043={{"fg",face="b0019h",mode=3,file="kan_z1b1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"},{"text"},line=229,text={ja={{"华乃并没有在意我泼冷水般的冷知识，她犹豫地，但又似乎期待着什么，脸颊泛红，张开了嘴。",{"rt2"}}}},linknext="block_00044",linkback="block_00042"},block_00015={{"fg",face="a0009",mode=3,file="kan_noa1000",ch="華乃",lv=2.2000000000000002,ex05="kan_noa0000",size="no",id=13,path=":fg/kan/no/",head="kan_noa"},{"text"},line=95,text={ja={{"毕竟这不是玩具，而是真实的东西……虽然不是真的，但在成人用品店卖的都是相当稳固的东西……在哪里买的？",{"rt2"}}}},linknext="block_00016",linkback="block_00014"},block_00087={{"text"},line=458,text={ja={{"「我就是为了这个才来的。只有在疼爱华乃的时候，我才最能感受到作为恋人的幸福。我的华乃真是可爱」",{"rt2"},name={"智宏"}}}},linknext="block_00088",linkback="block_00086"},block_00059={{"text"},line=330,text={ja={{"你问哪里不行，其实全都不行……但如果硬要从火中取栗的话。",{"rt2"}}}},linknext="block_00060",linkback="block_00058"},block_00079={{"text"},line=429,text={vo={{"vo",file="fem_kan_10034",ch="kan"}},ja={{"「今天，请答应我的请求……」",{"rt2"},name={"華乃"}}}},linknext="block_00080",linkback="block_00078"},block_00028={{"text"},line=156,text={ja={{"「啊，但我……并不是个S，我觉得我可能做不出对恋人太过分的事，可能达不到华乃你百分百的期待」",{"rt2"},name={"智宏"}}}},linknext="block_00029",linkback="block_00027"},block_00068={{"text"},line=374,text={ja={{"不过今天她看起来很兴奋，如果真让她口交，一定会感觉非常棒……对此我还是有些向往的。",{"rt2"}}}},linknext="block_00069",linkback="block_00067"},block_00042={{"text"},line=225,text={ja={{"「「稍微提一下」这个词经常被用来指只是触及话题的一小部分，但实际上，它似乎是用来指代话题最重要的部分的。」",{"rt2"},name={"智宏"}}}},linknext="block_00043",linkback="block_00041"},block_00026={{"fg",face="a0046h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"},{"fgact",id=13,act="びっくり",ch="華乃",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10008",ch="kan"}},ja={{"「啊，等等！光是想象就让我心跳加速了！稍、稍等一下，让我先平复一下呼吸」",{"rt2"},name={"華乃"}}}},linkback="block_00025",delay={vl01={{"fg",face="a0028h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"}}},linknext="block_00027",line=148},block_00008={{"msgoff"},{"bg",zoom=120,ay=720,by=-360,bx=-640,file="zbg006a",sync=0,path=":bg/",ax=1280},{"fg",face="b0001",mode=3,sync=0,file="kan_z2b1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0000",size="z2",head="kan_z2b",id=13,path=":fg/kan/z2/",resize=1},{"extrans",time=500},{"text"},line=68,text={ja={{"于是我轻轻地推了华乃——刚要动手，却被她阻止了。",{"rt2"}}}},linknext="block_00009",linkback="block_00007"},block_00030={{"text"},line=166,text={ja={{"「嗯、嗯。如果是这样的话」",{"rt2"},name={"智宏"}}}},linknext="block_00031",linkback="block_00029"},block_00086={{"text"},line=455,text={vo={{"vo",file="fem_kan_10037",ch="kan"}},ja={{"「你会好好的对待我，像对待一个女孩子一样，温柔地疼爱我吗……？」",{"rt2"},name={"華乃"}}}},linknext="block_00087",linkback="block_00085"},block_00009={{"fg",mode=-1,mx=-40,id=13,ch="華乃",lv=2.3999999999999999},{"text"},line=72,text={ja={{"我怀疑着什么，保持这个姿势不动，华乃依然躲避着我的目光，慢慢从我手臂下滑出。",{"rt2"}}}},linknext="block_00010",linkback="block_00008"},block_00029={{"fg",face="a0007h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"},{"text"},text={vo={{"vo",file="fem_kan_10009",ch="kan"}},ja={{"「我、我也不是想被做过分的事情！只是那个，想试试被喜欢的人随心所欲的对待，或者说被他本能地玩弄……」",{"rt2"},name={"華乃"}}}},linkback="block_00028",delay={vl02={{"fg",face="a0019h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"}},vl01={{"fg",face="a0020h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"}}},linknext="block_00030",line=163},block_00032={{"text"},line=170,text={ja={{"好，我全身血气上涌。既然本人都请求我这样做了，那我就做得更淫荡一些吧，嘿嘿。",{"rt2"}}}},linknext="block_00033",linkback="block_00031"},block_00017={{"text"},line=107,text={ja={{"「吓……并没有。只是没想到会发展成这样，所以有些慌乱。」",{"rt2"},name={"智宏"}}}},linknext="block_00018",linkback="block_00016"},block_00025={{"fg",face="a0019h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"},{"text"},text={vo={{"vo",file="fem_kan_10007",ch="kan"}},ja={{"「正因为有点害怕才好。我想被任意摆布……不知道会被做什么更好」",{"rt2"},name={"華乃"}}}},linkback="block_00024",delay={vl01={{"fg",face="a0039h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"}}},linknext="block_00026",line=141},block_00010={{"bg",file="bg006a",time=500,path=":bg/"},{"se",file="se物音",id=1},{"text"},line=78,text={ja={{"一度以为我被拒绝了，没过多久，华乃突然站了起来，朝衣柜的方向走去。",{"rt2"}}}},linknext="block_00011",linkback="block_00009"},block_00050={{"bgm",file="bgm29"},{"fg",face="b0010",mode=3,file="kan_z1b1010",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0010",size="z1",id=13,path=":fg/kan/z1/",head="kan_z1b"},{"text"},text={vo={{"vo",file="fem_kan_10019",ch="kan"}},ja={{"「诶……诶等、等一下？ 诶，那个？ 啊？ 啊啊！？ 啊啊啊啊啊啊啊！？」",{"rt2"},name={"華乃"}}}},linkback="block_00049",delay={vl01={{"fg",face="b0046h",mode=3,file="kan_z1b1010",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0010",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"},{"fgact",id=13,act="びっくり",ch="華乃",lv=2.2999999999999998}},vl02={{"fg",face="a0008h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"}},vl03={{"fg",face="a0045h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"},{"fgact",id=13,act="quake",ch="華乃",lv=2.2999999999999998}}},linknext="block_00051",line=291},block_00037={{"text"},line=198,text={ja={{"「但是，揭示自己不能对他人说的部分，不也会让人兴奋吗？因为那是心底深处隐藏的部分，我希望你只向我展示。」",{"rt2"},name={"智宏"}}}},linknext="block_00038",linkback="block_00036"},block_00045={{"fg",face="a0052h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"},{"text"},text={vo={{"vo",file="fem_kan_10015",ch="kan"}},ja={{"「我想在床上被迫四脚朝天，阴茎在我的阴唇上摩擦，被持续挑逗，然后我会乞求「请给我阴茎」。」",{"rt2"},name={"華乃"}}}},linkback="block_00044",delay={vl02={{"fg",face="a0034h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"}},vl01={{"fg",face="a0012h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"}}},linknext="block_00046",line=248},block_00069={{"fg",face="a0052",mode=3,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,path=":fg/kan/z2/",head="kan_z2a"},{"fgact",id=13,act="ジャンプ",ch="華乃",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10028",ch="kan"}},ja={{"「啊，对了……如果你不能抓我的头发，那颈圈可以吗？」",{"rt2"},name={"華乃"}}}},linkback="block_00068",delay={vl01={{"fg",face="a0006",mode=3,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,path=":fg/kan/z2/",head="kan_z2a"}}},linknext="block_00070",line=381},block_00014={{"text"},line=91,text={ja={{"因为是生日嘛……我原本以为会有甜蜜的爱爱，但现在我却被我想象中的幻想和眼前的手铐的差距吓到了。",{"rt2"}}}},linknext="block_00015",linkback="block_00013"},block_00089={{"text"},line=477,text={ja={{"华乃爬到我的腿上，开始撒娇。在蹭了蹭我的脸颊之后，她亲了我几次。",{"rt2"}}}},linknext="block_00090",linkback="block_00088"},block_00072={{"fg",face="a0012",mode=3,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,path=":fg/kan/z2/",head="kan_z2a"},{"text"},text={vo={{"vo",file="fem_kan_10029",ch="kan"}},ja={{"「就、就轻轻地就好……我也不希望你用力过猛」",{"rt2"},name={"華乃"}}}},linkback="block_00071",delay={vl01={{"fg",face="a0018",mode=3,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,path=":fg/kan/z2/",head="kan_z2a"}}},linknext="block_00073",line=396},block_00093={{"msgoff"},{"bg",ax=1280,ay=720,by=-360,bx=-640,file="zbg006a",path=":bg/",sync=0},{"fg",face="b0052h",mode=3,sync=0,file="kan_z1b1110",ch="華乃",lv=2.2999999999999998,id=13,ex05="kan_z1b0010",size="z1",head="kan_z1b",hoho="h",path=":fg/kan/z1/",resize=1},{"extrans",time=500},{"text"},text={vo={{"vo",file="fem_kan_10041",ch="kan"}},ja={{"「话说，如果你想让我穿什么我就穿什么，如果你想把我当做玩具，我这儿还有束缚装备？」",{"rt2"},name={"華乃"}}}},linkback="block_00092",delay={vl01={{"fg",face="b0015h",mode=3,file="kan_z1b1110",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0010",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"}}},linknext="block_00094",line=506},block_00004={{"bgm",stop=1},{"cgdel",id=-1},{"bg",file="white",time=1000,path=":bg/"},{"bg",file="bg006a",path=":bg/",sync=0},{"fg",face="b0053h",mode=3,sync=0,file="kan_nob1100",ch="華乃",lv=2.2000000000000002,ex05="kan_nob0000",size="no",id=13,hoho="h",path=":fg/kan/no/",head="kan_nob"},{"extrans"},{"bgm",file="bgm02"},{"text"},line=46,text={ja={{"在恋情开始的第五个月庆祝恋人的生日，正好是个适合两人独处庆祝的周日。",{"rt2"}}}},linknext="block_00005",linkback="block_00003"},label={top={label=1,block="block_00000"}},block_00058={{"msgoff"},{"bg",zoom=120,ay=720,by=-360,bx=-640,file="zbg006a",sync=0,path=":bg/",ax=1280},{"fg",face="a0003",mode=3,sync=0,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",head="kan_z2a",id=13,path=":fg/kan/z2/",resize=1},{"extrans",time=500},{"text"},line=328,text={vo={{"vo",file="fem_kan_10022",ch="kan"}},ja={{"「嗯，那我问你……哪里不行了？」",{"rt2"},name={"華乃"}}}},linknext="block_00059",linkback="block_00057"},block_00056={{"fg",face="a0012",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,path=":fg/kan/z1/",head="kan_z1a"},{"text"},text={vo={{"vo",file="fem_kan_10021",ch="kan"}},ja={{"「那、那个……但是今天，是我的生日……」",{"rt2"},name={"華乃"}}}},linkback="block_00055",delay={vl01={{"fg",face="a0018",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,path=":fg/kan/z1/",head="kan_z1a"}}},linknext="block_00057",line=316},block_00023={{"text"},line=132,text={ja={{"听到这些话，我原本有些萎靡的欲望完全觉醒了。对，我也想在华乃的生日那天好好爱爱。",{"rt2"}}}},linknext="block_00024",linkback="block_00022"},block_00018={{"text"},line=110,text={ja={{"「今天是华乃的生日嘛。我想尽可能满足你的愿望。」",{"rt2"},name={"智宏"}}}},linknext="block_00019",linkback="block_00017"},block_00035={{"fg",face="a0009h",mode=3,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2a"},{"fgact",id=13,act="いいえ",ch="華乃",lv=2.3999999999999999},{"text"},text={vo={{"vo",file="fem_kan_10010",ch="kan"}},ja={{"「啊……但，那样的话，即使是你也会退缩的吧，我会暴露出自己的幻想……呃，即使是面对喜欢的人，也会犹豫吧？」",{"rt2"},name={"華乃"}}}},linkback="block_00034",delay={vl01={{"fg",face="a0002h",mode=3,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2a"}}},linknext="block_00036",line=189},block_00063={{"text"},line=350,text={ja={{"「等等。如果让你这么做，华乃你会觉得开心吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00064",linkback="block_00062"},block_00052={{"text"},line=295,text={ja={{"因为我并不认为「稍微提一下」这个词在这里被正确地使用……我以为她只是想尝试一些稍微变态的玩法，结果她直接说出了核心……",{"rt2"}}}},linknext="block_00053",linkback="block_00051"},block_00066={{"fg",face="a0035h",mode=3,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2a"},{"text"},text={vo={{"vo",file="fem_kan_10027",ch="kan"}},ja={{"「我想随心所欲地玩弄他的命根子，用手活和口交让他彻底地一次又一次地达到高潮，但是今天我想被虐待」",{"rt2"},name={"華乃"}}}},linkback="block_00065",delay={vl01={{"fg",face="a0032h",mode=3,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2a"}}},linknext="block_00067",line=370},block_00019={{"fg",face="a0008h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"},{"text"},text={vo={{"vo",file="fem_kan_10004",ch="kan"}},ja={{"「啊。你真的好温柔……谢谢你，我好感动……」",{"rt2"},name={"華乃"}}}},linkback="block_00018",delay={vl01={{"fg",face="a0023h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"}}},linknext="block_00020",line=116},block_00064={{"fg",face="b0022h",mode=3,file="kan_z2b1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"},{"text"},text={vo={{"vo",file="fem_kan_10025",ch="kan"}},ja={{"「我喜欢口交的时候，感觉像在尽心尽力地侍奉……我也喜欢看他没有后备的表情，想看他在我的口中拼命地摇动腰部」",{"rt2"},name={"華乃"}}}},linkback="block_00063",delay={vl02={{"fg",face="b0035h",mode=3,file="kan_z2b1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"}},vl01={{"fg",face="b0031h",mode=3,file="kan_z2b1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"}}},linknext="block_00065",line=357},block_00074={{"fg",face="a0046",mode=3,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,path=":fg/kan/z2/",head="kan_z2a"},{"fgact",id=13,act="おじぎ",ch="華乃",lv=2.3999999999999999},{"text"},line=405,text={vo={{"vo",file="fem_kan_10030",ch="kan"}},ja={{"「拜托了！只要轻轻地『啪！』一下就好了！」",{"rt2"},name={"華乃"}}}},linknext="block_00075",linkback="block_00073"},block_00095={{"bgm",stop=1,time=2000},{"bg",file="white",time=2000,path=":bg/"},{"excall",file="02_華乃01_h01"},{"text"},line=516,linkback="block_00094"},block_00067={{"text"},line=372,text={ja={{"她又开始陶醉了……什么叫做想被虐待的心情。她在这种心情的日子里是怎么度过的，我的女朋友。",{"rt2"}}}},linknext="block_00068",linkback="block_00066"},block_00094={{"text"},line=509,text={ja={{"「那个……还是等我积累一些经验再说吧」",{"rt2"},name={"智宏"}}}},linknext="block_00095",linkback="block_00093"},block_00001={{"msgoff"},{"bg",set="sd_kan_31",id=6,tagreset="1",sync=0,file="sd_kan_31_01",time=400,path=":sd/sd_kan_31/",lv=5},{"extrans"},{"bgm",file="bgm05"},{"text"},line=24,text={ja={{"——即便是这样，那位恋人此刻却在我面前跪坐。",{"rt2"}}}},linknext="block_00002",linkback="block_00000"},block_00092={{"text"},line=495,text={ja={{"我再次抚摩她的后脑勺，她把脸颊贴在我的肩膀上，让身体更靠近我。即将能和她那份幸福的亲密，让我深感欢喜……",{"rt2"}}}},linknext="block_00093",linkback="block_00091"},block_00091={{"fg",face="b0006h",mode=3,file="kan_z2b1110",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0010",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"},{"text"},text={vo={{"vo",file="fem_kan_10040",ch="kan"}},ja={{"「当然，你也是我的。你是我的男朋友，那个会温馨照顾我的男朋友。我喜欢你。」",{"rt2"},name={"華乃"}}}},linkback="block_00090",delay={vl02={{"fg",face="b0055h",mode=3,file="kan_z2b1110",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0010",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"}},vl01={{"fg",face="b0031h",mode=3,file="kan_z2b1110",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0010",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"}}},linknext="block_00092",line=493},block_00020={{"text"},line=118,text={ja={{"华乃露出害羞的开心表情，有些恍惚地看着我。那表情本身倒是很适合生日的恋人。只是如果没有手铐在手里就更好了。",{"rt2"}}}},linknext="block_00021",linkback="block_00019"},block_00031={{"text"},line=168,text={ja={{"如果我可以随便摆弄华乃的身体，作为男人可不是正好呢。如果可以随便抚摸这对规格外的大胸和全身的柔软肉感……",{"rt2"}}}},linknext="block_00032",linkback="block_00030"},block_00090={{"bgm",file="bgm18"},{"fg",face="a0055h",mode=3,file="kan_z2a1100",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2a"},{"text"},text={vo={{"vo",file="fem_kan_10039",ch="kan"}},ja={{"「嘿嘿，我被你称作「我的华乃」，我好喜欢「我的」这个词。我是你的，是你喜欢的人的东西，我喜欢被你这样对待。」",{"rt2"},name={"華乃"}}}},linkback="block_00089",delay={vl02={{"fg",face="a0055h",mode=3,file="kan_z2a1100",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2a"}},vl01={{"fg",face="a0022h",mode=3,file="kan_z2a1100",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2a"}}},linknext="block_00091",line=486},block_00002={{"text"},line=29,text={vo={{"vo",file="fem_kan_10001",ch="kan"}},ja={{"「请答应我今天的请求」",{"rt2"},name={"華乃"}}}},linknext="block_00003",linkback="block_00001"},block_00071={{"fg",face="a0045",mode=3,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,path=":fg/kan/z2/",head="kan_z2a"},{"fgact",act="いいえ",id=13,time=80,ch="華乃",lv=2.3999999999999999},{"text"},line=390,text={ja={{"「还有像打屁股这种事也是不行的」",{"rt2"},name={"智宏"}}}},linknext="block_00072",linkback="block_00070"},block_00044={{"bgm",stop=1},{"fg",face="b0052h",mode=3,file="kan_z1b1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"},{"text"},text={vo={{"vo",file="fem_kan_10014",ch="kan"}},ja={{"「我想在喉咙深处被强行塞进阴茎，然后被迫口交，之后被赞赏说「做得很好」，然后抚摸我的头，作为奖励，我想被深深地亲吻。」",{"rt2"},name={"華乃"}}}},linkback="block_00043",delay={vl01={{"fg",face="b0035h",mode=3,file="kan_z1b1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"}}},linknext="block_00045",line=240},block_00061={{"fg",face="a0009",mode=3,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,path=":fg/kan/z2/",head="kan_z2a"},{"text"},text={vo={{"vo",file="fem_kan_10023",ch="kan"}},ja={{"「啊，那扯头发就算了！虽然视觉上我挺喜欢的……但是被扯头发会疼的！嗯，现实中我还是放弃吧！」",{"rt2"},name={"華乃"}}}},linkback="block_00060",delay={vl02={{"fg",face="a0003",mode=3,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,path=":fg/kan/z2/",head="kan_z2a"},{"fgact",id=13,act="うなづく",ch="華乃",lv=2.3999999999999999}},vl01={{"fg",face="a0033",mode=3,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,path=":fg/kan/z2/",head="kan_z2a"}}},linknext="block_00062",line=341},block_00055={{"text"},line=310,text={ja={{"「请等一下。华乃也应该无法接受与自己的性癖好不符的性行为吧」",{"rt2"},name={"智宏"}}}},linknext="block_00056",linkback="block_00054"},block_00078={{"text"},line=426,text={vo={{"vo",file="fem_kan_10033",ch="kan"}},ja={{"「为了今天我调整了身体……甚至计算了日子，准备好即使你在我体内射精也没关系……我都做好准备了……所以……」",{"rt2"},name={"華乃"}}}},linknext="block_00079",linkback="block_00077"},block_00060={{"text"},line=333,text={ja={{"「虐待的话，我还能尽力去做，但是暴力就不行了……华乃不是也说过不喜欢疼痛吗？比如扯头发之类的」",{"rt2"},name={"智宏"}}}},linknext="block_00061",linkback="block_00059"},block_00088={{"bgm",stop=1},{"msgoff"},{"bg",zoom=120,ay=720,by=-360,bx=-640,file="zbg006a",sync=0,path=":bg/",ax=1280},{"fg",face="a0035h",mode=3,sync=0,file="kan_z2a1100",ch="華乃",lv=2.3999999999999999,ex05="kan_z2a0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2a"},{"extrans",time=500},{"fgact",id=13,act="ジャンプ",ch="華乃",lv=2.3999999999999999},{"text"},line=475,text={vo={{"vo",file="fem_kan_10038",ch="kan"}},ja={{"「咕噜～」",{"rt2"},name={"華乃"}}}},linknext="block_00089",linkback="block_00087"},block_00085={{"text"},line=452,text={ja={{"「那是肯定的。我会让华乃坐在我的膝盖上，一边抱着你，一边摸摸你的头，还会亲吻你。不论是额头，还是脸颊，甚至鼻尖，我都会亲。」",{"rt2"},name={"智宏"}}}},linknext="block_00086",linkback="block_00084"},block_00084={{"se",file="seコミ_目を輝かせる",id=1},{"bg",set="sd_kan_31",face="1",id=6,sync=0,file="sd_kan_31_04",time=400,path=":sd/sd_kan_31/",lv=5},{"text"},line=449,text={vo={{"vo",file="fem_kan_10036",ch="kan"}},ja={{"「真的吗……？做完之后，会紧紧抱住我，摸摸我的头吗……？」",{"rt2"},name={"華乃"}}}},linknext="block_00085",linkback="block_00083"},block_00083={{"text"},line=442,text={ja={{"「当然会。说起来，其实我今天也非常期待这个呢」",{"rt2"},name={"智宏"}}}},linknext="block_00084",linkback="block_00082"},block_00082={{"bg",set="sd_kan_31",face="1",id=6,sync=0,file="sd_kan_31_02",time=400,path=":sd/sd_kan_31/",lv=5},{"text"},line=439,text={vo={{"vo",file="fem_kan_10035",ch="kan"}},ja={{"「真的吗……？你是真心的，而不是出于义务，会和我做爱吗……？」",{"rt2"},name={"華乃"}}}},linknext="block_00083",linkback="block_00081"},block_00081={{"text"},line=434,text={ja={{"我觉得能永远拥抱你，和你过一生的甜蜜生活，根本不需要打你的屁股。但是，我又不想看到我的恋人跪在我面前，所以只好答应她的请求。",{"rt2"}}}},linknext="block_00082",linkback="block_00080"},block_00013={{"text"},line=89,text={ja={{"虽然这是个色情的请求，但不是简单的换衣服，而是要求进行变态的游戏。",{"rt2"}}}},linknext="block_00014",linkback="block_00012"},block_00054={{"fg",face="a0046h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"},{"text"},text={vo={{"vo",file="fem_kan_10020",ch="kan"}},ja={{"「等一下……你、你让人家，说出不仅仅是羞耻，而且作为女性是致命的事情……啊啊啊啊啊！？ 不行吗？ 啊啊啊啊啊！？」",{"rt2"},name={"華乃"}}}},linkback="block_00053",delay={vl01={{"fg",face="a0042h",mode=3,ex01="a0099",file="kan_z1a1010",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0010",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"},{"fgact",id=13,act="びっくり",ch="華乃",lv=2.2999999999999998}},vl02={{"fg",face="a0036",mode=3,ex01="a0099",file="kan_z1a1010",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0010",size="z1",id=13,path=":fg/kan/z1/",head="kan_z1a"}},vl03={{"fg",face="a0046",mode=3,file="kan_z1a1010",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0010",size="z1",id=13,path=":fg/kan/z1/",head="kan_z1a"},{"fgact",id=13,act="quake",ch="華乃",lv=2.2999999999999998}}},linknext="block_00055",line=307},block_00053={{"text"},line=297,text={ja={{"这不仅仅是提出一点希望，而是性癖好中最猥亵的部分……等等，刚才那个是华乃的极限吧？ 她不会是刚才还有所保留吧？",{"rt2"}}}},linknext="block_00054",linkback="block_00052"},block_00070={{"text"},line=384,text={ja={{"「你说有颈圈，但是你现在就这么直接说出来，就像是觉得「既然不能抓头发，那就用颈圈拉我」这种想法很正常一样。这绝对不行」",{"rt2"},name={"智宏"}}}},linknext="block_00071",linkback="block_00069"},block_00075={{"msgoff"},{"fg",mode=-2,sync=0},{"bg",set="sd_kan_31",id=6,tagreset="1",sync=0,file="sd_kan_31_01",time=400,path=":sd/sd_kan_31/",lv=5},{"extrans",time=500},{"se",file="se倒れる(ベッド)",id=1},{"text"},line=417,text={ja={{"我深爱的恋人就在我眼前开始跪下。她低头的力度使得礼物的贝雷帽飞了出去。这突如其来的情景让我感到震惊，甚至无法去阻止。",{"rt2"}}}},linknext="block_00076",linkback="block_00074"},block_00024={{"text"},line=135,text={ja={{"「只是束缚住身体的动作就好吗？哇，真厉害……要是我，把身体的自由完全交给对方还是有点害怕的」",{"rt2"},name={"智宏"}}}},linknext="block_00025",linkback="block_00023"},block_00073={{"text"},line=399,text={ja={{"「就算是轻轻地……我也没有打人的兴趣啊」",{"rt2"},name={"智宏"}}}},linknext="block_00074",linkback="block_00072"},block_00076={{"text"},line=420,text={vo={{"vo",file="fem_kan_10031",ch="kan"}},ja={{"「拜托了！我一直都很期待今天这一天！」",{"rt2"},name={"華乃"}}}},linknext="block_00077",linkback="block_00075"},block_00047={{"fg",face="b0008h",mode=3,file="kan_z1b1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"},{"fgact",id=13,act="びっくり",ch="華乃",lv=2.2999999999999998},{"text"},text={vo={{"vo",file="fem_kan_10017",ch="kan"}},ja={{"「当，当然，我并不是想让任何人这样对我，只有我喜欢的人，即使被虐待，我也会觉得很舒服……因为我会把我的身体交给他！」",{"rt2"},name={"華乃"}}}},linkback="block_00046",delay={vl02={{"fg",face="b0007h",mode=3,file="kan_z1b1010",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0010",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"}},vl01={{"fg",face="b0002h",mode=3,file="kan_z1b1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"}}},linknext="block_00048",line=266},block_00034={{"text"},line=182,text={ja={{"「我按照华乃的说法行动，可能并不是华乃真正希望的，但我还是想听听……哪怕只是一部分。」",{"rt2"},name={"智宏"}}}},linknext="block_00035",linkback="block_00033"},block_00062={{"fg",face="b0002h",mode=3,file="kan_z2b1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"},{"text"},text={vo={{"vo",file="fem_kan_10024",ch="kan"}},ja={{"「但是我还是想试试被掐住头做口交……那个，像口交玩具那样……」",{"rt2"},name={"華乃"}}}},linkback="block_00061",delay={vl01={{"fg",face="b0035h",mode=3,file="kan_z2b1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"}}},linknext="block_00063",line=347},block_00005={{"text"},line=48,text={ja={{"我买了两块小蛋糕，去了她的房间。她的父母都不在家，直到晚上才会回来。",{"rt2"}}}},linknext="block_00006",linkback="block_00004"},block_00048={{"fg",face="b0019h",mode=3,file="kan_z1b1010",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0010",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"},{"text"},text={vo={{"vo",file="fem_kan_10018",ch="kan"}},ja={{"「如果是你的话，我觉得被当做工具也没关系，我想试试被当做玩具……怎、怎么样？」",{"rt2"},name={"華乃"}}}},linkback="block_00047",delay={vl01={{"fg",face="b0035h",mode=3,file="kan_z1b1010",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0010",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"}}},linknext="block_00049",line=274},block_00080={{"text"},line=432,text={ja={{"「我、我知道了，就别再跪着了」",{"rt2"},name={"智宏"}}}},linknext="block_00081",linkback="block_00079"},block_00006={{"msgoff"},{"bg",ax=1280,ay=720,by=-360,bx=-640,file="zbg006a",path=":bg/",sync=0},{"fg",face="b0023h",mode=3,sync=0,file="kan_z1b1100",ch="華乃",lv=2.2999999999999998,id=13,ex05="kan_z1b0000",size="z1",head="kan_z1b",hoho="h",path=":fg/kan/z1/",resize=1},{"extrans",time=500},{"text"},line=56,text={ja={{"我送给她的礼物是一顶贝雷帽。就在上周，我们一起去选购，是她亲自挑选并满意的商品。",{"rt2"}}}},linknext="block_00007",linkback="block_00005"},block_00007={{"fg",face="a0032h",mode=3,file="kan_z1a1010",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0010",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"},{"text"},line=60,text={ja={{"华乃在我面前戴上了帽子给我看，看到这般情景的我不禁赞叹她的可爱。自然地，我们的气氛变得愉快，不知何时我们紧紧握住了对方的手。",{"rt2"}}}},linknext="block_00008",linkback="block_00006"},block_00003={{"text"},line=31,text={ja={{"我到底怎么会变成现在这样。我开始回想，一切怎么会走到这一步。",{"rt2"}}}},linknext="block_00004",linkback="block_00002"},block_00077={{"text"},line=423,text={vo={{"vo",file="fem_kan_10032",ch="kan"}},ja={{"「我一直期待着生日那天能尽情地做爱……享受到极致的快感，然后被你永远地紧紧拥抱，一辈子都在一起……」",{"rt2"},name={"華乃"}}}},linknext="block_00078",linkback="block_00076"},block_00041={{"msgoff"},{"bg",ax=1280,ay=720,by=-360,bx=-640,file="zbg006a",path=":bg/",sync=0},{"fg",face="b0034h",mode=3,sync=0,file="kan_z1b1010",ch="華乃",lv=2.2999999999999998,id=13,ex05="kan_z1b0010",size="z1",head="kan_z1b",hoho="h",path=":fg/kan/z1/",resize=1},{"extrans",time=500},{"text"},text={vo={{"vo",file="fem_kan_10013",ch="kan"}},ja={{"「那，那只是稍微提一下，我并不是一直在想这种事情……」",{"rt2"},name={"華乃"}}}},linkback="block_00040",delay={vl01={{"fg",face="b0018h",mode=3,file="kan_z1b1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"}}},linknext="block_00042",line=222},block_00027={{"fg",face="a0028h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"},{"fgact",id=13,act="おじぎ",ch="華乃",lv=2.2999999999999998},{"text"},line=153,text={ja={{"华乃弯腰，把手放在胸口，喘息着调整粗重的呼吸。看来她对这个期待非常高……",{"rt2"}}}},linknext="block_00028",linkback="block_00026"},block_00012={{"fg",face="a0035h",mode=3,file="kan_noa1000",ch="華乃",lv=2.2000000000000002,ex05="kan_noa0000",size="no",id=13,hoho="h",path=":fg/kan/no/",head="kan_noa"},{"text"},line=87,text={vo={{"vo",file="fem_kan_10002",ch="kan"}},ja={{"「我想试试被抖M虐待一次」",{"rt2"},name={"華乃"}}}},linknext="block_00013",linkback="block_00011"},block_00049={{"text"},line=279,text={ja={{"「对不起，这对我来说太重了。今天我们就正常地做爱吧」",{"rt2"},name={"智宏"}}}},linknext="block_00050",linkback="block_00048"},block_00057={{"text"},line=319,text={ja={{"「唔，你这么一说……我想满足你的期待，但是，呐！」",{"rt2"},name={"智宏"}}}},linknext="block_00058",linkback="block_00056"},block_00065={{"fg",face="b0019h",mode=3,file="kan_z2b1010",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0010",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"},{"text"},text={vo={{"vo",file="fem_kan_10026",ch="kan"}},ja={{"「偶尔也有想被当成物品的时候……但是，如果做得好，我希望得到表扬。说我想成为宠物，这样说就可以了吧」",{"rt2"},name={"華乃"}}}},linkback="block_00064",delay={vl02={{"fg",face="b0052h",mode=3,file="kan_z2b1010",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0010",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"}},vl01={{"fg",face="b0055h",mode=3,file="kan_z2b1010",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0010",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"}}},linknext="block_00066",line=364},block_00021={{"fg",face="b0019h",mode=3,file="kan_z1b1010",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0010",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"},{"text"},text={vo={{"vo",file="fem_kan_10005",ch="kan"}},ja={{"「其实我一直想试试被狠狠虐待一番，但我们才刚在一起半年，把自己想做的那种爱爱强加给你总觉得……嘛」",{"rt2"},name={"華乃"}}}},linkback="block_00020",delay={vl01={{"fg",face="b0034h",mode=3,file="kan_z1b1010",ch="華乃",lv=2.2999999999999998,ex05="kan_z1b0010",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1b"}}},linknext="block_00022",line=124},block_00046={{"fg",face="a0031h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"},{"text"},text={vo={{"vo",file="fem_kan_10016",ch="kan"}},ja={{"「我想在后面被猛烈地撞击，就像抓住自行车把手一样抓住我的头发，我想让我的脑袋一片空白，感到愉快，如果我露出无力的表情，我希望你能打我的屁股。」",{"rt2"},name={"華乃"}}}},linkback="block_00045",delay={vl02={{"fg",face="a0019h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"}},vl01={{"fg",face="a0048h",mode=3,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",id=13,hoho="h",path=":fg/kan/z1/",head="kan_z1a"}}},linknext="block_00047",line=256},block_00040={{"text"},line=211,text={ja={{"华乃腿间微微摩擦，斜着眼睛望着我。哦哦哦哦哦，这个动作有点太诱人了……我突然很想占有她，交给我吧。",{"rt2"}}}},linknext="block_00041",linkback="block_00039"},block_00036={{"fg",face="b0020h",mode=3,file="kan_z2b1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"},{"text"},text={vo={{"vo",file="fem_kan_10011",ch="kan"}},ja={{"「我会暴露出自己性爱喜好中最尖锐的部分……普段我想要被怎么对待，都会暴露出来。」",{"rt2"},name={"華乃"}}}},linkback="block_00035",delay={vl01={{"fg",face="b0029h",mode=3,file="kan_z2b1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"}}},linknext="block_00037",line=195},block_00000={{"savetitle",text="ぼくの担生日"},{"bg",file="bg099a",time=2000,path=":bg/"},{"text"},line=12,text={ja={{"1月31日。今天是我深爱的恋人的生日——",{"rt2"}}}},linknext="block_00001"},block_00039={{"fg",face="b0017h",mode=3,file="kan_z2b1010",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0010",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"},{"text"},text={vo={{"vo",file="fem_kan_10012",ch="kan"}},ja={{"「你、你真是个变态……那真的很淫荡……笨蛋。」",{"rt2"},name={"華乃"}}}},linkback="block_00038",delay={vl01={{"fg",face="b0019h",mode=3,file="kan_z2b1010",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0010",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"}}},linknext="block_00040",line=209},block_00038={{"fg",face="b0002h",mode=3,file="kan_z2b1000",ch="華乃",lv=2.3999999999999999,ex05="kan_z2b0000",size="z2",id=13,hoho="h",path=":fg/kan/z2/",head="kan_z2b"},{"text"},line=203,text={ja={{"「我想知道华乃平时在想什么，自己一个人在做什么。」",{"rt2"},name={"智宏"}}}},linknext="block_00039",linkback="block_00037"},block_00016={{"msgoff"},{"bg",ax=1280,ay=720,by=-360,bx=-640,file="zbg006a",path=":bg/",sync=0},{"fg",face="a0002",mode=3,sync=0,file="kan_z1a1000",ch="華乃",lv=2.2999999999999998,ex05="kan_z1a0000",size="z1",head="kan_z1a",id=13,path=":fg/kan/z1/",resize=1},{"extrans",time=500},{"text"},line=104,text={vo={{"vo",file="fem_kan_10003",ch="kan"}},ja={{"「啊，啊？你被吓到了……？」",{"rt2"},name={"華乃"}}}},linknext="block_00017",linkback="block_00015"},block_00033={{"msgoff"},{"bg",zoom=120,ay=720,by=-360,bx=-640,file="zbg006a",sync=0,path=":bg/",ax=1280},{"fg",face="a0010h",mode=3,sync=0,file="kan_z2a1000",ch="華乃",lv=2.3999999999999999,id=13,ex05="kan_z2a0000",size="z2",head="kan_z2a",hoho="h",path=":fg/kan/z2/",resize=1},{"extrans",time=500},{"text"},line=179,text={ja={{"「出于谨慎，仅仅作为参考……真的只是作为参考，我可以问问华乃想要被怎么对待吗？」",{"rt2"},name={"智宏"}}}},linknext="block_00034",linkback="block_00032"},block_00051={{"text"},line=293,text={ja={{"就像在电影高潮部分被叫到名字，被拉回现实的观众一样，华乃的脸色从陶醉变为尖叫。",{"rt2"}}}},linknext="block_00052",linkback="block_00050"},block_00011={{"fg",face="a0020h",mode=3,mx=-40,ch="華乃",lv=2.2000000000000002,id=13,head="kan_noa",ex05="kan_noa0000",size="no",file="kan_noa1000",hoho="h",path=":fg/kan/no/",resize=1},{"text"},line=82,text={ja={{"原来是这样，难道是要换上性感的服装来取悦我吗——当我脑海中浮现出这样美好的念头时，眼前却骤然出现了一只硕大的手铐。",{"rt2"}}}},linknext="block_00012",linkback="block_00010"}}