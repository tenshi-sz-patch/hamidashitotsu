astver = 2.0
ast = {block_00038={{"bg",face="1",path=":ev/ev_ame_h34/",ay=720,set="ev_ame_h34",y=250,ax=1280,by=-360,bx=-640,sync=0,x=500,time=400,file="zev_ame_h34_11"},{"text"},linkback="block_00037",linknext="block_00039",text={ja={{"「诶，好过分！这是开长弄上去的哦！？明明是自己射出来的东西，好过分！」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01541"}}},line=179},block_00006={{"bg",face="1",path=":ev/ev_ame_h34/",sync=0,set="ev_ame_h34",time=400,file="ev_ame_h34_07"},{"text"},linkback="block_00005",linknext="block_00007",text={ja={{"「啊、呀、好厉害、啊啊、嗯！呀、啊啊、不行了……啊啊、呀！忍不住了……啊啊、呀！」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01515"}}},line=46},block_00022={{"text"},linkback="block_00021",linknext="block_00023",text={ja={{"「那真是太可惜了。如果我再努力一点，也许天梨就能达到高潮了」",{"rt2"},name={"智宏"}}}},line=121},block_00007={{"text"},linkback="block_00006",linknext="block_00008",text={ja={{"「要出来了！射进天梨的里面！」",{"rt2"},name={"智宏"}}}},line=49},block_00039={{"text"},linkback="block_00038",linknext="block_00040",text={ja={{"生气的天梨直接亲了过来。还蹭了蹭我的脸颊。她虽然很可爱，但一生气就会发火。这个动作，简直就像只小狗一样。太可爱了。",{"rt2"}}}},line=181},block_00018={{"text"},linkback="block_00017",linknext="block_00019",text={ja={{"「嗯？」",{"rt2"},name={"智宏"}}}},line=107},block_00037={{"text"},linkback="block_00036",linknext="block_00038",text={ja={{"「不行，现在天梨脸上还沾着精液。等用纸巾擦干净后再说吧」",{"rt2"},name={"智宏"}}}},line=174},block_00034={{"bg",face="1",path=":ev/ev_ame_h34/",sync=0,set="ev_ame_h34",time=400,file="ev_ame_h34_12"},{"text"},linkback="block_00033",linknext="block_00035",text={ja={{"「嗯、嗯。只要开长能开心就好……虽然有点担心，但是在能达到高潮的时候，我会努力不忍耐地去尝试的……」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01539"}}},line=164},block_00024={{"text"},linkback="block_00023",linknext="block_00025",text={ja={{"「啊啊——不过，下次再有同样的感觉的时候……就算尿尿出来也没关系，我想让你一直忍到最后……」",{"rt2"},name={"智宏"}}}},line=129},block_00026={{"bg",face="1",path=":ev/ev_ame_h34/",sync=0,set="ev_ame_h34",time=400,file="ev_ame_h34_11"},{"text"},linkback="block_00025",linknext="block_00027",text={ja={{"「可、可是，有点害怕呢……毕竟，如果是尿尿的话就麻烦了……」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01535"}}},line=137},block_00011={{"bg",face="1",path=":ev/ev_ame_h34/",sync=0,set="ev_ame_h34",time=400,file="ev_ame_h34_09"},{"text"},linkback="block_00010",linknext="block_00012",text={ja={{"「啊，好厉害……哈啊，哈，啊啊……啊，看起来很舒服的表情……太好了……」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01519"}}},line=72},block_00008={{"text"},linkback="block_00007",linknext="block_00009",text={ja={{"「拿出来吧，可以的……啊，哎呀，射得好多……感觉好舒服！」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01516"}}},line=52},block_00002={{"text"},linkback="block_00001",linknext="block_00003",text={ja={{"「嗯，可以的……！随着智宏喜欢的方式来……！」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01512"}}},line=21},block_00031={{"text"},linkback="block_00030",linknext="block_00032",text={ja={{"虽然一开始对失败的风险感到害怕，但是知道如果能让我高潮，你会有多开心，我突然对此产生了兴趣。",{"rt2"}}}},line=153},block_00013={{"bg",face="1",path=":ev/ev_ame_h34/",sync=0,set="ev_ame_h34",time=400,file="ev_ame_h34_12"},{"text"},linkback="block_00012",linknext="block_00014",text={ja={{"天梨从我的头上温柔地抚摸着我疲惫的脸庞。这个孩子真是无私地……奉献给我。",{"rt2"}}}},line=80},block_00014={{"savetitle",text="ホワイトハウス"},{"text"},linkback="block_00013",linknext="block_00015",text={ja={{"「顺便问一下，天梨之前几次说『忍不住了』是指……？」",{"rt2"},name={"智宏"}}}},line=94},block_00003={{"text"},linkback="block_00002",linknext="block_00004",text={ja={{"为了不让天梨逃走，我将手放在大腿上的位置转移到了腰上，将即将射精的东西猛烈地深入她的内部。",{"rt2"}}}},line=23},block_00020={{"text"},linkback="block_00019",linknext="block_00021",text={ja={{"「而且，和平时想要尿尿的感觉有点不一样……好像被强迫出来一样……而且从全身上都感觉到了……」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01532"}}},line=115},block_00033={{"text"},linkback="block_00032",linknext="block_00034",text={ja={{"「听说女孩子很难达到高潮。如果有下次的机会，我也想试着努力一下」",{"rt2"},name={"智宏"}}}},line=159},block_00009={{"se",stop=1,id=3,time=500},{"lvo",ch="ame",stop=1},{"bg",file="white",path=":bg/",sync=1,id=9,lv=9,time=100},{"bg",alpha=0,file="white",path=":bg/",sync=1,id=9,lv=9,time=300},{"bg",alpha=255,file="white",path=":bg/",sync=1,id=9,lv=9,time=500},{"cgdel",lv=9,id=9},{"bg",file="ev_ame_h34_08",vague=100,sync=0,set="ev_ame_h34",time=1000,path=":ev/ev_ame_h34/"},{"se",file="seエロ_射精中出し5",pan=0,id=9,time=0,vol=100},{"ex",time=100,func="wait"},{"text"},linkback="block_00008",linknext="block_00010",text={ja={{"「啊！哎呀呀呀呀呀呀！啊，知道射出来了……呀呀呀呀呀！」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01517"}}},line=64},block_00000={{"savetitle",text="内務長官"},{"bg",file="ev_ame_h34_07",path=":ev/ev_ame_h34/",sync=0,time=0,set="ev_ame_h34"},{"se",file="seエロ_ピストン：中",id=2,loop=1},{"lvo",ch="ame",file="fem_ame_10102",loop=1},{"bgm",file="bgm20"},{"text"},linknext="block_00001",text={ja={{"如果天梨为了今天做好了准备的话……！就让我顺从这份心意吧……！",{"rt2"}}}},line=15},label={top={label=1,block="block_00000"}},block_00040={{"scene",file="scene19"},{"bgm",stop=1,time=3000},{"se",stop=1,id=-1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",file="black",time=3000,path=":bg/"},{"cacheclear"},{"ex",time=1000,func="wait"},{"bg",file="catch05",path=":cg/",id=9,lv=9,time=500},{"bgm",loop=0,file="bgm96"},{"ex",time=6000,func="wait"},{"bgm",stop=1,time=3000},{"se",stop=1,id=-1,time=3000},{"vostop",stop=1,time=3000},{"ntclear"},{"cgdel",id=-1},{"bg",file="black",time=3000,path=":bg/"},{"cacheclear"},{"ex",time=1000,func="wait"},{"excall",file="05_天梨17"},{"text"},linkback="block_00039",line=191},block_00015={{"bg",face="1",path=":ev/ev_ame_h34/",sync=0,set="ev_ame_h34",time=400,file="ev_ame_h34_09"},{"text"},linkback="block_00014",linknext="block_00016",text={ja={{"「嗯」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01529"}}},line=99},block_00036={{"bg",file="zev_ame_h34_09",path=":ev/ev_ame_h34/",ay=720,set="ev_ame_h34",by=-360,bx=-640,y=250,x=500,time=500,ax=1280},{"text"},linkback="block_00035",linknext="block_00037",text={ja={{"「嘿嘿嘿，开长真温柔……那、那个，可以亲吻吗？」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01540"}}},line=171},block_00035={{"text"},linkback="block_00034",linknext="block_00036",text={ja={{"她真是一个全心全意为我付出的女孩子。我伸出手，轻轻抚摸着她的脸颊。",{"rt2"}}}},line=166},block_00032={{"text"},linkback="block_00031",linknext="block_00033",text={ja={{"「当我高潮的时候，开长也会感到舒服呢……额……那、那个，我也想试试努力一下……」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01538"}}},line=156},block_00028={{"text"},linkback="block_00027",linknext="block_00029",text={ja={{"「可、可是智宏这么说，我很抱歉也很害羞……就算你说可以尿在你身上，我还是有点犹豫」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01536"}}},line=143},block_00004={{"se",stop=1,id=2,time=500},{"lvo",ch="ame",stop=1},{"bg",face="1",path=":ev/ev_ame_h34/",sync=0,set="ev_ame_h34",time=400,file="ev_ame_h34_18"},{"se",file="seエロ_ピストン：強",id=3,loop=1},{"lvo",ch="ame",file="fem_ame_10104",loop=1},{"text"},linkback="block_00003",linknext="block_00005",text={ja={{"「啊、呀、啊啊、啊、嗯！啊、剧烈地、啊啊、啊、呀！啊啊、啊、哈啊、啊、呀！」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01513"}}},line=37},block_00019={{"bg",face="1",path=":ev/ev_ame_h34/",sync=0,set="ev_ame_h34",time=400,file="ev_ame_h34_10"},{"text"},linkback="block_00018",linknext="block_00020",text={ja={{"「啊，没，没关系！我想我一直忍到最后了……但是，明明事先去过洗手间了，感觉有点奇怪……」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01531"}}},line=112},block_00023={{"bg",face="1",path=":ev/ev_ame_h34/",sync=0,set="ev_ame_h34",time=400,file="ev_ame_h34_09"},{"text"},linkback="block_00022",linknext="block_00024",text={ja={{"「但、但是，我也不确定这到底是不是高潮的感觉！如果真的尿出来就麻烦了……」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01534"}}},line=126},block_00027={{"text"},linkback="block_00026",linknext="block_00028",text={ja={{"「如果是尿尿的话，那个时候你可以尿在我身上。虽然每次都这样有点麻烦，但失败也是成功的一部分……」",{"rt2"},name={"智宏"}}}},line=140},block_00021={{"text"},linkback="block_00020",linknext="block_00022",text={ja={{"「难道这就是所谓的高潮感觉吗……」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01533"}}},line=118},block_00030={{"bg",face="1",path=":ev/ev_ame_h34/",sync=0,set="ev_ame_h34",time=400,file="ev_ame_h34_10"},{"text"},linkback="block_00029",linknext="block_00031",text={ja={{"「诶！真的吗！？」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01537"}}},line=151},block_00012={{"text"},linkback="block_00011",linknext="block_00013",text={ja={{"在进入天梨的体内的同时，我射出了一次刺痛的射精，在幸福的巅峰中我四肢无力地放松下来。",{"rt2"}}}},line=74},block_00029={{"text"},linkback="block_00028",linknext="block_00030",text={ja={{"「但是，听说女孩子高潮的时候收缩最舒服……」",{"rt2"},name={"智宏"}}}},line=146},block_00025={{"text"},linkback="block_00024",linknext="block_00026",text={ja={{"「如果天梨能够高潮的话，我想一直让她高潮到最后。如果我们两个一起高潮，我也会更加舒服……彼此之间的喜欢会更深」",{"rt2"},name={"智宏"}}}},line=132},block_00016={{"text"},linkback="block_00015",linknext="block_00017",text={ja={{"这是无意识中说出的话吗……不过，天梨害羞地移开了目光。看来她自己也意识到了自己说出来了。",{"rt2"}}}},line=101},block_00001={{"text"},linkback="block_00000",linknext="block_00002",text={ja={{"「那就毫不客气地……！射进天梨的里面……！」",{"rt2"},name={"智宏"}}}},line=18},block_00005={{"text"},linkback="block_00004",linknext="block_00006",text={ja={{"「呀、啊、好厉害！啊、这样拼命的智宏，我喜欢看到……啊、啊啊、呀！好深哦！」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01514"}}},line=40},block_00017={{"text"},linkback="block_00016",linknext="block_00018",text={ja={{"「嗯……当会长在帮我的时候，好像有好几次忍尿的感觉……」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01530"}}},line=104},block_00010={{"text"},linkback="block_00009",linknext="block_00011",text={ja={{"「嗯，嗯，射了好多……啊，啊啊，肚子里面……感觉到了……啊啊，啊，哎，啊啊……」",{"rt2"},name={"天梨"}}},vo={{"vo",ch="ame",file="fem_ame_01518"}}},line=67}}