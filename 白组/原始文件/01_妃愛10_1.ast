			vo = {
				{"vo", file="fem_him_00008", ch="him"},
			},
			ja = {
				{
					name = {"妃愛のマネージャー"},
					"「理由……吗。用一句话概括就是，一个怀揣梦想初出茅庐的新人，看到了演艺圈肮脏的内幕。写好辞职信的第二天，被闪烁着纯真光芒的她所拯救了。」",
					{"rt2"},
				},
			},
		},
		linkback = "block_00099",
		linknext = "block_00101",
		line = 472,
	},
	block_00101 = {
		{"text"},
		text = {
			ja = {
